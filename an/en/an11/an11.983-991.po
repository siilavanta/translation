msgctxt "an11.983-991:1.1"
msgid "Aṅguttara Nikāya 11"
msgstr "Numbered Discourses 11"

#. </p><p class="pannasaka">
msgctxt "an11.983-991:2.1"
msgid "Paṭhamapaṇṇāsaka"
msgstr "The First Fifty"

#. </p><p class="vagga">
msgctxt "an11.983-991:3.1"
msgid "Rāgapeyyāla"
msgstr "4. Abbreviated Texts Beginning with Greed"

#. </p><h1>
msgctxt "an11.983-991:4.1"
msgid "AN 11.983–991"
msgstr "983–991"

#. </h1></div><p><a class="sc" id="1"></a>
msgctxt "an11.983-991:5.1"
msgid "“Rāgassa, bhikkhave, pariññāya…"
msgstr "“For the complete understanding of greed …"

msgctxt "an11.983-991:5.2"
msgid "parikkhayāya…"
msgstr "complete ending …"

msgctxt "an11.983-991:5.3"
msgid "pahānāya…"
msgstr "giving up …"

msgctxt "an11.983-991:5.4"
msgid "khayāya…"
msgstr "ending …"

msgctxt "an11.983-991:5.5"
msgid "vayāya…"
msgstr "vanishing …"

msgctxt "an11.983-991:5.6"
msgid "virāgāya…"
msgstr "fading away …"

msgctxt "an11.983-991:5.7"
msgid "nirodhāya…"
msgstr "cessation …"

msgctxt "an11.983-991:5.8"
msgid "cāgāya…"
msgstr "giving away …"

msgctxt "an11.983-991:5.9"
msgid "paṭinissaggāya…"
msgstr "letting go of negligence"

msgctxt "an11.983-991:5.10"
msgid "ime ekādasa dhammā bhāvetabbā”."
msgstr "these eleven things should be developed.”"
