#. HTML: <section class="sutta" id="pli-tv-kd3"><article><div class="hgroup"><p class="collection">
msgctxt "pli-tv-kd3:0.1"
msgid "Theravāda Vinayapiṭaka"
msgstr ""

#. HTML: </p><p class="division">
msgctxt "pli-tv-kd3:0.2"
msgid "Mahāvagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-kd3:0.3"
msgid "Vassūpanāyikakkhandhaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-kd3:1.1.0"
msgid "1. Vassūpanāyikānujānana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli1.137
msgctxt "pli-tv-kd3:1.1.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pli-tv-kd3:1.1.2"
msgid "Tena kho pana samayena bhagavatā bhikkhūnaṃ vassāvāso apaññatto hoti."
msgstr ""

#. VAR: Tedha → te idha (c1)
msgctxt "pli-tv-kd3:1.1.3"
msgid "Tedha bhikkhū hemantampi gimhampi vassampi cārikaṃ caranti."
msgstr ""

msgctxt "pli-tv-kd3:1.2.1"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:1.2.2"
msgid "“kathañhi nāma samaṇā sakyaputtiyā hemantampi gimhampi vassampi cārikaṃ carissanti, haritāni tiṇāni sammaddantā, ekindriyaṃ jīvaṃ viheṭhentā, bahū khuddake pāṇe saṅghātaṃ āpādentā."
msgstr ""

#. VAR: saṅkasāyissanti → saṅkāsayissanti (bj, s1-3) | saṅkāpayissanti (pts1)
msgctxt "pli-tv-kd3:1.2.3"
msgid "Ime hi nāma aññatitthiyā durakkhātadhammā vassāvāsaṃ allīyissanti saṅkasāyissanti."
msgstr ""

msgctxt "pli-tv-kd3:1.2.4"
msgid "Ime hi nāma sakuntakā rukkhaggesu kulāvakāni karitvā vassāvāsaṃ allīyissanti saṅkasāyissanti."
msgstr ""

msgctxt "pli-tv-kd3:1.2.5"
msgid "Ime pana samaṇā sakyaputtiyā hemantampi gimhampi vassampi cārikaṃ caranti, haritāni tiṇāni sammaddantā, ekindriyaṃ jīvaṃ viheṭhentā, bahū khuddake pāṇe saṅghātaṃ āpādentā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-kd3:1.3.1"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd3:1.3.2"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:1.3.3"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd3:1.3.4"
msgid "“anujānāmi, bhikkhave, vassaṃ upagantun”ti."
msgstr ""

msgctxt "pli-tv-kd3:2.1.1"
msgid "Atha kho bhikkhūnaṃ etadahosi—"
msgstr ""

msgctxt "pli-tv-kd3:2.1.2"
msgid "“kadā nu kho vassaṃ upagantabban”ti?"
msgstr ""

msgctxt "pli-tv-kd3:2.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:2.1.4"
msgid "“Anujānāmi, bhikkhave, vassāne vassaṃ upagantun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-kd3:2.2.1"
msgid "Atha kho bhikkhūnaṃ etadahosi—"
msgstr ""

msgctxt "pli-tv-kd3:2.2.2"
msgid "“kati nu kho vassūpanāyikā”ti?"
msgstr ""

msgctxt "pli-tv-kd3:2.2.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:2.2.4"
msgid "“Dvemā, bhikkhave, vassūpanāyikā—"
msgstr ""

msgctxt "pli-tv-kd3:2.2.5"
msgid "purimikā, pacchimikā."
msgstr ""

msgctxt "pli-tv-kd3:2.2.6"
msgid "Aparajjugatāya āsāḷhiyā purimikā upagantabbā, māsagatāya āsāḷhiyā pacchimikā upagantabbā—"
msgstr ""

msgctxt "pli-tv-kd3:2.2.7"
msgid "imā kho, bhikkhave, dve vassūpanāyikā”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:3.1.0"
msgid "2. Vassānecārikāpaṭikkhepādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc4, pts-vp-pli1.138
msgctxt "pli-tv-kd3:3.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū vassaṃ upagantvā antarāvassaṃ cārikaṃ caranti."
msgstr ""

msgctxt "pli-tv-kd3:3.1.2"
msgid "Manussā tatheva ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:3.1.3"
msgid "“kathañhi nāma samaṇā sakyaputtiyā hemantampi gimhampi vassampi cārikaṃ carissanti, haritāni tiṇāni sammaddantā, ekindriyaṃ jīvaṃ viheṭhentā, bahū khuddake pāṇe saṅghātaṃ āpādentā."
msgstr ""

msgctxt "pli-tv-kd3:3.1.4"
msgid "Ime hi nāma aññatitthiyā durakkhātadhammā vassāvāsaṃ allīyissanti saṅkasāyissanti."
msgstr ""

msgctxt "pli-tv-kd3:3.1.5"
msgid "Ime hi nāma sakuntakā rukkhaggesu kulāvakāni karitvā vassāvāsaṃ allīyissanti saṅkasāyissanti."
msgstr ""

msgctxt "pli-tv-kd3:3.1.6"
msgid "Ime pana samaṇā sakyaputtiyā hemantampi gimhampi vassampi cārikaṃ caranti, haritāni tiṇāni sammaddantā, ekindriyaṃ jīvaṃ viheṭhentā, bahū khuddake pāṇe saṅghātaṃ āpādentā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-kd3:3.2.1"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd3:3.2.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd3:3.2.4"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:3.2.5"
msgid "“kathañhi nāma chabbaggiyā bhikkhū vassaṃ upagantvā antarāvassaṃ cārikaṃ carissantī”ti?"
msgstr ""

msgctxt "pli-tv-kd3:3.2.6"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd3:3.2.8"
msgid "atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd3:3.2.9"
msgid "“na, bhikkhave, vassaṃ upagantvā purimaṃ vā temāsaṃ pacchimaṃ vā temāsaṃ avasitvā cārikā pakkamitabbā."
msgstr ""

msgctxt "pli-tv-kd3:3.2.10"
msgid "Yo pakkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-kd3:4.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū na icchanti vassaṃ upagantuṃ."
msgstr ""

msgctxt "pli-tv-kd3:4.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:4.1.3"
msgid "“Na, bhikkhave, vassaṃ na upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:4.1.4"
msgid "Yo na upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-kd3:4.2.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū tadahu vassūpanāyikāya vassaṃ anupagantukāmā sañcicca āvāsaṃ atikkamanti."
msgstr ""

msgctxt "pli-tv-kd3:4.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:4.2.3"
msgid "“Na, bhikkhave, tadahu vassūpanāyikāya vassaṃ anupagantukāmena sañcicca āvāso atikkamitabbo."
msgstr ""

msgctxt "pli-tv-kd3:4.2.4"
msgid "Yo atikkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-kd3:4.3.1"
msgid "Tena kho pana samayena rājā māgadho seniyo bimbisāro vassaṃ ukkaḍḍhitukāmo bhikkhūnaṃ santike dūtaṃ pāhesi—"
msgstr ""

msgctxt "pli-tv-kd3:4.3.2"
msgid "“yadi panāyyā āgame juṇhe vassaṃ upagaccheyyun”ti."
msgstr ""

msgctxt "pli-tv-kd3:4.3.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:4.3.4"
msgid "“Anujānāmi, bhikkhave, rājūnaṃ anuvattitun”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:5.1.0"
msgid "3. Sattāhakaraṇīyānujānana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc9, pts-vp-pli1.139
msgctxt "pli-tv-kd3:5.1.1"
msgid "Atha kho bhagavā rājagahe yathābhirantaṃ viharitvā yena sāvatthi tena cārikaṃ pakkāmi."
msgstr ""

msgctxt "pli-tv-kd3:5.1.2"
msgid "Anupubbena cārikaṃ caramāno yena sāvatthi tadavasari."
msgstr ""

msgctxt "pli-tv-kd3:5.1.3"
msgid "Tatra sudaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-kd3:5.1.4"
msgid "Tena kho pana samayena kosalesu janapade udenena upāsakena saṃghaṃ uddissa vihāro kārāpito hoti."
msgstr ""

msgctxt "pli-tv-kd3:5.1.5"
msgid "So bhikkhūnaṃ santike dūtaṃ pāhesi—"
msgstr ""

msgctxt "pli-tv-kd3:5.1.6"
msgid "“āgacchantu bhadantā, icchāmi dānañca dātuṃ, dhammañca sotuṃ, bhikkhū ca passitun”ti."
msgstr ""

msgctxt "pli-tv-kd3:5.2.1"
msgid "Bhikkhū evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-kd3:5.2.2"
msgid "“bhagavatā, āvuso, paññattaṃ—‘"
msgstr ""

msgctxt "pli-tv-kd3:5.2.3"
msgid "na vassaṃ upagantvā purimaṃ vā temāsaṃ pacchimaṃ vā temāsaṃ avasitvā cārikā pakkamitabbā’ti."
msgstr ""

msgctxt "pli-tv-kd3:5.2.4"
msgid "Āgametu udeno upāsako, yāva bhikkhū vassaṃ vasanti."
msgstr ""

msgctxt "pli-tv-kd3:5.2.5"
msgid "Vassaṃvuṭṭhā āgamissanti."
msgstr ""

msgctxt "pli-tv-kd3:5.2.6"
msgid "Sace panassa accāyikaṃ karaṇīyaṃ, tattheva āvāsikānaṃ bhikkhūnaṃ santike vihāraṃ patiṭṭhāpetū”ti."
msgstr ""

msgctxt "pli-tv-kd3:5.3.1"
msgid "Udeno upāsako ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-kd3:5.3.2"
msgid "“kathañhi nāma bhadantā mayā pahite na āgacchissanti."
msgstr ""

msgctxt "pli-tv-kd3:5.3.3"
msgid "Ahañhi dāyako kārako saṃghupaṭṭhāko”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-kd3:5.3.4"
msgid "Assosuṃ kho bhikkhū udenassa upāsakassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pli-tv-kd3:5.3.5"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:5.4.1"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd3:5.4.2"
msgid "“anujānāmi, bhikkhave, sattannaṃ sattāhakaraṇīyena pahite gantuṃ, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:5.4.3"
msgid "Bhikkhussa, bhikkhuniyā, sikkhamānāya, sāmaṇerassa, sāmaṇeriyā, upāsakassa, upāsikāya—"
msgstr ""

msgctxt "pli-tv-kd3:5.4.4"
msgid "anujānāmi, bhikkhave, imesaṃ sattannaṃ sattāhakaraṇīyena pahite gantuṃ, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:5.4.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-kd3:5.5.1"
msgid "Idha pana, bhikkhave, upāsakena saṃghaṃ uddissa vihāro kārāpito hoti."
msgstr ""

msgctxt "pli-tv-kd3:5.5.2"
msgid "So ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:5.5.3"
msgid "āgacchantu bhadantā, icchāmi dānañca dātuṃ, dhammañca sotuṃ, bhikkhū ca passitun’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, pahite, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:5.5.4"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc12, pts-vp-pli1.140
msgctxt "pli-tv-kd3:5.6.1"
msgid "Idha pana, bhikkhave, upāsakena saṃghaṃ uddissa aḍḍhayogo kārāpito hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.3"
msgid "pāsādo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.4"
msgid "hammiyaṃ kārāpitaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.5"
msgid "guhā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.6"
msgid "pariveṇaṃ kārāpitaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.7"
msgid "koṭṭhako kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.8"
msgid "upaṭṭhānasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.9"
msgid "aggisālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.10"
msgid "kappiyakuṭi kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.11"
msgid "vaccakuṭi kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.12"
msgid "caṅkamo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.13"
msgid "caṅkamanasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.14"
msgid "udapāno kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.15"
msgid "udapānasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.16"
msgid "jantāgharaṃ kārāpitaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.17"
msgid "jantāgharasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.18"
msgid "pokkharaṇī kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.19"
msgid "maṇḍapo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.20"
msgid "ārāmo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.6.21"
msgid "ārāmavatthu kārāpitaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd3:5.6.22"
msgid "So ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:5.6.23"
msgid "āgacchantu bhadantā, icchāmi dānañca dātuṃ, dhammañca sotuṃ, bhikkhū ca passitun’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, pahite, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:5.6.24"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-kd3:5.7.1"
msgid "Idha pana, bhikkhave, upāsakena sambahule bhikkhū uddissa … pe …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.3"
msgid "ekaṃ bhikkhuṃ uddissa vihāro kārāpito hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.5"
msgid "aḍḍhayogo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.6"
msgid "pāsādo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.7"
msgid "hammiyaṃ kārāpitaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.8"
msgid "guhā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.9"
msgid "pariveṇaṃ kārāpitaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.10"
msgid "koṭṭhako kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.11"
msgid "upaṭṭhānasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.12"
msgid "aggisālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.13"
msgid "kappiyakuṭi kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.14"
msgid "vaccakuṭi kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.15"
msgid "caṅkamo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.16"
msgid "caṅkamanasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.17"
msgid "udapāno kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.18"
msgid "udapānasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.19"
msgid "jantāgharaṃ kārāpitaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.20"
msgid "jantāgharasālā kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.21"
msgid "pokkharaṇī kārāpitā hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.22"
msgid "maṇḍapo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.23"
msgid "ārāmo kārāpito hoti …"
msgstr ""

msgctxt "pli-tv-kd3:5.7.24"
msgid "ārāmavatthu kārāpitaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd3:5.7.25"
msgid "So ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—"
msgstr ""

msgctxt "pli-tv-kd3:5.7.26"
msgid "‘āgacchantu bhadantā, icchāmi dānañca dātuṃ, dhammañca sotuṃ, bhikkhū ca passitun’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, pahite, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:5.7.27"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

