msgctxt "pli-tv-kd3:6.12.4"
msgid "‘gilānabhattaṃ vā pariyesissāmi, gilānupaṭṭhākabhattaṃ vā pariyesissāmi, gilānabhesajjaṃ vā pariyesissāmi, pucchissāmi vā, upaṭṭhahissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.12.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc33
msgctxt "pli-tv-kd3:6.13.1"
msgid "Idha pana, bhikkhave, bhikkhuniyā anabhirati uppannā hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.13.2"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.13.3"
msgid "anabhirati me uppannā, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.13.4"
msgid "anabhirataṃ vūpakāsessāmi vā, vūpakāsāpessāmi vā, dhammakathaṃ vāssā karissāmī’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.13.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-kd3:6.14.1"
msgid "Idha pana, bhikkhave, bhikkhuniyā kukkuccaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.14.2"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.14.3"
msgid "kukkuccaṃ me uppannaṃ, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.14.4"
msgid "kukkuccaṃ vinodessāmi vā, vinodāpessāmi vā, dhammakathaṃ vāssā karissāmī’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.14.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-kd3:6.15.1"
msgid "Idha pana, bhikkhave, bhikkhuniyā diṭṭhigataṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.15.2"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.15.3"
msgid "diṭṭhigataṃ me uppannaṃ, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.15.4"
msgid "diṭṭhigataṃ vivecessāmi vā, vivecāpessāmi vā, dhammakathaṃ vāssā karissāmī’ti."
