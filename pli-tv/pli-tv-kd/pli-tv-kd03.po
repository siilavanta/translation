msgid "Tā vuttā tā vakkhanti “garuko kho, bhaginiyo, saṃghabhedo vutto bhagavatā;"
msgstr ""

msgctxt "pli-tv-kd3:11.13.8"
msgid "mā bhaginīnaṃ saṃghabhedo ruccitthā”ti."
msgstr ""

msgctxt "pli-tv-kd3:11.13.9"
msgid "Karissanti tāsaṃ vacanaṃ, sussūsissanti, sotaṃ odahissantī’ti, pakkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:11.13.10"
msgid "Anāpatti vassacchedassā”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:12.1.0"
msgid "9. Vajādīsuvassūpagamana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc89, pts-vp-pli1.152
msgctxt "pli-tv-kd3:12.1.1"
msgid "Tena kho pana samayena aññataro bhikkhu vaje vassaṃ upagantukāmo hoti."
msgstr ""

msgctxt "pli-tv-kd3:12.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.1.3"
msgid "“Anujānāmi, bhikkhave, vaje vassaṃ upagantun”ti."
msgstr ""

msgctxt "pli-tv-kd3:12.1.4"
msgid "Vajo vuṭṭhāsi."
msgstr ""

msgctxt "pli-tv-kd3:12.1.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.1.6"
msgid "“Anujānāmi, bhikkhave, yena vajo tena gantun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc90
msgctxt "pli-tv-kd3:12.2.1"
msgid "Tena kho pana samayena aññataro bhikkhu upakaṭṭhāya vassūpanāyikāya satthena gantukāmo hoti."
msgstr ""

msgctxt "pli-tv-kd3:12.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.2.3"
msgid "“Anujānāmi, bhikkhave, satthe vassaṃ upagantun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc91
msgctxt "pli-tv-kd3:12.2.4"
msgid "Tena kho pana samayena aññataro bhikkhu upakaṭṭhāya vassūpanāyikāya nāvāya gantukāmo hoti."
msgstr ""

msgctxt "pli-tv-kd3:12.2.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.2.6"
msgid "“Anujānāmi, bhikkhave, nāvāya vassaṃ upagantun”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:12.3.0"
msgid "10. Vassaṃanupagantabbaṭṭhāna"
msgstr ""

#. HTML: </h2><p>
#. REF: sc92
msgctxt "pli-tv-kd3:12.3.1"
msgid "Tena kho pana samayena bhikkhū rukkhasusire vassaṃ upagacchanti."
msgstr ""

msgctxt "pli-tv-kd3:12.3.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:12.3.3"
msgid "“seyyathāpi pisācillikā”ti."
msgstr ""

msgctxt "pli-tv-kd3:12.3.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.3.5"
msgid "“Na, bhikkhave, rukkhasusire vassaṃ upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.3.6"
msgid "Yo upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc93
msgctxt "pli-tv-kd3:12.4.1"
msgid "Tena kho pana samayena bhikkhū rukkhaviṭabhiyā vassaṃ upagacchanti."
msgstr ""

msgctxt "pli-tv-kd3:12.4.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:12.4.3"
msgid "“seyyathāpi migaluddakā”ti."
msgstr ""

msgctxt "pli-tv-kd3:12.4.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.4.5"
msgid "“Na, bhikkhave, rukkhaviṭabhiyā vassaṃ upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.4.6"
msgid "Yo upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc94
msgctxt "pli-tv-kd3:12.5.1"
msgid "Tena kho pana samayena bhikkhū ajjhokāse vassaṃ upagacchanti."
msgstr ""

msgctxt "pli-tv-kd3:12.5.2"
msgid "Deve vassante rukkhamūlampi nibbakosampi upadhāvanti."
msgstr ""

msgctxt "pli-tv-kd3:12.5.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.5.4"
msgid "“Na, bhikkhave, ajjhokāse vassaṃ upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.5.5"
msgid "Yo upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc95
msgctxt "pli-tv-kd3:12.6.1"
msgid "Tena kho pana samayena bhikkhū asenāsanikā vassaṃ upagacchanti."
msgstr ""

msgctxt "pli-tv-kd3:12.6.2"
msgid "Sītenapi kilamanti, uṇhenapi kilamanti."
msgstr ""

msgctxt "pli-tv-kd3:12.6.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.6.4"
msgid "“Na, bhikkhave, asenāsanikena vassaṃ upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.6.5"
msgid "Yo upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc96
msgctxt "pli-tv-kd3:12.7.1"
msgid "Tena kho pana samayena bhikkhū chavakuṭikāya vassaṃ upagacchanti."
msgstr ""

msgctxt "pli-tv-kd3:12.7.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:12.7.3"
msgid "“seyyathāpi chavaḍāhakā”ti."
msgstr ""

msgctxt "pli-tv-kd3:12.7.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.7.5"
msgid "“Na, bhikkhave, chavakuṭikāya vassaṃ upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.7.6"
msgid "Yo upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc97
msgctxt "pli-tv-kd3:12.8.1"
msgid "Tena kho pana samayena bhikkhū chatte vassaṃ upagacchanti."
msgstr ""

msgctxt "pli-tv-kd3:12.8.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:12.8.3"
msgid "“seyyathāpi gopālakā”ti."
msgstr ""

msgctxt "pli-tv-kd3:12.8.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.8.5"
msgid "“Na, bhikkhave, chatte vassaṃ upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.8.6"
msgid "Yo upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc98, pts-vp-pli1.153
msgctxt "pli-tv-kd3:12.9.1"
msgid "Tena kho pana samayena bhikkhū cāṭiyā vassaṃ upagacchanti."
msgstr ""

msgctxt "pli-tv-kd3:12.9.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:12.9.3"
msgid "“seyyathāpi titthiyā”ti."
msgstr ""

msgctxt "pli-tv-kd3:12.9.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.9.5"
msgid "“Na, bhikkhave, cāṭiyā vassaṃ upagantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:12.9.6"
msgid "Yo upagaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:13.1.0"
msgid "11. Adhammikakatikā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc99
msgctxt "pli-tv-kd3:13.1.1"
msgid "Tena kho pana samayena sāvatthiyā saṃghena evarūpā katikā katā hoti—"
msgstr ""

msgctxt "pli-tv-kd3:13.1.2"
msgid "“antarāvassaṃ na pabbājetabban”ti."
msgstr ""

msgctxt "pli-tv-kd3:13.1.3"
msgid "Visākhāya migāramātuyā nattā bhikkhū upasaṅkamitvā pabbajjaṃ yāci."
msgstr ""

msgctxt "pli-tv-kd3:13.1.4"
msgid "Bhikkhū evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-kd3:13.1.5"
msgid "“saṃghena kho, āvuso, evarūpā katikā katā ‘antarāvassaṃ na pabbājetabban’ti."
msgstr ""

msgctxt "pli-tv-kd3:13.1.6"
msgid "Āgamehi, āvuso, yāva bhikkhū vassaṃ vasanti."
msgstr ""

msgctxt "pli-tv-kd3:13.1.7"
msgid "Vassaṃvuṭṭhā pabbājessantī”ti."
msgstr ""

msgctxt "pli-tv-kd3:13.1.8"
msgid "Atha kho te bhikkhū vassaṃvuṭṭhā visākhāya migāramātuyā nattāraṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd3:13.1.9"
msgid "“ehi dāni, āvuso, pabbajāhī”ti."
msgstr ""

msgctxt "pli-tv-kd3:13.1.10"
msgid "So evamāha—“"
msgstr ""

#. VAR: abhirameyyāmahaṃ → abhirameyyañcāhaṃ (bj) | abhirameyyāmāhaṃ (pts1)
msgctxt "pli-tv-kd3:13.1.11"
msgid "sacāhaṃ, bhante, pabbajito assaṃ, abhirameyyāmahaṃ."
msgstr ""

msgctxt "pli-tv-kd3:13.1.12"
msgid "Na dānāhaṃ, bhante, pabbajissāmī”ti."
msgstr ""

msgctxt "pli-tv-kd3:13.2.1"
msgid "Visākhā migāramātā ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-kd3:13.2.2"
msgid "“kathañhi nāma ayyā evarūpaṃ katikaṃ karissanti—‘"
msgstr ""

msgctxt "pli-tv-kd3:13.2.3"
msgid "na antarāvassaṃ pabbājetabban’ti."
msgstr ""

msgctxt "pli-tv-kd3:13.2.4"
msgid "Kaṃ kālaṃ dhammo na caritabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc100
msgctxt "pli-tv-kd3:13.2.5"
msgid "Assosuṃ kho bhikkhū visākhāya migāramātuyā ujjhāyantiyā khiyyantiyā vipācentiyā."
msgstr ""

msgctxt "pli-tv-kd3:13.2.6"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:13.2.7"
msgid "“Na, bhikkhave, evarūpā katikā kātabbā—‘"
msgstr ""

msgctxt "pli-tv-kd3:13.2.8"
msgid "na antarāvassaṃ pabbājetabban’ti."
msgstr ""

msgctxt "pli-tv-kd3:13.2.9"
msgid "Yo kareyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:14.1.0"
msgid "12. Paṭissavadukkaṭāpatti"
msgstr ""

#. HTML: </h2><p>
#. REF: sc101
msgctxt "pli-tv-kd3:14.1.1"
msgid "Tena kho pana samayena āyasmatā upanandena sakyaputtena rañño pasenadissa kosalassa vassāvāso paṭissuto hoti purimikāya."
msgstr ""

msgctxt "pli-tv-kd3:14.1.2"
msgid "So taṃ āvāsaṃ gacchanto addasa antarāmagge dve āvāse bahucīvarake."
msgstr ""

msgctxt "pli-tv-kd3:14.1.3"
msgid "Tassa etadahosi—"
msgstr ""

msgctxt "pli-tv-kd3:14.1.4"
msgid "“yannūnāhaṃ imesu dvīsu āvāsesu vassaṃ vaseyyaṃ."
msgstr ""

#. VAR: bahuṃ cīvaraṃ → bahucīvaraṃ (si, pts1, mr)
msgctxt "pli-tv-kd3:14.1.5"
msgid "Evaṃ me bahuṃ cīvaraṃ uppajjissatī”ti."
msgstr ""

msgctxt "pli-tv-kd3:14.1.6"
msgid "So tesu dvīsu āvāsesu vassaṃ vasi."
msgstr ""

msgctxt "pli-tv-kd3:14.1.7"
msgid "Rājā pasenadi kosalo ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-kd3:14.1.8"
msgid "“kathañhi nāma ayyo upanando sakyaputto amhākaṃ vassāvāsaṃ paṭissuṇitvā visaṃvādessati."
msgstr ""

msgctxt "pli-tv-kd3:14.1.9"
msgid "Nanu bhagavatā anekapariyāyena musāvādo garahito, musāvādā veramaṇī pasatthā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc102, pts-vp-pli1.154
msgctxt "pli-tv-kd3:14.2.1"
msgid "Assosuṃ kho bhikkhū rañño pasenadissa kosalassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pli-tv-kd3:14.2.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd3:14.2.4"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd3:14.2.5"
msgid "“kathañhi nāma āyasmā upanando sakyaputto rañño pasenadissa kosalassa vassāvāsaṃ paṭissuṇitvā visaṃvādessati."
msgstr ""

msgctxt "pli-tv-kd3:14.2.6"
msgid "Nanu bhagavatā anekapariyāyena musāvādo garahito, musāvādā veramaṇī pasatthā”ti."
msgstr ""

msgctxt "pli-tv-kd3:14.3.1"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd3:14.3.3"
msgid "atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ upanandaṃ sakyaputtaṃ paṭipucchi—"
msgstr ""

msgctxt "pli-tv-kd3:14.3.4"
msgid "“saccaṃ kira tvaṃ, upananda, rañño pasenadissa kosalassa vassāvāsaṃ paṭissuṇitvā visaṃvādesī”ti?"
msgstr ""

msgctxt "pli-tv-kd3:14.3.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd3:14.3.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-kd3:14.3.8"
msgid "kathañhi nāma tvaṃ, moghapurisa, rañño pasenadissa kosalassa vassāvāsaṃ paṭissuṇitvā visaṃvādessasi."
msgstr ""

msgctxt "pli-tv-kd3:14.3.9"
msgid "Nanu mayā, moghapurisa, anekapariyāyena musāvādo garahito, musāvādā veramaṇī pasatthā."
msgstr ""

msgctxt "pli-tv-kd3:14.3.10"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-kd3:14.3.12"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd3:14.3.14"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

#. HTML: </p><p>
#. REF: sc103
msgctxt "pli-tv-kd3:14.4.1"
msgid "“Idha pana, bhikkhave, bhikkhunā vassāvāso paṭissuto hoti purimikāya."
msgstr ""

msgctxt "pli-tv-kd3:14.4.2"
msgid "So taṃ āvāsaṃ gacchanto passati antarāmagge dve āvāse bahucīvarake."
msgstr ""

msgctxt "pli-tv-kd3:14.4.3"
msgid "Tassa evaṃ hoti—"
msgstr ""

msgctxt "pli-tv-kd3:14.4.4"
msgid "‘yannūnāhaṃ imesu dvīsu āvāsesu vassaṃ vaseyyaṃ."
msgstr ""

msgctxt "pli-tv-kd3:14.4.5"
msgid "Evaṃ me bahuṃ cīvaraṃ uppajjissatī’ti."
msgstr ""

msgctxt "pli-tv-kd3:14.4.6"
msgid "So tesu dvīsu āvāsesu vassaṃ vasati."
msgstr ""

msgctxt "pli-tv-kd3:14.4.7"
msgid "Tassa, bhikkhave, bhikkhuno purimikā ca na paññāyati, paṭissave ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc104
msgctxt "pli-tv-kd3:14.5.1"
msgid "Idha pana, bhikkhave, bhikkhunā vassāvāso paṭissuto hoti purimikāya."
msgstr ""

#. VAR: pāṭipade → pāṭipadena (pts1, mr)
msgctxt "pli-tv-kd3:14.5.2"
msgid "So taṃ āvāsaṃ gacchanto bahiddhā uposathaṃ karoti, pāṭipade vihāraṃ upeti, senāsanaṃ paññapeti, pānīyaṃ paribhojanīyaṃ upaṭṭhāpeti, pariveṇaṃ sammajjati."
msgstr ""

msgctxt "pli-tv-kd3:14.5.3"
msgid "So tadaheva akaraṇīyo pakkamati."
msgstr ""

msgctxt "pli-tv-kd3:14.5.4"
msgid "Tassa, bhikkhave, bhikkhuno purimikā ca na paññāyati, paṭissave ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc105
msgctxt "pli-tv-kd3:14.5.5"
msgid "Idha pana, bhikkhave, bhikkhunā vassāvāso paṭissuto hoti purimikāya."
msgstr ""

msgctxt "pli-tv-kd3:14.5.6"
msgid "So taṃ āvāsaṃ gacchanto bahiddhā uposathaṃ karoti, pāṭipade vihāraṃ upeti, senāsanaṃ paññapeti, pānīyaṃ paribhojanīyaṃ upaṭṭhāpeti, pariveṇaṃ sammajjati."
msgstr ""

msgctxt "pli-tv-kd3:14.5.7"
msgid "So tadaheva sakaraṇīyo pakkamati."
msgstr ""

msgctxt "pli-tv-kd3:14.5.8"
msgid "Tassa, bhikkhave, bhikkhuno purimikā ca na paññāyati, paṭissave ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc106
msgctxt "pli-tv-kd3:14.6.1"
msgid "Idha pana, bhikkhave, bhikkhunā vassāvāso paṭissuto hoti purimikāya."
msgstr ""

msgctxt "pli-tv-kd3:14.6.2"
msgid "So taṃ āvāsaṃ gacchanto bahiddhā uposathaṃ karoti, pāṭipade vihāraṃ upeti, senāsanaṃ paññapeti, pānīyaṃ paribhojanīyaṃ upaṭṭhāpeti, pariveṇaṃ sammajjati."
msgstr ""

msgctxt "pli-tv-kd3:14.6.3"
msgid "So dvīhatīhaṃ vasitvā akaraṇīyo pakkamati."
msgstr ""

msgctxt "pli-tv-kd3:14.6.4"
msgid "Tassa, bhikkhave, bhikkhuno purimikā ca na paññāyati, paṭissave ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc107
msgctxt "pli-tv-kd3:14.6.5"
msgid "Idha pana, bhikkhave, bhikkhunā vassāvāso paṭissuto hoti purimikāya."
msgstr ""

msgctxt "pli-tv-kd3:14.6.6"
msgid "So taṃ āvāsaṃ gacchanto bahiddhā uposathaṃ karoti, pāṭipade vihāraṃ upeti, senāsanaṃ paññapeti, pānīyaṃ paribhojanīyaṃ upaṭṭhāpeti, pariveṇaṃ sammajjati."
msgstr ""

msgctxt "pli-tv-kd3:14.6.7"
msgid "So dvīhatīhaṃ vasitvā sakaraṇīyo pakkamati."
msgstr ""

msgctxt "pli-tv-kd3:14.6.8"
msgid "Tassa, bhikkhave, bhikkhuno purimikā ca na paññāyati, paṭissave ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc108
msgctxt "pli-tv-kd3:14.6.9"
msgid "Idha pana, bhikkhave, bhikkhunā vassāvāso paṭissuto hoti purimikāya."
msgstr ""

msgctxt "pli-tv-kd3:14.6.10"
msgid "So taṃ āvāsaṃ gacchanto bahiddhā uposathaṃ karoti, pāṭipade vihāraṃ upeti, senāsanaṃ paññapeti, pānīyaṃ paribhojanīyaṃ upaṭṭhāpeti, pariveṇaṃ sammajjati."
msgstr ""

msgctxt "pli-tv-kd3:14.6.11"
msgid "So dvīhatīhaṃ vasitvā sattāhakaraṇīyena pakkamati."
msgstr ""

