msgctxt "pli-tv-kd3:6.19.6"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-kd3:6.20.1"
msgid "Kataṃ vā panassā hoti saṃghena kammaṃ—"
msgstr ""

msgctxt "pli-tv-kd3:6.20.2"
msgid "tajjanīyaṃ vā, niyassaṃ vā, pabbājanīyaṃ vā, paṭisāraṇīyaṃ vā, ukkhepanīyaṃ vā."
msgstr ""

msgctxt "pli-tv-kd3:6.20.3"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.20.4"
msgid "saṃgho me kammaṃ akāsi, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—"
msgstr ""

msgctxt "pli-tv-kd3:6.20.5"
msgid "‘kinti nu kho sammā vatteyya, lomaṃ pāteyya, netthāraṃ vatteyya, saṃgho taṃ kammaṃ paṭippassambheyyā’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.20.6"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc41
msgctxt "pli-tv-kd3:6.21.1"
msgid "Idha pana, bhikkhave, sikkhamānā gilānā hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.21.2"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.21.3"
msgid "ahañhi gilānā, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—"
msgstr ""

msgctxt "pli-tv-kd3:6.21.4"
msgid "‘gilānabhattaṃ vā pariyesissāmi, gilānupaṭṭhākabhattaṃ vā pariyesissāmi, gilānabhesajjaṃ vā pariyesissāmi, pucchissāmi vā, upaṭṭhahissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.21.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc42, pts-vp-pli1.146
msgctxt "pli-tv-kd3:6.22.1"
msgid "Idha pana, bhikkhave, sikkhamānāya anabhirati uppannā hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd3:6.22.3"
msgid "sikkhamānāya kukkuccaṃ uppannaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:6.22.4"
msgid "sikkhamānāya diṭṭhigataṃ uppannaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:6.22.5"
msgid "sikkhamānāya sikkhā kupitā hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.22.6"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.22.7"
msgid "sikkhā me kupitā, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.22.8"
msgid "sikkhāsamādānaṃ ussukkaṃ karissāmī’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.22.9"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc43
msgctxt "pli-tv-kd3:6.23.1"
