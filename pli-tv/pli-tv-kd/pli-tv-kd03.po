msgstr ""

msgctxt "pli-tv-kd3:6.23.2"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.23.3"
msgid "ahañhi upasampajjitukāmā, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.23.4"
msgid "upasampadaṃ ussukkaṃ karissāmi vā, anussāvessāmi vā, gaṇapūrako vā bhavissāmī’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.23.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc44
msgctxt "pli-tv-kd3:6.24.1"
msgid "Idha pana, bhikkhave, sāmaṇero gilāno hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.24.2"
msgid "So ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.24.3"
msgid "ahañhi gilāno, āgacchantu bhikkhū, icchāmi bhikkhūnaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—"
msgstr ""

msgctxt "pli-tv-kd3:6.24.4"
msgid "‘gilānabhattaṃ vā pariyesissāmi, gilānupaṭṭhākabhattaṃ vā pariyesissāmi, gilānabhesajjaṃ vā pariyesissāmi, pucchissāmi vā, upaṭṭhahissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.24.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-kd3:6.25.1"
msgid "Idha pana, bhikkhave, sāmaṇerassa anabhirati uppannā hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd3:6.25.3"
msgid "sāmaṇerassa kukkuccaṃ uppannaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:6.25.4"
msgid "sāmaṇerassa diṭṭhigataṃ uppannaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:6.25.5"
msgid "sāmaṇero vassaṃ pucchitukāmo hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.25.6"
msgid "So ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.25.7"
msgid "ahañhi vassaṃ pucchitukāmo, āgacchantu bhikkhū, icchāmi bhikkhūnaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—"
msgstr ""

msgctxt "pli-tv-kd3:6.25.8"
msgid "‘pucchissāmi vā, ācikkhissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.25.9"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc46
msgctxt "pli-tv-kd3:6.26.1"
msgid "Idha pana, bhikkhave, sāmaṇero upasampajjitukāmo hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.26.2"
msgid "So ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.26.3"
msgid "ahañhi upasampajjitukāmo, āgacchantu bhikkhū, icchāmi bhikkhūnaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.26.4"
msgid "upasampadaṃ ussukkaṃ karissāmi vā, anussāvessāmi vā, gaṇapūrako vā bhavissāmī’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.26.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc47, pts-vp-pli1.147
msgctxt "pli-tv-kd3:6.27.1"
msgid "Idha pana, bhikkhave, sāmaṇerī gilānā hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.27.2"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.27.3"
msgid "ahañhi gilānā, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—"
msgstr ""

msgctxt "pli-tv-kd3:6.27.4"
msgid "‘gilānabhattaṃ vā pariyesissāmi, gilānupaṭṭhākabhattaṃ vā pariyesissāmi, gilānabhesajjaṃ vā pariyesissāmi, pucchissāmi vā, upaṭṭhahissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.27.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-kd3:6.28.1"
msgid "Idha pana, bhikkhave, sāmaṇeriyā anabhirati uppannā hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd3:6.28.3"
msgid "sāmaṇeriyā kukkuccaṃ uppannaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:6.28.4"
msgid "sāmaṇeriyā diṭṭhigataṃ uppannaṃ hoti …"
msgstr ""

msgctxt "pli-tv-kd3:6.28.5"
msgid "sāmaṇerī vassaṃ pucchitukāmā hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.28.6"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.28.7"
msgid "ahañhi vassaṃ pucchitukāmā, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.28.8"
msgid "pucchissāmi vā, ācikkhissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.28.9"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc49
msgctxt "pli-tv-kd3:6.29.1"
msgid "Idha pana, bhikkhave, sāmaṇerī sikkhaṃ samādiyitukāmā hoti."
msgstr ""

msgctxt "pli-tv-kd3:6.29.2"
msgid "Sā ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.29.3"
msgid "ahañhi sikkhaṃ samādiyitukāmā, āgacchantu ayyā, icchāmi ayyānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—‘"
msgstr ""

msgctxt "pli-tv-kd3:6.29.4"
msgid "sikkhāsamādānaṃ ussukkaṃ karissāmī’ti."
msgstr ""

msgctxt "pli-tv-kd3:6.29.5"
msgid "Sattāhaṃ sannivatto kātabbo”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:7.1.0"
msgid "5. Sattannaṃappahitepianujānana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc50
msgctxt "pli-tv-kd3:7.1.1"
msgid "Tena kho pana samayena aññatarassa bhikkhuno mātā gilānā hoti."
msgstr ""

msgctxt "pli-tv-kd3:7.1.2"
msgid "Sā puttassa santike dūtaṃ pāhesi—"
msgstr ""

msgctxt "pli-tv-kd3:7.1.3"
msgid "“ahañhi gilānā, āgacchatu me putto, icchāmi puttassa āgatan”ti."
msgstr ""

msgctxt "pli-tv-kd3:7.1.4"
msgid "Atha kho tassa bhikkhuno etadahosi—"
msgstr ""

msgctxt "pli-tv-kd3:7.1.5"
msgid "“bhagavatā paññattaṃ—"
msgstr ""

msgctxt "pli-tv-kd3:7.1.6"
msgid "‘sattannaṃ sattāhakaraṇīyena pahite gantuṃ, na tveva appahite;"
msgstr ""

msgctxt "pli-tv-kd3:7.1.7"
msgid "pañcannaṃ sattāhakaraṇīyena appahitepi gantuṃ, pageva pahite’ti."
msgstr ""

msgctxt "pli-tv-kd3:7.1.8"
msgid "Ayañca me mātā gilānā, sā ca anupāsikā, kathaṃ nu kho mayā paṭipajjitabban”ti?"
msgstr ""

msgctxt "pli-tv-kd3:7.1.9"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:7.2.1"
msgid "“Anujānāmi, bhikkhave, sattannaṃ sattāhakaraṇīyena appahitepi gantuṃ, pageva pahite."
msgstr ""

msgctxt "pli-tv-kd3:7.2.2"
msgid "Bhikkhussa, bhikkhuniyā, sikkhamānāya, sāmaṇerassa, sāmaṇeriyā, mātuyā ca pitussa ca—"
msgstr ""

msgctxt "pli-tv-kd3:7.2.3"
msgid "anujānāmi, bhikkhave, imesaṃ sattannaṃ sattāhakaraṇīyena appahitepi gantuṃ, pageva pahite."
msgstr ""

msgctxt "pli-tv-kd3:7.2.4"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc51
msgctxt "pli-tv-kd3:7.3.1"
msgid "Idha pana, bhikkhave, bhikkhussa mātā gilānā hoti."
msgstr ""

msgctxt "pli-tv-kd3:7.3.2"
msgid "Sā ce puttassa santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:7.3.3"
msgid "ahañhi gilānā, āgacchatu me putto, icchāmi puttassa āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—"
msgstr ""

msgctxt "pli-tv-kd3:7.3.4"
msgid "‘gilānabhattaṃ vā pariyesissāmi, gilānupaṭṭhākabhattaṃ vā pariyesissāmi, gilānabhesajjaṃ vā pariyesissāmi, pucchissāmi vā, upaṭṭhahissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:7.3.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc52, pts-vp-pli1.148
msgctxt "pli-tv-kd3:7.4.1"
msgid "Idha pana, bhikkhave, bhikkhussa pitā gilāno hoti."
msgstr ""

msgctxt "pli-tv-kd3:7.4.2"
msgid "So ce puttassa santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:7.4.3"
msgid "ahañhi gilāno, āgacchatu me putto, icchāmi puttassa āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, appahitepi, pageva pahite—"
msgstr ""

msgctxt "pli-tv-kd3:7.4.4"
msgid "‘gilānabhattaṃ vā pariyesissāmi, gilānupaṭṭhākabhattaṃ vā pariyesissāmi, gilānabhesajjaṃ vā pariyesissāmi, pucchissāmi vā, upaṭṭhahissāmi vā’ti."
msgstr ""

msgctxt "pli-tv-kd3:7.4.5"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:7.5.0"
msgid "6. Pahiteyevaanujānana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc53
msgctxt "pli-tv-kd3:7.5.1"
msgid "Idha pana, bhikkhave, bhikkhussa bhātā gilāno hoti."
msgstr ""

msgctxt "pli-tv-kd3:7.5.2"
msgid "So ce bhātuno santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:7.5.3"
msgid "ahañhi gilāno, āgacchatu me bhātā, icchāmi bhātuno āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, pahite, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:7.5.4"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc54
msgctxt "pli-tv-kd3:7.6.1"
msgid "Idha pana, bhikkhave, bhikkhussa bhaginī gilānā hoti."
msgstr ""

msgctxt "pli-tv-kd3:7.6.2"
msgid "Sā ce bhātuno santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:7.6.3"
msgid "ahañhi gilānā, āgacchatu me bhātā, icchāmi bhātuno āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, pahite, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:7.6.4"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-kd3:7.7.1"
msgid "Idha pana, bhikkhave, bhikkhussa ñātako gilāno hoti."
msgstr ""

msgctxt "pli-tv-kd3:7.7.2"
msgid "So ce bhikkhussa santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:7.7.3"
msgid "ahañhi gilāno, āgacchatu bhadanto, icchāmi bhadantassa āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, pahite, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:7.7.4"
msgid "Sattāhaṃ sannivatto kātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-kd3:7.8.1"
msgid "Idha pana, bhikkhave, bhikkhugatiko gilāno hoti."
msgstr ""

msgctxt "pli-tv-kd3:7.8.2"
msgid "So ce bhikkhūnaṃ santike dūtaṃ pahiṇeyya—‘"
msgstr ""

msgctxt "pli-tv-kd3:7.8.3"
msgid "ahañhi gilāno, āgacchantu bhadantā, icchāmi bhadantānaṃ āgatan’ti, gantabbaṃ, bhikkhave, sattāhakaraṇīyena, pahite, na tveva appahite."
msgstr ""

msgctxt "pli-tv-kd3:7.8.4"
msgid "Sattāhaṃ sannivatto kātabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc57
msgctxt "pli-tv-kd3:8.1.1"
msgid "Tena kho pana samayena saṃghassa vihāro undriyati."
msgstr ""

msgctxt "pli-tv-kd3:8.1.2"
msgid "Aññatarena upāsakena araññe bhaṇḍaṃ chedāpitaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd3:8.1.3"
msgid "So bhikkhūnaṃ santike dūtaṃ pāhesi—"
msgstr ""

msgctxt "pli-tv-kd3:8.1.4"
msgid "“sace bhadantā taṃ bhaṇḍaṃ āvahāpeyyuṃ, dajjāhaṃ taṃ bhaṇḍan”ti."
msgstr ""

msgctxt "pli-tv-kd3:8.1.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd3:8.1.6"
msgid "“Anujānāmi, bhikkhave, saṃghakaraṇīyena gantuṃ."
msgstr ""

msgctxt "pli-tv-kd3:8.1.7"
msgid "Sattāhaṃ sannivatto kātabbo”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc58
msgctxt "pli-tv-kd3:8.1.8"
msgid "Vassāvāsabhāṇavāro niṭṭhito."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd3:9.1.0"
msgid "7. Antarāyeanāpattivassacchedavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc59
msgctxt "pli-tv-kd3:9.1.1"
msgid "Tena kho pana samayena kosalesu janapade aññatarasmiṃ āvāse vassūpagatā bhikkhū vāḷehi ubbāḷhā honti."
msgstr ""

msgctxt "pli-tv-kd3:9.1.2"
msgid "Gaṇhiṃsupi paripātiṃsupi."
msgstr ""

msgctxt "pli-tv-kd3:9.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-kd3:9.1.4"
msgid "“Idha pana, bhikkhave, vassūpagatā bhikkhū vāḷehi ubbāḷhā honti."
msgstr ""

msgctxt "pli-tv-kd3:9.1.5"
msgid "Gaṇhantipi paripātentipi."
msgstr ""

msgctxt "pli-tv-kd3:9.1.6"
msgid "Eseva antarāyoti pakkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:9.1.7"
msgid "Anāpatti vassacchedassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc61, pts-vp-pli1.149
msgctxt "pli-tv-kd3:9.1.8"
msgid "Idha pana, bhikkhave, vassūpagatā bhikkhū sarīsapehi ubbāḷhā honti."
msgstr ""

msgctxt "pli-tv-kd3:9.1.9"
msgid "Ḍaṃsantipi paripātentipi."
msgstr ""

msgctxt "pli-tv-kd3:9.1.10"
msgid "Eseva antarāyoti pakkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:9.1.11"
msgid "Anāpatti vassacchedassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-kd3:9.2.1"
msgid "Idha pana, bhikkhave, vassūpagatā bhikkhū corehi ubbāḷhā honti."
msgstr ""

msgctxt "pli-tv-kd3:9.2.2"
msgid "Vilumpantipi ākoṭentipi."
msgstr ""

msgctxt "pli-tv-kd3:9.2.3"
msgid "Eseva antarāyoti pakkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:9.2.4"
msgid "Anāpatti vassacchedassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc63
msgctxt "pli-tv-kd3:9.2.5"
msgid "Idha pana, bhikkhave, vassūpagatā bhikkhū pisācehi ubbāḷhā honti."
msgstr ""

#. VAR: hanantipi → ojampi haranti (bj, pts1) | harantipi (s1-3)
msgctxt "pli-tv-kd3:9.2.6"
msgid "Āvisantipi hanantipi."
msgstr ""

msgctxt "pli-tv-kd3:9.2.7"
msgid "Eseva antarāyoti pakkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd3:9.2.8"
msgid "Anāpatti vassacchedassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-kd3:9.3.1"
msgid "Idha pana, bhikkhave, vassūpagatānaṃ bhikkhūnaṃ gāmo agginā daḍḍho hoti."
msgstr ""

msgctxt "pli-tv-kd3:9.3.2"
msgid "Bhikkhū piṇḍakena kilamanti."
msgstr ""

msgctxt "pli-tv-kd3:9.3.3"
