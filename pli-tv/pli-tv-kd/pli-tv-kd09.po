msgid "saṃghabhedako, bhikkhave …"
msgstr ""

msgctxt "pli-tv-kd9:4.10.17"
msgid "lohituppādako, bhikkhave …"
msgstr ""

msgctxt "pli-tv-kd9:4.10.18"
msgid "ubhatobyañjanako, bhikkhave, appatto, osāraṇaṃ, tañce saṃgho osāreti—"
