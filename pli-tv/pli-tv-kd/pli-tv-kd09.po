msgstr ""

msgctxt "pli-tv-kd9:4.5.22"
msgid "lohituppādakavīso kammaṃ kareyya …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.23"
msgid "ubhatobyañjanakavīso kammaṃ kareyya …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.24"
msgid "nānāsaṃvāsakavīso kammaṃ kareyya …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.25"
msgid "nānāsīmāya ṭhitavīso kammaṃ kareyya …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.26"
msgid "iddhiyā vehāse ṭhitavīso kammaṃ kareyya …"
