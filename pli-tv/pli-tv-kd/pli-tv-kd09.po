#. REF: sc40
msgctxt "pli-tv-kd9:4.11.41"
msgid "Vāsabhagāmabhāṇavāro niṭṭhito paṭhamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd9:5.1.0"
msgid "7. Adhammakammādikathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc41, pts-vp-pli1.323
msgctxt "pli-tv-kd9:5.1.1"
msgid "Idha pana, bhikkhave, bhikkhussa na hoti āpatti daṭṭhabbā."
msgstr ""

msgctxt "pli-tv-kd9:5.1.2"
msgid "Tamenaṃ codeti saṃgho vā sambahulā vā ekapuggalo vā—‘"
msgstr ""

msgctxt "pli-tv-kd9:5.1.3"
msgid "āpattiṃ tvaṃ, āvuso, āpanno, passasetaṃ āpattin’ti?"
