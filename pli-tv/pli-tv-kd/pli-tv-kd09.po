#. HTML: <section class="sutta" id="pli-tv-kd9"><article><div class="hgroup"><p class="collection">
msgctxt "pli-tv-kd9:0.1"
msgid "Theravāda Vinayapiṭaka"
msgstr ""

#. HTML: </p><p class="division">
msgctxt "pli-tv-kd9:0.2"
msgid "Mahāvagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-kd9:0.3"
msgid "Campeyyakkhandhaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-kd9:1.1.0"
msgid "1. Kassapagottabhikkhuvatthu"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli1.312
msgctxt "pli-tv-kd9:1.1.1"
msgid "Tena samayena buddho bhagavā campāyaṃ viharati gaggarāya pokkharaṇiyā tīre."
msgstr ""

msgctxt "pli-tv-kd9:1.1.2"
msgid "Tena kho pana samayena kāsīsu janapade vāsabhagāmo nāma hoti."
msgstr ""

msgctxt "pli-tv-kd9:1.1.3"
msgid "Tattha kassapagotto nāma bhikkhu āvāsiko hoti tantibaddho ussukkaṃ āpanno—"
msgstr ""

msgctxt "pli-tv-kd9:1.1.4"
msgid "“kinti anāgatā ca pesalā bhikkhū āgaccheyyuṃ, āgatā ca pesalā bhikkhū phāsu vihareyyuṃ, ayañca āvāso vuddhiṃ viruḷhiṃ vepullaṃ āpajjeyyā”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.1.5"
msgid "Tena kho pana samayena sambahulā bhikkhū kāsīsu cārikaṃ caramānā yena vāsabhagāmo tadavasaruṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.1.6"
msgid "Addasā kho kassapagotto bhikkhu te bhikkhū dūratova āgacchante, disvāna āsanaṃ paññapesi, pādodakaṃ pādapīṭhaṃ pādakathalikaṃ upanikkhipi, paccuggantvā pattacīvaraṃ paṭiggahesi, pānīyena āpucchi, nahāne ussukkaṃ akāsi, ussukkampi akāsi yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.1.7"
msgid "Atha kho tesaṃ āgantukānaṃ bhikkhūnaṃ etadahosi—"
msgstr ""

msgctxt "pli-tv-kd9:1.1.8"
msgid "“bhaddako kho ayaṃ, āvuso, āvāsiko bhikkhu nahāne ussukkaṃ karoti, ussukkampi karoti yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.1.9"
msgid "Handa mayaṃ, āvuso, idheva vāsabhagāme nivāsaṃ kappemā”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.1.10"
msgid "Atha kho te āgantukā bhikkhū tattheva vāsabhagāme nivāsaṃ kappesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-kd9:1.2.1"
msgid "Atha kho kassapagottassa bhikkhuno etadahosi—"
msgstr ""

msgctxt "pli-tv-kd9:1.2.2"
msgid "“yo kho imesaṃ āgantukānaṃ bhikkhūnaṃ āgantukakilamatho so paṭippassaddho."
msgstr ""

msgctxt "pli-tv-kd9:1.2.3"
msgid "Yepime gocare appakataññuno tedānime gocare pakataññuno."
msgstr ""

msgctxt "pli-tv-kd9:1.2.4"
msgid "Dukkaraṃ kho pana parakulesu yāvajīvaṃ ussukkaṃ kātuṃ, viññatti ca manussānaṃ amanāpā."
msgstr ""

msgctxt "pli-tv-kd9:1.2.5"
msgid "Yannūnāhaṃ na ussukkaṃ kareyyaṃ yāguyā khādanīye bhattasmin”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.2.6"
msgid "So na ussukkaṃ akāsi yāguyā khādanīye bhattasmiṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc3, pts-vp-pli1.313
msgctxt "pli-tv-kd9:1.2.7"
msgid "Atha kho tesaṃ āgantukānaṃ bhikkhūnaṃ etadahosi—"
msgstr ""

msgctxt "pli-tv-kd9:1.2.8"
msgid "“pubbe khvāyaṃ, āvuso, āvāsiko bhikkhu nahāne ussukkaṃ akāsi, ussukkampi akāsi yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.2.9"
msgid "Sodānāyaṃ na ussukkaṃ karoti yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.2.10"
msgid "Duṭṭhodānāyaṃ, āvuso, āvāsiko bhikkhu."
msgstr ""

#. VAR: āvāsikaṃ → imaṃ āvāsikaṃ (s1-3) | āvāsiko (pts1)
msgctxt "pli-tv-kd9:1.2.11"
msgid "Handa mayaṃ, āvuso, āvāsikaṃ bhikkhuṃ ukkhipāmā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-kd9:1.3.1"
msgid "Atha kho te āgantukā bhikkhū sannipatitvā kassapagottaṃ bhikkhuṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd9:1.3.2"
msgid "“pubbe kho tvaṃ, āvuso, nahāne ussukkaṃ karosi, ussukkampi karosi yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.3.3"
msgid "Sodāni tvaṃ na ussukkaṃ karosi yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.3.4"
msgid "Āpattiṃ tvaṃ, āvuso, āpanno."
msgstr ""

msgctxt "pli-tv-kd9:1.3.5"
msgid "Passasetaṃ āpattin”ti?"
msgstr ""

msgctxt "pli-tv-kd9:1.3.6"
msgid "“Natthi me, āvuso, āpatti, yamahaṃ passeyyan”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.3.7"
msgid "Atha kho te āgantukā bhikkhū kassapagottaṃ bhikkhuṃ āpattiyā adassane ukkhipiṃsu."
msgstr ""

#. HTML: </p><p>
#. REF: sc5, pts-vp-pli1.314
msgctxt "pli-tv-kd9:1.3.8"
msgid "Atha kho kassapagottassa bhikkhuno etadahosi—“"
msgstr ""

#. VAR: camhi → vamhi (?)
msgctxt "pli-tv-kd9:1.3.9"
msgid "ahaṃ kho etaṃ na jānāmi ‘āpatti vā esā anāpatti vā, āpanno camhi anāpanno vā, ukkhitto camhi anukkhitto vā, dhammikena vā adhammikena vā, kuppena vā akuppena vā, ṭhānārahena vā aṭṭhānārahena vā’."
msgstr ""

msgctxt "pli-tv-kd9:1.3.10"
msgid "Yannūnāhaṃ campaṃ gantvā bhagavantaṃ etamatthaṃ puccheyyan”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.4.1"
msgid "Atha kho kassapagotto bhikkhu senāsanaṃ saṃsāmetvā pattacīvaramādāya yena campā tena pakkāmi."
msgstr ""

msgctxt "pli-tv-kd9:1.4.2"
msgid "Anupubbena yena campā yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd9:1.4.3"
msgid "Āciṇṇaṃ kho panetaṃ buddhānaṃ bhagavantānaṃ āgantukehi bhikkhūhi saddhiṃ paṭisammodituṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.4.4"
msgid "Atha kho bhagavā kassapagottaṃ bhikkhuṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd9:1.4.5"
msgid "“kacci, bhikkhu, khamanīyaṃ, kacci yāpanīyaṃ, kacci appakilamathena addhānaṃ āgato, kuto ca tvaṃ, bhikkhu, āgacchasī”ti?"
msgstr ""

msgctxt "pli-tv-kd9:1.4.6"
msgid "“Khamanīyaṃ, bhagavā, yāpanīyaṃ, bhagavā;"
msgstr ""

msgctxt "pli-tv-kd9:1.4.7"
msgid "appakilamathena cāhaṃ, bhante, addhānaṃ āgato."
msgstr ""

msgctxt "pli-tv-kd9:1.5.1"
msgid "Atthi, bhante, kāsīsu janapade vāsabhagāmo nāma."
msgstr ""

msgctxt "pli-tv-kd9:1.5.2"
msgid "Tatthāhaṃ, bhagavā, āvāsiko tantibaddho ussukkaṃ āpanno—"
msgstr ""

msgctxt "pli-tv-kd9:1.5.3"
msgid "‘kinti anāgatā ca pesalā bhikkhū āgaccheyyuṃ, āgatā ca pesalā bhikkhū phāsu vihareyyuṃ, ayañca āvāso vuddhiṃ viruḷhiṃ vepullaṃ āpajjeyyā’ti."
msgstr ""

msgctxt "pli-tv-kd9:1.5.4"
msgid "Atha kho, bhante, sambahulā bhikkhū kāsīsu cārikaṃ caramānā yena vāsabhagāmo tadavasaruṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.5"
msgid "Addasaṃ kho ahaṃ, bhante, te bhikkhū dūratova āgacchante, disvāna āsanaṃ paññapesiṃ, pādodakaṃ pādapīṭhaṃ pādakathalikaṃ upanikkhipiṃ, paccuggantvā pattacīvaraṃ paṭiggahesiṃ, pānīyena apucchiṃ, nahāne ussukkaṃ akāsiṃ, ussukkampi akāsiṃ yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.6"
msgid "Atha kho tesaṃ, bhante, āgantukānaṃ bhikkhūnaṃ etadahosi—‘"
msgstr ""

msgctxt "pli-tv-kd9:1.5.7"
msgid "bhaddako kho ayaṃ āvuso āvāsiko bhikkhu nahāne ussukkaṃ karoti, ussukkampi karoti yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.8"
msgid "Handa mayaṃ, āvuso, idheva vāsabhagāme nivāsaṃ kappemā’ti."
msgstr ""

msgctxt "pli-tv-kd9:1.5.9"
msgid "Atha kho te, bhante, āgantukā bhikkhū tattheva vāsabhagāme nivāsaṃ kappesuṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.10"
msgid "Tassa mayhaṃ, bhante, etadahosi—‘"
msgstr ""

msgctxt "pli-tv-kd9:1.5.11"
msgid "yo kho imesaṃ āgantukānaṃ bhikkhūnaṃ āgantukakilamatho so paṭippassaddho."
msgstr ""

msgctxt "pli-tv-kd9:1.5.12"
msgid "Yepime gocare appakataññuno tedānime gocare pakataññuno."
msgstr ""

msgctxt "pli-tv-kd9:1.5.13"
msgid "Dukkaraṃ kho pana parakulesu yāvajīvaṃ ussukkaṃ kātuṃ, viññatti ca manussānaṃ amanāpā."
msgstr ""

msgctxt "pli-tv-kd9:1.5.14"
msgid "Yannūnāhaṃ na ussukkaṃ kareyyaṃ yāguyā khādanīye bhattasmin’ti."
msgstr ""

msgctxt "pli-tv-kd9:1.5.15"
msgid "So kho ahaṃ, bhante, na ussukkaṃ akāsiṃ yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.16"
msgid "Atha kho tesaṃ, bhante, āgantukānaṃ bhikkhūnaṃ etadahosi—‘"
msgstr ""

msgctxt "pli-tv-kd9:1.5.17"
msgid "pubbe khvāyaṃ, āvuso, āvāsiko bhikkhu nahāne ussukkaṃ karoti, ussukkampi karoti yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.18"
msgid "Sodānāyaṃ na ussukkaṃ karoti yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.19"
msgid "Duṭṭhodānāyaṃ, āvuso, āvāsiko bhikkhu."
msgstr ""

msgctxt "pli-tv-kd9:1.5.20"
msgid "Handa mayaṃ, āvuso, āvāsikaṃ bhikkhuṃ ukkhipāmā’ti."
msgstr ""

msgctxt "pli-tv-kd9:1.5.21"
msgid "Atha kho te, bhante, āgantukā bhikkhū sannipatitvā maṃ etadavocuṃ—‘"
msgstr ""

msgctxt "pli-tv-kd9:1.5.22"
msgid "pubbe kho tvaṃ, āvuso, nahāne ussukkaṃ karosi, ussukkampi karosi yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.23"
msgid "Sodāni tvaṃ na ussukkaṃ karosi yāguyā khādanīye bhattasmiṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.5.24"
msgid "Āpattiṃ tvaṃ, āvuso, āpanno."
msgstr ""

msgctxt "pli-tv-kd9:1.5.25"
msgid "Passasetaṃ āpattin’ti?"
msgstr ""

msgctxt "pli-tv-kd9:1.5.26"
msgid "‘Natthi me, āvuso, āpatti yamahaṃ passeyyan’ti."
msgstr ""

msgctxt "pli-tv-kd9:1.5.27"
msgid "Atha kho te, bhante, āgantukā bhikkhū maṃ āpattiyā adassane ukkhipiṃsu."
msgstr ""

msgctxt "pli-tv-kd9:1.5.28"
msgid "Tassa mayhaṃ, bhante, etadahosi—‘"
msgstr ""

msgctxt "pli-tv-kd9:1.5.29"
msgid "ahaṃ kho etaṃ na jānāmi “āpatti vā esā anāpatti vā, āpanno camhi anāpanno vā, ukkhitto camhi anukkhitto vā, dhammikena vā adhammikena vā, kuppena vā akuppena vā, ṭhānārahena vā aṭṭhānārahena vā”."
msgstr ""

msgctxt "pli-tv-kd9:1.5.30"
msgid "Yannūnāhaṃ campaṃ gantvā bhagavantaṃ etamatthaṃ puccheyyan’ti."
msgstr ""

msgctxt "pli-tv-kd9:1.5.31"
msgid "Tato ahaṃ, bhagavā, āgacchāmī”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.6.1"
msgid "“Anāpatti esā, bhikkhu, nesā āpatti."
msgstr ""

msgctxt "pli-tv-kd9:1.6.2"
msgid "Anāpannosi, nasi āpanno."
msgstr ""

msgctxt "pli-tv-kd9:1.6.3"
msgid "Anukkhittosi, nasi ukkhitto."
msgstr ""

msgctxt "pli-tv-kd9:1.6.4"
msgid "Adhammikenāsi kammena ukkhitto kuppena aṭṭhānārahena."
msgstr ""

msgctxt "pli-tv-kd9:1.6.5"
msgid "Gaccha tvaṃ, bhikkhu, tattheva vāsabhagāme nivāsaṃ kappehī”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.6.6"
msgid "“Evaṃ, bhante”ti kho kassapagotto bhikkhu bhagavato paṭissuṇitvā uṭṭhāyāsanā bhagavantaṃ abhivādetvā padakkhiṇaṃ katvā yena vāsabhagāmo tena pakkāmi."
msgstr ""

#. HTML: </p><p>
#. REF: sc6, pts-vp-pli1.315
msgctxt "pli-tv-kd9:1.7.1"
msgid "Atha kho tesaṃ āgantukānaṃ bhikkhūnaṃ ahudeva kukkuccaṃ, ahu vippaṭisāro—"
msgstr ""

msgctxt "pli-tv-kd9:1.7.2"
msgid "“alābhā vata no, na vata no lābhā;"
msgstr ""

msgctxt "pli-tv-kd9:1.7.3"
msgid "dulladdhaṃ vata no, na vata no suladdhaṃ, ye mayaṃ suddhaṃ bhikkhuṃ anāpattikaṃ avatthusmiṃ akāraṇe ukkhipimhā."
msgstr ""

msgctxt "pli-tv-kd9:1.7.4"
msgid "Handa mayaṃ, āvuso, campaṃ gantvā bhagavato santike accayaṃ accayato desemā”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.7.5"
msgid "Atha kho te āgantukā bhikkhū senāsanaṃ saṃsāmetvā pattacīvaramādāya yena campā tena pakkamiṃsu."
msgstr ""

msgctxt "pli-tv-kd9:1.7.6"
msgid "Anupubbena yena campā yena bhagavā tenupasaṅkamiṃsu, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdiṃsu."
msgstr ""

msgctxt "pli-tv-kd9:1.7.7"
msgid "Āciṇṇaṃ kho panetaṃ buddhānaṃ bhagavantānaṃ āgantukehi bhikkhūhi saddhiṃ paṭisammodituṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.7.8"
msgid "Atha kho bhagavā te bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd9:1.7.9"
msgid "“kacci, bhikkhave, khamanīyaṃ, kacci yāpanīyaṃ, kaccittha appakilamathena addhānaṃ āgatā, kuto ca tumhe, bhikkhave, āgacchathā”ti?"
msgstr ""

msgctxt "pli-tv-kd9:1.7.10"
msgid "“Khamanīyaṃ, bhagavā, yāpanīyaṃ, bhagavā;"
msgstr ""

msgctxt "pli-tv-kd9:1.7.11"
msgid "appakilamathena ca mayaṃ, bhante, addhānaṃ āgatā."
msgstr ""

msgctxt "pli-tv-kd9:1.7.12"
msgid "Atthi, bhante, kāsīsu janapade vāsabhagāmo nāma."
msgstr ""

msgctxt "pli-tv-kd9:1.7.13"
msgid "Tato mayaṃ, bhagavā, āgacchāmā”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.8.1"
msgid "“Tumhe, bhikkhave, āvāsikaṃ bhikkhuṃ ukkhipitthā”ti?"
msgstr ""

msgctxt "pli-tv-kd9:1.8.2"
msgid "“Evaṃ, bhante”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.8.3"
msgid "“Kismiṃ, bhikkhave, vatthusmiṃ kāraṇe”ti?"
msgstr ""

msgctxt "pli-tv-kd9:1.8.4"
msgid "“Avatthusmiṃ, bhagavā, akāraṇe”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.8.5"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-kd9:1.8.6"
msgid "“ananucchavikaṃ, moghapurisā, ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:1.8.7"
msgid "Kathañhi nāma tumhe, moghapurisā, suddhaṃ bhikkhuṃ anāpattikaṃ avatthusmiṃ akāraṇe ukkhipissatha."
msgstr ""

msgctxt "pli-tv-kd9:1.8.8"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-kd9:1.8.10"
msgid "vigarahitvā dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd9:1.8.11"
msgid "“na, bhikkhave, suddho bhikkhu anāpattiko avatthusmiṃ akāraṇe ukkhipitabbo."
msgstr ""

msgctxt "pli-tv-kd9:1.8.12"
msgid "Yo ukkhipeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-kd9:1.9.1"
msgid "Atha kho te bhikkhū uṭṭhāyāsanā ekaṃsaṃ uttarāsaṅgaṃ karitvā bhagavato pādesu sirasā nipatitvā bhagavantaṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd9:1.9.2"
msgid "“accayo no, bhante, accagamā yathābāle yathāmūḷhe yathāakusale, ye mayaṃ suddhaṃ bhikkhuṃ anāpattikaṃ avatthusmiṃ akāraṇe ukkhipimhā."
msgstr ""

#. VAR: āyatiṃ → āyatiṃ ca (bj)
msgctxt "pli-tv-kd9:1.9.3"
msgid "Tesaṃ no, bhante, bhagavā accayaṃ accayato paṭiggaṇhātu āyatiṃ saṃvarāyā”ti."
msgstr ""

msgctxt "pli-tv-kd9:1.9.4"
msgid "“Taggha tumhe, bhikkhave, accayo accagamā yathābāle yathāmūḷhe yathāakusale, ye tumhe suddhaṃ bhikkhuṃ anāpattikaṃ avatthusmiṃ akāraṇe ukkhipittha."
msgstr ""

msgctxt "pli-tv-kd9:1.9.5"
msgid "Yato ca kho tumhe, bhikkhave, accayaṃ accayato disvā yathādhammaṃ paṭikarotha, taṃ vo mayaṃ paṭiggaṇhāma."
msgstr ""

msgctxt "pli-tv-kd9:1.9.6"
msgid "Vuddhihesā, bhikkhave, ariyassa vinaye yo accayaṃ accayato disvā yathādhammaṃ paṭikaroti, āyatiṃ saṃvaraṃ āpajjatī”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd9:2.1.0"
msgid "2. Adhammenavaggādikammakathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc8
msgctxt "pli-tv-kd9:2.1.1"
msgid "Tena kho pana samayena campāyaṃ bhikkhū evarūpāni kammāni karonti—"
msgstr ""

msgctxt "pli-tv-kd9:2.1.2"
msgid "adhammena vaggakammaṃ karonti, adhammena samaggakammaṃ karonti;"
msgstr ""

msgctxt "pli-tv-kd9:2.1.3"
msgid "dhammena vaggakammaṃ karonti, dhammapatirūpakena vaggakammaṃ karonti;"
msgstr ""

msgctxt "pli-tv-kd9:2.1.4"
msgid "dhammapatirūpakena samaggakammaṃ karonti;"
msgstr ""

msgctxt "pli-tv-kd9:2.1.5"
msgid "ekopi ekaṃ ukkhipati, ekopi dve ukkhipati, ekopi sambahule ukkhipati, ekopi saṃghaṃ ukkhipati;"
msgstr ""

msgctxt "pli-tv-kd9:2.1.6"
msgid "dvepi ekaṃ ukkhipanti, dvepi dve ukkhipanti, dvepi sambahule ukkhipanti, dvepi saṃghaṃ ukkhipanti;"
msgstr ""

msgctxt "pli-tv-kd9:2.1.7"
msgid "sambahulāpi ekaṃ ukkhipanti;"
msgstr ""

msgctxt "pli-tv-kd9:2.1.8"
msgid "sambahulāpi dve ukkhipanti, sambahulāpi sambahule ukkhipanti, sambahulāpi saṃghaṃ ukkhipanti;"
msgstr ""

msgctxt "pli-tv-kd9:2.1.9"
msgid "saṃghopi saṃghaṃ ukkhipati."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-kd9:2.2.1"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd9:2.2.3"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd9:2.2.4"
msgid "“kathañhi nāma campāyaṃ bhikkhū evarūpāni kammāni karissanti—"
msgstr ""

msgctxt "pli-tv-kd9:2.2.5"
msgid "adhammena vaggakammaṃ karissanti, adhammena samaggakammaṃ karissanti, dhammena vaggakammaṃ karissanti, dhammapatirūpakena vaggakammaṃ karissanti, dhammapatirūpakena samaggakammaṃ karissanti, ekopi ekaṃ ukkhipissati, ekopi dve ukkhipissati, ekopi sambahule ukkhipissati, ekopi saṃghaṃ ukkhipissati, dvepi ekaṃ ukkhipissanti, dvepi dve ukkhipissanti, dvepi sambahule ukkhipissanti, dvepi saṃghaṃ ukkhipissanti, sambahulāpi ekaṃ ukkhipissanti, sambahulāpi dve ukkhipissanti, sambahulāpi sambahule ukkhipissanti, sambahulāpi saṃghaṃ ukkhipissanti, saṃghopi saṃghaṃ ukkhipissatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10, pts-vp-pli1.316
msgctxt "pli-tv-kd9:2.2.6"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd9:2.2.8"
msgid "“saccaṃ kira, bhikkhave, campāyaṃ bhikkhū evarūpāni kammāni karonti—"
msgstr ""

msgctxt "pli-tv-kd9:2.2.9"
msgid "adhammena vaggakammaṃ karonti … pe …"
msgstr ""

msgctxt "pli-tv-kd9:2.2.11"
msgid "saṃghopi saṃghaṃ ukkhipatī”ti?"
msgstr ""

msgctxt "pli-tv-kd9:2.2.12"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd9:2.2.13"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-kd9:2.2.14"
msgid "“ananucchavikaṃ, bhikkhave, tesaṃ moghapurisānaṃ ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:2.2.15"
msgid "Kathañhi nāma te, bhikkhave, moghapurisā evarūpāni kammāni karissanti—"
msgstr ""

msgctxt "pli-tv-kd9:2.2.16"
msgid "adhammena vaggakammaṃ karissanti … pe …"
msgstr ""

msgctxt "pli-tv-kd9:2.2.18"
msgid "saṃghopi saṃghaṃ ukkhipissati."
msgstr ""

msgctxt "pli-tv-kd9:2.2.19"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-kd9:2.2.21"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd9:2.2.23"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

#. HTML: </p><p>
#. REF: sc11
#. VAR: adhammena → adhammena ce bhikkhave (s1-3)
msgctxt "pli-tv-kd9:2.3.1"
msgid "“Adhammena ce, bhikkhave, vaggakammaṃ akammaṃ na ca karaṇīyaṃ, adhammena samaggakammaṃ akammaṃ na ca karaṇīyaṃ, dhammena vaggakammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.3.2"
msgid "dhammapatirūpakena vaggakammaṃ akammaṃ na ca karaṇīyaṃ, dhammapatirūpakena samaggakammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.3.3"
msgid "ekopi ekaṃ ukkhipati akammaṃ na ca karaṇīyaṃ, ekopi dve ukkhipati akammaṃ na ca karaṇīyaṃ, ekopi sambahule ukkhipati akammaṃ na ca karaṇīyaṃ, ekopi saṃghaṃ ukkhipati akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.3.4"
msgid "dvepi ekaṃ ukkhipanti akammaṃ na ca karaṇīyaṃ, dvepi dve ukkhipanti akammaṃ na ca karaṇīyaṃ, dvepi sambahule ukkhipanti akammaṃ na ca karaṇīyaṃ, dvepi saṃghaṃ ukkhipanti akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.3.5"
msgid "sambahulāpi ekaṃ ukkhipanti akammaṃ na ca karaṇīyaṃ, sambahulāpi dve ukkhipanti akammaṃ na ca karaṇīyaṃ, sambahulāpi sambahule ukkhipanti akammaṃ na ca karaṇīyaṃ, sambahulāpi saṃghaṃ ukkhipanti akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.3.6"
msgid "saṃghopi saṃghaṃ ukkhipati akammaṃ na ca karaṇīyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-kd9:2.4.1"
msgid "Cattārimāni, bhikkhave, kammāni—"
msgstr ""

msgctxt "pli-tv-kd9:2.4.2"
msgid "adhammena vaggakammaṃ, adhammena samaggakammaṃ, dhammena vaggakammaṃ, dhammena samaggakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:2.4.3"
msgid "Tatra, bhikkhave, yadidaṃ adhammena vaggakammaṃ, idaṃ, bhikkhave, kammaṃ adhammattā vaggattā kuppaṃ aṭṭhānārahaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.4.4"
msgid "na, bhikkhave, evarūpaṃ kammaṃ kātabbaṃ, na ca mayā evarūpaṃ kammaṃ anuññātaṃ."
msgstr ""

msgctxt "pli-tv-kd9:2.4.5"
msgid "Tatra, bhikkhave, yadidaṃ adhammena samaggakammaṃ, idaṃ, bhikkhave, kammaṃ adhammattā kuppaṃ aṭṭhānārahaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.4.6"
msgid "na, bhikkhave, evarūpaṃ kammaṃ kātabbaṃ, na ca mayā evarūpaṃ kammaṃ anuññātaṃ."
msgstr ""

msgctxt "pli-tv-kd9:2.4.7"
msgid "Tatra, bhikkhave, yadidaṃ dhammena vaggakammaṃ, idaṃ, bhikkhave, kammaṃ vaggattā kuppaṃ aṭṭhānārahaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.4.8"
msgid "na, bhikkhave, evarūpaṃ kammaṃ kātabbaṃ, na ca mayā evarūpaṃ kammaṃ anuññātaṃ."
msgstr ""

msgctxt "pli-tv-kd9:2.4.9"
msgid "Tatra, bhikkhave, yadidaṃ dhammena samaggakammaṃ, idaṃ, bhikkhave, kammaṃ dhammattā samaggattā akuppaṃ ṭhānārahaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:2.4.10"
msgid "evarūpaṃ, bhikkhave, kammaṃ kātabbaṃ, evarūpañca mayā kammaṃ anuññātaṃ."
msgstr ""

msgctxt "pli-tv-kd9:2.4.11"
msgid "Tasmātiha, bhikkhave, evarūpaṃ kammaṃ karissāma yadidaṃ dhammena samagganti—"
msgstr ""

msgctxt "pli-tv-kd9:2.4.12"
msgid "evañhi vo, bhikkhave, sikkhitabban”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd9:3.1.0"
msgid "3. Ñattivipannakammādikathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc13, pts-vp-pli1.317
msgctxt "pli-tv-kd9:3.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū evarūpāni kammāni karonti—"
msgstr ""

msgctxt "pli-tv-kd9:3.1.2"
msgid "adhammena vaggakammaṃ karonti, adhammena samaggakammaṃ karonti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.3"
msgid "dhammena vaggakammaṃ karonti, dhammapatirūpakena vaggakammaṃ karonti, dhammapatirūpakena samaggakammaṃ karonti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.4"
msgid "ñattivipannampi kammaṃ karonti anussāvanasampannaṃ, anussāvanavipannampi kammaṃ karonti ñattisampannaṃ, ñattivipannampi anussāvanavipannampi kammaṃ karonti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.5"
msgid "aññatrāpi dhammā kammaṃ karonti, aññatrāpi vinayā kammaṃ karonti, aññatrāpi satthusāsanā kammaṃ karonti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.6"
msgid "paṭikuṭṭhakatampi kammaṃ karonti adhammikaṃ kuppaṃ aṭṭhānārahaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-kd9:3.1.7"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd9:3.1.9"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd9:3.1.10"
msgid "“kathañhi nāma chabbaggiyā bhikkhū evarūpāni kammāni karissanti—"
msgstr ""

msgctxt "pli-tv-kd9:3.1.11"
msgid "adhammena vaggakammaṃ karissanti, adhammena samaggakammaṃ karissanti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.12"
msgid "dhammena vaggakammaṃ karissanti, dhammapatirūpakena vaggakammaṃ karissanti, dhammapatirūpakena samaggakammaṃ karissanti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.13"
msgid "ñattivipannampi kammaṃ karissanti anussāvanasampannaṃ, anussāvanavipannampi kammaṃ karissanti ñattisampannaṃ, ñattivipannampi anussāvanavipannampi kammaṃ karissanti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.14"
msgid "aññatrāpi dhammā kammaṃ karissanti, aññatrāpi vinayā kammaṃ karissanti, aññatrāpi satthusāsanā kammaṃ karissanti;"
msgstr ""

msgctxt "pli-tv-kd9:3.1.15"
msgid "paṭikuṭṭhakatampi kammaṃ karissanti adhammikaṃ kuppaṃ aṭṭhānārahan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-kd9:3.1.16"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd9:3.1.18"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhū evarūpāni kammāni karonti—"
msgstr ""

msgctxt "pli-tv-kd9:3.1.19"
msgid "adhammena vaggakammaṃ karonti … pe …"
msgstr ""

msgctxt "pli-tv-kd9:3.1.21"
msgid "paṭikuṭṭhakatampi kammaṃ karonti adhammikaṃ kuppaṃ aṭṭhānārahan”ti?"
msgstr ""

msgctxt "pli-tv-kd9:3.1.22"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd9:3.1.23"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-kd9:3.1.25"
msgid "vigarahitvā dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-kd9:3.2.1"
msgid "“Adhammena ce, bhikkhave, vaggakammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.2"
msgid "adhammena samaggakammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.3"
msgid "dhammena vaggakammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.4"
msgid "dhammapatirūpakena vaggakammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.5"
msgid "dhammapatirūpakena samaggakammaṃ akammaṃ na ca karaṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.2.6"
msgid "Ñattivipannañce, bhikkhave, kammaṃ anussāvanasampannaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.7"
msgid "anussāvanavipannañce, bhikkhave, kammaṃ ñattisampannaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.8"
msgid "ñattivipannañce, bhikkhave, kammaṃ anussāvanavipannaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.9"
msgid "aññatrāpi dhammā kammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.10"
msgid "aññatrāpi vinayā kammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.11"
msgid "aññatrāpi satthusāsanā kammaṃ akammaṃ na ca karaṇīyaṃ;"
msgstr ""

msgctxt "pli-tv-kd9:3.2.12"
msgid "paṭikuṭṭhakatañce, bhikkhave, kammaṃ adhammikaṃ kuppaṃ aṭṭhānārahaṃ akammaṃ na ca karaṇīyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-kd9:3.3.1"
msgid "Chayimāni, bhikkhave, kammāni—"
msgstr ""

msgctxt "pli-tv-kd9:3.3.2"
msgid "adhammakammaṃ, vaggakammaṃ, samaggakammaṃ, dhammapatirūpakena vaggakammaṃ, dhammapatirūpakena samaggakammaṃ, dhammena samaggakammaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc18, pts-vp-pli1.318
msgctxt "pli-tv-kd9:3.3.3"
msgid "Katamañca, bhikkhave, adhammakammaṃ?"
msgstr ""

msgctxt "pli-tv-kd9:3.3.4"
msgid "Ñattidutiye ce, bhikkhave, kamme ekāya ñattiyā kammaṃ karoti, na ca kammavācaṃ anussāveti—"
msgstr ""

msgctxt "pli-tv-kd9:3.3.5"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.3.6"
msgid "Ñattidutiye ce, bhikkhave, kamme dvīhi ñattīhi kammaṃ karoti, na ca kammavācaṃ anussāveti—"
msgstr ""

msgctxt "pli-tv-kd9:3.3.7"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.3.8"
msgid "Ñattidutiye ce, bhikkhave, kamme ekāya kammavācāya kammaṃ karoti, na ca ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd9:3.3.9"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.3.10"
msgid "Ñattidutiye ce, bhikkhave, kamme dvīhi kammavācāhi kammaṃ karoti, na ca ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd9:3.3.11"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.1"
msgid "Ñatticatutthe ce, bhikkhave, kamme ekāya ñattiyā kammaṃ karoti, na ca kammavācaṃ anussāveti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.2"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.3"
msgid "Ñatticatutthe ce, bhikkhave, kamme dvīhi ñattīhi kammaṃ karoti, na ca kammavācaṃ anussāveti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.4"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.5"
msgid "Ñatticatutthe ce, bhikkhave, kamme tīhi ñattīhi kammaṃ karoti, na ca kammavācaṃ anussāveti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.6"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.7"
msgid "Ñatticatutthe ce, bhikkhave, kamme catūhi ñattīhi kammaṃ karoti, na ca kammavācaṃ anussāveti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.8"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.9"
msgid "Ñatticatutthe ce, bhikkhave, kamme ekāya kammavācāya kammaṃ karoti, na ca ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.10"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.11"
msgid "Ñatticatutthe ce, bhikkhave, kamme dvīhi kammavācāhi kammaṃ karoti, na ca ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.12"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.13"
msgid "Ñatticatutthe ce, bhikkhave, kamme tīhi kammavācāhi kammaṃ karoti, na ca ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.14"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.15"
msgid "Ñatticatutthe ce, bhikkhave, kamme catūhi kammavācāhi kammaṃ karoti, na ca ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd9:3.4.16"
msgid "adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.17"
msgid "Idaṃ vuccati, bhikkhave, adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.4.18"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-kd9:3.5.1"
msgid "Katamañca, bhikkhave, vaggakammaṃ?"
msgstr ""

msgctxt "pli-tv-kd9:3.5.2"
msgid "Ñattidutiye ce, bhikkhave, kamme yāvatikā bhikkhū kammappattā te anāgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd9:3.5.3"
msgid "vaggakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.5.4"
msgid "Ñattidutiye ce, bhikkhave, kamme yāvatikā bhikkhū kammappattā te āgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd9:3.5.5"
msgid "vaggakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.5.6"
msgid "Ñattidutiye ce, bhikkhave, kamme yāvatikā bhikkhū kammappattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd9:3.5.7"
msgid "vaggakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.5.8"
msgid "Ñatticatutthe ce, bhikkhave, kamme yāvatikā bhikkhū kammappattā te anāgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd9:3.5.9"
msgid "vaggakammaṃ."
msgstr ""

msgctxt "pli-tv-kd9:3.5.10"
msgid "Ñatticatutthe ce, bhikkhave, kamme yāvatikā bhikkhū kammappattā te āgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti—"
msgstr ""

