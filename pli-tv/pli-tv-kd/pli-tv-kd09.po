#. HTML: </h2><p>
#. REF: sc56, pts-vp-pli1.326
msgctxt "pli-tv-kd9:6.1.1"
msgid "Atha kho āyasmā upāli yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd9:6.1.2"
msgid "Ekamantaṃ nisinno kho āyasmā upāli bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd9:6.1.3"
msgid "“yo nu kho, bhante, samaggo saṃgho sammukhākaraṇīyaṃ kammaṃ asammukhā karoti, dhammakammaṃ nu kho taṃ, bhante, vinayakamman”ti?"
msgstr ""

msgctxt "pli-tv-kd9:6.1.4"
msgid "“Adhammakammaṃ taṃ, upāli, avinayakamman”ti."
msgstr ""

msgctxt "pli-tv-kd9:6.2.1"
msgid "“Yo nu kho, bhante, samaggo saṃgho paṭipucchākaraṇīyaṃ kammaṃ appaṭipucchā karoti … pe …"
msgstr ""

msgctxt "pli-tv-kd9:6.2.3"
msgid "paṭiññāyakaraṇīyaṃ kammaṃ appaṭiññāya karoti …"
msgstr ""

msgctxt "pli-tv-kd9:6.2.4"
msgid "sativinayārahassa amūḷhavinayaṃ deti …"
msgstr ""

msgctxt "pli-tv-kd9:6.2.5"
msgid "amūḷhavinayārahassa tassapāpiyasikākammaṃ karoti …"
msgstr ""

msgctxt "pli-tv-kd9:6.2.6"
msgid "tassapāpiyasikākammārahassa tajjanīyakammaṃ karoti …"
msgstr ""

msgctxt "pli-tv-kd9:6.2.7"
msgid "tajjanīyakammārahassa niyassakammaṃ karoti …"
msgstr ""

msgctxt "pli-tv-kd9:6.2.8"
msgid "niyassakammārahassa pabbājanīyakammaṃ karoti …"
msgstr ""

msgctxt "pli-tv-kd9:6.2.9"
