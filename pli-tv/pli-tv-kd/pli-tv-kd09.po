#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-kd9:4.8.1"
msgid "Kassa ca, bhikkhave, saṃghamajjhe paṭikkosanā ruhati?"
msgstr ""

#. VAR: ānantarikassāpi → anantarikassāpi (s1-3)
msgctxt "pli-tv-kd9:4.8.2"
msgid "Bhikkhussa, bhikkhave, pakatattassa samānasaṃvāsakassa samānasīmāya ṭhitassa antamaso ānantarikassāpi bhikkhuno viññāpentassa saṃghamajjhe paṭikkosanā ruhati."
msgstr ""

#. VAR: Imassa → imassa kho (s1-3, pts1)
msgctxt "pli-tv-kd9:4.8.3"
msgid "Imassa, bhikkhave, saṃghamajjhe paṭikkosanā ruhati."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd9:4.9.0"
msgid "6. Dvenissāraṇādikathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc36
msgctxt "pli-tv-kd9:4.9.1"
msgid "Dvemā, bhikkhave, nissāraṇā."
msgstr ""

msgctxt "pli-tv-kd9:4.9.2"
msgid "Atthi, bhikkhave, puggalo appatto nissāraṇaṃ."
msgstr ""

msgctxt "pli-tv-kd9:4.9.3"
msgid "Tañce saṃgho nissāreti, ekacco sunissārito, ekacco dunnissārito."
msgstr ""

msgctxt "pli-tv-kd9:4.9.4"
msgid "Katamo ca, bhikkhave, puggalo appatto nissāraṇaṃ, tañce saṃgho nissāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.9.5"
msgid "dunnissārito?"
msgstr ""

msgctxt "pli-tv-kd9:4.9.6"
msgid "Idha pana, bhikkhave, bhikkhu suddho hoti anāpattiko."
msgstr ""

msgctxt "pli-tv-kd9:4.9.7"
msgid "Tañce saṃgho nissāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.9.8"
msgid "dunnissārito."
msgstr ""

msgctxt "pli-tv-kd9:4.9.9"
msgid "Ayaṃ vuccati, bhikkhave, puggalo appatto nissāraṇaṃ, tañce saṃgho nissāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.9.10"
msgid "dunnissārito."
msgstr ""

#. HTML: </p><p>
#. REF: sc37, pts-vp-pli1.322
msgctxt "pli-tv-kd9:4.9.11"
msgid "Katamo ca, bhikkhave, puggalo appatto nissāraṇaṃ, tañce saṃgho nissāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.9.12"
msgid "sunissārito?"
msgstr ""

msgctxt "pli-tv-kd9:4.9.13"
msgid "Idha pana, bhikkhave, bhikkhu bālo hoti abyatto āpattibahulo anapadāno, gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi, tañce saṃgho nissāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.9.14"
msgid "sunissārito."
msgstr ""

msgctxt "pli-tv-kd9:4.9.15"
msgid "Ayaṃ vuccati, bhikkhave, puggalo appatto nissāraṇaṃ, tañce saṃgho nissāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.9.16"
msgid "sunissārito."
msgstr ""

#. HTML: </p><p>
#. REF: sc38
msgctxt "pli-tv-kd9:4.10.1"
msgid "Dvemā, bhikkhave, osāraṇā."
msgstr ""

msgctxt "pli-tv-kd9:4.10.2"
msgid "Atthi, bhikkhave, puggalo appatto osāraṇaṃ tañce saṃgho osāreti, ekacco sosārito, ekacco dosārito."
msgstr ""

msgctxt "pli-tv-kd9:4.10.3"
msgid "Katamo ca, bhikkhave, puggalo appatto osāraṇaṃ, tañce saṃgho osāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.10.4"
msgid "dosārito?"
msgstr ""

msgctxt "pli-tv-kd9:4.10.5"
msgid "Paṇḍako, bhikkhave, appatto osāraṇaṃ, tañce saṃgho osāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.10.6"
msgid "dosārito."
msgstr ""

msgctxt "pli-tv-kd9:4.10.7"
msgid "Theyyasaṃvāsako, bhikkhave, appatto osāraṇaṃ, tañce saṃgho osāreti—"
msgstr ""

msgctxt "pli-tv-kd9:4.10.8"
msgid "dosārito."
msgstr ""

