#. HTML: </p><h2>
msgctxt "pli-tv-kd9:4.1.0"
msgid "4. Catuvaggakaraṇādikathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc24, pts-vp-pli1.320
msgctxt "pli-tv-kd9:4.1.1"
msgid "Pañca saṃghā—"
msgstr ""

msgctxt "pli-tv-kd9:4.1.2"
msgid "catuvaggo bhikkhusaṃgho pañcavaggo bhikkhusaṃgho, dasavaggo bhikkhusaṃgho, vīsativaggo bhikkhusaṃgho, atirekavīsativaggo bhikkhusaṃgho."
msgstr ""

msgctxt "pli-tv-kd9:4.1.3"
msgid "Tatra, bhikkhave, yvāyaṃ catuvaggo bhikkhusaṃgho, ṭhapetvā tīṇi kammāni—"
msgstr ""

msgctxt "pli-tv-kd9:4.1.4"
msgid "upasampadaṃ pavāraṇaṃ abbhānaṃ, dhammena samaggo sabbakammesu kammappatto."
msgstr ""

msgctxt "pli-tv-kd9:4.1.5"
msgid "Tatra, bhikkhave, yvāyaṃ pañcavaggo bhikkhusaṃgho, ṭhapetvā dve kammāni—"
msgstr ""

msgctxt "pli-tv-kd9:4.1.6"
msgid "majjhimesu janapadesu upasampadaṃ abbhānaṃ, dhammena samaggo sabbakammesu kammappatto."
msgstr ""

msgctxt "pli-tv-kd9:4.1.7"
msgid "Tatra, bhikkhave, yvāyaṃ dasavaggo bhikkhusaṃgho, ṭhapetvā ekaṃ kammaṃ—"
msgstr ""

msgctxt "pli-tv-kd9:4.1.8"
msgid "abbhānaṃ, dhammena samaggo sabbakammesu kammappatto."
msgstr ""

msgctxt "pli-tv-kd9:4.1.9"
msgid "Tatra, bhikkhave, yvāyaṃ vīsativaggo bhikkhusaṃgho dhammena samaggo sabbakammesu kammappatto."
msgstr ""

msgctxt "pli-tv-kd9:4.1.10"
msgid "Tatra, bhikkhave, yvāyaṃ atirekavīsativaggo bhikkhusaṃgho dhammena samaggo sabbakammesu kammappatto."
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-kd9:4.2.1"
