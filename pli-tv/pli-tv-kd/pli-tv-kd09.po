#. HTML: </p><p class="endsutta">
#. REF: sc30
msgctxt "pli-tv-kd9:4.5.1"
msgid "Dasavaggakaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-kd9:4.5.2"
msgid "Vīsativaggakaraṇañce, bhikkhave, kammaṃ bhikkhunivīso kammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-kd9:4.5.3"
msgid "akammaṃ na ca karaṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:4.5.4"
msgid "Vīsativaggakaraṇañce, bhikkhave, kammaṃ sikkhamānavīso kammaṃ kareyya … pe …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.6"
msgid "sāmaṇeravīso kammaṃ kareyya …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.7"
msgid "sāmaṇerivīso kammaṃ kareyya …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.8"
msgid "sikkhaṃ paccakkhātakavīso kammaṃ kareyya …"
msgstr ""

msgctxt "pli-tv-kd9:4.5.9"
msgid "antimavatthuṃ ajjhāpannakavīso kammaṃ kareyya …"
