msgstr ""

msgctxt "pli-tv-kd9:7.20.75"
msgid "Tatra ce bhikkhūnaṃ evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.76"
msgid "ayaṃ kho, āvuso, bhikkhu saṃghena āpattiyā appaṭikamme ukkhepanīyakammakato sammā vattati, lomaṃ pāteti, netthāraṃ vattati, āpattiyā appaṭikamme ukkhepanīyassa kammassa paṭippassaddhiṃ yācati."
msgstr ""

msgctxt "pli-tv-kd9:7.20.77"
msgid "Handassa mayaṃ āpattiyā appaṭikamme ukkhepanīyakammaṃ paṭippassambhemā’ti."
msgstr ""

msgctxt "pli-tv-kd9:7.20.78"
msgid "Te tassa āpattiyā appaṭikamme ukkhepanīyakammaṃ paṭippassambhenti—"
msgstr ""

msgctxt "pli-tv-kd9:7.20.79"
msgid "adhammena vaggā … pe …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.81"
msgid "adhammena samaggā …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.82"
msgid "dhammena vaggā …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.83"
msgid "dhammapatirūpakena vaggā …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.84"
msgid "dhammapatirūpakena samaggā."
msgstr ""

msgctxt "pli-tv-kd9:7.20.85"
msgid "Tatraṭṭho saṃgho vivadati—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.86"
msgid "adhammena vaggakammaṃ, adhammena samaggakammaṃ, dhammena vaggakammaṃ, dhammapatirūpakena vaggakammaṃ, dhammapatirūpakena samaggakammaṃ, akataṃ kammaṃ dukkaṭaṃ kammaṃ puna kātabbaṃ kamman’ti."
msgstr ""

msgctxt "pli-tv-kd9:7.20.87"
msgid "Tatra, bhikkhave, ye te bhikkhū evamāhaṃsu—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.88"
msgid "dhammapatirūpakena samaggakamman’ti, ye ca te bhikkhū evamāhaṃsu—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.89"
msgid "akataṃ kammaṃ dukkaṭaṃ kammaṃ puna kātabbaṃ kamman’ti, ime tattha bhikkhū dhammavādino."
msgstr ""

msgctxt "pli-tv-kd9:7.20.90"
msgid "Imepi pañca vārā saṃkhittā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd9:7.20.91"
msgid "36. Appaṭinissaggeukkhepanīyakammapaṭippassaddhikathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc123
msgctxt "pli-tv-kd9:7.20.92"
msgid "Idha pana, bhikkhave, bhikkhu saṃghena pāpikāya diṭṭhiyā appaṭinissagge ukkhepanīyakammakato sammā vattati, lomaṃ pāteti, netthāraṃ vattati, pāpikāya diṭṭhiyā appaṭinissagge ukkhepanīyassa kammassa paṭippassaddhiṃ yācati."
msgstr ""

msgctxt "pli-tv-kd9:7.20.93"
msgid "Tatra ce bhikkhūnaṃ evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.94"
msgid "ayaṃ kho, āvuso, bhikkhu saṃghena pāpikāya diṭṭhiyā appaṭinissagge ukkhepanīyakammakato sammā vattati, lomaṃ pāteti, netthāraṃ vattati, pāpikāya diṭṭhiyā appaṭinissagge ukkhepanīyassa kammassa paṭippassaddhiṃ yācati."
msgstr ""

msgctxt "pli-tv-kd9:7.20.95"
msgid "Handassa mayaṃ pāpikāya diṭṭhiyā appaṭinissagge ukkhepanīyakammaṃ paṭippassambhemā’ti."
msgstr ""

msgctxt "pli-tv-kd9:7.20.96"
msgid "Te tassa pāpikāya diṭṭhiyā appaṭinissagge ukkhepanīyakammaṃ paṭippassambhenti—"
msgstr ""

msgctxt "pli-tv-kd9:7.20.97"
msgid "adhammena vaggā … pe …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.99"
msgid "adhammena samaggā …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.100"
msgid "dhammena vaggā …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.101"
msgid "dhammapatirūpakena vaggā …"
msgstr ""

msgctxt "pli-tv-kd9:7.20.102"
msgid "dhammapatirūpakena samaggā."
msgstr ""

msgctxt "pli-tv-kd9:7.20.103"
msgid "Tatraṭṭho saṃgho vivadati—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.104"
msgid "adhammena vaggakammaṃ, adhammena samaggakammaṃ, dhammena vaggakammaṃ, dhammapatirūpakena vaggakammaṃ, dhammapatirūpakena samaggakammaṃ, akataṃ kammaṃ dukkaṭaṃ kammaṃ puna kātabbaṃ kamman’ti."
msgstr ""

msgctxt "pli-tv-kd9:7.20.105"
msgid "Tatra, bhikkhave, ye te bhikkhū evamāhaṃsu—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.106"
msgid "dhammapatirūpakena samaggakamman’ti, ye ca te bhikkhū evamāhaṃsu—‘"
msgstr ""

msgctxt "pli-tv-kd9:7.20.107"
msgid "akataṃ kammaṃ dukkaṭaṃ kammaṃ puna kātabbaṃ kamman’ti, ime tattha bhikkhū dhammavādino."
msgstr ""

msgctxt "pli-tv-kd9:7.20.108"
msgid "Imepi pañca vārā saṃkhittā”."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc124
msgctxt "pli-tv-kd9:7.20.109"
msgid "Campeyyakkhandhako navamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc125
msgctxt "pli-tv-kd9:7.20.110"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc126
msgctxt "pli-tv-kd9:7.20.111"
msgid "Campāyaṃ bhagavā āsi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.112"
msgid "vatthu vāsabhagāmake;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.113"
msgid "Āgantukānamussukkaṃ,"
msgstr ""

#. HTML: <br>
#. VAR: icchitabbake → icchitabbako (mr)
msgctxt "pli-tv-kd9:7.20.114"
msgid "akāsi icchitabbake."
msgstr ""

#. HTML: </p><p>
#. REF: sc127
msgctxt "pli-tv-kd9:7.20.115"
msgid "Pakataññunoti ñatvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.116"
msgid "ussukkaṃ na karī tadā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.117"
msgid "Ukkhitto na karotīti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.118"
msgid "sāgamā jinasantike."
msgstr ""

#. HTML: </p><p>
#. REF: sc128, pts-vp-pli1.334
msgctxt "pli-tv-kd9:7.20.119"
msgid "Adhammena vaggakammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.120"
msgid "samaggaṃ adhammena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.121"
msgid "Dhammena vaggakammañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.122"
msgid "patirūpakena vaggikaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc129
msgctxt "pli-tv-kd9:7.20.123"
msgid "Patirūpakena samaggaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.124"
msgid "eko ukkhipatekakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.125"
msgid "Eko ca dve sambahule,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.126"
msgid "saṃghaṃ ukkhipatekako."
msgstr ""

#. HTML: </p><p>
#. REF: sc130
msgctxt "pli-tv-kd9:7.20.127"
msgid "Duvepi sambahulāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.128"
msgid "saṃgho saṃghañca ukkhipi;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.129"
msgid "Sabbaññupavaro sutvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.130"
msgid "adhammanti paṭikkhipi."
msgstr ""

#. HTML: </p><p>
#. REF: sc131
msgctxt "pli-tv-kd9:7.20.131"
msgid "Ñattivipannaṃ yaṃ kammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.132"
msgid "sampannaṃ anusāvanaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.133"
msgid "Anussāvanavipannaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.134"
msgid "sampannaṃ ñattiyā ca yaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc132
msgctxt "pli-tv-kd9:7.20.135"
msgid "Ubhayena vipannañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.136"
msgid "aññatra dhammameva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.137"
msgid "Vinayā satthu paṭikuṭṭhaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.138"
msgid "kuppaṃ aṭṭhānārahikaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc133
msgctxt "pli-tv-kd9:7.20.139"
msgid "Adhammavaggaṃ samaggaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.140"
msgid "Dhamma patirūpāni ye duve;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.141"
msgid "Dhammeneva ca sāmaggiṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.142"
msgid "Anuññāsi tathāgato."
msgstr ""

#. HTML: </p><p>
#. REF: sc134
msgctxt "pli-tv-kd9:7.20.143"
msgid "Catuvaggo pañcavaggo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.144"
msgid "dasavaggo ca vīsati;"
msgstr ""

#. HTML: <br>
#. VAR: Parovīsativaggo ca → atirekavīsativaggo (s1-3)
msgctxt "pli-tv-kd9:7.20.145"
msgid "Parovīsativaggo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.146"
msgid "saṃgho pañcavidho tathā."
msgstr ""

#. HTML: </p><p>
#. REF: sc135
msgctxt "pli-tv-kd9:7.20.147"
msgid "Ṭhapetvā upasampadaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.148"
msgid "yañca kammaṃ pavāraṇaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.149"
msgid "Abbhānakammena saha,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.150"
msgid "catuvaggehi kammiko."
msgstr ""

#. HTML: </p><p>
#. REF: sc136
msgctxt "pli-tv-kd9:7.20.151"
msgid "Duve kamme ṭhapetvāna,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.152"
msgid "majjhadesūpasampadaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.153"
msgid "Abbhānaṃ pañcavaggiko,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.154"
msgid "sabbakammesu kammiko."
msgstr ""

#. HTML: </p><p>
#. REF: sc137
msgctxt "pli-tv-kd9:7.20.155"
msgid "Abbhānekaṃ ṭhapetvāna,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.156"
msgid "ye bhikkhū dasavaggikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.157"
msgid "Sabbakammakaro saṃgho,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.158"
msgid "vīso sabbattha kammiko."
msgstr ""

#. HTML: </p><p>
#. REF: sc138
msgctxt "pli-tv-kd9:7.20.159"
msgid "Bhikkhunī sikkhamānā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.160"
msgid "sāmaṇero sāmaṇerī;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.161"
msgid "Paccakkhātantimavatthū,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.162"
msgid "ukkhittāpattidassane."
msgstr ""

#. HTML: </p><p>
#. REF: sc139
msgctxt "pli-tv-kd9:7.20.163"
msgid "Appaṭikamme diṭṭhiyā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.164"
msgid "paṇḍako theyyasaṃvāsakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.165"
msgid "Titthiyā tiracchānagataṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.166"
msgid "mātu pitu ca ghātakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc140
msgctxt "pli-tv-kd9:7.20.167"
msgid "Arahaṃ bhikkhunīdūsi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.168"
msgid "bhedakaṃ lohituppādaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.169"
msgid "Byañjanaṃ nānāsaṃvāsaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.170"
msgid "nānāsīmāya iddhiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc141
msgctxt "pli-tv-kd9:7.20.171"
msgid "Yassa saṃgho kare kammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.172"
msgid "hontete catuvīsati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.173"
msgid "Sambuddhena paṭikkhittā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.174"
msgid "na hete gaṇapūrakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc142
msgctxt "pli-tv-kd9:7.20.175"
msgid "Pārivāsikacatuttho,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.176"
msgid "parivāsaṃ dadeyya vā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.177"
msgid "Mūlā mānattamabbheyya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.178"
msgid "akammaṃ na ca karaṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc143
msgctxt "pli-tv-kd9:7.20.179"
msgid "Mūlā arahamānattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.180"
msgid "abbhānārahameva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.181"
msgid "Na kammakārakā pañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.182"
msgid "sambuddhena pakāsitā."
msgstr ""

#. HTML: </p><p>
#. REF: sc144
msgctxt "pli-tv-kd9:7.20.183"
msgid "Bhikkhunī sikkhamānā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.184"
msgid "sāmaṇero sāmaṇerikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.185"
msgid "Paccakkhantimaummattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.186"
msgid "khittāvedanadassane."
msgstr ""

#. HTML: </p><p>
#. REF: sc145
msgctxt "pli-tv-kd9:7.20.187"
msgid "Appaṭikamme diṭṭhiyā,"
msgstr ""

#. HTML: <br>
#. VAR: byañjanā → ito paraṃ s1-3 potthakesu diyaḍḍhagāthāhi abhabbapuggalā samattaṃ
msgctxt "pli-tv-kd9:7.20.188"
msgid "paṇḍakāpi ca byañjanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.189"
msgid "Nānāsaṃvāsakā sīmā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.190"
msgid "vehāsaṃ yassa kamma ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc146, pts-vp-pli1.335
msgctxt "pli-tv-kd9:7.20.191"
msgid "Aṭṭhārasannametesaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.192"
msgid "paṭikkosaṃ na ruhati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.193"
msgid "Bhikkhussa pakatattassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.194"
msgid "ruhati paṭikkosanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc147
msgctxt "pli-tv-kd9:7.20.195"
msgid "Suddhassa dunnisārito,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.196"
msgid "bālo hi sunissārito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.197"
msgid "Paṇḍako theyyasaṃvāso,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.198"
msgid "pakkanto tiracchānagato."
msgstr ""

#. HTML: </p><p>
#. REF: sc148
msgctxt "pli-tv-kd9:7.20.199"
msgid "Mātu pitu arahanta,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.200"
msgid "dūsako saṃghabhedako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.201"
msgid "Lohituppādako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.202"
msgid "ubhatobyañjano ca yo."
msgstr ""

#. HTML: </p><p>
#. REF: sc149
msgctxt "pli-tv-kd9:7.20.203"
msgid "Ekādasannaṃ etesaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.204"
msgid "osāraṇaṃ na yujjati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.205"
msgid "Hatthapādaṃ tadubhayaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.206"
msgid "kaṇṇanāsaṃ tadūbhayaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc150
msgctxt "pli-tv-kd9:7.20.207"
msgid "Aṅguli aḷakaṇḍaraṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.208"
msgid "phaṇaṃ khujjo ca vāmano;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.209"
msgid "Gaṇḍī lakkhaṇakasā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.210"
msgid "likhitako ca sīpadī."
msgstr ""

#. HTML: </p><p>
#. REF: sc151
msgctxt "pli-tv-kd9:7.20.211"
msgid "Pāpā parisakāṇo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.212"
msgid "kuṇī khañjo hatopi ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.213"
msgid "Iriyāpathadubbalo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.214"
msgid "andho mūgo ca badhiro."
msgstr ""

#. HTML: </p><p>
#. REF: sc152
msgctxt "pli-tv-kd9:7.20.215"
msgid "Andhamūgandhabadhiro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.216"
msgid "mūgabadhirameva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.217"
msgid "Andhamūgabadhiro ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.218"
msgid "dvattiṃsete anūnakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc153
msgctxt "pli-tv-kd9:7.20.219"
msgid "Tesaṃ osāraṇaṃ hoti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.220"
msgid "sambuddhena pakāsitaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.221"
msgid "Daṭṭhabbā paṭikātabbā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.222"
msgid "nissajjetā na vijjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc154
msgctxt "pli-tv-kd9:7.20.223"
msgid "Tassa ukkhepanā kammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.224"
msgid "satta honti adhammikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.225"
msgid "Āpannaṃ anuvattantaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.226"
msgid "satta tepi adhammikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc155
msgctxt "pli-tv-kd9:7.20.227"
msgid "Āpannaṃ nānuvattantaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.228"
msgid "satta kammā sudhammikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.229"
msgid "Sammukhā paṭipucchā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.230"
msgid "paṭiññāya ca kāraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc156
msgctxt "pli-tv-kd9:7.20.231"
msgid "Sati amūḷhapāpikā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.232"
msgid "tajjanīniyassena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.233"
msgid "Pabbājanīya paṭisāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.234"
msgid "ukkhepaparivāsa ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc157
msgctxt "pli-tv-kd9:7.20.235"
msgid "Mūlā mānattaabbhānā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.236"
msgid "tatheva upasampadā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.237"
msgid "Aññaṃ kareyya aññassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.238"
msgid "soḷasete adhammikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc158
msgctxt "pli-tv-kd9:7.20.239"
msgid "Taṃ taṃ kareyya taṃ tassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.240"
msgid "soḷasete sudhammikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.241"
msgid "Paccāropeyya aññaññaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.242"
msgid "soḷasete adhammikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc159
#. VAR: tammūlakaṃ tassa → dve dve mūlā katā tassa (s1-3) | dodotamūlakantassa (mr)
msgctxt "pli-tv-kd9:7.20.243"
msgid "Dve dve tammūlakaṃ tassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.244"
msgid "tepi soḷasa dhammikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.245"
msgid "Ekekamūlakaṃ cakkaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.246"
msgid "“adhamman”ti jinobravi."
msgstr ""

#. HTML: </p><p>
#. REF: sc160
msgctxt "pli-tv-kd9:7.20.247"
msgid "Akāsi tajjanīyaṃ kammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.248"
msgid "saṃgho bhaṇḍanakārako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.249"
msgid "Adhammena vaggakammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.250"
msgid "aññaṃ āvāsaṃ gacchi so."
msgstr ""

#. HTML: </p><p>
#. REF: sc161
msgctxt "pli-tv-kd9:7.20.251"
msgid "Tatthādhammena samaggā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.252"
msgid "tassa tajjanīyaṃ karuṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.253"
msgid "Aññattha vaggādhammena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.254"
msgid "tassa tajjanīyaṃ karuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc162
msgctxt "pli-tv-kd9:7.20.255"
msgid "Patirūpena vaggāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.256"
msgid "samaggāpi tathā karuṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.257"
msgid "Adhammena samaggā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.258"
msgid "dhammena vaggameva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc163
msgctxt "pli-tv-kd9:7.20.259"
msgid "Patirūpakena vaggā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.260"
msgid "samaggā ca ime padā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.261"
msgid "Ekekamūlakaṃ katvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.262"
msgid "cakkaṃ bandhe vicakkhaṇo."
msgstr ""

#. HTML: </p><p>
#. REF: sc164
msgctxt "pli-tv-kd9:7.20.263"
msgid "Bālā byattassa niyassaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.264"
msgid "pabbāje kuladūsakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.265"
msgid "Paṭisāraṇīyaṃ kammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.266"
msgid "kare akkosakassa ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc165, pts-vp-pli1.336
msgctxt "pli-tv-kd9:7.20.267"
msgid "Adassanāppaṭikamme,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.268"
msgid "yo ca diṭṭhiṃ na nissajje;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.269"
msgid "Tesaṃ ukkhepanīyakammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.270"
msgid "satthavāhena bhāsitaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc166
#. VAR: Upari nayakammānaṃ → upavinayakammānaṃ (s1-3) | ukkhepaniyakammānaṃ (pts1) | ukkhepanīyakammānaṃ (mr)
msgctxt "pli-tv-kd9:7.20.271"
msgid "Upari nayakammānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.272"
msgid "pañño tajjanīyaṃ naye;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.273"
msgid "Tesaṃyeva anulomaṃ,"
msgstr ""

#. HTML: <br>
#. VAR: yācite → yācito (bj) | yācati (s1-3)
msgctxt "pli-tv-kd9:7.20.274"
msgid "sammā vattati yācite."
msgstr ""

#. HTML: </p><p>
#. REF: sc167
msgctxt "pli-tv-kd9:7.20.275"
msgid "Passaddhi tesaṃ kammānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.276"
msgid "heṭṭhā kammanayena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.277"
msgid "Tasmiṃ tasmiṃ tu kammesu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.278"
msgid "tatraṭṭho ca vivadati."
msgstr ""

#. HTML: </p><p>
#. REF: sc168
msgctxt "pli-tv-kd9:7.20.279"
msgid "Akataṃ dukkaṭañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.280"
msgid "punakātabbakanti ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.281"
msgid "Kamme passaddhiyā cāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.282"
msgid "te bhikkhū dhammavādino."
msgstr ""

#. HTML: </p><p>
#. REF: sc169
msgctxt "pli-tv-kd9:7.20.283"
msgid "Vipattibyādhite disvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.284"
msgid "kammappatte mahāmuni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.285"
msgid "Paṭippassaddhimakkhāsi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd9:7.20.286"
msgid "sallakattova osadhanti."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc170
msgctxt "pli-tv-kd9:7.20.287"
msgid "Imamhi khandhake vatthūni chattiṃsāti."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc171
msgctxt "pli-tv-kd9:7.20.288"
msgid "Campeyyakkhandhako niṭṭhito."
msgstr ""

