msgid "yassa saṃgho kammaṃ karoti, taṃvīso kammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-kd9:4.5.28"
msgid "akammaṃ na ca karaṇīyaṃ."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc32
msgctxt "pli-tv-kd9:4.6.1"
msgid "Vīsativaggakaraṇaṃ."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd9:4.6.2"
msgid "5. Pārivāsikādikathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc33, pts-vp-pli1.321
msgctxt "pli-tv-kd9:4.6.3"
msgid "Pārivāsikacatuttho ce, bhikkhave, parivāsaṃ dadeyya, mūlāya paṭikasseyya, mānattaṃ dadeyya, taṃvīso abbheyya—"
msgstr ""

msgctxt "pli-tv-kd9:4.6.4"
msgid "akammaṃ na ca karaṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:4.6.5"
msgid "Mūlāyapaṭikassanārahacatuttho ce, bhikkhave, parivāsaṃ dadeyya, mūlāya paṭikasseyya, mānattaṃ dadeyya, taṃvīso abbheyya—"
msgstr ""

msgctxt "pli-tv-kd9:4.6.6"
msgid "akammaṃ na ca karaṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:4.6.7"
msgid "Mānattārahacatuttho ce, bhikkhave, parivāsaṃ dadeyya, mūlāya paṭikasseyya, mānattaṃ dadeyya, taṃvīso abbheyya—"
msgstr ""

msgctxt "pli-tv-kd9:4.6.8"
msgid "akammaṃ na ca karaṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:4.6.9"
msgid "Mānattacārikacatuttho ce, bhikkhave, parivāsaṃ dadeyya, mūlāya paṭikasseyya, mānattaṃ dadeyya, taṃvīso abbheyya—"
msgstr ""

msgctxt "pli-tv-kd9:4.6.10"
msgid "akammaṃ na ca karaṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd9:4.6.11"
msgid "Abbhānārahacatuttho ce, bhikkhave, parivāsaṃ dadeyya, mūlāya paṭikasseyya, mānattaṃ dadeyya, taṃvīso abbheyya—"
msgstr ""

msgctxt "pli-tv-kd9:4.6.12"
msgid "akammaṃ na ca karaṇīyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-kd9:4.7.1"
msgid "Ekaccassa, bhikkhave, saṃghamajjhe paṭikkosanā ruhati, ekaccassa na ruhati."
msgstr ""

msgctxt "pli-tv-kd9:4.7.2"
msgid "Kassa ca, bhikkhave, saṃghamajjhe paṭikkosanā na ruhati?"
msgstr ""

msgctxt "pli-tv-kd9:4.7.3"
msgid "Bhikkhuniyā, bhikkhave, saṃghamajjhe paṭikkosanā na ruhati."
msgstr ""

