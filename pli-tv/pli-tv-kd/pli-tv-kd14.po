#. HTML: </p><h2>
msgctxt "pli-tv-kd14:4.1.0"
msgid "2. Sativinaya"
msgstr ""

#. HTML: </h2><p>
#. REF: sc23
msgctxt "pli-tv-kd14:4.1.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pli-tv-kd14:4.1.2"
msgid "Tena kho pana samayena āyasmatā dabbena mallaputtena jātiyā sattavassena arahattaṃ sacchikataṃ hoti."
msgstr ""

msgctxt "pli-tv-kd14:4.1.3"
msgid "Yaṃ kiñci sāvakena pattabbaṃ sabbaṃ tena anuppattaṃ hoti."
msgstr ""

#. VAR: uttari → uttariṃ (bj, pts1)
msgctxt "pli-tv-kd14:4.1.4"
msgid "Natthi cassa kiñci uttari karaṇīyaṃ, katassa vā paticayo."
msgstr ""

msgctxt "pli-tv-kd14:4.1.5"
msgid "Atha kho āyasmato dabbassa mallaputtassa rahogatassa paṭisallīnassa evaṃ cetaso parivitakko udapādi—"
msgstr ""

msgctxt "pli-tv-kd14:4.1.6"
msgid "“mayā kho jātiyā sattavassena arahattaṃ sacchikataṃ."
msgstr ""

msgctxt "pli-tv-kd14:4.1.7"
msgid "Yaṃ kiñci sāvakena pattabbaṃ sabbaṃ mayā anuppattaṃ."
msgstr ""

msgctxt "pli-tv-kd14:4.1.8"
msgid "Natthi ca me kiñci uttarikaraṇīyaṃ, katassa vā paticayo."
msgstr ""

msgctxt "pli-tv-kd14:4.1.9"
msgid "Kiṃ nu kho ahaṃ saṃghassa veyyāvaccaṃ kareyyan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc24, pts-vp-pli2.75
msgctxt "pli-tv-kd14:4.1.10"
msgid "Atha kho āyasmato dabbassa mallaputtassa etadahosi—"
msgstr ""

msgctxt "pli-tv-kd14:4.1.11"
msgid "“yannūnāhaṃ saṃghassa senāsanañca paññapeyyaṃ bhattāni ca uddiseyyan”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.2.1"
msgid "Atha kho āyasmā dabbo mallaputto sāyanhasamayaṃ paṭisallānā vuṭṭhito yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd14:4.2.2"
msgid "Ekamantaṃ nisinno kho āyasmā dabbo mallaputto bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.2.3"
msgid "“idha mayhaṃ, bhante, rahogatassa paṭisallīnassa evaṃ cetaso parivitakko udapādi—‘"
msgstr ""

msgctxt "pli-tv-kd14:4.2.4"
msgid "mayā kho jātiyā sattavassena arahattaṃ sacchikataṃ."
msgstr ""

msgctxt "pli-tv-kd14:4.2.5"
msgid "Yaṃ kiñci sāvakena pattabbaṃ, sabbaṃ mayā anuppattaṃ."
msgstr ""

msgctxt "pli-tv-kd14:4.2.6"
msgid "Natthi ca me kiñci uttarikaraṇīyaṃ, katassa vā paticayo."
msgstr ""

msgctxt "pli-tv-kd14:4.2.7"
msgid "Kiṃ nu kho ahaṃ saṃghassa veyyāvaccaṃ kareyyan’ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.2.8"
msgid "Tassa mayhaṃ, bhante, etadahosi—"
msgstr ""

msgctxt "pli-tv-kd14:4.2.9"
msgid "‘yannūnāhaṃ saṃghassa senāsanañca paññapeyyaṃ bhattāni ca uddiseyyan’ti."
msgstr ""

msgctxt "pli-tv-kd14:4.2.10"
msgid "Icchāmahaṃ, bhante, saṃghassa senāsanañca paññāpetuṃ bhattāni ca uddisitun”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.2.11"
msgid "“Sādhu sādhu, dabba."
msgstr ""

#. VAR: uddisāhī”ti → uddisāti (^pi-tv-bu-vb-ss8#1 [Saṅghādisesā 8: Duṭṭhadosasikkhāpada])
msgctxt "pli-tv-kd14:4.2.12"
msgid "Tena hi tvaṃ, dabba, saṃghassa senāsanañca paññapehi bhattāni ca uddisāhī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.2.13"
msgid "“Evaṃ, bhante”ti kho āyasmā dabbo mallaputto bhagavato paccassosi."
msgstr ""

msgctxt "pli-tv-kd14:4.3.1"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd14:4.3.2"
msgid "“tena hi, bhikkhave, saṃgho dabbaṃ mallaputtaṃ senāsanapaññāpakañca bhattuddesakañca sammannatu."
msgstr ""

msgctxt "pli-tv-kd14:4.3.3"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

#. VAR: mallaputto yācitabbo → dabbo yācitabbo (bj, s1-3, pts1, mr)
msgctxt "pli-tv-kd14:4.3.4"
msgid "Paṭhamaṃ dabbo mallaputto yācitabbo."
msgstr ""

msgctxt "pli-tv-kd14:4.3.5"
msgid "Yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-kd14:4.3.6"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:4.3.7"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho āyasmantaṃ dabbaṃ mallaputtaṃ senāsanapaññāpakañca bhattuddesakañca sammanneyya."
msgstr ""

msgctxt "pli-tv-kd14:4.3.8"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-kd14:4.3.9"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:4.3.10"
msgid "Saṃgho āyasmantaṃ dabbaṃ mallaputtaṃ senāsanapaññāpakañca bhattuddesakañca sammannati."
msgstr ""

msgctxt "pli-tv-kd14:4.3.11"
msgid "Yassāyasmato khamati āyasmato dabbassa mallaputtassa senāsanapaññāpakassa ca bhattuddesakassa ca sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:4.3.12"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-kd14:4.3.13"
msgid "Sammato saṃghena āyasmā dabbo mallaputto senāsanapaññāpako ca bhattuddesako ca."
msgstr ""

msgctxt "pli-tv-kd14:4.3.14"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc28, pts-vp-pli2.76
msgctxt "pli-tv-kd14:4.4.1"
msgid "Sammato ca panāyasmā dabbo mallaputto sabhāgānaṃ bhikkhūnaṃ ekajjhaṃ senāsanaṃ paññapeti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.2"
msgid "Ye te bhikkhū suttantikā tesaṃ ekajjhaṃ senāsanaṃ paññapeti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.3"
msgid "“te aññamaññaṃ suttantaṃ saṅgāyissantī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.4"
msgid "Ye te bhikkhū vinayadharā tesaṃ ekajjhaṃ senāsanaṃ paññapeti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.5"
msgid "“te aññamaññaṃ vinayaṃ vinicchinissantī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.6"
msgid "Ye te bhikkhū dhammakathikā tesaṃ ekajjhaṃ senāsanaṃ paññapeti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.7"
msgid "“te aññamaññaṃ dhammaṃ sākacchissantī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.8"
msgid "Ye te bhikkhū jhāyino tesaṃ ekajjhaṃ senāsanaṃ paññapeti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.9"
msgid "“te aññamaññaṃ na byābādhissantī”ti."
msgstr ""

#. VAR: kāyadaḷhibahulā → kāyadaḍḍhibahulā (bj) | kāyadaḷhībahulā (pts1)
msgctxt "pli-tv-kd14:4.4.10"
msgid "Ye te bhikkhū tiracchānakathikā kāyadaḷhibahulā viharanti tesampi ekajjhaṃ senāsanaṃ paññapeti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.11"
msgid "“imāyapime āyasmanto ratiyā acchissantī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.12"
msgid "Yepi te bhikkhū vikāle āgacchanti tesampi tejodhātuṃ samāpajjitvā teneva ālokena senāsanaṃ paññapeti;"
msgstr ""

msgctxt "pli-tv-kd14:4.4.13"
msgid "apissu bhikkhū sañcicca vikāle āgacchanti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.14"
msgid "“mayaṃ āyasmato dabbassa mallaputtassa iddhipāṭihāriyaṃ passissāmā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-kd14:4.4.15"
msgid "Te āyasmantaṃ dabbaṃ mallaputtaṃ upasaṅkamitvā evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.16"
msgid "“amhākaṃ, āvuso dabba, senāsanaṃ paññapehī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.17"
msgid "Te āyasmā dabbo mallaputto evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.18"
msgid "“kattha āyasmantā icchanti kattha paññapemī”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.4.19"
msgid "Te sañcicca dūre apadisanti—"
msgstr ""

msgctxt "pli-tv-kd14:4.4.20"
msgid "“amhākaṃ, āvuso dabba, gijjhakūṭe pabbate senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.21"
msgid "Amhākaṃ, āvuso, corapapāte senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.22"
msgid "Amhākaṃ, āvuso, isigilipasse kāḷasilāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.23"
msgid "Amhākaṃ, āvuso, vebhārapasse sattapaṇṇiguhāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.24"
msgid "Amhākaṃ, āvuso, sītavane sappasoṇḍikapabbhāre senāsanaṃ paññapehi."
msgstr ""

#. VAR: gotamakakandarāyaṃ → gotamakandarāyaṃ (bj) | gomaṭakandarāyaṃ (s1-3, km, pts1)
msgctxt "pli-tv-kd14:4.4.25"
msgid "Amhākaṃ, āvuso, gotamakakandarāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.26"
msgid "Amhākaṃ, āvuso, tindukakandarāyaṃ senāsanaṃ paññapehi."
msgstr ""

#. VAR: tapodakandarāyaṃ → kapotakandarāyaṃ (si, s1-3, mr)
msgctxt "pli-tv-kd14:4.4.27"
msgid "Amhākaṃ, āvuso, tapodakandarāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.28"
msgid "Amhākaṃ, āvuso, tapodārāme senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.29"
msgid "Amhākaṃ, āvuso, jīvakambavane senāsanaṃ paññapehi."
msgstr ""

msgctxt "pli-tv-kd14:4.4.30"
msgid "Amhākaṃ, āvuso, maddakucchimhi migadāye senāsanaṃ paññapehī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-kd14:4.4.31"
msgid "Tesaṃ āyasmā dabbo mallaputto tejodhātuṃ samāpajjitvā aṅguliyā jalamānāya purato purato gacchati."
msgstr ""

msgctxt "pli-tv-kd14:4.4.32"
msgid "Tepi teneva ālokena āyasmato dabbassa mallaputtassa piṭṭhito piṭṭhito gacchanti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.33"
msgid "Tesaṃ āyasmā dabbo mallaputto evaṃ senāsanaṃ paññapeti—“"
msgstr ""

#. VAR: bibbohanaṃ → bimbohanaṃ (bj, s1-3, km, pts1)
msgctxt "pli-tv-kd14:4.4.34"
msgid "ayaṃ mañco, idaṃ pīṭhaṃ, ayaṃ bhisi, idaṃ bibbohanaṃ, idaṃ vaccaṭṭhānaṃ, idaṃ passāvaṭṭhānaṃ, idaṃ pānīyaṃ, idaṃ paribhojanīyaṃ, ayaṃ kattaradaṇḍo, idaṃ saṃghassa katikasaṇṭhānaṃ, imaṃ kālaṃ pavisitabbaṃ, imaṃ kālaṃ nikkhamitabban”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.4.35"
msgid "Tesaṃ āyasmā dabbo mallaputto evaṃ senāsanaṃ paññapetvā punadeva veḷuvanaṃ paccāgacchati."
msgstr ""

#. HTML: </p><p>
#. REF: sc31, pts-vp-pli2.77
#. VAR: mettiyabhūmajakā → mettiyabhummajakā (bj, s1-3, km, pts1)
msgctxt "pli-tv-kd14:4.5.1"
msgid "Tena kho pana samayena mettiyabhūmajakā bhikkhū navakā ceva honti appapuññā ca."
msgstr ""

msgctxt "pli-tv-kd14:4.5.2"
msgid "Yāni saṃghassa lāmakāni senāsanāni tāni tesaṃ pāpuṇanti lāmakāni ca bhattāni."
msgstr ""

msgctxt "pli-tv-kd14:4.5.3"
msgid "Tena kho pana samayena rājagahe manussā icchanti therānaṃ bhikkhūnaṃ abhisaṅkhārikaṃ piṇḍapātaṃ dātuṃ—"
msgstr ""

msgctxt "pli-tv-kd14:4.5.4"
msgid "sappimpi, telampi, uttaribhaṅgampi."
msgstr ""

msgctxt "pli-tv-kd14:4.5.5"
msgid "Mettiyabhūmajakānaṃ pana bhikkhūnaṃ pākatikaṃ denti—"
msgstr ""

#. VAR: yathārandhaṃ → yathāraddhaṃ (bj, s1-3, pts1)
msgctxt "pli-tv-kd14:4.5.6"
msgid "yathārandhaṃ kaṇājakaṃ bilaṅgadutiyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:4.5.7"
msgid "Te pacchābhattaṃ piṇḍapātapaṭikkantā there bhikkhū pucchanti—"
msgstr ""

#. VAR: tumhākaṃ kiṃ ahosī”ti → kiṃ nāhosīti (s1-3, km)
msgctxt "pli-tv-kd14:4.5.8"
msgid "“tumhākaṃ, āvuso, bhattagge kiṃ ahosi, tumhākaṃ kiṃ ahosī”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.5.9"
msgid "Ekacce therā evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd14:4.5.10"
msgid "“amhākaṃ, āvuso, sappi ahosi, telaṃ ahosi, uttaribhaṅgaṃ ahosī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.5.11"
msgid "Mettiyabhūmajakā pana bhikkhū evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd14:4.5.12"
msgid "“amhākaṃ, āvuso, na kiñci ahosi—"
msgstr ""

msgctxt "pli-tv-kd14:4.5.13"
msgid "pākatikaṃ yathārandhaṃ kaṇājakaṃ bilaṅgadutiyan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-kd14:4.6.1"
msgid "Tena kho pana samayena kalyāṇabhattiko gahapati saṃghassa catukkabhattaṃ deti niccabhattaṃ."
msgstr ""

msgctxt "pli-tv-kd14:4.6.2"
msgid "So bhattagge saputtadāro upatiṭṭhitvā parivisati—"
msgstr ""

msgctxt "pli-tv-kd14:4.6.3"
msgid "aññe odanena pucchanti, aññe sūpena pucchanti, aññe telena pucchanti, aññe uttaribhaṅgena pucchanti."
msgstr ""

msgctxt "pli-tv-kd14:4.6.4"
msgid "Tena kho pana samayena kalyāṇabhattikassa gahapatino bhattaṃ svātanāya mettiyabhūmajakānaṃ bhikkhūnaṃ uddiṭṭhaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd14:4.6.5"
msgid "Atha kho kalyāṇabhattiko gahapati ārāmaṃ agamāsi kenacideva karaṇīyena."
msgstr ""

msgctxt "pli-tv-kd14:4.6.6"
msgid "So yenāyasmā dabbo mallaputto tenupasaṅkami, upasaṅkamitvā āyasmantaṃ dabbaṃ mallaputtaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd14:4.6.7"
msgid "Ekamantaṃ nisinnaṃ kho kalyāṇabhattikaṃ gahapatiṃ āyasmā dabbo mallaputto dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṃsesi."
msgstr ""

msgctxt "pli-tv-kd14:4.6.8"
msgid "Atha kho kalyāṇabhattiko gahapati āyasmatā dabbena mallaputtena dhammiyā kathāya sandassito samādapito samuttejito sampahaṃsito āyasmantaṃ dabbaṃ mallaputtaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.6.9"
msgid "“kassa, bhante, amhākaṃ ghare svātanāya bhattaṃ uddiṭṭhan”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.6.10"
msgid "“Mettiyabhūmajakānaṃ kho, gahapati, bhikkhūnaṃ tumhākaṃ ghare svātanāya bhattaṃ uddiṭṭhan”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.6.11"
msgid "Atha kho kalyāṇabhattiko gahapati anattamano ahosi."
msgstr ""

msgctxt "pli-tv-kd14:4.6.12"
msgid "Kathañhi nāma pāpabhikkhū amhākaṃ ghare bhuñjissantīti gharaṃ gantvā dāsiṃ āṇāpesi—"
msgstr ""

msgctxt "pli-tv-kd14:4.6.13"
msgid "“ye, je, sve bhattikā āgacchanti te koṭṭhake āsanaṃ paññapetvā kaṇājakena bilaṅgadutiyena parivisā”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.6.14"
msgid "“Evaṃ, ayyā”ti kho sā dāsī kalyāṇabhattikassa gahapatino paccassosi."
msgstr ""

#. HTML: </p><p>
#. REF: sc33, pts-vp-pli2.78
msgctxt "pli-tv-kd14:4.7.1"
msgid "Atha kho mettiyabhūmajakā bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd14:4.7.2"
msgid "“hiyyo kho, āvuso, amhākaṃ kalyāṇabhattikassa gahapatino bhattaṃ uddiṭṭhaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:4.7.3"
msgid "sve amhe kalyāṇabhattiko gahapati saputtadāro upatiṭṭhitvā parivisissati;"
msgstr ""

msgctxt "pli-tv-kd14:4.7.4"
msgid "aññe odanena pucchissanti, aññe sūpena pucchissanti, aññe telena pucchissanti, aññe uttaribhaṅgena pucchissantī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.5"
msgid "Te teneva somanassena na cittarūpaṃ rattiyā supiṃsu."
msgstr ""

msgctxt "pli-tv-kd14:4.7.6"
msgid "Atha kho mettiyabhūmajakā bhikkhū pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya yena kalyāṇabhattikassa gahapatino nivesanaṃ tenupasaṅkamiṃsu."
msgstr ""

msgctxt "pli-tv-kd14:4.7.7"
msgid "Addasā kho sā dāsī mettiyabhūmajake bhikkhū dūratova āgacchante;"
msgstr ""

msgctxt "pli-tv-kd14:4.7.8"
msgid "disvāna koṭṭhake āsanaṃ paññapetvā mettiyabhūmajake bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.7.9"
msgid "“nisīdatha, bhante”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.10"
msgid "Atha kho mettiyabhūmajakānaṃ bhikkhūnaṃ etadahosi—“"
msgstr ""

#. VAR: nisīdāpiyāmā”ti → nisīdāpeyyāmāti (si, pts1, mr)
msgctxt "pli-tv-kd14:4.7.11"
msgid "nissaṃsayaṃ kho na tāva bhattaṃ siddhaṃ bhavissati yathā mayaṃ koṭṭhake nisīdāpiyāmā”ti."
msgstr ""

#. VAR: kaṇājakena → kāṇājakena (s1-3, km)
msgctxt "pli-tv-kd14:4.7.12"
msgid "Atha kho sā dāsī kaṇājakena bilaṅgadutiyena upagañchi—"
msgstr ""

msgctxt "pli-tv-kd14:4.7.13"
msgid "“bhuñjatha, bhante”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.14"
msgid "“Mayaṃ kho, bhagini, niccabhattikā”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.15"
msgid "“Jānāmi ayyā niccabhattikāti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.16"
msgid "Api cāhaṃ hiyyova gahapatinā āṇattā—‘"
msgstr ""

msgctxt "pli-tv-kd14:4.7.17"
msgid "ye, je, sve bhattikā āgacchanti, te koṭṭhake āsanaṃ paññapetvā kaṇājakena bilaṅgadutiyena parivisā’ti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.18"
msgid "Bhuñjatha, bhante”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.19"
msgid "Atha kho mettiyabhūmajakā bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd14:4.7.20"
msgid "“hiyyo kho, āvuso, kalyāṇabhattiko gahapati ārāmaṃ agamāsi dabbassa mallaputtassa santike."
msgstr ""

#. VAR: antare paribhinnā”ti → santike paribhinnāti (s1-3, km)
msgctxt "pli-tv-kd14:4.7.21"
msgid "Nissaṃsayaṃ kho mayaṃ dabbena mallaputtena gahapatino antare paribhinnā”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.7.22"
msgid "Te teneva domanassena na cittarūpaṃ bhuñjiṃsu."
msgstr ""

msgctxt "pli-tv-kd14:4.7.23"
msgid "Atha kho mettiyabhūmajakā bhikkhū pacchābhattaṃ piṇḍapātapaṭikkantā ārāmaṃ gantvā pattacīvaraṃ paṭisāmetvā bahārāmakoṭṭhake saṅghāṭipallatthikāya nisīdiṃsu tuṇhībhūtā maṅkubhūtā pattakkhandhā adhomukhā pajjhāyantā appaṭibhānā."
msgstr ""

#. HTML: </p><p>
#. REF: sc34, pts-vp-pli2.79
#. VAR: kho mettiyā → sā mettiyā (bj, s1-3, mr)
msgctxt "pli-tv-kd14:4.8.1"
msgid "Atha kho mettiyā bhikkhunī yena mettiyabhūmajakā bhikkhū tenupasaṅkami, upasaṅkamitvā mettiyabhūmajake bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.8.2"
msgid "“vandāmi, ayyā”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.8.3"
msgid "Evaṃ vutte, mettiyabhūmajakā bhikkhū nālapiṃsu."
msgstr ""

msgctxt "pli-tv-kd14:4.8.4"
msgid "Dutiyampi kho … pe …"
msgstr ""

msgctxt "pli-tv-kd14:4.8.6"
msgid "tatiyampi kho mettiyā bhikkhunī mettiyabhūmajake bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.8.7"
msgid "“vandāmi, ayyā”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.8.8"
msgid "Tatiyampi kho mettiyabhūmajakā bhikkhū nālapiṃsu."
msgstr ""

msgctxt "pli-tv-kd14:4.8.9"
msgid "“Kyāhaṃ ayyānaṃ aparajjhāmi?"
msgstr ""

msgctxt "pli-tv-kd14:4.8.10"
msgid "Kissa maṃ ayyā nālapantī”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.8.11"
msgid "“Tathā hi pana tvaṃ, bhagini, amhe dabbena mallaputtena viheṭhiyamāne ajjhupekkhasī”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.8.12"
msgid "“Kyāhaṃ, ayyā, karomī”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.8.13"
msgid "“Sace kho tvaṃ, bhagini, iccheyyāsi, ajjeva bhagavā āyasmantaṃ dabbaṃ mallaputtaṃ nāsāpeyyā”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.8.14"
msgid "“Kyāhaṃ, ayyā, karomi?"
msgstr ""

msgctxt "pli-tv-kd14:4.8.15"
msgid "Kiṃ mayā sakkā kātun”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.8.16"
msgid "“Ehi tvaṃ, bhagini, yena bhagavā tenupasaṅkama, upasaṅkamitvā bhagavantaṃ evaṃ vadehi—"
msgstr ""

msgctxt "pli-tv-kd14:4.8.17"
msgid "‘idaṃ, bhante, nacchannaṃ nappatirūpaṃ, yāyaṃ, bhante, disā abhayā anītikā anupaddavā sāyaṃ disā sabhayā saītikā saupaddavā;"
msgstr ""

#. VAR: tato savātaṃ → tato pavātaṃ (bj, s1-3, km, pts1)
msgctxt "pli-tv-kd14:4.8.18"
msgid "yato nivātaṃ tato savātaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:4.8.19"
msgid "udakaṃ maññe ādittaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:4.8.20"
msgid "ayyenamhi dabbena mallaputtena dūsitā’”ti."
msgstr ""

#. VAR: paṭissutvā → paṭissuṇitvā (s1-3, km)
msgctxt "pli-tv-kd14:4.8.21"
msgid "“Evaṃ, ayyā”ti kho mettiyā bhikkhunī mettiyabhūmajakānaṃ bhikkhūnaṃ paṭissutvā yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhāsi."
msgstr ""

msgctxt "pli-tv-kd14:4.8.22"
msgid "Ekamantaṃ ṭhitā kho mettiyā bhikkhunī bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.8.23"
msgid "“idaṃ, bhante, nacchannaṃ nappatirūpaṃ, yāyaṃ, bhante, disā abhayā anītikā anupaddavā sāyaṃ disā sabhayā saītikā saupaddavā;"
msgstr ""

msgctxt "pli-tv-kd14:4.8.24"
msgid "yato nivātaṃ tato savātaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:4.8.25"
msgid "udakaṃ maññe ādittaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:4.8.26"
msgid "ayyenamhi dabbena mallaputtena dūsitā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-kd14:4.9.1"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ dabbaṃ mallaputtaṃ paṭipucchi—"
msgstr ""

msgctxt "pli-tv-kd14:4.9.2"
msgid "“sarasi tvaṃ, dabba, evarūpaṃ kattā yathāyaṃ bhikkhunī āhā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.9.3"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.4"
msgid "Dutiyampi kho bhagavā āyasmantaṃ dabbaṃ mallaputtaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.9.5"
msgid "“sarasi tvaṃ, dabba, evarūpaṃ kattā yathāyaṃ bhikkhunī āhā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.9.6"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.7"
msgid "Tatiyampi kho bhagavā āyasmantaṃ dabbaṃ mallaputtaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd14:4.9.8"
msgid "“sarasi tvaṃ, dabba, evarūpaṃ kattā yathāyaṃ bhikkhunī āhā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.9.9"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.10"
msgid "“Na kho, dabba, dabbā evaṃ nibbeṭhenti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.11"
msgid "Sace tayā kataṃ katanti vadehi."
msgstr ""

msgctxt "pli-tv-kd14:4.9.12"
msgid "Sace akataṃ akatanti vadehī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.13"
msgid "“Yatohaṃ, bhante, jāto nābhijānāmi supinantenapi methunaṃ dhammaṃ paṭisevitā, pageva jāgaro”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.14"
msgid "Atha kho bhagavā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd14:4.9.15"
msgid "“tena hi, bhikkhave, mettiyaṃ bhikkhuniṃ nāsetha."
msgstr ""

msgctxt "pli-tv-kd14:4.9.16"
msgid "Ime ca bhikkhū anuyuñjathā”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.17"
msgid "Idaṃ vatvā bhagavā uṭṭhāyāsanā vihāraṃ pāvisi."
msgstr ""

#. HTML: </p><p>
#. REF: sc36
msgctxt "pli-tv-kd14:4.9.18"
msgid "Atha kho te bhikkhū mettiyaṃ bhikkhuniṃ nāsesuṃ."
msgstr ""

msgctxt "pli-tv-kd14:4.9.19"
msgid "Atha kho mettiyabhūmajakā bhikkhū te bhikkhū etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd14:4.9.20"
msgid "“māvuso, mettiyaṃ bhikkhuniṃ nāsetha, na sā kiñci aparajjhati;"
msgstr ""

msgctxt "pli-tv-kd14:4.9.21"
msgid "amhehi sā ussāhitā kupitehi anattamanehi cāvanādhippāyehī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.22"
msgid "“Kiṃ pana tumhe, āvuso, āyasmantaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsethā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.9.23"
msgid "“Evamāvuso”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.24"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:4.9.26"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd14:4.9.27"
msgid "“kathañhi nāma mettiyabhūmajakā bhikkhū āyasmantaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsessantī”ti."
msgstr ""

msgctxt "pli-tv-kd14:4.9.28"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:4.9.30"
msgid "“saccaṃ kira, bhikkhave, mettiyabhūmajakā bhikkhū dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsentī”ti?"
msgstr ""

msgctxt "pli-tv-kd14:4.9.31"
msgid "“Saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:4.9.33"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:4.9.35"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

#. HTML: </p><p>
#. REF: sc37, pts-vp-pli2.80
msgctxt "pli-tv-kd14:4.10.1"
msgid "“Tena hi, bhikkhave, saṃgho dabbassa mallaputtassa sativepullappattassa sativinayaṃ detu."
msgstr ""

msgctxt "pli-tv-kd14:4.10.2"
msgid "Evañca pana, bhikkhave, dātabbo—"
msgstr ""

msgctxt "pli-tv-kd14:4.10.3"
msgid "tena, bhikkhave, dabbena mallaputtena saṃghaṃ upasaṅkamitvā, ekaṃsaṃ uttarāsaṅgaṃ karitvā, vuḍḍhānaṃ bhikkhūnaṃ pāde vanditvā, ukkuṭikaṃ nisīditvā, añjaliṃ paggahetvā, evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd14:4.10.4"
msgid "ime maṃ, bhante, mettiyabhūmajakā bhikkhū amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

#. VAR: Sohaṃ → sohaṃ bhante (pts1, mr)
msgctxt "pli-tv-kd14:4.10.5"
msgid "Sohaṃ, bhante, sativepullappatto saṃghaṃ sativinayaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.6"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd14:4.10.7"
msgid "Tatiyampi yācitabbo—‘"
msgstr ""

msgctxt "pli-tv-kd14:4.10.8"
msgid "ime maṃ, bhante, mettiyabhūmajakā bhikkhū amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.9"
msgid "Sohaṃ sativepullappatto tatiyampi, bhante, saṃghaṃ sativinayaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.10"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc38
msgctxt "pli-tv-kd14:4.10.11"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:4.10.12"
msgid "Ime mettiyabhūmajakā bhikkhū āyasmantaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.13"
msgid "Āyasmā dabbo mallaputto sativepullappatto saṃghaṃ sativinayaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd14:4.10.14"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho āyasmato dabbassa mallaputtassa sativepullappattassa sativinayaṃ dadeyya."
msgstr ""

msgctxt "pli-tv-kd14:4.10.15"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc39
msgctxt "pli-tv-kd14:4.10.16"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:4.10.17"
msgid "Ime mettiyabhūmajakā bhikkhū āyasmantaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.18"
msgid "Āyasmā dabbo mallaputto sativepullappatto saṃghaṃ sativinayaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd14:4.10.19"
msgid "Saṃgho āyasmato dabbassa mallaputtassa sativepullappattassa sativinayaṃ deti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.20"
msgid "Yassāyasmato khamati āyasmato dabbassa mallaputtassa sativepullappattassa sativinayassa dānaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:4.10.21"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-kd14:4.10.22"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:4.10.24"
msgid "tatiyampi etamatthaṃ vadāmi—"
msgstr ""

msgctxt "pli-tv-kd14:4.10.25"
msgid "suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:4.10.26"
msgid "Ime mettiyabhūmajakā bhikkhū āyasmantaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.27"
msgid "Āyasmā dabbo mallaputto sativepullappatto saṃghaṃ sativinayaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd14:4.10.28"
msgid "Saṃgho āyasmato dabbassa mallaputtassa sativepullappattassa sativinayaṃ deti."
msgstr ""

msgctxt "pli-tv-kd14:4.10.29"
msgid "Yassāyasmato khamati āyasmato dabbassa mallaputtassa sativepullappattassa sativinayassa dānaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:4.10.30"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc41
msgctxt "pli-tv-kd14:4.10.31"
msgid "Dinno saṃghena āyasmato dabbassa mallaputtassa sativepullappattassa sativinayo."
msgstr ""

msgctxt "pli-tv-kd14:4.10.32"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc42
msgctxt "pli-tv-kd14:4.11.1"
msgid "Pañcimāni, bhikkhave, dhammikāni sativinayassa dānāni."
msgstr ""

msgctxt "pli-tv-kd14:4.11.2"
msgid "Suddho hoti bhikkhu anāpattiko, anuvadanti ca naṃ, yācati ca, tassa saṃgho sativinayaṃ deti dhammena samaggena—"
msgstr ""

msgctxt "pli-tv-kd14:4.11.3"
msgid "imāni kho, bhikkhave, pañca dhammikāni sativinayassa dānānī”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd14:5.1.0"
msgid "3. Amūḷhavinaya"
msgstr ""

#. HTML: </h2><p>
#. REF: sc43, pts-vp-pli2.81
msgctxt "pli-tv-kd14:5.1.1"
msgid "Tena kho pana samayena gaggo bhikkhu ummattako hoti, cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.1.2"
msgid "Tena ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ hoti bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.1.3"
msgid "Bhikkhū gaggaṃ bhikkhuṃ ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:5.1.4"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.1.5"
msgid "So evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-kd14:5.1.6"
msgid "“ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.1.7"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.1.8"
msgid "Nāhaṃ taṃ sarāmi."
msgstr ""

msgctxt "pli-tv-kd14:5.1.9"
msgid "Mūḷhena me etaṃ katan”ti."
msgstr ""

msgctxt "pli-tv-kd14:5.1.10"
msgid "Evampi naṃ vuccamānā codenteva—"
msgstr ""

msgctxt "pli-tv-kd14:5.1.11"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.1.12"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:5.1.14"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd14:5.1.15"
msgid "“kathañhi nāma bhikkhū gaggaṃ bhikkhuṃ ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codessanti—"
msgstr ""

msgctxt "pli-tv-kd14:5.1.16"
msgid "‘saratāyasmā evarūpiṃ āpattiṃ āpajjitā’ti."
msgstr ""

msgctxt "pli-tv-kd14:5.1.17"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:5.1.18"
msgid "ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.1.19"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.1.20"
msgid "Nāhaṃ taṃ sarāmi."
msgstr ""

msgctxt "pli-tv-kd14:5.1.21"
msgid "Mūḷhena me etaṃ katan’ti."
msgstr ""

msgctxt "pli-tv-kd14:5.1.22"
msgid "Evampi naṃ vuccamānā codenteva—‘"
msgstr ""

msgctxt "pli-tv-kd14:5.1.23"
msgid "saratāyasmā evarūpiṃ āpattiṃ āpajjitā’”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.1.24"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:5.1.26"
msgid "“saccaṃ kira, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd14:5.1.28"
msgid "saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:5.1.30"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:5.1.32"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

#. HTML: </p><p>
#. REF: sc44
msgctxt "pli-tv-kd14:5.1.33"
msgid "“Tena hi, bhikkhave, saṃgho gaggassa bhikkhuno amūḷhassa amūḷhavinayaṃ detu."
msgstr ""

msgctxt "pli-tv-kd14:5.2.1"
msgid "Evañca pana, bhikkhave, dātabbo—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.2"
msgid "tena, bhikkhave, gaggena bhikkhunā saṃghaṃ upasaṅkamitvā, ekaṃsaṃ uttarāsaṅgaṃ karitvā, vuḍḍhānaṃ bhikkhūnaṃ pāde vanditvā, ukkuṭikaṃ nisīditvā, añjaliṃ paggahetvā, evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd14:5.2.3"
msgid "ahaṃ, bhante, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.2.4"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.2.5"
msgid "Maṃ bhikkhū ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.6"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.2.7"
msgid "Tyāhaṃ evaṃ vadāmi—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.8"
msgid "“ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.2.9"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.2.10"
msgid "Nāhaṃ taṃ sarāmi."
msgstr ""

msgctxt "pli-tv-kd14:5.2.11"
msgid "Mūḷhena me etaṃ katan”ti."
msgstr ""

msgctxt "pli-tv-kd14:5.2.12"
msgid "Evampi maṃ vuccamānā codenteva—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.13"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.2.14"
msgid "Sohaṃ, bhante, amūḷho saṃghaṃ amūḷhavinayaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:5.2.15"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd14:5.2.16"
msgid "Tatiyampi yācitabbo—‘"
msgstr ""

msgctxt "pli-tv-kd14:5.2.17"
msgid "ahaṃ, bhante, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.2.18"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.2.19"
msgid "Maṃ bhikkhū ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.20"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.2.21"
msgid "Tyāhaṃ evaṃ vadāmi—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.22"
msgid "“ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.2.23"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.2.24"
msgid "Nāhaṃ taṃ sarāmi mūḷhena me etaṃ katan”ti."
msgstr ""

msgctxt "pli-tv-kd14:5.2.25"
msgid "Evampi maṃ vuccamānā codenteva—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.26"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

#. VAR: Sohaṃ amūḷho → sohaṃ bhante amūḷho (bj, pts1, mr)
msgctxt "pli-tv-kd14:5.2.27"
msgid "Sohaṃ amūḷho tatiyampi, bhante, saṃghaṃ amūḷhavinayaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:5.2.28"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc45, pts-vp-pli2.82
msgctxt "pli-tv-kd14:5.2.29"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:5.2.30"
msgid "Ayaṃ gaggo bhikkhu ummattako ahosi cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.2.31"
msgid "Tena ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.2.32"
msgid "Bhikkhū gaggaṃ bhikkhuṃ ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.33"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.2.34"
msgid "So evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.35"
msgid "“ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:5.2.36"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:5.2.37"
msgid "Nāhaṃ taṃ sarāmi, mūḷhena me etaṃ katan”ti."
msgstr ""

msgctxt "pli-tv-kd14:5.2.38"
msgid "Evampi naṃ vuccamānā codenteva—"
msgstr ""

msgctxt "pli-tv-kd14:5.2.39"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti?"
msgstr ""

msgctxt "pli-tv-kd14:5.2.40"
msgid "So amūḷho saṃghaṃ amūḷhavinayaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd14:5.2.41"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho gaggassa bhikkhuno amūḷhassa amūḷhavinayaṃ dadeyya."
msgstr ""

msgctxt "pli-tv-kd14:5.2.42"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
