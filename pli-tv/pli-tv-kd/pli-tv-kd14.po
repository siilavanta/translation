msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.28.54"
msgid "Ayaṃ itthannāmo bhikkhu ummattako ahosi cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:14.28.55"
msgid "Tena ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.56"
msgid "Taṃ bhikkhū ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.57"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.58"
msgid "So evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.59"
msgid "“ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
