msgid "Saṃgho gaggassa bhikkhuno amūḷhassa amūḷhavinayaṃ deti."
msgstr ""

msgctxt "pli-tv-kd14:5.2.56"
msgid "Yassāyasmato khamati gaggassa bhikkhuno amūḷhassa amūḷhavinayassa dānaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:5.2.57"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc47
msgctxt "pli-tv-kd14:5.2.58"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:5.2.60"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-kd14:5.2.61"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-kd14:5.2.62"
msgid "Dinno saṃghena gaggassa bhikkhuno amūḷhassa amūḷhavinayo."
msgstr ""

msgctxt "pli-tv-kd14:5.2.63"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc49
msgctxt "pli-tv-kd14:6.1.1"
msgid "Tīṇimāni, bhikkhave, adhammikāni amūḷhavinayassa dānāni, tīṇi dhammikāni."
msgstr ""

msgctxt "pli-tv-kd14:6.1.2"
msgid "Katamāni tīṇi adhammikāni amūḷhavinayassa dānāni?"
msgstr ""

#. HTML: </p><p>
#. REF: sc50
msgctxt "pli-tv-kd14:6.1.3"
msgid "Idha pana, bhikkhave, bhikkhu āpattiṃ āpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:6.1.4"
msgid "Tamenaṃ codeti saṃgho vā, sambahulā vā, ekapuggalo vā—‘"
msgstr ""

msgctxt "pli-tv-kd14:6.1.5"
msgid "saratāyasmā evarūpiṃ āpattiṃ āpajjitā’ti?"
msgstr ""

msgctxt "pli-tv-kd14:6.1.6"
msgid "So saramānova evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:6.1.7"
msgid "na kho ahaṃ, āvuso, sarāmi evarūpiṃ āpattiṃ āpajjitā’ti."
msgstr ""

msgctxt "pli-tv-kd14:6.1.8"
msgid "Tassa saṃgho amūḷhavinayaṃ deti."
msgstr ""

msgctxt "pli-tv-kd14:6.1.9"
msgid "Adhammikaṃ amūḷhavinayassa dānaṃ."
msgstr ""

