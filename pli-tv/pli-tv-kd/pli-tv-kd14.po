msgstr ""

msgctxt "pli-tv-kd14:14.28.68"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc194
msgctxt "pli-tv-kd14:14.28.69"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.28.71"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-kd14:14.28.72"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc195
msgctxt "pli-tv-kd14:14.28.73"
msgid "Dinno saṃghena itthannāmassa bhikkhuno amūḷhassa amūḷhavinayo."
msgstr ""

msgctxt "pli-tv-kd14:14.28.74"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc196, pts-vp-pli2.101
msgctxt "pli-tv-kd14:14.28.75"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.76"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.28.77"
msgid "Sammukhāvinayena ca, amūḷhavinayena ca."
msgstr ""

msgctxt "pli-tv-kd14:14.28.78"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.28.79"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.28.81"
msgid "kiñca tattha amūḷhavinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.28.82"
msgid "Yā amūḷhavinayassa kammassa kiriyā karaṇaṃ upagamanaṃ ajjhupagamanaṃ adhivāsanā appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.83"
msgid "idaṃ tattha amūḷhavinayasmiṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.84"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.28.85"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.29.0"
msgid "9.7. Tassapāpiyasikāvinaya"
msgstr ""

#. HTML: </h3><p>
#. REF: sc197
msgctxt "pli-tv-kd14:14.29.1"
msgid "Siyā anuvādādhikaraṇaṃ dve samathe anāgamma—"
msgstr ""

msgctxt "pli-tv-kd14:14.29.2"
msgid "sativinayañca, amūḷhavinayañca;"
msgstr ""

msgctxt "pli-tv-kd14:14.29.3"
msgid "dvīhi samathehi sammeyya—"
msgstr ""

msgctxt "pli-tv-kd14:14.29.4"
msgid "sammukhāvinayena ca, tassapāpiyasikāya cāti?"
msgstr ""

msgctxt "pli-tv-kd14:14.29.5"
msgid "Siyātissa vacanīyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.29.6"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-kd14:14.29.7"
msgid "Idha pana, bhikkhave, bhikkhu bhikkhuṃ saṃghamajjhe garukāya āpattiyā codeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.29.8"
msgid "saratāyasmā evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.29.9"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.29.10"
msgid "na kho ahaṃ, āvuso, sarāmi evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.29.11"
msgid "Tamenaṃ so nibbeṭhentaṃ ativeṭheti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.29.12"
msgid "iṅghāyasmā sādhukameva jānāhi, yadi sarasi evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.29.13"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.29.14"
msgid "na kho ahaṃ, āvuso, sarāmi evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā."
msgstr ""

msgctxt "pli-tv-kd14:14.29.15"
msgid "Sarāmi ca kho ahaṃ, āvuso, evarūpiṃ appamattikaṃ āpattiṃ āpajjitā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.29.16"
msgid "Tamenaṃ so nibbeṭhentaṃ ativeṭheti—"
msgstr ""

msgctxt "pli-tv-kd14:14.29.17"
msgid "‘iṅghāyasmā sādhukameva jānāhi, yadi sarasi evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.29.18"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.29.19"
msgid "imañhi nāmāhaṃ, āvuso, appamattikaṃ āpattiṃ āpajjitvā apuṭṭho paṭijānissāmi."
msgstr ""

msgctxt "pli-tv-kd14:14.29.20"
msgid "Kiṃ panāhaṃ evarūpiṃ garukaṃ āpattiṃ āpajjitvā, pārājikaṃ vā pārājikasāmantaṃ vā, puṭṭho na paṭijānissāmī’ti?"
msgstr ""

msgctxt "pli-tv-kd14:14.29.21"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.29.22"
msgid "imañhi nāma tvaṃ, āvuso, appamattikaṃ āpattiṃ āpajjitvā apuṭṭho na paṭijānissasi."
msgstr ""

msgctxt "pli-tv-kd14:14.29.23"
msgid "Kiṃ pana tvaṃ evarūpiṃ garukaṃ āpattiṃ āpajjitvā, pārājikaṃ vā pārājikasāmantaṃ vā, apuṭṭho paṭijānissasi?"
msgstr ""

msgctxt "pli-tv-kd14:14.29.24"
msgid "Iṅghāyasmā sādhukameva jānāhi, yadi sarasi evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.29.25"
msgid "So evaṃ vadesi—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.29.26"
msgid "sarāmi kho ahaṃ, āvuso, evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā."
msgstr ""

msgctxt "pli-tv-kd14:14.29.27"
msgid "Davā me etaṃ vuttaṃ, ravā me etaṃ vuttaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.29.28"
msgid "nāhaṃ taṃ sarāmi evarūpiṃ garukaṃ āpattiṃ āpajjitā, pārājikaṃ vā pārājikasāmantaṃ vā’ti."
msgstr ""

#. VAR: Tassa kho, bhikkhave → tassa kho taṃ bhikkhave (bj, mr) | tassa khvetaṃ bhikkhave (s1-3)
msgctxt "pli-tv-kd14:14.29.29"
msgid "Tassa kho, bhikkhave, bhikkhuno tassapāpiyasikākammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.29.30"
msgid "Evañca pana, bhikkhave, kātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.29.31"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc198
msgctxt "pli-tv-kd14:14.29.32"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.29.33"
msgid "Ayaṃ itthannāmo bhikkhu saṃghamajjhe garukāya āpattiyā anuyuñjiyamāno avajānitvā paṭijānāti, paṭijānitvā avajānāti, aññenāññaṃ paṭicarati, sampajānamusā bhāsati."
msgstr ""

msgctxt "pli-tv-kd14:14.29.34"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmassa bhikkhuno tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:14.29.35"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc199
msgctxt "pli-tv-kd14:14.29.36"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.29.37"
msgid "Ayaṃ itthannāmo bhikkhu saṃghamajjhe garukāya āpattiyā anuyuñjiyamāno avajānitvā paṭijānāti, paṭijānitvā avajānāti, aññenāññaṃ paṭicarati, sampajānamusā bhāsati."
msgstr ""

msgctxt "pli-tv-kd14:14.29.38"
msgid "Saṃgho itthannāmassa bhikkhuno tassapāpiyasikākammaṃ karoti."
msgstr ""

msgctxt "pli-tv-kd14:14.29.39"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno tassapāpiyasikākammassa karaṇaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:14.29.40"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc200
msgctxt "pli-tv-kd14:14.29.41"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.29.43"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-kd14:14.29.44"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc201
msgctxt "pli-tv-kd14:14.29.45"
msgid "Kataṃ saṃghena itthannāmassa bhikkhuno tassapāpiyasikākammaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.29.46"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc202, pts-vp-pli2.102
msgctxt "pli-tv-kd14:14.29.47"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.29.48"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.29.49"
msgid "Sammukhāvinayena ca, tassapāpiyasikāya ca."
msgstr ""

msgctxt "pli-tv-kd14:14.29.50"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.29.51"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.29.53"
msgid "kā ca tattha tassapāpiyasikāya?"
msgstr ""

msgctxt "pli-tv-kd14:14.29.54"
msgid "Yā tassapāpiyasikākammassa kiriyā karaṇaṃ upagamanaṃ ajjhupagamanaṃ adhivāsanā appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-kd14:14.29.55"
msgid "ayaṃ tattha tassapāpiyasikāya."
msgstr ""

msgctxt "pli-tv-kd14:14.29.56"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.29.57"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.30.0"
msgid "9.8. Paṭiññātakaraṇa"
msgstr ""

#. HTML: </h3><p>
#. REF: sc203
msgctxt "pli-tv-kd14:14.30.1"
msgid "Āpattādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.2"
msgid "Āpattādhikaraṇaṃ tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.30.3"
msgid "sammukhāvinayena ca, paṭiññātakaraṇena ca, tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-kd14:14.30.4"
msgid "Siyā āpattādhikaraṇaṃ ekaṃ samathaṃ anāgamma—"
msgstr ""

msgctxt "pli-tv-kd14:14.30.5"
msgid "tiṇavatthārakaṃ, dvīhi samathehi sammeyya—"
msgstr ""

msgctxt "pli-tv-kd14:14.30.6"
msgid "sammukhāvinayena ca, paṭiññātakaraṇena cāti?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.7"
msgid "Siyātissa vacanīyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.30.8"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.9"
msgid "Idha pana, bhikkhave, bhikkhu lahukaṃ āpattiṃ āpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.30.10"
msgid "Tena, bhikkhave, bhikkhunā ekaṃ bhikkhuṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.30.11"
msgid "ahaṃ, āvuso, itthannāmaṃ āpattiṃ āpanno;"
msgstr ""

msgctxt "pli-tv-kd14:14.30.12"
msgid "taṃ paṭidesemī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.30.13"
msgid "Tena vattabbo—"
msgstr ""

msgctxt "pli-tv-kd14:14.30.14"
msgid "‘passasī’ti?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.15"
msgid "‘Āma passāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.30.16"
msgid "‘Āyatiṃ saṃvareyyāsī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc204, pts-vp-pli2.103
msgctxt "pli-tv-kd14:14.30.17"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.30.18"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.19"
msgid "Sammukhāvinayena ca, paṭiññātakaraṇena ca."
msgstr ""

msgctxt "pli-tv-kd14:14.30.20"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.21"
msgid "Dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.30.23"
msgid "kā ca tattha puggalasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.24"
msgid "Yo ca deseti, yassa ca deseti, ubho sammukhībhūtā honti—"
msgstr ""

msgctxt "pli-tv-kd14:14.30.25"
msgid "ayaṃ tattha puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.30.26"
msgid "Kiñca tattha paṭiññātakaraṇasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.30.27"
msgid "Yā paṭiññātakaraṇassa kammassa kiriyā karaṇaṃ upagamanaṃ ajjhupagamanaṃ adhivāsanā appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-kd14:14.30.28"
msgid "idaṃ tattha paṭiññātakaraṇasmiṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.30.29"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ paṭiggāhako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.31.1"
msgid "Evañcetaṃ labhetha, iccetaṃ kusalaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.31.2"
msgid "No ce labhetha, tena, bhikkhave, bhikkhunā sambahule bhikkhū upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā vuḍḍhānaṃ bhikkhūnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassu vacanīyā—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.31.3"
msgid "ahaṃ, bhante, itthannāmaṃ āpattiṃ āpanno;"
msgstr ""

msgctxt "pli-tv-kd14:14.31.4"
msgid "taṃ paṭidesemī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.31.5"
msgid "Byattena bhikkhunā paṭibalena te bhikkhū ñāpetabbā—"
msgstr ""

#. HTML: </p><p>
#. REF: sc205
msgctxt "pli-tv-kd14:14.31.6"
msgid "‘Suṇantu me āyasmantā."
msgstr ""

msgctxt "pli-tv-kd14:14.31.7"
msgid "Ayaṃ itthannāmo bhikkhu āpattiṃ sarati, vivarati, uttāniṃ karoti deseti."
msgstr ""

msgctxt "pli-tv-kd14:14.31.8"
msgid "Yadāyasmantānaṃ pattakallaṃ, ahaṃ itthannāmassa bhikkhuno āpattiṃ paṭiggaṇheyyan’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.31.9"
msgid "Tena vattabbo—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.31.10"
msgid "passasī’ti?"
msgstr ""

msgctxt "pli-tv-kd14:14.31.11"
msgid "‘Āma passāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.31.12"
msgid "‘Āyatiṃ saṃvareyyāsī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc206
msgctxt "pli-tv-kd14:14.31.13"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.31.14"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.31.15"
msgid "Sammukhāvinayena ca, paṭiññātakaraṇena ca."
msgstr ""

msgctxt "pli-tv-kd14:14.31.16"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.31.17"
msgid "Dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.31.19"
msgid "kā ca tattha puggalasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.31.20"
msgid "Yo ca deseti, yassa ca deseti, ubho sammukhībhūtā honti—"
msgstr ""

msgctxt "pli-tv-kd14:14.31.21"
msgid "ayaṃ tattha puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.31.22"
msgid "Kiñca tattha paṭiññātakaraṇasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.31.23"
msgid "Yā paṭiññātakaraṇassa kammassa kiriyā karaṇaṃ upagamanaṃ ajjhupagamanaṃ adhivāsanā appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-kd14:14.31.24"
msgid "idaṃ tattha paṭiññātakaraṇasmiṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.31.25"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ paṭiggāhako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.32.1"
msgid "Evañcetaṃ labhetha, iccetaṃ kusalaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.32.2"
msgid "No ce labhetha, tena, bhikkhave, bhikkhunā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā vuḍḍhānaṃ bhikkhūnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.32.3"
msgid "ahaṃ, bhante, itthannāmaṃ āpattiṃ āpanno;"
msgstr ""

msgctxt "pli-tv-kd14:14.32.4"
msgid "taṃ paṭidesemī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.32.5"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc207
msgctxt "pli-tv-kd14:14.32.6"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.32.7"
msgid "Ayaṃ itthannāmo bhikkhu āpattiṃ sarati, vivarati, uttāniṃ karoti, deseti."
msgstr ""

msgctxt "pli-tv-kd14:14.32.8"
msgid "Yadi saṃghassa pattakallaṃ, ahaṃ itthannāmassa bhikkhuno āpattiṃ paṭiggaṇheyyan’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.32.9"
msgid "Tena vattabbo—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.32.10"
msgid "passasī’ti?"
msgstr ""

msgctxt "pli-tv-kd14:14.32.11"
msgid "‘Āma passāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.32.12"
msgid "‘Āyatiṃ saṃvareyyāsī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc208
msgctxt "pli-tv-kd14:14.32.13"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.32.14"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.32.15"
msgid "Sammukhāvinayena ca, paṭiññātakaraṇena ca."
msgstr ""

msgctxt "pli-tv-kd14:14.32.16"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.32.17"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.32.19"
msgid "evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ paṭiggāhako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.32.20"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.33.0"
msgid "9.9. Tiṇavatthāraka"
msgstr ""

#. HTML: </h3><p>
#. REF: sc209
msgctxt "pli-tv-kd14:14.33.1"
msgid "Siyā āpattādhikaraṇaṃ ekaṃ samathaṃ anāgamma—"
msgstr ""

msgctxt "pli-tv-kd14:14.33.2"
msgid "paṭiññātakaraṇaṃ, dvīhi samathehi sammeyya—"
msgstr ""

msgctxt "pli-tv-kd14:14.33.3"
msgid "sammukhāvinayena ca, tiṇavatthārakena cāti?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.4"
msgid "Siyātissa vacanīyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.5"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.6"
msgid "Idha pana, bhikkhave, bhikkhūnaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ hoti bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.7"
msgid "Tatra ce bhikkhūnaṃ evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.33.8"
msgid "amhākaṃ kho bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.9"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyyā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.33.10"
msgid "Anujānāmi, bhikkhave, evarūpaṃ adhikaraṇaṃ tiṇavatthārakena vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.11"
msgid "Evañca pana, bhikkhave, vūpasametabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.12"
msgid "Sabbeheva ekajjhaṃ sannipatitabbaṃ, sannipatitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc210
msgctxt "pli-tv-kd14:14.33.13"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.33.14"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.15"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:14.33.16"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho imaṃ adhikaraṇaṃ tiṇavatthārakena vūpasameyya, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttanti."
msgstr ""

msgctxt "pli-tv-kd14:14.33.17"
msgid "Ekatopakkhikānaṃ bhikkhūnaṃ byattena bhikkhunā paṭibalena sako pakkho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc211
msgctxt "pli-tv-kd14:14.33.18"
msgid "Suṇantu me āyasmantā."
msgstr ""

msgctxt "pli-tv-kd14:14.33.19"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.20"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:14.33.21"
msgid "Yadāyasmantānaṃ pattakallaṃ, ahaṃ yā ceva āyasmantānaṃ āpatti, yā ca attano āpatti, āyasmantānañceva atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena deseyyaṃ, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttan’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc212
msgctxt "pli-tv-kd14:14.33.22"
msgid "Athāparesaṃ ekatopakkhikānaṃ bhikkhūnaṃ byattena bhikkhunā paṭibalena sako pakkho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc213
msgctxt "pli-tv-kd14:14.33.23"
msgid "‘Suṇantu me āyasmantā."
msgstr ""

msgctxt "pli-tv-kd14:14.33.24"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.25"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:14.33.26"
msgid "Yadāyasmantānaṃ pattakallaṃ, ahaṃ yā ceva āyasmantānaṃ āpatti, yā ca attano āpatti, āyasmantānañceva atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena deseyyaṃ, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttan’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc214
msgctxt "pli-tv-kd14:14.33.27"
msgid "Athāparesaṃ ekatopakkhikānaṃ bhikkhūnaṃ byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc215
msgctxt "pli-tv-kd14:14.33.28"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.33.29"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.30"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:14.33.31"
msgid "Yadi saṃghassa pattakallaṃ, ahaṃ yā ceva imesaṃ āyasmantānaṃ āpatti, yā ca attano āpatti, imesañceva āyasmantānaṃ atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena deseyyaṃ, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.32"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc216
msgctxt "pli-tv-kd14:14.33.33"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.33.34"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.35"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:14.33.36"
msgid "Ahaṃ yā ceva imesaṃ āyasmantānaṃ āpatti, yā ca attano āpatti, imesañceva āyasmantānaṃ atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena desemi, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.37"
msgid "Yassāyasmato khamati amhākaṃ imāsaṃ āpattīnaṃ saṃghamajjhe tiṇavatthārakena desanā, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:14.33.38"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc217
msgctxt "pli-tv-kd14:14.33.39"
msgid "Desitā amhākaṃ imā āpattiyo saṃghamajjhe tiṇavatthārakena, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.40"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc218
msgctxt "pli-tv-kd14:14.33.41"
msgid "Athāparesaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.33.43"
msgid "evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc219
msgctxt "pli-tv-kd14:14.33.44"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.45"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.46"
msgid "Sammukhāvinayena ca, tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-kd14:14.33.47"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.48"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā."
msgstr ""

#. HTML: </p><p>
#. REF: sc220
msgctxt "pli-tv-kd14:14.33.49"
msgid "Kā ca tattha saṃghasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.50"
msgid "Yāvatikā bhikkhū kammappattā, te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā na paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.33.51"
msgid "ayaṃ tattha saṃghasammukhatā."
msgstr ""

#. HTML: </p><p>
#. REF: sc221, pts-vp-pli2.104
msgctxt "pli-tv-kd14:14.33.52"
msgid "Kā ca tattha dhammasammukhatā, vinayasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.53"
msgid "Yena dhammena yena vinayena yena satthusāsanena taṃ adhikaraṇaṃ vūpasammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.33.54"
msgid "ayaṃ tattha dhammasammukhatā, vinayasammukhatā."
msgstr ""

#. HTML: </p><p>
#. REF: sc222
msgctxt "pli-tv-kd14:14.33.55"
msgid "Kā ca tattha puggalasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.56"
msgid "Yo ca deseti, yassa ca deseti, ubho sammukhībhūtā honti—"
msgstr ""

msgctxt "pli-tv-kd14:14.33.57"
msgid "ayaṃ tattha puggalasammukhatā."
msgstr ""

#. HTML: </p><p>
#. REF: sc223
msgctxt "pli-tv-kd14:14.33.58"
msgid "Kiñca tattha tiṇavatthārakasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.33.59"
msgid "Yā tiṇavatthārakassa kammassa kiriyā karaṇaṃ upagamanaṃ ajjhupagamanaṃ adhivāsanā appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-kd14:14.33.60"
msgid "idaṃ tattha tiṇavatthārakasmiṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.33.61"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ paṭiggāhako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.33.62"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc224
msgctxt "pli-tv-kd14:14.34.1"
msgid "Kiccādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-kd14:14.34.2"
msgid "Kiccādhikaraṇaṃ ekena samathena sammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.34.3"
msgid "sammukhāvinayenā”ti."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc225
msgctxt "pli-tv-kd14:14.34.4"
msgid "Samathakkhandhako niṭṭhito catuttho."
msgstr ""

