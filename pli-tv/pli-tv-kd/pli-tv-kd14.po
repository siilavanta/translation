#. HTML: <section class="sutta" id="pli-tv-kd14"><article><div class="hgroup"><p class="collection">
msgctxt "pli-tv-kd14:0.1"
msgid "Theravāda Vinayapiṭaka"
msgstr ""

#. HTML: </p><p class="division">
msgctxt "pli-tv-kd14:0.2"
msgid "Cūḷavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-kd14:0.3"
msgid "Samathakkhandhaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-kd14:0.4"
msgid "1. Sammukhāvinaya"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli2.73
msgctxt "pli-tv-kd14:1.1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-kd14:1.1.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū asammukhībhūtānaṃ bhikkhūnaṃ kammāni karonti—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.3"
msgid "tajjanīyampi, niyassampi, pabbājanīyampi, paṭisāraṇīyampi, ukkhepanīyampi."
msgstr ""

msgctxt "pli-tv-kd14:1.1.4"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:1.1.6"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.7"
msgid "“kathañhi nāma chabbaggiyā bhikkhū asammukhībhūtānaṃ bhikkhūnaṃ kammāni karissanti—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.8"
msgid "tajjanīyampi, niyassampi, pabbājanīyampi, paṭisāraṇīyampi, ukkhepanīyampī”ti."
msgstr ""

msgctxt "pli-tv-kd14:1.1.9"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:1.1.11"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhū asammukhībhūtānaṃ bhikkhūnaṃ kammāni karonti—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.12"
msgid "tajjanīyampi, niyassampi, pabbājanīyampi, paṭisāraṇīyampi, ukkhepanīyampī”ti?"
msgstr ""

msgctxt "pli-tv-kd14:1.1.13"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd14:1.1.14"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.15"
msgid "“ananucchavikaṃ, bhikkhave, tesaṃ moghapurisānaṃ ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:1.1.16"
msgid "Kathañhi nāma te, bhikkhave, moghapurisā asammukhībhūtānaṃ bhikkhūnaṃ kammāni karissanti—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.17"
msgid "tajjanīyampi, niyassampi, pabbājanīyampi, paṭisāraṇīyampi, ukkhepanīyampi."
msgstr ""

msgctxt "pli-tv-kd14:1.1.18"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-kd14:1.1.20"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:1.1.22"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.23"
msgid "“Na, bhikkhave, asammukhībhūtānaṃ bhikkhūnaṃ kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:1.1.24"
msgid "tajjanīyaṃ vā, niyassaṃ vā, pabbājanīyaṃ vā, paṭisāraṇīyaṃ vā, ukkhepanīyaṃ vā."
msgstr ""

msgctxt "pli-tv-kd14:1.1.25"
msgid "Yo kareyya, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-kd14:2.1.1"
msgid "Adhammavādī puggalo adhammavādī sambahulā adhammavādī saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:2.1.2"
msgid "Dhammavādī puggalo dhammavādī sambahulā dhammavādī saṃgho."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:2.1.3"
msgid "1.1. Kaṇhapakkhanavaka"
msgstr ""

#. HTML: </h3><p>
#. REF: sc3, pts-vp-pli2.74
msgctxt "pli-tv-kd14:2.1.4"
msgid "Adhammavādī puggalo dhammavādiṃ puggalaṃ saññāpeti nijjhāpeti pekkheti anupekkheti dasseti anudasseti—‘"
msgstr ""

msgctxt "pli-tv-kd14:2.1.5"
msgid "ayaṃ dhammo, ayaṃ vinayo, idaṃ satthusāsanaṃ, imaṃ gaṇhāhi, imaṃ rocehī’ti."
msgstr ""

msgctxt "pli-tv-kd14:2.1.6"
msgid "Evañcetaṃ adhikaraṇaṃ vūpasammati, adhammena vūpasammati sammukhāvinayapatirūpakena."
msgstr ""

msgctxt "pli-tv-kd14:2.1.7"
msgid "(1)"
msgstr ""

