msgctxt "pli-tv-kd14:14.5.26"
msgid "Yo hoti anuvādo bahujanāhitāya bahujanāsukhāya bahuno janassa anatthāya ahitāya dukkhāya devamanussānaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.5.27"
msgid "Evarūpañce tumhe, bhikkhave, anuvādamūlaṃ ajjhattaṃ vā bahiddhā vā samanupasseyyātha, tatra tumhe, bhikkhave, tasseva pāpakassa anuvādamūlassa pahānāya vāyameyyātha."
msgstr ""

msgctxt "pli-tv-kd14:14.5.28"
msgid "Evarūpañce tumhe, bhikkhave, anuvādamūlaṃ ajjhattaṃ vā bahiddhā vā na samanupasseyyātha, tatra tumhe, bhikkhave, tasseva pāpakassa anuvādamūlassa āyatiṃ anavassavāya paṭipajjeyyātha."
msgstr ""

msgctxt "pli-tv-kd14:14.5.29"
msgid "Evametassa pāpakassa anuvādamūlassa pahānaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.5.30"
msgid "Evametassa pāpakassa anuvādamūlassa āyatiṃ anavassavo hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.5.31"
msgid "Imāni cha anuvādamūlāni anuvādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc115
msgctxt "pli-tv-kd14:14.5.32"
msgid "Katamāni tīṇi akusalamūlāni anuvādādhikaraṇassa mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.5.33"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ luddhacittā anuvadanti, duṭṭhacittā anuvadanti, mūḷhacittā anuvadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.5.34"
msgid "sīlavipattiyā vā, ācāravipattiyā vā, diṭṭhivipattiyā vā, ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-kd14:14.5.35"
msgid "Imāni tīṇi akusalamūlāni anuvādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc116
msgctxt "pli-tv-kd14:14.5.36"
msgid "Katamāni tīṇi kusalamūlāni anuvādādhikaraṇassa mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.5.37"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ aluddhacittā anuvadanti, aduṭṭhacittā anuvadanti, amūḷhacittā anuvadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.5.38"
msgid "sīlavipattiyā vā, ācāravipattiyā vā, diṭṭhivipattiyā vā, ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-kd14:14.5.39"
msgid "Imāni tīṇi kusalamūlāni anuvādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc117
#. VAR: Katamo kāyo → katamo ca kāyo (s1-3, km)
msgctxt "pli-tv-kd14:14.5.40"
msgid "Katamo kāyo anuvādādhikaraṇassa mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.5.41"
msgid "Idhekacco dubbaṇṇo hoti, duddassiko, okoṭimako, bahvābādho, kāṇo vā, kuṇī vā, khañjo vā, pakkhahato vā, yena naṃ anuvadanti."
msgstr ""

msgctxt "pli-tv-kd14:14.5.42"
msgid "Ayaṃ kāyo anuvādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc118
msgctxt "pli-tv-kd14:14.5.43"
msgid "Katamā vācā anuvādādhikaraṇassa mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.5.44"
msgid "Idhekacco dubbaco hoti, mammano, eḷagalavāco, yāya naṃ anuvadanti."
msgstr ""

msgctxt "pli-tv-kd14:14.5.45"
msgid "Ayaṃ vācā anuvādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc119
msgctxt "pli-tv-kd14:14.6.1"
msgid "Āpattādhikaraṇassa kiṃ mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.6.2"
msgid "Cha āpattisamuṭṭhānā āpattādhikaraṇassa mūlaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.6.3"
msgid "Atthāpatti kāyato samuṭṭhāti, na vācato, na cittato."
msgstr ""

msgctxt "pli-tv-kd14:14.6.4"
msgid "Atthāpatti vācato samuṭṭhāti, na kāyato, na cittato."
msgstr ""

msgctxt "pli-tv-kd14:14.6.5"
msgid "Atthāpatti kāyato ca vācato ca samuṭṭhāti, na cittato."
msgstr ""

msgctxt "pli-tv-kd14:14.6.6"
msgid "Atthāpatti kāyato ca cittato ca samuṭṭhāti, na vācato."
msgstr ""

msgctxt "pli-tv-kd14:14.6.7"
msgid "Atthāpatti vācato ca cittato ca samuṭṭhāti, na kāyato."
msgstr ""

msgctxt "pli-tv-kd14:14.6.8"
msgid "Atthāpatti kāyato ca vācato ca cittato ca samuṭṭhāti."
msgstr ""

msgctxt "pli-tv-kd14:14.6.9"
msgid "Ime cha āpattisamuṭṭhānā āpattādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc120
msgctxt "pli-tv-kd14:14.7.1"
msgid "Kiccādhikaraṇassa kiṃ mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.7.2"
msgid "Kiccādhikaraṇassa ekaṃ mūlaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.7.3"
msgid "saṃgho."
msgstr ""

#. HTML: </p><p>
#. REF: sc121, pts-vp-pli2.91
msgctxt "pli-tv-kd14:14.8.1"
msgid "Vivādādhikaraṇaṃ kusalaṃ, akusalaṃ, abyākataṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.8.2"
msgid "Vivādādhikaraṇaṃ siyā kusalaṃ, siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.8.3"
msgid "Tattha katamaṃ vivādādhikaraṇaṃ kusalaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.8.4"
msgid "Idha pana, bhikkhave, bhikkhū kusalacittā vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.8.5"
msgid "dhammoti vā adhammoti vā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.8.7"
msgid "duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

msgctxt "pli-tv-kd14:14.8.8"
msgid "Yaṃ tattha bhaṇḍanaṃ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhagaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.8.9"
msgid "idaṃ vuccati vivādādhikaraṇaṃ kusalaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc122
msgctxt "pli-tv-kd14:14.8.10"
msgid "Tattha katamaṃ vivādādhikaraṇaṃ akusalaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.8.11"
msgid "Idha pana, bhikkhave, bhikkhū akusalacittā vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.8.12"
msgid "dhammoti vā adhammoti vā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.8.14"
msgid "duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

msgctxt "pli-tv-kd14:14.8.15"
msgid "Yaṃ tattha bhaṇḍanaṃ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhagaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.8.16"
msgid "idaṃ vuccati vivādādhikaraṇaṃ akusalaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc123
msgctxt "pli-tv-kd14:14.8.17"
msgid "Tattha katamaṃ vivādādhikaraṇaṃ abyākataṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.8.18"
msgid "Idha pana, bhikkhave, bhikkhū abyākatacittā vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.8.19"
msgid "dhammoti vā adhammoti vā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.8.21"
msgid "duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

msgctxt "pli-tv-kd14:14.8.22"
msgid "Yaṃ tattha bhaṇḍanaṃ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhagaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.8.23"
msgid "idaṃ vuccati vivādādhikaraṇaṃ abyākataṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc124
msgctxt "pli-tv-kd14:14.9.1"
msgid "Anuvādādhikaraṇaṃ kusalaṃ, akusalaṃ, abyākataṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.9.2"
msgid "Anuvādādhikaraṇaṃ siyā kusalaṃ, siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.9.3"
msgid "Tattha katamaṃ anuvādādhikaraṇaṃ kusalaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.9.4"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ kusalacittā anuvadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.9.5"
msgid "sīlavipattiyā vā, ācāravipattiyā vā, diṭṭhivipattiyā vā, ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-kd14:14.9.6"
msgid "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.9.7"
msgid "idaṃ vuccati anuvādādhikaraṇaṃ kusalaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc125
msgctxt "pli-tv-kd14:14.9.8"
msgid "Tattha katamaṃ anuvādādhikaraṇaṃ akusalaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.9.9"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ akusalacittā anuvadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.9.10"
msgid "sīlavipattiyā vā, ācāravipattiyā vā, diṭṭhivipattiyā vā, ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-kd14:14.9.11"
msgid "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.9.12"
msgid "idaṃ vuccati anuvādādhikaraṇaṃ akusalaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc126
msgctxt "pli-tv-kd14:14.9.13"
msgid "Tattha katamaṃ anuvādādhikaraṇaṃ abyākataṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.9.14"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ abyākatacittā anuvadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.9.15"
msgid "sīlavipattiyā vā, ācāravipattiyā vā, diṭṭhivipattiyā vā, ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-kd14:14.9.16"
msgid "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.9.17"
msgid "idaṃ vuccati anuvādādhikaraṇaṃ abyākataṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc127
#. VAR: kusalaṃ → idaṃ padaṃ bj, s1-3 potthakesu
msgctxt "pli-tv-kd14:14.10.1"
msgid "Āpattādhikaraṇaṃ kusalaṃ, akusalaṃ, abyākataṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.10.2"
msgid "Āpattādhikaraṇaṃ siyā akusalaṃ, siyā abyākataṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.10.3"
msgid "natthi āpattādhikaraṇaṃ kusalaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.10.4"
msgid "Tattha katamaṃ āpattādhikaraṇaṃ akusalaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.10.5"
msgid "Yaṃ jānanto sañjānanto cecca abhivitaritvā vītikkamo—"
msgstr ""

msgctxt "pli-tv-kd14:14.10.6"
msgid "idaṃ vuccati āpattādhikaraṇaṃ akusalaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc128
msgctxt "pli-tv-kd14:14.10.7"
msgid "Tattha katamaṃ āpattādhikaraṇaṃ abyākataṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.10.8"
msgid "Yaṃ ajānanto asañjānanto acecca anabhivitaritvā vītikkamo—"
msgstr ""

msgctxt "pli-tv-kd14:14.10.9"
msgid "idaṃ vuccati āpattādhikaraṇaṃ abyākataṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc129
msgctxt "pli-tv-kd14:14.11.1"
msgid "Kiccādhikaraṇaṃ kusalaṃ, akusalaṃ, abyākataṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.11.2"
msgid "Kiccādhikaraṇaṃ siyā kusalaṃ, siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.11.3"
msgid "Tattha katamaṃ kiccādhikaraṇaṃ kusalaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.11.4"
msgid "Yaṃ saṃgho kusalacitto kammaṃ karoti—"
msgstr ""

msgctxt "pli-tv-kd14:14.11.5"
msgid "apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.11.6"
msgid "idaṃ vuccati kiccādhikaraṇaṃ kusalaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc130, pts-vp-pli2.92
msgctxt "pli-tv-kd14:14.11.7"
msgid "Tattha katamaṃ kiccādhikaraṇaṃ akusalaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.11.8"
msgid "Yaṃ saṃgho akusalacitto kammaṃ karoti—"
msgstr ""

msgctxt "pli-tv-kd14:14.11.9"
msgid "apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.11.10"
msgid "idaṃ vuccati kiccādhikaraṇaṃ akusalaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc131
msgctxt "pli-tv-kd14:14.11.11"
msgid "Tattha katamaṃ kiccādhikaraṇaṃ abyākataṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.11.12"
msgid "Yaṃ saṃgho abyākatacitto kammaṃ karoti—"
msgstr ""

msgctxt "pli-tv-kd14:14.11.13"
msgid "apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.11.14"
msgid "idaṃ vuccati kiccādhikaraṇaṃ abyākataṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc132
msgctxt "pli-tv-kd14:14.12.1"
msgid "Vivādo vivādādhikaraṇaṃ, vivādo no adhikaraṇaṃ, adhikaraṇaṃ no vivādo, adhikaraṇañceva vivādo ca."
msgstr ""

msgctxt "pli-tv-kd14:14.12.2"
msgid "Siyā vivādo vivādādhikaraṇaṃ, siyā vivādo no adhikaraṇaṃ, siyā adhikaraṇaṃ no vivādo, siyā adhikaraṇañceva vivādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc133
msgctxt "pli-tv-kd14:14.12.3"
msgid "Tattha katamo vivādo vivādādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.12.4"
msgid "Idha pana, bhikkhave, bhikkhū vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.12.5"
msgid "dhammoti vā adhammoti vā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.12.7"
msgid "duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

msgctxt "pli-tv-kd14:14.12.8"
msgid "Yaṃ tattha bhaṇḍanaṃ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhagaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.12.9"
msgid "ayaṃ vivādo vivādādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc134
msgctxt "pli-tv-kd14:14.12.10"
msgid "Tattha katamo vivādo no adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.12.11"
msgid "Mātāpi puttena vivadati, puttopi mātarā vivadati, pitāpi puttena vivadati, puttopi pitarā vivadati, bhātāpi bhātarā vivadati, bhātāpi bhaginiyā vivadati, bhaginīpi bhātarā vivadati, sahāyopi sahāyena vivadati—"
msgstr ""

msgctxt "pli-tv-kd14:14.12.12"
msgid "ayaṃ vivādo no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc135
msgctxt "pli-tv-kd14:14.12.13"
msgid "Tattha katamaṃ adhikaraṇaṃ no vivādo?"
msgstr ""

msgctxt "pli-tv-kd14:14.12.14"
msgid "Anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ, kiccādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.12.15"
msgid "idaṃ adhikaraṇaṃ no vivādo."
msgstr ""

#. HTML: </p><p>
#. REF: sc136
msgctxt "pli-tv-kd14:14.12.16"
msgid "Tattha katamaṃ adhikaraṇañceva vivādo ca?"
msgstr ""

msgctxt "pli-tv-kd14:14.12.17"
msgid "Vivādādhikaraṇaṃ adhikaraṇañceva vivādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc137
msgctxt "pli-tv-kd14:14.13.1"
msgid "Anuvādo anuvādādhikaraṇaṃ, anuvādo no adhikaraṇaṃ, adhikaraṇaṃ no anuvādo, adhikaraṇañceva anuvādo ca."
msgstr ""

msgctxt "pli-tv-kd14:14.13.2"
msgid "Siyā anuvādo anuvādādhikaraṇaṃ, siyā anuvādo no adhikaraṇaṃ, siyā adhikaraṇaṃ no anuvādo, siyā adhikaraṇañceva anuvādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc138
msgctxt "pli-tv-kd14:14.13.3"
msgid "Tattha katamo anuvādo anuvādādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.13.4"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ anuvadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.13.5"
msgid "sīlavipattiyā vā, ācāravipattiyā vā, diṭṭhivipattiyā vā, ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-kd14:14.13.6"
msgid "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.13.7"
msgid "ayaṃ anuvādo anuvādādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc139
msgctxt "pli-tv-kd14:14.13.8"
msgid "Tattha katamo anuvādo no adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.13.9"
msgid "Mātāpi puttaṃ anuvadati, puttopi mātaraṃ anuvadati, pitāpi puttaṃ anuvadati, puttopi pitaraṃ anuvadati, bhātāpi bhātaraṃ anuvadati, bhātāpi bhaginiṃ anuvadati, bhaginīpi bhātaraṃ anuvadati, sahāyopi sahāyaṃ anuvadati—"
msgstr ""

msgctxt "pli-tv-kd14:14.13.10"
msgid "ayaṃ anuvādo no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc140
msgctxt "pli-tv-kd14:14.13.11"
msgid "Tattha katamaṃ adhikaraṇaṃ no anuvādo?"
msgstr ""

msgctxt "pli-tv-kd14:14.13.12"
msgid "Āpattādhikaraṇaṃ, kiccādhikaraṇaṃ, vivādādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.13.13"
msgid "idaṃ adhikaraṇaṃ no anuvādo."
msgstr ""

#. HTML: </p><p>
#. REF: sc141
msgctxt "pli-tv-kd14:14.13.14"
msgid "Tattha katamaṃ adhikaraṇañceva anuvādo ca?"
msgstr ""

msgctxt "pli-tv-kd14:14.13.15"
msgid "Anuvādādhikaraṇaṃ adhikaraṇañceva anuvādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc142
msgctxt "pli-tv-kd14:14.14.1"
msgid "Āpatti āpattādhikaraṇaṃ, āpatti no adhikaraṇaṃ, adhikaraṇaṃ no āpatti, adhikaraṇañceva āpatti ca."
msgstr ""

msgctxt "pli-tv-kd14:14.14.2"
msgid "Siyā āpatti āpattādhikaraṇaṃ, siyā āpatti no adhikaraṇaṃ, siyā adhikaraṇaṃ no āpatti, siyā adhikaraṇañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc143, pts-vp-pli2.93
msgctxt "pli-tv-kd14:14.14.3"
msgid "Tattha katamaṃ āpatti āpattādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.14.4"
msgid "Pañcapi āpattikkhandhā āpattādhikaraṇaṃ, sattapi āpattikkhandhā āpattādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.14.5"
msgid "ayaṃ āpatti āpattādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc144
msgctxt "pli-tv-kd14:14.14.6"
msgid "Tattha katamaṃ āpatti no adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.14.7"
msgid "Sotāpatti samāpatti—"
msgstr ""

msgctxt "pli-tv-kd14:14.14.8"
msgid "ayaṃ āpatti no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc145
msgctxt "pli-tv-kd14:14.14.9"
msgid "Tattha katamaṃ adhikaraṇaṃ no āpatti?"
msgstr ""

msgctxt "pli-tv-kd14:14.14.10"
msgid "Kiccādhikaraṇaṃ, vivādādhikaraṇaṃ, anuvādādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.14.11"
msgid "idaṃ adhikaraṇaṃ no āpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc146
msgctxt "pli-tv-kd14:14.14.12"
msgid "Tattha katamaṃ adhikaraṇañceva āpatti ca?"
msgstr ""

msgctxt "pli-tv-kd14:14.14.13"
msgid "Āpattādhikaraṇaṃ adhikaraṇañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc147
msgctxt "pli-tv-kd14:14.15.1"
msgid "Kiccaṃ kiccādhikaraṇaṃ, kiccaṃ no adhikaraṇaṃ, adhikaraṇaṃ no kiccaṃ, adhikaraṇañceva kiccañca."
msgstr ""

msgctxt "pli-tv-kd14:14.15.2"
msgid "Siyā kiccaṃ kiccādhikaraṇaṃ, siyā kiccaṃ no adhikaraṇaṃ, siyā adhikaraṇaṃ no kiccaṃ, siyā adhikaraṇañceva kiccañca."
msgstr ""

#. HTML: </p><p>
#. REF: sc148
msgctxt "pli-tv-kd14:14.15.3"
msgid "Tattha katamaṃ kiccaṃ kiccādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.15.4"
msgid "Yā saṃghassa kiccayatā, karaṇīyatā, apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.15.5"
msgid "idaṃ kiccaṃ kiccādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc149
msgctxt "pli-tv-kd14:14.15.6"
msgid "Tattha katamaṃ kiccaṃ no adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.15.7"
msgid "Ācariyakiccaṃ, upajjhāyakiccaṃ, samānupajjhāyakiccaṃ, samānācariyakiccaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.15.8"
msgid "idaṃ kiccaṃ no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc150
msgctxt "pli-tv-kd14:14.15.9"
msgid "Tattha katamaṃ adhikaraṇaṃ no kiccaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.15.10"
msgid "Vivādādhikaraṇaṃ, anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.15.11"
msgid "idaṃ adhikaraṇaṃ no kiccaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc151
msgctxt "pli-tv-kd14:14.15.12"
msgid "Tattha katamaṃ adhikaraṇañceva kiccañca?"
msgstr ""

msgctxt "pli-tv-kd14:14.15.13"
msgid "Kiccādhikaraṇaṃ adhikaraṇañceva kiccañca."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd14:14.16.0"
msgid "9. 9 Adhikaraṇavūpasamanasamatha"
msgstr ""

#. HTML: </h2><h3>
msgctxt "pli-tv-kd14:14.16.1"
msgid "9.1. Sammukhāvinaya"
msgstr ""

#. HTML: </h3><p>
#. REF: sc152
msgctxt "pli-tv-kd14:14.16.2"
msgid "Vivādādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.3"
msgid "Vivādādhikaraṇaṃ dvīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.16.4"
msgid "sammukhāvinayena ca, yebhuyyasikāya ca."
msgstr ""

msgctxt "pli-tv-kd14:14.16.5"
msgid "Siyā vivādādhikaraṇaṃ ekaṃ samathaṃ anāgamma yebhuyyasikaṃ, ekena samathena sameyya—"
msgstr ""

msgctxt "pli-tv-kd14:14.16.6"
msgid "sammukhāvinayenāti?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.7"
msgid "Siyātissa vacanīyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.16.8"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.9"
msgid "Idha pana, bhikkhave, bhikkhū vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.16.10"
msgid "dhammoti vā adhammoti vā, vinayoti vā avinayoti vā, bhāsitaṃ lapitaṃ tathāgatenāti vā abhāsitaṃ alapitaṃ tathāgatenāti vā, āciṇṇaṃ tathāgatenāti vā anāciṇṇaṃ tathāgatenāti vā, paññattaṃ tathāgatenāti vā apaññattaṃ tathāgatenāti vā, āpattīti vā anāpattīti vā, lahukā āpattīti vā garukā āpattīti vā, sāvasesā āpattīti vā anavasesā āpattīti vā, duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc153, pts-vp-pli2.94
msgctxt "pli-tv-kd14:14.16.11"
msgid "Te ce, bhikkhave, bhikkhū sakkonti taṃ adhikaraṇaṃ vūpasametuṃ, idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.16.12"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.13"
msgid "Sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-kd14:14.16.14"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.15"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.16.16"
msgid "Kā ca tattha saṃghasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.17"
msgid "Yāvatikā bhikkhū kammappattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā na paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.16.18"
msgid "ayaṃ tattha saṃghasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.16.19"
msgid "Kā ca tattha dhammasammukhatā, vinayasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.20"
msgid "Yena dhammena yena vinayena yena satthusāsanena taṃ adhikaraṇaṃ vūpasammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.16.21"
msgid "ayaṃ tattha dhammasammukhatā, vinayasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.16.22"
msgid "Kā ca tattha puggalasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.16.23"
msgid "Yo ca vivadati, yena ca vivadati, ubho atthapaccatthikā sammukhībhūtā honti—"
msgstr ""

msgctxt "pli-tv-kd14:14.16.24"
msgid "ayaṃ tattha puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.16.25"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.16.26"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc154
#. VAR: sambahulā → bahutarā (bj, s1-3, pts1)
msgctxt "pli-tv-kd14:14.17.1"
msgid "Te ce, bhikkhave, bhikkhū na sakkonti taṃ adhikaraṇaṃ tasmiṃ āvāse vūpasametuṃ, tehi, bhikkhave, bhikkhūhi, yasmiṃ āvāse sambahulā bhikkhū, so āvāso gantabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.18.1"
msgid "Te ce, bhikkhave, bhikkhū taṃ āvāsaṃ gacchantā antarāmagge sakkonti taṃ adhikaraṇaṃ vūpasametuṃ, idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.18.2"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.18.3"
msgid "Sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-kd14:14.18.4"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.18.5"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.18.6"
msgid "Kā ca tattha saṃghasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.18.7"
msgid "Yāvatikā bhikkhū kammappattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā na paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.18.8"
msgid "ayaṃ tattha saṃghasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.18.9"
msgid "Kā ca tattha dhammasammukhatā, vinayasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.18.10"
msgid "Yena dhammena yena vinayena yena satthusāsanena taṃ adhikaraṇaṃ vūpasammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.18.11"
msgid "ayaṃ tattha dhammasammukhatā, vinayasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.18.12"
msgid "Kā ca tattha puggalasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.18.13"
msgid "Yo ca vivadati, yena ca vivadati, ubho atthapaccatthikā sammukhībhūtā honti—"
msgstr ""

msgctxt "pli-tv-kd14:14.18.14"
msgid "ayaṃ tattha puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.18.15"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.18.16"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc155
msgctxt "pli-tv-kd14:14.18.17"
msgid "Te ce, bhikkhave, bhikkhū taṃ āvāsaṃ āgacchantā antarāmagge na sakkonti taṃ adhikaraṇaṃ vūpasametuṃ, tehi, bhikkhave, bhikkhūhi, taṃ āvāsaṃ gantvā āvāsikā bhikkhū evamassu vacanīyā—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.18.18"
msgid "idaṃ kho, āvuso, adhikaraṇaṃ evaṃ jātaṃ, evaṃ samuppannaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.18.19"
msgid "sādhāyasmantā imaṃ adhikaraṇaṃ vūpasamentu dhammena vinayena satthusāsanena, yathayidaṃ adhikaraṇaṃ suvūpasantaṃ assā’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc156
msgctxt "pli-tv-kd14:14.18.20"
msgid "Sace, bhikkhave, āvāsikā bhikkhū vuḍḍhatarā honti, āgantukā bhikkhū navakatarā, tehi, bhikkhave, āvāsikehi bhikkhūhi āgantukā bhikkhū evamassu vacanīyā—"
msgstr ""

msgctxt "pli-tv-kd14:14.18.21"
msgid "‘iṅgha tumhe, āyasmanto, muhuttaṃ ekamantaṃ hotha, yāva mayaṃ mantemā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.18.22"
msgid "Sace pana, bhikkhave, āvāsikā bhikkhū navakatarā honti, āgantukā bhikkhū vuḍḍhatarā, tehi, bhikkhave, āvāsikehi bhikkhūhi āgantukā bhikkhū evamassu vacanīyā—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.18.23"
msgid "tena hi tumhe, āyasmanto, muhuttaṃ idheva tāva hotha, yāva mayaṃ mantemā’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc157, pts-vp-pli2.95
msgctxt "pli-tv-kd14:14.18.24"
msgid "Sace pana, bhikkhave, āvāsikānaṃ bhikkhūnaṃ mantayamānānaṃ evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.18.25"
msgid "na mayaṃ sakkoma imaṃ adhikaraṇaṃ vūpasametuṃ dhammena vinayena satthusāsanenā’ti, na taṃ adhikaraṇaṃ āvāsikehi bhikkhūhi sampaṭicchitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.18.26"
msgid "Sace pana, bhikkhave, āvāsikānaṃ bhikkhūnaṃ mantayamānānaṃ evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.18.27"
msgid "sakkoma mayaṃ imaṃ adhikaraṇaṃ vūpasametuṃ dhammena vinayena satthusāsanenā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.18.28"
msgid "Tehi, bhikkhave, āvāsikehi bhikkhūhi āgantukā bhikkhū evamassu vacanīyā—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.18.29"
msgid "sace tumhe, āyasmanto, amhākaṃ imaṃ adhikaraṇaṃ yathājātaṃ yathāsamuppannaṃ ārocessatha, yathā ca mayaṃ imaṃ adhikaraṇaṃ vūpasamessāma dhammena vinayena satthusāsanena tathā suvūpasantaṃ bhavissati."
msgstr ""

msgctxt "pli-tv-kd14:14.18.30"
msgid "Evaṃ mayaṃ imaṃ adhikaraṇaṃ sampaṭicchissāma."
msgstr ""

msgctxt "pli-tv-kd14:14.18.31"
msgid "No ce tumhe, āyasmanto, amhākaṃ imaṃ adhikaraṇaṃ yathājātaṃ yathāsamuppannaṃ ārocessatha, yathā ca mayaṃ imaṃ adhikaraṇaṃ vūpasamessāma dhammena vinayena satthusāsanena tathā na suvūpasantaṃ bhavissati, na mayaṃ imaṃ adhikaraṇaṃ sampaṭicchissāmā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.18.32"
msgid "Evaṃ supariggahitaṃ kho, bhikkhave, katvā āvāsikehi bhikkhūhi taṃ adhikaraṇaṃ sampaṭicchitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc158
msgctxt "pli-tv-kd14:14.18.33"
msgid "Tehi, bhikkhave, āgantukehi bhikkhūhi āvāsikā bhikkhū evamassu vacanīyā—"
msgstr ""

msgctxt "pli-tv-kd14:14.18.34"
msgid "‘yathājātaṃ yathāsamuppannaṃ mayaṃ imaṃ adhikaraṇaṃ āyasmantānaṃ ārocessāma."
msgstr ""

msgctxt "pli-tv-kd14:14.18.35"
msgid "Sace āyasmantā sakkonti ettakena vā ettakena vā antarena imaṃ adhikaraṇaṃ vūpasametuṃ dhammena vinayena satthusāsanena tathā suvūpasantaṃ bhavissati."
msgstr ""

msgctxt "pli-tv-kd14:14.18.36"
msgid "Evaṃ mayaṃ imaṃ adhikaraṇaṃ āyasmantānaṃ niyyādessāma."
msgstr ""

msgctxt "pli-tv-kd14:14.18.37"
msgid "No ce āyasmantā sakkonti ettakena vā ettakena vā antarena imaṃ adhikaraṇaṃ vūpasametuṃ dhammena vinayena satthusāsanena tathā na suvūpasantaṃ bhavissati, na mayaṃ imaṃ adhikaraṇaṃ āyasmantānaṃ niyyādessāma."
msgstr ""

msgctxt "pli-tv-kd14:14.18.38"
msgid "Mayameva imassa adhikaraṇassa sāmino bhavissāmā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.18.39"
msgid "Evaṃ supariggahitaṃ kho, bhikkhave, katvā āgantukehi bhikkhūhi taṃ adhikaraṇaṃ āvāsikānaṃ bhikkhūnaṃ niyyādetabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc159
msgctxt "pli-tv-kd14:14.18.40"
msgid "Te ce, bhikkhave, bhikkhū sakkonti taṃ adhikaraṇaṃ vūpasametuṃ, idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.18.41"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.18.42"
msgid "Sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-kd14:14.18.43"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.18.44"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.18.46"
msgid "evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.18.47"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.19.0"
msgid "9.2. Ubbāhikāyavūpasamana"
msgstr ""

#. HTML: </h3><p>
#. REF: sc160, pts-vp-pli2.96
#. VAR: anantāni → anaggāni (bj, pts1)
#. VAR: na cekassa → na cetassa (bj, s1-3, mr)
msgctxt "pli-tv-kd14:14.19.1"
msgid "Tehi ce, bhikkhave, bhikkhūhi tasmiṃ adhikaraṇe vinicchiyamāne anantāni ceva bhassāni jāyanti, na cekassa bhāsitassa attho viññāyati, anujānāmi, bhikkhave, evarūpaṃ adhikaraṇaṃ ubbāhikāya vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.19.2"
msgid "Dasahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo—"
msgstr ""

msgctxt "pli-tv-kd14:14.19.3"
msgid "sīlavā hoti, pātimokkhasaṃvarasaṃvuto viharati ācāragocarasampanno aṇumattesu vajjesu bhayadassāvī, samādāya sikkhati sikkhāpadesu;"
msgstr ""

#. VAR: dhātā → dhatā (bj, s1-3, pts1)
msgctxt "pli-tv-kd14:14.19.4"
msgid "bahussuto hoti sutadharo sutasannicayo, ye te dhammā ādikalyāṇā majjhekalyāṇā pariyosānakalyāṇā sātthaṃ sabyañjanaṃ kevalaparipuṇṇaṃ parisuddhaṃ brahmacariyaṃ abhivadanti, tathārūpassa dhammā bahussutā honti, dhātā vacasā paricitā manasānupekkhitā diṭṭhiyā suppaṭividdhā;"
msgstr ""

msgctxt "pli-tv-kd14:14.19.5"
msgid "ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso;"
msgstr ""

#. VAR: ṭhito → cheko (pts1, mr)
msgctxt "pli-tv-kd14:14.19.6"
msgid "vinaye kho pana ṭhito hoti asaṃhīro;"
msgstr ""

msgctxt "pli-tv-kd14:14.19.7"
msgid "paṭibalo hoti ubho atthapaccatthike assāsetuṃ saññāpetuṃ nijjhāpetuṃ pekkhetuṃ pasādetuṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.19.8"
msgid "adhikaraṇasamuppādavūpasamanakusalo hoti;"
msgstr ""

msgctxt "pli-tv-kd14:14.19.9"
msgid "adhikaraṇaṃ jānāti;"
msgstr ""

msgctxt "pli-tv-kd14:14.19.10"
msgid "adhikaraṇasamudayaṃ jānāti;"
msgstr ""

msgctxt "pli-tv-kd14:14.19.11"
msgid "adhikaraṇanirodhaṃ jānāti;"
msgstr ""

msgctxt "pli-tv-kd14:14.19.12"
msgid "adhikaraṇanirodhagāminipaṭipadaṃ jānāti."
msgstr ""

msgctxt "pli-tv-kd14:14.19.13"
msgid "Anujānāmi, bhikkhave, imehi dasahaṅgehi samannāgataṃ bhikkhuṃ ubbāhikāya sammannituṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.20.1"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.20.2"
msgid "Paṭhamaṃ bhikkhu yācitabbo, yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc161
msgctxt "pli-tv-kd14:14.20.3"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.20.4"
msgid "Amhākaṃ imasmiṃ adhikaraṇe vinicchiyamāne anantāni ceva bhassāni jāyanti, na cekassa bhāsitassa attho viññāyati."
msgstr ""

msgctxt "pli-tv-kd14:14.20.5"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmañca itthannāmañca bhikkhuṃ sammanneyya ubbāhikāya imaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.20.6"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc162
msgctxt "pli-tv-kd14:14.20.7"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.20.8"
msgid "Amhākaṃ imasmiṃ adhikaraṇe vinicchiyamāne anantāni ceva bhassāni jāyanti, na cekassa bhāsitassa attho viññāyati."
msgstr ""

msgctxt "pli-tv-kd14:14.20.9"
msgid "Saṃgho itthannāmañca itthannāmañca bhikkhuṃ sammannati ubbāhikāya imaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.20.10"
msgid "Yassāyasmato khamati itthannāmassa ca itthannāmassa ca bhikkhuno sammuti ubbāhikāya imaṃ adhikaraṇaṃ vūpasametuṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:14.20.11"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc163
msgctxt "pli-tv-kd14:14.20.12"
msgid "Sammato saṃghena itthannāmo ca itthannāmo ca bhikkhu ubbāhikāya imaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.20.13"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc164
msgctxt "pli-tv-kd14:14.21.1"
msgid "Te ce, bhikkhave, bhikkhū sakkonti taṃ adhikaraṇaṃ ubbāhikāya vūpasametuṃ, idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.21.2"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.21.3"
msgid "Sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-kd14:14.21.4"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.21.5"
msgid "Dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.21.7"
msgid "evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc165
msgctxt "pli-tv-kd14:14.22.1"
msgid "Tehi ce, bhikkhave, bhikkhūhi tasmiṃ adhikaraṇe vinicchiyamāne tatrāssa bhikkhu dhammakathiko, tassa neva suttaṃ āgataṃ hoti, no suttavibhaṅgo, so atthaṃ asallakkhento byañjanacchāyāya atthaṃ paṭibāhati, byattena bhikkhunā paṭibalena te bhikkhū ñāpetabbā—"
msgstr ""

#. HTML: </p><p>
#. REF: sc166, pts-vp-pli2.97
msgctxt "pli-tv-kd14:14.22.2"
msgid "‘Suṇantu me āyasmantā."
msgstr ""

msgctxt "pli-tv-kd14:14.22.3"
msgid "Ayaṃ itthannāmo bhikkhu dhammakathiko."
msgstr ""

msgctxt "pli-tv-kd14:14.22.4"
msgid "Imassa neva suttaṃ āgataṃ hoti, no suttavibhaṅgo."
msgstr ""

msgctxt "pli-tv-kd14:14.22.5"
msgid "So atthaṃ asallakkhento byañjanacchāyāya atthaṃ paṭibāhati."
msgstr ""

msgctxt "pli-tv-kd14:14.22.6"
msgid "Yadāyasmantānaṃ pattakallaṃ, itthannāmaṃ bhikkhuṃ vuṭṭhāpetvā avasesā imaṃ adhikaraṇaṃ vūpasameyyāmā’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc167
msgctxt "pli-tv-kd14:14.22.7"
msgid "Te ce, bhikkhave, bhikkhū taṃ bhikkhuṃ vuṭṭhāpetvā sakkonti taṃ adhikaraṇaṃ vūpasametuṃ, idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.22.8"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.22.9"
msgid "Sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-kd14:14.22.10"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.22.11"
msgid "Dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.22.13"
msgid "evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc168
msgctxt "pli-tv-kd14:14.23.1"
msgid "Tehi ce, bhikkhave, bhikkhūhi tasmiṃ adhikaraṇe vinicchiyamāne tatrāssa bhikkhu dhammakathiko, tassa suttañhi kho āgataṃ hoti, no suttavibhaṅgo, so atthaṃ asallakkhento byañjanacchāyāya atthaṃ paṭibāhati, byattena bhikkhunā paṭibalena te bhikkhū ñāpetabbā—"
msgstr ""

#. HTML: </p><p>
#. REF: sc169
msgctxt "pli-tv-kd14:14.23.2"
msgid "‘Suṇantu me āyasmantā."
msgstr ""

msgctxt "pli-tv-kd14:14.23.3"
msgid "Ayaṃ itthannāmo bhikkhu dhammakathiko."
msgstr ""

msgctxt "pli-tv-kd14:14.23.4"
msgid "Imassa suttañhi kho āgataṃ hoti, no suttavibhaṅgo."
msgstr ""

msgctxt "pli-tv-kd14:14.23.5"
msgid "So atthaṃ asallakkhento byañjanacchāyāya atthaṃ paṭibāhati."
msgstr ""

msgctxt "pli-tv-kd14:14.23.6"
msgid "Yadāyasmantānaṃ pattakallaṃ, itthannāmaṃ bhikkhuṃ vuṭṭhāpetvā avasesā imaṃ adhikaraṇaṃ vūpasameyyāmā’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc170
msgctxt "pli-tv-kd14:14.23.7"
msgid "Te ce, bhikkhave, bhikkhū taṃ bhikkhuṃ vuṭṭhāpetvā sakkonti taṃ adhikaraṇaṃ vūpasametuṃ, idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.23.8"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.23.9"
msgid "Sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-kd14:14.23.10"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.23.11"
msgid "Dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.23.13"
msgid "evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.24.0"
msgid "9.3. Yebhuyyasikāvinaya"
msgstr ""

#. HTML: </h3><p>
#. REF: sc171
msgctxt "pli-tv-kd14:14.24.1"
msgid "Te ce, bhikkhave, bhikkhū na sakkonti taṃ adhikaraṇaṃ ubbāhikāya vūpasametuṃ, tehi, bhikkhave, bhikkhūhi taṃ adhikaraṇaṃ saṃghassa niyyādetabbaṃ—‘"
msgstr ""

#. VAR: na mayaṃ → na ca mayaṃ (mr)
msgctxt "pli-tv-kd14:14.24.2"
msgid "na mayaṃ, bhante, sakkoma imaṃ adhikaraṇaṃ ubbāhikāya vūpasametuṃ, saṃghova imaṃ adhikaraṇaṃ vūpasametū’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.24.3"
msgid "Anujānāmi, bhikkhave, evarūpaṃ adhikaraṇaṃ yebhuyyasikāya vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.24.4"
msgid "Pañcahaṅgehi samannāgato bhikkhu salākaggāhāpako sammannitabbo—"
msgstr ""

msgctxt "pli-tv-kd14:14.24.5"
msgid "yo na chandāgatiṃ gaccheyya, na dosāgatiṃ gaccheyya, na mohāgatiṃ gaccheyya, na bhayāgatiṃ gaccheyya, gahitāgahitañca jāneyya … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.24.7"
msgid "evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.24.8"
msgid "Paṭhamaṃ bhikkhu yācitabbo, yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc172
msgctxt "pli-tv-kd14:14.24.9"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.24.10"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuṃ salākaggāhāpakaṃ sammanneyya."
msgstr ""

msgctxt "pli-tv-kd14:14.24.11"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc173
msgctxt "pli-tv-kd14:14.24.12"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.24.13"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ salākaggāhāpakaṃ sammannati."
msgstr ""

msgctxt "pli-tv-kd14:14.24.14"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno salākaggāhāpakassa sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:14.24.15"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc174
msgctxt "pli-tv-kd14:14.24.16"
msgid "Sammato saṃghena itthannāmo bhikkhu salākaggāhāpako."
msgstr ""

msgctxt "pli-tv-kd14:14.24.17"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc175
msgctxt "pli-tv-kd14:14.24.18"
msgid "Tena salākaggāhāpakena bhikkhunā salākā gāhetabbā."
msgstr ""

msgctxt "pli-tv-kd14:14.24.19"
msgid "Yathā bahutarā bhikkhū dhammavādino vadanti tathā taṃ adhikaraṇaṃ vūpasametabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.24.20"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.24.21"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.24.22"
msgid "Sammukhāvinayena ca, yebhuyyasikāya ca."
msgstr ""

msgctxt "pli-tv-kd14:14.24.23"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.24.24"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.24.25"
msgid "Kā ca tattha saṃghasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.24.26"
msgid "Yāvatikā bhikkhū kammappattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā na paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.24.27"
msgid "ayaṃ tattha saṃghasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.24.28"
msgid "Kā ca tattha dhammasammukhatā, vinayasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.24.29"
msgid "Yena dhammena yena vinayena yena satthusāsanena taṃ adhikaraṇaṃ vūpasammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.24.30"
msgid "ayaṃ tattha dhammasammukhatā, vinayasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.24.31"
msgid "Kā ca tattha puggalasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.24.32"
msgid "Yo ca vivadati, yena ca vivadati, ubho atthapaccatthikā sammukhībhūtā honti—"
msgstr ""

msgctxt "pli-tv-kd14:14.24.33"
msgid "ayaṃ tattha puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.24.34"
msgid "Kā ca tattha yebhuyyasikāya?"
msgstr ""

msgctxt "pli-tv-kd14:14.24.35"
msgid "Yā yebhuyyasikākammassa kiriyā karaṇaṃ upagamanaṃ ajjhupagamanaṃ adhivāsanā appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-kd14:14.24.36"
msgid "ayaṃ tattha yebhuyyasikāya."
msgstr ""

msgctxt "pli-tv-kd14:14.24.37"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.24.38"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyan”ti."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.25.0"
msgid "9.4. Tividhasalākaggāha"
msgstr ""

#. HTML: </h3><p>
#. REF: sc176, pts-vp-pli2.98
msgctxt "pli-tv-kd14:14.25.1"
msgid "Tena kho pana samayena sāvatthiyā evaṃ jātaṃ evaṃ samuppannaṃ adhikaraṇaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.25.2"
msgid "Atha kho te bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.3"
msgid "asantuṭṭhā sāvatthiyā saṃghassa adhikaraṇavūpasamanena—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.4"
msgid "assosuṃ kho amukasmiṃ kira āvāse sambahulā therā viharanti bahussutā āgatāgamā dhammadharā vinayadharā mātikādharā paṇḍitā viyattā medhāvino lajjino kukkuccakā sikkhākāmā."
msgstr ""

msgctxt "pli-tv-kd14:14.25.5"
msgid "Te ce therā imaṃ adhikaraṇaṃ vūpasameyyuṃ dhammena vinayena satthusāsanena, evamidaṃ adhikaraṇaṃ suvūpasantaṃ assāti."
msgstr ""

msgctxt "pli-tv-kd14:14.25.6"
msgid "Atha kho te bhikkhū taṃ āvāsaṃ gantvā te there etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.7"
msgid "“idaṃ, bhante, adhikaraṇaṃ evaṃ jātaṃ, evaṃ samuppannaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.25.8"
msgid "Sādhu, bhante, therā imaṃ adhikaraṇaṃ vūpasamentu dhammena vinayena satthusāsanena, yathayidaṃ adhikaraṇaṃ suvūpasantaṃ assā”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.25.9"
msgid "Atha kho te therā—"
msgstr ""

#. VAR: tathā suvūpasantanti → yathā suvūpasantaṃ (bj, s1-3)
msgctxt "pli-tv-kd14:14.25.10"
msgid "yathā sāvatthiyā saṃghena adhikaraṇaṃ vūpasamitaṃ tathā suvūpasantanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.11"
msgid "tathā taṃ adhikaraṇaṃ vūpasamesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc177
msgctxt "pli-tv-kd14:14.25.12"
msgid "Atha kho te bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.13"
msgid "asantuṭṭhā sāvatthiyā saṃghassa adhikaraṇavūpasamanena, asantuṭṭhā sambahulānaṃ therānaṃ adhikaraṇavūpasamanena—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.14"
msgid "assosuṃ kho amukasmiṃ kira āvāse tayo therā viharanti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.25.16"
msgid "dve therā viharanti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.25.18"
msgid "eko thero viharati bahussuto āgatāgamo dhammadharo vinayadharo mātikādharo paṇḍito viyatto medhāvī lajjī kukkuccako sikkhākāmo."
msgstr ""

msgctxt "pli-tv-kd14:14.25.19"
msgid "So ce thero imaṃ adhikaraṇaṃ vūpasameyya dhammena vinayena satthusāsanena, evamidaṃ adhikaraṇaṃ suvūpasantaṃ assāti."
msgstr ""

msgctxt "pli-tv-kd14:14.25.20"
msgid "Atha kho te bhikkhū taṃ āvāsaṃ gantvā taṃ theraṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.21"
msgid "“idaṃ, bhante, adhikaraṇaṃ evaṃ jātaṃ, evaṃ samuppannaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.25.22"
msgid "Sādhu, bhante, thero imaṃ adhikaraṇaṃ vūpasametu dhammena vinayena satthusāsanena, yathayidaṃ adhikaraṇaṃ suvūpasantaṃ assā”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.25.23"
msgid "Atha kho so thero—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.24"
msgid "yathā sāvatthiyā saṃghena adhikaraṇaṃ vūpasamitaṃ, yathā sambahulehi therehi adhikaraṇaṃ vūpasamitaṃ, yathā tīhi therehi adhikaraṇaṃ vūpasamitaṃ, yathā dvīhi therehi adhikaraṇaṃ vūpasamitaṃ, tathā suvūpasantanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.25.25"
msgid "tathā taṃ adhikaraṇaṃ vūpasamesi."
msgstr ""

#. HTML: </p><p>
#. REF: sc178
msgctxt "pli-tv-kd14:14.25.26"
msgid "Atha kho te bhikkhū asantuṭṭhā sāvatthiyā saṃghassa adhikaraṇavūpasamanena, asantuṭṭhā sambahulānaṃ therānaṃ adhikaraṇavūpasamanena, asantuṭṭhā tiṇṇaṃ therānaṃ adhikaraṇavūpasamanena, asantuṭṭhā dvinnaṃ therānaṃ adhikaraṇavūpasamanena, asantuṭṭhā ekassa therassa adhikaraṇavūpasamanena, yena bhagavā tenupasaṅkamiṃsu, upasaṅkamitvā bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

#. HTML: </p><p>
#. REF: sc179
msgctxt "pli-tv-kd14:14.25.28"
msgid "“Nihatametaṃ, bhikkhave, adhikaraṇaṃ santaṃ vūpasantaṃ suvūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.26.1"
msgid "Anujānāmi, bhikkhave, tesaṃ bhikkhūnaṃ saññattiyā tayo salākaggāhe—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.2"
msgid "gūḷhakaṃ, sakaṇṇajappakaṃ, vivaṭakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc180, pts-vp-pli2.99
msgctxt "pli-tv-kd14:14.26.3"
msgid "Kathañca, bhikkhave, gūḷhako salākaggāho hoti?"
msgstr ""

msgctxt "pli-tv-kd14:14.26.4"
msgid "Tena salākaggāhāpakena bhikkhunā salākāyo vaṇṇāvaṇṇāyo katvā ekameko bhikkhu upasaṅkamitvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.5"
msgid "‘ayaṃ evaṃvādissa salākā, ayaṃ evaṃvādissa salākā."
msgstr ""

msgctxt "pli-tv-kd14:14.26.6"
msgid "Yaṃ icchasi taṃ gaṇhāhī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.26.7"
msgid "Gahite vattabbo—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.26.8"
msgid "mā ca kassaci dassehī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.26.9"
msgid "Sace jānāti—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.10"
msgid "adhammavādī bahutarāti, duggahoti, paccukkaḍḍhitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.26.11"
msgid "Sace jānāti—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.12"
msgid "dhammavādī bahutarāti, suggahoti, sāvetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.26.13"
msgid "Evaṃ kho, bhikkhave, gūḷhako salākaggāho hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc181
msgctxt "pli-tv-kd14:14.26.14"
msgid "Kathañca, bhikkhave, sakaṇṇajappako salākaggāho hoti?"
msgstr ""

msgctxt "pli-tv-kd14:14.26.15"
msgid "Tena salākaggāhāpakena bhikkhunā ekamekassa bhikkhuno upakaṇṇake ārocetabbaṃ—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.26.16"
msgid "ayaṃ evaṃvādissa salākā, ayaṃ evaṃvādissa salākā."
msgstr ""

msgctxt "pli-tv-kd14:14.26.17"
msgid "Yaṃ icchasi taṃ gaṇhāhī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.26.18"
msgid "Gahite vattabbo—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.19"
msgid "‘mā ca kassaci ārocehī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.26.20"
msgid "Sace jānāti—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.21"
msgid "adhammavādī bahutarāti, duggahoti, paccukkaḍḍhitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.26.22"
msgid "Sace jānāti—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.23"
msgid "dhammavādī bahutarāti, suggahoti, sāvetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.26.24"
msgid "Evaṃ kho, bhikkhave, sakaṇṇajappako salākaggāho hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc182
msgctxt "pli-tv-kd14:14.26.25"
msgid "Kathañca, bhikkhave, vivaṭako salākaggāho hoti?"
msgstr ""

msgctxt "pli-tv-kd14:14.26.26"
msgid "Sace jānāti—"
msgstr ""

msgctxt "pli-tv-kd14:14.26.27"
msgid "dhammavādī bahutarāti, vissaṭṭheneva vivaṭena gāhetabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.26.28"
msgid "Evaṃ kho, bhikkhave, vivaṭako salākaggāho hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.26.29"
msgid "Ime kho, bhikkhave, tayo salākaggāhā”ti."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.27.0"
msgid "9.5. Sativinaya"
msgstr ""

#. HTML: </h3><p>
#. REF: sc183
msgctxt "pli-tv-kd14:14.27.1"
msgid "“Anuvādādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-kd14:14.27.2"
msgid "Anuvādādhikaraṇaṃ catūhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-kd14:14.27.3"
msgid "sammukhāvinayena ca, sativinayena ca, amūḷhavinayena ca, tassapāpiyasikāya ca."
msgstr ""

msgctxt "pli-tv-kd14:14.27.4"
msgid "Siyā anuvādādhikaraṇaṃ dve samathe anāgamma—"
msgstr ""

msgctxt "pli-tv-kd14:14.27.5"
msgid "amūḷhavinayañca, tassapāpiyasikañca;"
msgstr ""

msgctxt "pli-tv-kd14:14.27.6"
msgid "dvīhi samathehi sammeyya—"
msgstr ""

msgctxt "pli-tv-kd14:14.27.7"
msgid "sammukhāvinayena ca, sativinayena cāti?"
msgstr ""

msgctxt "pli-tv-kd14:14.27.8"
msgid "Siyātissa vacanīyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.27.9"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-kd14:14.27.10"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

#. VAR: Tassa kho, bhikkhave → tassa kho taṃ bhikkhave (bj, s1-3, pts1, mr)
msgctxt "pli-tv-kd14:14.27.11"
msgid "Tassa kho, bhikkhave, bhikkhuno sativepullappattassa sativinayo dātabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.27.12"
msgid "Evañca pana, bhikkhave, dātabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc184
msgctxt "pli-tv-kd14:14.27.13"
msgid "Tena, bhikkhave, bhikkhunā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.27.15"
msgid "evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.27.16"
msgid "maṃ, bhante, bhikkhū amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

msgctxt "pli-tv-kd14:14.27.17"
msgid "Sohaṃ, bhante, sativepullappatto saṃghaṃ sativinayaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.27.18"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.27.19"
msgid "Tatiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.27.20"
msgid "Byattenaṃ bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc185, pts-vp-pli2.100
msgctxt "pli-tv-kd14:14.27.21"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.27.22"
msgid "Bhikkhū itthannāmaṃ bhikkhuṃ amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

msgctxt "pli-tv-kd14:14.27.23"
msgid "So sativepullappatto saṃghaṃ sativinayaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd14:14.27.24"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmassa bhikkhuno sativepullappattassa sativinayaṃ dadeyya."
msgstr ""

msgctxt "pli-tv-kd14:14.27.25"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc186
msgctxt "pli-tv-kd14:14.27.26"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.27.27"
msgid "Bhikkhū itthannāmaṃ bhikkhuṃ amūlikāya sīlavipattiyā anuddhaṃsenti."
msgstr ""

msgctxt "pli-tv-kd14:14.27.28"
msgid "So sativepullappatto saṃghaṃ sativinayaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd14:14.27.29"
msgid "Saṃgho itthannāmassa bhikkhuno sativepullappattassa sativinayaṃ deti."
msgstr ""

msgctxt "pli-tv-kd14:14.27.30"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno sativepullappattassa sativinayassa dānaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:14.27.31"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc187
msgctxt "pli-tv-kd14:14.27.32"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.27.34"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-kd14:14.27.35"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc188
msgctxt "pli-tv-kd14:14.27.36"
msgid "Dinno saṃghena itthannāmassa bhikkhuno sativepullappattassa sativinayo."
msgstr ""

msgctxt "pli-tv-kd14:14.27.37"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc189
msgctxt "pli-tv-kd14:14.27.38"
msgid "Idaṃ vuccati, bhikkhave, adhikaraṇaṃ vūpasantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.27.39"
msgid "Kena vūpasantaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.27.40"
msgid "Sammukhāvinayena ca sativinayena ca."
msgstr ""

msgctxt "pli-tv-kd14:14.27.41"
msgid "Kiñca tattha sammukhāvinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.27.42"
msgid "Saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.27.44"
msgid "kā ca tattha puggalasammukhatā?"
msgstr ""

msgctxt "pli-tv-kd14:14.27.45"
msgid "Yo ca anuvadati, yañca anuvadati, ubho sammukhībhūtā honti—"
msgstr ""

msgctxt "pli-tv-kd14:14.27.46"
msgid "ayaṃ tattha puggalasammukhatā."
msgstr ""

msgctxt "pli-tv-kd14:14.27.47"
msgid "Kiñca tattha sativinayasmiṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.27.48"
msgid "Yā sativinayassa kammassa kiriyā karaṇaṃ upagamanaṃ ajjhupagamanaṃ adhivāsanā appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-kd14:14.27.49"
msgid "idaṃ tattha sativinayasmiṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.27.50"
msgid "Evaṃ vūpasantañce, bhikkhave, adhikaraṇaṃ kārako ukkoṭeti, ukkoṭanakaṃ pācittiyaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:14.27.51"
msgid "chandadāyako khīyati, khīyanakaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:14.28.0"
msgid "9.6. Amūḷhavinaya"
msgstr ""

#. HTML: </h3><p>
#. REF: sc190
msgctxt "pli-tv-kd14:14.28.1"
msgid "Siyā anuvādādhikaraṇaṃ dve samathe anāgamma—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.2"
msgid "sativinayañca, tassapāpiyasikañca;"
msgstr ""

msgctxt "pli-tv-kd14:14.28.3"
msgid "dvīhi samathehi sammeyya—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.4"
msgid "sammukhāvinayena ca, amūḷhavinayena cāti?"
msgstr ""

msgctxt "pli-tv-kd14:14.28.5"
msgid "Siyātissa vacanīyaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.6"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-kd14:14.28.7"
msgid "Idha pana, bhikkhave, bhikkhu ummattako hoti cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:14.28.8"
msgid "Tena ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ hoti bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.9"
msgid "Taṃ bhikkhū ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.10"
msgid "‘saratāyasmā evarūpiṃ āpattiṃ āpajjitā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.11"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.28.12"
msgid "ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:14.28.13"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.14"
msgid "Nāhaṃ taṃ sarāmi."
msgstr ""

msgctxt "pli-tv-kd14:14.28.15"
msgid "Mūḷhena me etaṃ katan’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.16"
msgid "Evampi naṃ vuccamānā codenteva—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.28.17"
msgid "saratāyasmā evarūpiṃ āpattiṃ āpajjitā’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.18"
msgid "Tassa kho, bhikkhave, bhikkhuno amūḷhassa amūḷhavinayo dātabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.28.19"
msgid "Evañca pana, bhikkhave, dātabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc191
msgctxt "pli-tv-kd14:14.28.20"
msgid "Tena, bhikkhave, bhikkhunā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.28.22"
msgid "evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd14:14.28.23"
msgid "ahaṃ, bhante, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:14.28.24"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.25"
msgid "Maṃ bhikkhū ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.26"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.27"
msgid "Tyāhaṃ evaṃ vadāmi—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.28"
msgid "“ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:14.28.29"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.30"
msgid "Nāhaṃ taṃ sarāmi."
msgstr ""

msgctxt "pli-tv-kd14:14.28.31"
msgid "Mūḷhena me etaṃ katan”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.32"
msgid "Evampi maṃ vuccamānā codenteva—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.33"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.34"
msgid "“Sohaṃ, bhante, amūḷho saṃghaṃ amūḷhavinayaṃ yācāmī”’ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.35"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.28.36"
msgid "Tatiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd14:14.28.37"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc192
msgctxt "pli-tv-kd14:14.28.38"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:14.28.39"
msgid "Ayaṃ itthannāmo bhikkhu ummattako ahosi cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:14.28.40"
msgid "Tena ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.41"
msgid "Taṃ bhikkhū ummattakena cittavipariyāsakatena ajjhāciṇṇena āpattiyā codenti—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.42"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.43"
msgid "So evaṃ vadeti—"
msgstr ""

