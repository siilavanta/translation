msgid "“ahaṃ kho, āvuso, ummattako ahosiṃ cittavipariyāsakato."
msgstr ""

msgctxt "pli-tv-kd14:14.28.45"
msgid "Tena me ummattakena cittavipariyāsakatena bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.28.46"
msgid "Nāhaṃ taṃ sarāmi."
msgstr ""

msgctxt "pli-tv-kd14:14.28.47"
msgid "Mūḷhena me etaṃ katan”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.48"
msgid "Evampi naṃ vuccamānā codenteva—"
msgstr ""

msgctxt "pli-tv-kd14:14.28.49"
msgid "“saratāyasmā evarūpiṃ āpattiṃ āpajjitā”ti."
msgstr ""

msgctxt "pli-tv-kd14:14.28.50"
msgid "So amūḷho saṃghaṃ amūḷhavinayaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd14:14.28.51"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmassa bhikkhuno amūḷhassa amūḷhavinayaṃ dadeyya."
msgstr ""

msgctxt "pli-tv-kd14:14.28.52"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc193
