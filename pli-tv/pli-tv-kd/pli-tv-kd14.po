msgid "Tassa saṃgho amūḷhavinayaṃ deti."
msgstr ""

msgctxt "pli-tv-kd14:6.1.26"
msgid "Adhammikaṃ amūḷhavinayassa dānaṃ."
msgstr ""

msgctxt "pli-tv-kd14:6.1.27"
msgid "Imāni tīṇi adhammikāni amūḷhavinayassa dānāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc53
msgctxt "pli-tv-kd14:6.2.1"
msgid "Katamāni tīṇi dhammikāni amūḷhavinayassa dānāni?"
msgstr ""

#. HTML: </p><p>
#. REF: sc54
