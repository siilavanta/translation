#. HTML: </p><h2>
msgctxt "pli-tv-kd14:7.1.0"
msgid "4. Paṭiññātakaraṇa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc57
msgctxt "pli-tv-kd14:7.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū appaṭiññāya bhikkhūnaṃ kammāni karonti—"
msgstr ""

msgctxt "pli-tv-kd14:7.1.2"
msgid "tajjanīyampi, niyassampi, pabbājanīyampi, paṭisāraṇīyampi, ukkhepanīyampi."
msgstr ""

msgctxt "pli-tv-kd14:7.1.3"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:7.1.5"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd14:7.1.6"
msgid "“kathañhi nāma chabbaggiyā bhikkhū appaṭiññāya bhikkhūnaṃ kammāni karissanti—"
msgstr ""

msgctxt "pli-tv-kd14:7.1.7"
msgid "tajjanīyampi, niyassampi, pabbājanīyampi, paṭisāraṇīyampi, ukkhepanīyampī”ti."
msgstr ""

msgctxt "pli-tv-kd14:7.1.8"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:7.1.10"
msgid "“saccaṃ kira, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd14:7.1.12"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:7.1.14"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:7.1.16"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd14:7.1.17"
msgid "“na, bhikkhave, appaṭiññāya bhikkhūnaṃ kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:7.1.18"
msgid "tajjanīyaṃ vā, niyassaṃ vā, pabbājanīyaṃ vā, paṭisāraṇīyaṃ vā, ukkhepanīyaṃ vā."
msgstr ""

msgctxt "pli-tv-kd14:7.1.19"
msgid "Yo kareyya, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-kd14:8.1.1"
msgid "Evaṃ kho, bhikkhave, adhammikaṃ hoti paṭiññātakaraṇaṃ, evaṃ dhammikaṃ."
msgstr ""

msgctxt "pli-tv-kd14:8.1.2"
msgid "Kathañca, bhikkhave, adhammikaṃ hoti paṭiññātakaraṇaṃ?"
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-kd14:8.1.3"
msgid "Bhikkhu pārājikaṃ ajjhāpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.4"
msgid "Tamenaṃ codeti saṃgho vā, sambahulā vā, ekapuggalo vā—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.5"
msgid "pārājikaṃ āyasmā ajjhāpanno’ti?"
msgstr ""

msgctxt "pli-tv-kd14:8.1.6"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.7"
msgid "na kho ahaṃ, āvuso, pārājikaṃ ajjhāpanno, saṃghādisesaṃ ajjhāpanno’ti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.8"
msgid "Taṃ saṃgho saṃghādisesena kāreti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.9"
msgid "Adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-kd14:8.1.10"
msgid "Bhikkhu pārājikaṃ ajjhāpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.11"
msgid "Tamenaṃ codeti saṃgho vā, sambahulā vā, ekapuggalo vā—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.12"
msgid "pārājikaṃ āyasmā ajjhāpanno’ti?"
msgstr ""

msgctxt "pli-tv-kd14:8.1.13"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.14"
msgid "na kho ahaṃ, āvuso, pārājikaṃ ajjhāpanno, thullaccayaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.16"
msgid "pācittiyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.18"
msgid "pāṭidesanīyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.20"
msgid "dukkaṭaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.22"
msgid "dubbhāsitaṃ ajjhāpanno’ti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.23"
msgid "Taṃ saṃgho dubbhāsitena kāreti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.24"
msgid "Adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc61, pts-vp-pli2.84
msgctxt "pli-tv-kd14:8.1.25"
msgid "Bhikkhu saṃghādisesaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.27"
msgid "thullaccayaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.29"
msgid "pācittiyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.31"
msgid "pāṭidesanīyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.33"
msgid "dukkaṭaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.35"
msgid "dubbhāsitaṃ ajjhāpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.36"
msgid "Tamenaṃ codeti saṃgho vā, sambahulā vā, ekapuggalo vā—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.37"
msgid "dubbhāsitaṃ āyasmā ajjhāpanno’ti?"
msgstr ""

msgctxt "pli-tv-kd14:8.1.38"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.39"
msgid "na kho ahaṃ, āvuso, dubbhāsitaṃ ajjhāpanno, pārājikaṃ ajjhāpanno’ti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.40"
msgid "Taṃ saṃgho pārājikena kāreti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.41"
msgid "Adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-kd14:8.1.42"
msgid "Bhikkhu dubbhāsitaṃ ajjhāpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.43"
msgid "Tamenaṃ codeti saṃgho vā, sambahulā vā, ekapuggalo vā—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.44"
msgid "dubbhāsitaṃ āyasmā ajjhāpanno’ti?"
msgstr ""

msgctxt "pli-tv-kd14:8.1.45"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.1.46"
msgid "na kho ahaṃ, āvuso, dubbhāsitaṃ ajjhāpanno, saṃghādisesaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.48"
msgid "thullaccayaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.50"
msgid "pācittiyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.52"
msgid "pāṭidesanīyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.1.54"
msgid "dukkaṭaṃ ajjhāpanno’ti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.55"
msgid "Taṃ saṃgho dukkaṭena kāreti."
msgstr ""

msgctxt "pli-tv-kd14:8.1.56"
msgid "Adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-kd14:8.1.57"
msgid "Evaṃ kho, bhikkhave, adhammikaṃ hoti paṭiññātakaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc63
msgctxt "pli-tv-kd14:8.2.1"
msgid "Kathañca, bhikkhave, dhammikaṃ hoti paṭiññātakaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:8.2.2"
msgid "Bhikkhu pārājikaṃ ajjhāpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:8.2.3"
msgid "Tamenaṃ codeti saṃgho vā, sambahulā vā, ekapuggalo vā—"
msgstr ""

msgctxt "pli-tv-kd14:8.2.4"
msgid "‘pārājikaṃ āyasmā ajjhāpanno’ti?"
msgstr ""

msgctxt "pli-tv-kd14:8.2.5"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.2.6"
msgid "āma, āvuso, pārājikaṃ ajjhāpanno’ti."
msgstr ""

msgctxt "pli-tv-kd14:8.2.7"
msgid "Taṃ saṃgho pārājikena kāreti."
msgstr ""

msgctxt "pli-tv-kd14:8.2.8"
msgid "Dhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-kd14:8.2.9"
msgid "Bhikkhu saṃghādisesaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.2.11"
msgid "thullaccayaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.2.13"
msgid "pācittiyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.2.15"
msgid "pāṭidesanīyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.2.17"
msgid "dukkaṭaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:8.2.19"
msgid "dubbhāsitaṃ ajjhāpanno hoti."
msgstr ""

msgctxt "pli-tv-kd14:8.2.20"
msgid "Tamenaṃ codeti saṃgho vā, sambahulā vā, ekapuggalo vā—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.2.21"
msgid "dubbhāsitaṃ āyasmā ajjhāpanno’ti?"
msgstr ""

msgctxt "pli-tv-kd14:8.2.22"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd14:8.2.23"
msgid "āma, āvuso, dubbhāsitaṃ ajjhāpanno’ti."
msgstr ""

msgctxt "pli-tv-kd14:8.2.24"
msgid "Taṃ saṃgho dubbhāsitena kāreti."
msgstr ""

msgctxt "pli-tv-kd14:8.2.25"
msgid "Dhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-kd14:8.2.26"
msgid "Evaṃ kho, bhikkhave, dhammikaṃ hoti paṭiññātakaraṇan”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd14:9.1.0"
msgid "5. Yebhuyyasikā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc65
msgctxt "pli-tv-kd14:9.1.1"
msgid "Tena kho pana samayena bhikkhū saṃghamajjhe bhaṇḍanajātā kalahajātā vivādāpannā aññamaññaṃ mukhasattīhi vitudantā viharanti, na sakkonti taṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:9.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd14:9.1.3"
msgid "“Anujānāmi, bhikkhave, evarūpaṃ adhikaraṇaṃ yebhuyyasikāya vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:9.1.4"
msgid "Pañcahaṅgehi samannāgato bhikkhu salākaggāhāpako sammannitabbo—"
msgstr ""

msgctxt "pli-tv-kd14:9.1.5"
msgid "yo na chandāgatiṃ gaccheyya, na dosāgatiṃ gaccheyya, na mohāgatiṃ gaccheyya, na bhayāgatiṃ gaccheyya, gahitāgahitañca jāneyya."
msgstr ""

msgctxt "pli-tv-kd14:9.1.6"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pli-tv-kd14:9.1.7"
msgid "Paṭhamaṃ bhikkhu yācitabbo, yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-kd14:9.1.8"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:9.1.9"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuṃ salākaggāhāpakaṃ sammanneyya."
msgstr ""

msgctxt "pli-tv-kd14:9.1.10"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-kd14:9.1.11"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:9.1.12"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ salākaggāhāpakaṃ sammannati."
msgstr ""

msgctxt "pli-tv-kd14:9.1.13"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno salākaggāhāpakassa sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:9.1.14"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-kd14:9.1.15"
msgid "Sammato saṃghena itthannāmo bhikkhu salākaggāhāpako."
msgstr ""

msgctxt "pli-tv-kd14:9.1.16"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc69, pts-vp-pli2.85
#. VAR: dasa dhammikā → dasa dhammikā salākaggāhā (mr)
msgctxt "pli-tv-kd14:10.1.1"
msgid "Dasayime, bhikkhave, adhammikā salākaggāhā, dasa dhammikā."
msgstr ""

msgctxt "pli-tv-kd14:10.1.2"
msgid "Katame dasa adhammikā salākaggāhā?"
msgstr ""

msgctxt "pli-tv-kd14:10.1.3"
msgid "Oramattakañca adhikaraṇaṃ hoti, na ca gatigataṃ hoti, na ca saritasāritaṃ hoti, jānāti adhammavādī bahutarāti, appeva nāma adhammavādī bahutarā assūti, jānāti saṃgho bhijjissatīti, appeva nāma saṃgho bhijjeyyāti, adhammena gaṇhanti, vaggā gaṇhanti, na ca yathādiṭṭhiyā gaṇhanti—"
msgstr ""

msgctxt "pli-tv-kd14:10.1.4"
msgid "ime dasa adhammikā salākaggāhā."
msgstr ""

#. HTML: </p><p>
#. REF: sc70
msgctxt "pli-tv-kd14:10.2.1"
msgid "Katame dasa dhammikā salākaggāhā?"
msgstr ""

msgctxt "pli-tv-kd14:10.2.2"
msgid "Na ca oramattakaṃ adhikaraṇaṃ hoti, gatigatañca hoti, saritasāritañca hoti, jānāti dhammavādī bahutarāti, appeva nāma dhammavādī bahutarā assūti, jānāti saṃgho na bhijjissatīti, appeva nāma saṃgho na bhijjeyyāti, dhammena gaṇhanti, samaggā gaṇhanti, yathādiṭṭhiyā ca gaṇhanti—"
msgstr ""

msgctxt "pli-tv-kd14:10.2.3"
msgid "ime dasa dhammikā salākaggāhā”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd14:11.1.0"
msgid "6. Tassapāpiyasikā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc71
msgctxt "pli-tv-kd14:11.1.1"
msgid "Tena kho pana samayena upavāḷo bhikkhu saṃghamajjhe āpattiyā anuyuñjiyamāno avajānitvā paṭijānāti, paṭijānitvā avajānāti, aññenaññaṃ paṭicarati, sampajānamusā bhāsati."
msgstr ""

msgctxt "pli-tv-kd14:11.1.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:11.1.4"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd14:11.1.5"
msgid "“kathañhi nāma upavāḷo bhikkhu saṃghamajjhe āpattiyā anuyuñjiyamāno avajānitvā paṭijānissati, paṭijānitvā avajānissati, aññenaññaṃ paṭicarissati, sampajānamusā bhāsissatī”ti."
msgstr ""

msgctxt "pli-tv-kd14:11.1.6"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:11.1.8"
msgid "“saccaṃ kira, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd14:11.1.10"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:11.1.12"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:11.1.14"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd14:11.1.15"
msgid "“tena hi, bhikkhave, saṃgho upavāḷassa bhikkhuno tassapāpiyasikākammaṃ karotu."
msgstr ""

msgctxt "pli-tv-kd14:11.2.1"
msgid "Evañca pana, bhikkhave, kātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:11.2.2"
msgid "Paṭhamaṃ upavāḷo bhikkhu codetabbo, codetvā sāretabbo, sāretvā āpattiṃ āropetabbo, āpattiṃ āropetvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-kd14:11.2.3"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:11.2.4"
msgid "Ayaṃ upavāḷo bhikkhu saṃghamajjhe āpattiyā anuyuñjiyamāno avajānitvā paṭijānāti, paṭijānitvā avajānāti, aññenaññaṃ paṭicarati, sampajānamusā bhāsati."
msgstr ""

msgctxt "pli-tv-kd14:11.2.5"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho upavāḷassa bhikkhuno tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:11.2.6"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-kd14:11.2.7"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:11.2.8"
msgid "Ayaṃ upavāḷo bhikkhu saṃghamajjhe āpattiyā anuyuñjiyamāno avajānitvā paṭijānāti, paṭijānitvā avajānāti, aññenaññaṃ paṭicarati, sampajānamusā bhāsati."
msgstr ""

msgctxt "pli-tv-kd14:11.2.9"
msgid "Saṃgho upavāḷassa bhikkhuno tassapāpiyasikākammaṃ karoti."
msgstr ""

msgctxt "pli-tv-kd14:11.2.10"
msgid "Yassāyasmato khamati upavāḷassa bhikkhuno tassapāpiyasikākammassa karaṇaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:11.2.11"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc74
msgctxt "pli-tv-kd14:11.2.12"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:11.2.14"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-kd14:11.2.15"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc75
msgctxt "pli-tv-kd14:11.2.16"
msgid "Kataṃ saṃghena upavāḷassa bhikkhuno tassapāpiyasikākammaṃ."
msgstr ""

msgctxt "pli-tv-kd14:11.2.17"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc76, pts-vp-pli2.86
msgctxt "pli-tv-kd14:12.1.1"
msgid "Pañcimāni, bhikkhave, dhammikāni tassapāpiyasikākammassa karaṇāni."
msgstr ""

msgctxt "pli-tv-kd14:12.1.2"
msgid "Asuci ca hoti, alajjī ca, sānuvādo ca, tassa saṃgho tassapāpiyasikākammaṃ karoti dhammena, samaggena—"
msgstr ""

msgctxt "pli-tv-kd14:12.1.3"
msgid "imāni kho, bhikkhave, pañca dhammikāni tassapāpiyasikākammassa karaṇāni."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:12.1.4"
msgid "6.1. Adhammakammadvādasaka"
msgstr ""

#. HTML: </h3><p>
#. REF: sc77
msgctxt "pli-tv-kd14:12.2.1"
msgid "Tīhi, bhikkhave, aṅgehi samannāgataṃ tassapāpiyasikākammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca."
msgstr ""

msgctxt "pli-tv-kd14:12.2.2"
msgid "Asammukhā kataṃ hoti, appaṭipucchākataṃ hoti, appaṭiññāya kataṃ hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:12.2.4"
msgid "adhammena kataṃ hoti, vaggena kataṃ hoti—"
msgstr ""

msgctxt "pli-tv-kd14:12.2.5"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgataṃ tassapāpiyasikākammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:12.2.6"
msgid "6.2. Dhammakammadvādasaka"
msgstr ""

#. HTML: </h3><p>
#. REF: sc78
msgctxt "pli-tv-kd14:12.2.7"
msgid "Tīhi, bhikkhave, aṅgehi samannāgataṃ tassapāpiyasikākammaṃ dhammakammañca hoti, vinayakammañca, suvūpasantañca."
msgstr ""

msgctxt "pli-tv-kd14:12.2.8"
msgid "Sammukhā kataṃ hoti, paṭipucchākataṃ hoti, paṭiññāya kataṃ hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:12.2.10"
msgid "dhammena kataṃ hoti, samaggena kataṃ hoti—"
msgstr ""

msgctxt "pli-tv-kd14:12.2.11"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgataṃ tassapāpiyasikākammaṃ dhammakammañca hoti, vinayakammañca, suvūpasantañca."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:12.2.12"
msgid "6.3. Ākaṅkhamānachakka"
msgstr ""

#. HTML: </h3><p>
#. REF: sc79
msgctxt "pli-tv-kd14:12.3.1"
msgid "Tīhi, bhikkhave, aṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.2"
msgid "Bhaṇḍanakārako hoti kalahakārako vivādakārako bhassakārako saṃghe adhikaraṇakārako;"
msgstr ""

msgctxt "pli-tv-kd14:12.3.3"
msgid "bālo hoti abyatto āpattibahulo anapadāno;"
msgstr ""

msgctxt "pli-tv-kd14:12.3.4"
msgid "gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi—"
msgstr ""

msgctxt "pli-tv-kd14:12.3.5"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.6"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc80
msgctxt "pli-tv-kd14:12.3.7"
msgid "Aparehipi, bhikkhave, tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.8"
msgid "Adhisīle sīlavipanno hoti, ajjhācāre ācāravipanno hoti, atidiṭṭhiyā diṭṭhivipanno hoti—"
msgstr ""

msgctxt "pli-tv-kd14:12.3.9"
msgid "imehi kho, bhikkhave …"
msgstr ""

msgctxt "pli-tv-kd14:12.3.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-kd14:12.3.11"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc81
msgctxt "pli-tv-kd14:12.3.12"
msgid "Aparehipi, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd14:12.3.14"
msgid "buddhassa avaṇṇaṃ bhāsati, dhammassa avaṇṇaṃ bhāsati, saṃghassa avaṇṇaṃ bhāsati—"
msgstr ""

msgctxt "pli-tv-kd14:12.3.15"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.16"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc82
msgctxt "pli-tv-kd14:12.3.17"
msgid "Tiṇṇaṃ, bhikkhave, bhikkhūnaṃ ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.18"
msgid "Eko bhaṇḍanakārako hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:12.3.20"
msgid "saṃghe adhikaraṇakārako;"
msgstr ""

msgctxt "pli-tv-kd14:12.3.21"
msgid "eko bālo hoti abyatto āpattibahulo anapadāno;"
msgstr ""

msgctxt "pli-tv-kd14:12.3.22"
msgid "eko gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi—"
msgstr ""

msgctxt "pli-tv-kd14:12.3.23"
msgid "imesaṃ kho, bhikkhave, tiṇṇaṃ bhikkhūnaṃ ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.24"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-kd14:12.3.25"
msgid "Aparesampi, bhikkhave, tiṇṇaṃ bhikkhūnaṃ ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.26"
msgid "Eko adhisīle sīlavipanno hoti, eko ajjhācāre ācāravipanno hoti, eko atidiṭṭhiyā diṭṭhivipanno hoti—"
msgstr ""

msgctxt "pli-tv-kd14:12.3.27"
msgid "imesaṃ kho, bhikkhave …"
msgstr ""

msgctxt "pli-tv-kd14:12.3.28"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-kd14:12.3.29"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc84
msgctxt "pli-tv-kd14:12.3.30"
msgid "Aparesampi, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd14:12.3.32"
msgid "eko buddhassa avaṇṇaṃ bhāsati, eko dhammassa avaṇṇaṃ bhāsati, eko saṃghassa avaṇṇaṃ bhāsati—"
msgstr ""

msgctxt "pli-tv-kd14:12.3.33"
msgid "imesaṃ kho, bhikkhave, tiṇṇaṃ bhikkhūnaṃ ākaṅkhamāno saṃgho tassapāpiyasikākammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd14:12.3.34"
msgid "(6)"
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd14:12.3.35"
msgid "6.4. Aṭṭhārasavatta"
msgstr ""

#. HTML: </h3><p>
#. REF: sc85
msgctxt "pli-tv-kd14:12.4.1"
msgid "Tassapāpiyasikākammakatena, bhikkhave, bhikkhunā sammā vattitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:12.4.2"
msgid "Tatrāyaṃ sammāvattanā—"
msgstr ""

#. HTML: </p><p>
#. REF: sc86
msgctxt "pli-tv-kd14:12.4.3"
msgid "Na upasampādetabbaṃ, na nissayo dātabbo, na sāmaṇero upaṭṭhāpetabbo, na bhikkhunovādakasammuti sāditabbā, sammatenapi bhikkhuniyo na ovaditabbā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:12.4.5"
msgid "na bhikkhūhi sampayojetabban”ti."
msgstr ""

msgctxt "pli-tv-kd14:12.5.1"
msgid "Atha kho saṃgho upavāḷassa bhikkhuno tassapāpiyasikākammaṃ akāsi."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd14:13.1.0"
msgid "7. Tiṇavatthāraka"
msgstr ""

#. HTML: </h2><p>
#. REF: sc87, pts-vp-pli2.87
msgctxt "pli-tv-kd14:13.1.1"
msgid "Tena kho pana samayena bhikkhūnaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ hoti bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.1.2"
msgid "Atha kho tesaṃ bhikkhūnaṃ etadahosi—"
msgstr ""

msgctxt "pli-tv-kd14:13.1.3"
msgid "“amhākaṃ kho bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

#. VAR: kakkhaḷattāya vāḷattāya → kakkhaḷatāya vāḷatāya (s1-3, km) | kakkhaḷattāya vālattāya (pts1)
msgctxt "pli-tv-kd14:13.1.4"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.1.5"
msgid "Kathaṃ nu kho amhehi paṭipajjitabban”ti?"
msgstr ""

msgctxt "pli-tv-kd14:13.1.6"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc88
msgctxt "pli-tv-kd14:13.1.7"
msgid "“Idha pana, bhikkhave, bhikkhūnaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ hoti bhāsitaparikkantaṃ."
msgstr ""

#. VAR: Tatra ce bhikkhūnaṃ → tatra ce bhikkhave bhikkhūnaṃ (s1-3)
msgctxt "pli-tv-kd14:13.1.8"
msgid "Tatra ce bhikkhūnaṃ evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd14:13.1.9"
msgid "amhākaṃ kho bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ;"
msgstr ""

msgctxt "pli-tv-kd14:13.1.10"
msgid "sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyyā’ti, anujānāmi, bhikkhave, evarūpaṃ adhikaraṇaṃ tiṇavatthārakena vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.2.1"
msgid "Evañca pana, bhikkhave, vūpasametabbaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.2.2"
msgid "Sabbeheva ekajjhaṃ sannipatitabbaṃ, sannipatitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc89
msgctxt "pli-tv-kd14:13.2.3"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:13.2.4"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.2.5"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.2.6"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho imaṃ adhikaraṇaṃ tiṇavatthārakena vūpasameyya, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttan’ti."
msgstr ""

msgctxt "pli-tv-kd14:13.3.1"
msgid "Ekatopakkhikānaṃ bhikkhūnaṃ byattena bhikkhunā paṭibalena sako pakkho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc90
msgctxt "pli-tv-kd14:13.3.2"
msgid "‘Suṇantu me āyasmantā."
msgstr ""

msgctxt "pli-tv-kd14:13.3.3"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.4"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.3.5"
msgid "Yadāyasmantānaṃ pattakallaṃ, ahaṃ yā ceva āyasmantānaṃ āpatti, yā ca attano āpatti, āyasmantānañceva atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena deseyyaṃ, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttan’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc91
msgctxt "pli-tv-kd14:13.3.6"
msgid "Athāparesaṃ ekatopakkhikānaṃ bhikkhūnaṃ byattena bhikkhunā paṭibalena sako pakkho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc92
msgctxt "pli-tv-kd14:13.3.7"
msgid "‘Suṇantu me āyasmantā."
msgstr ""

msgctxt "pli-tv-kd14:13.3.8"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.9"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.3.10"
msgid "Yadāyasmantānaṃ pattakallaṃ, ahaṃ yā ceva āyasmantānaṃ āpatti, yā ca attano āpatti, āyasmantānañceva atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena deseyyaṃ, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttan’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc93
msgctxt "pli-tv-kd14:13.3.11"
msgid "Athāparesaṃ ekatopakkhikānaṃ bhikkhūnaṃ byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc94
msgctxt "pli-tv-kd14:13.3.12"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:13.3.13"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.14"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.3.15"
msgid "Yadi saṃghassa pattakallaṃ, ahaṃ yā ceva imesaṃ āyasmantānaṃ āpatti, yā ca attano āpatti, imesañceva āyasmantānaṃ atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena deseyyaṃ, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.16"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc95
msgctxt "pli-tv-kd14:13.3.17"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:13.3.18"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.19"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.3.20"
msgid "Ahaṃ yā ceva imesaṃ āyasmantānaṃ āpatti, yā ca attano āpatti, imesañceva āyasmantānaṃ atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena desemi, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.21"
msgid "Yassāyasmato khamati amhākaṃ imāsaṃ āpattīnaṃ saṃghamajjhe tiṇavatthārakena desanā, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:13.3.22"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc96, pts-vp-pli2.88
msgctxt "pli-tv-kd14:13.3.23"
msgid "Desitā amhākaṃ imā āpattiyo saṃghamajjhe tiṇavatthārakena, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.24"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc97
msgctxt "pli-tv-kd14:13.3.25"
msgid "Athāparesaṃ ekatopakkhikānaṃ bhikkhūnaṃ byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc98
msgctxt "pli-tv-kd14:13.3.26"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:13.3.27"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.28"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.3.29"
msgid "Yadi saṃghassa pattakallaṃ, ahaṃ yā ceva imesaṃ āyasmantānaṃ āpatti, yā ca attano āpatti, imesañceva āyasmantānaṃ atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena deseyyaṃ, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.30"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc99
msgctxt "pli-tv-kd14:13.3.31"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd14:13.3.32"
msgid "Amhākaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ viharataṃ bahuṃ assāmaṇakaṃ ajjhāciṇṇaṃ bhāsitaparikkantaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.33"
msgid "Sace mayaṃ imāhi āpattīhi aññamaññaṃ kāressāma, siyāpi taṃ adhikaraṇaṃ kakkhaḷattāya vāḷattāya bhedāya saṃvatteyya."
msgstr ""

msgctxt "pli-tv-kd14:13.3.34"
msgid "Ahaṃ yā ceva imesaṃ āyasmantānaṃ āpatti, yā ca attano āpatti, imesañceva āyasmantānaṃ atthāya, attano ca atthāya, saṃghamajjhe tiṇavatthārakena desemi, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.35"
msgid "Yassāyasmato khamati amhākaṃ imāsaṃ āpattīnaṃ saṃghamajjhe tiṇavatthārakena desanā, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd14:13.3.36"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc100
msgctxt "pli-tv-kd14:13.3.37"
msgid "Desitā amhākaṃ imā āpattiyo saṃghamajjhe tiṇavatthārakena, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ."
msgstr ""

msgctxt "pli-tv-kd14:13.3.38"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc101
msgctxt "pli-tv-kd14:13.4.1"
msgid "Evañca pana, bhikkhave, te bhikkhū tāhi āpattīhi vuṭṭhitā honti, ṭhapetvā thullavajjaṃ, ṭhapetvā gihippaṭisaṃyuttaṃ, ṭhapetvā diṭṭhāvikammaṃ, ṭhapetvā ye na tattha hontī”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd14:14.1.1"
msgid "8. Adhikaraṇa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc102
#. VAR:  bhikkhūpi bhikkhūhi vivadanti →  etthantare pāṭho sī , syā1-3, kaṃ potthakesu
msgctxt "pli-tv-kd14:14.1.1"
msgid "Tena kho pana samayena bhikkhūpi bhikkhūhi vivadanti, bhikkhūpi bhikkhunīhi vivadanti, bhikkhuniyopi bhikkhūhi vivadanti, channopi bhikkhu bhikkhunīnaṃ anupakhajja bhikkhūhi saddhiṃ vivadati, bhikkhunīnaṃ pakkhaṃ gāheti."
msgstr ""

msgctxt "pli-tv-kd14:14.1.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.1.4"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd14:14.1.5"
msgid "“kathañhi nāma channo bhikkhu bhikkhunīnaṃ anupakhajja bhikkhūhi saddhiṃ vivadissati, bhikkhunīnaṃ pakkhaṃ gāhessatīti."
msgstr ""

msgctxt "pli-tv-kd14:14.1.6"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.1.8"
msgid "saccaṃ kira, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.1.10"
msgid "saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.1.12"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.1.14"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

#. HTML: </p><p>
#. REF: sc103
msgctxt "pli-tv-kd14:14.2.1"
msgid "“Cattārimāni, bhikkhave, adhikaraṇāni—"
msgstr ""

#. VAR: kiccādhikaraṇaṃ → kiccādhikaraṇañca (mr)
msgctxt "pli-tv-kd14:14.2.2"
msgid "vivādādhikaraṇaṃ, anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ, kiccādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc104
msgctxt "pli-tv-kd14:14.2.3"
msgid "Tattha katamaṃ vivādādhikaraṇaṃ?"
msgstr ""

#. VAR: bhikkhave, bhikkhū → bhikkhū bhikkhuṃ (mr)
msgctxt "pli-tv-kd14:14.2.4"
msgid "Idha pana, bhikkhave, bhikkhū vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.2.5"
msgid "dhammoti vā adhammoti vā, vinayoti vā avinayoti vā, bhāsitaṃ lapitaṃ tathāgatenāti vā abhāsitaṃ alapitaṃ tathāgatenāti vā, āciṇṇaṃ tathāgatenāti vā anāciṇṇaṃ tathāgatenāti vā, paññattaṃ tathāgatenāti vā apaññattaṃ tathāgatenāti vā, āpattīti vā anāpattīti vā, lahukā āpattīti vā garukā āpattīti vā, sāvasesā āpattīti vā anavasesā āpattīti vā, duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā?"
msgstr ""

msgctxt "pli-tv-kd14:14.2.6"
msgid "Yaṃ tattha bhaṇḍanaṃ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhagaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.2.7"
msgid "idaṃ vuccati vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.2.8"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc105
msgctxt "pli-tv-kd14:14.2.9"
msgid "Tattha katamaṃ anuvādādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.2.10"
msgid "Idha pana, bhikkhave, bhikkhū bhikkhuṃ anuvadanti sīlavipattiyā vā ācāravipattiyā vā diṭṭhivipattiyā vā ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-kd14:14.2.11"
msgid "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.2.12"
msgid "idaṃ vuccati anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.2.13"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc106, pts-vp-pli2.89
msgctxt "pli-tv-kd14:14.2.14"
msgid "Tattha katamaṃ āpattādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.2.15"
msgid "Pañcapi āpattikkhandhā āpattādhikaraṇaṃ, sattapi āpattikkhandhā āpattādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.2.16"
msgid "idaṃ vuccati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.2.17"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc107
msgctxt "pli-tv-kd14:14.2.18"
msgid "Tattha katamaṃ kiccādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.2.19"
msgid "Yā saṃghassa kiccayatā, karaṇīyatā, apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ—"
msgstr ""

msgctxt "pli-tv-kd14:14.2.20"
msgid "idaṃ vuccati kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.2.21"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc108
msgctxt "pli-tv-kd14:14.3.1"
msgid "Vivādādhikaraṇassa kiṃ mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.3.2"
msgid "Cha vivādamūlāni vivādādhikaraṇassa mūlaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.3.3"
msgid "Tīṇipi akusalamūlāni vivādādhikaraṇassa mūlaṃ, tīṇipi kusalamūlāni vivādādhikaraṇassa mūlaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.3.4"
msgid "Katamāni cha vivādamūlāni vivādādhikaraṇassa mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.3.5"
msgid "Idha pana, bhikkhave, bhikkhu kodhano hoti upanāhī."
msgstr ""

msgctxt "pli-tv-kd14:14.3.6"
msgid "Yo so, bhikkhave, bhikkhu kodhano hoti upanāhī, so sattharipi agāravo viharati appatisso, dhammepi agāravo viharati appatisso, saṃghepi agāravo viharati appatisso, sikkhāyapi na paripūrakārī hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.3.7"
msgid "Yo so, bhikkhave, bhikkhu sattharipi agāravo viharati appatisso, dhammepi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.3.9"
msgid "saṃghepi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.3.11"
msgid "sikkhāyapi na paripūrakārī, so saṃghe vivādaṃ janeti."
msgstr ""

msgctxt "pli-tv-kd14:14.3.12"
msgid "Yo hoti vivādo bahujanāhitāya bahujanāsukhāya bahuno janassa anatthāya ahitāya dukkhāya devamanussānaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.3.13"
msgid "Evarūpañce tumhe, bhikkhave, vivādamūlaṃ ajjhattaṃ vā bahiddhā vā samanupasseyyātha, tatra tumhe, bhikkhave, tasseva pāpakassa vivādamūlassa pahānāya vāyameyyātha."
msgstr ""

msgctxt "pli-tv-kd14:14.3.14"
msgid "Evarūpañce tumhe, bhikkhave, vivādamūlaṃ ajjhattaṃ vā bahiddhā vā na samanupasseyyātha, tatra tumhe, bhikkhave, tasseva pāpakassa vivādamūlassa āyatiṃ anavassavāya paṭipajjeyyātha."
msgstr ""

msgctxt "pli-tv-kd14:14.3.15"
msgid "Evametassa pāpakassa vivādamūlassa pahānaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.3.16"
msgid "Evametassa pāpakassa vivādamūlassa āyatiṃ anavassavo hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc109
msgctxt "pli-tv-kd14:14.3.17"
msgid "Puna caparaṃ, bhikkhave, bhikkhu makkhī hoti paḷāsī … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.3.19"
msgid "issukī hoti maccharī, saṭho hoti māyāvī, pāpiccho hoti micchādiṭṭhī, sandiṭṭhiparāmāsī hoti ādhānaggāhī duppaṭinissaggī."
msgstr ""

msgctxt "pli-tv-kd14:14.3.20"
msgid "Yo so, bhikkhave, bhikkhu sandiṭṭhiparāmāsī hoti ādhānaggāhī duppaṭinissaggī, so sattharipi agāravo viharati appatisso, dhammepi agāravo viharati appatisso, saṃghepi agāravo viharati appatisso, sikkhāyapi na paripūrakārī hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.3.21"
msgid "Yo so, bhikkhave, bhikkhu sattharipi agāravo viharati appatisso, dhammepi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.3.23"
msgid "saṃghepi … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.3.25"
msgid "sikkhāyapi na paripūrakārī, so saṃghe vivādaṃ janeti."
msgstr ""

msgctxt "pli-tv-kd14:14.3.26"
msgid "Yo hoti vivādo bahujanāhitāya bahujanāsukhāya bahuno janassa anatthāya ahitāya dukkhāya devamanussānaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.3.27"
msgid "Evarūpañce tumhe, bhikkhave, vivādamūlaṃ ajjhattaṃ vā bahiddhā vā samanupasseyyātha, tatra tumhe, bhikkhave, tasseva pāpakassa vivādamūlassa pahānāya vāyameyyātha."
msgstr ""

msgctxt "pli-tv-kd14:14.3.28"
msgid "Evarūpañce tumhe, bhikkhave, vivādamūlaṃ ajjhattaṃ vā bahiddhā vā na samanupasseyyātha, tatra tumhe, bhikkhave, tasseva pāpakassa vivādamūlassa āyatiṃ anavassavāya paṭipajjeyyātha."
msgstr ""

msgctxt "pli-tv-kd14:14.3.29"
msgid "Evametassa pāpakassa vivādamūlassa pahānaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.3.30"
msgid "Evametassa pāpakassa vivādamūlassa āyatiṃ anavassavo hoti."
msgstr ""

msgctxt "pli-tv-kd14:14.3.31"
msgid "Imāni cha vivādamūlāni vivādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc110
msgctxt "pli-tv-kd14:14.4.1"
msgid "Katamāni tīṇi akusalamūlāni vivādādhikaraṇassa mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.4.2"
msgid "Idha pana, bhikkhave, bhikkhū luddhacittā vivadanti, duṭṭhacittā vivadanti, mūḷhacittā vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.4.3"
msgid "dhammoti vā adhammoti vā, vinayoti vā avinayoti vā, bhāsitaṃ lapitaṃ tathāgatenāti vā abhāsitaṃ alapitaṃ tathāgatenāti vā, āciṇṇaṃ tathāgatenāti vā anāciṇṇaṃ tathāgatenāti vā, paññattaṃ tathāgatenāti vā apaññattaṃ tathāgatenāti vā, āpattīti vā anāpattīti vā, lahukā āpattīti vā garukā āpattīti vā, sāvasesā āpattīti vā anavasesā āpattīti vā, duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

msgctxt "pli-tv-kd14:14.4.4"
msgid "Imāni tīṇi akusalamūlāni vivādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc111, pts-vp-pli2.90
msgctxt "pli-tv-kd14:14.4.5"
msgid "Katamāni tīṇi kusalamūlāni vivādādhikaraṇassa mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.4.6"
msgid "Idha pana, bhikkhave, bhikkhū aluddhacittā vivadanti, aduṭṭhacittā vivadanti, amūḷhacittā vivadanti—"
msgstr ""

msgctxt "pli-tv-kd14:14.4.7"
msgid "dhammoti vā adhammoti vā … pe …"
msgstr ""

msgctxt "pli-tv-kd14:14.4.9"
msgid "duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

msgctxt "pli-tv-kd14:14.4.10"
msgid "Imāni tīṇi kusalamūlāni vivādādhikaraṇassa mūlaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc112
msgctxt "pli-tv-kd14:14.5.1"
msgid "“Anuvādādhikaraṇassa kiṃ mūlaṃ?"
msgstr ""

msgctxt "pli-tv-kd14:14.5.2"
msgid "Cha anuvādamūlāni anuvādādhikaraṇassa mūlaṃ."
msgstr ""

msgctxt "pli-tv-kd14:14.5.3"
msgid "Tīṇipi akusalamūlāni anuvādādhikaraṇassa mūlaṃ, tīṇipi kusalamūlāni anuvādādhikaraṇassa mūlaṃ, kāyopi anuvādādhikaraṇassa mūlaṃ, vācāpi anuvādādhikaraṇassa mūlaṃ."
msgstr ""

