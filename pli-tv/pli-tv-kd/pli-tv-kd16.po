msgstr ""

msgctxt "pli-tv-kd16:2.3.28"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.3.29"
msgid "“Anujānāmi, bhikkhave, kuḷīrapādakaṃ pīṭhan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc19 
msgctxt "pli-tv-kd16:2.3.30"
msgid "Tena kho pana samayena saṃghassa sosāniko āhaccapādako mañco uppanno hoti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.31"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.3.32"
msgid "“Anujānāmi, bhikkhave, āhaccapādakaṃ mañcan”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.33"
msgid "Āhaccapādakaṃ pīṭhaṃ uppannaṃ hoti."
