msgid "chādanamattenapi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.35"
msgid "bandhanamattenapi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.36"
msgid "bhaṇḍikāṭṭhapanamattenapi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.37"
msgid "khaṇḍaphullapaṭisaṅkharaṇamattenapi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.38"
msgid "paribhaṇḍakaraṇamattenapi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.39"
msgid "vīsativassikampi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.40"
msgid "tiṃsavassikampi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.41"
msgid "yāvajīvikampi …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.42"
msgid "dhūmakālikampi pariyositaṃ vihāraṃ navakammaṃ dassantī”ti."
msgstr ""

msgctxt "pli-tv-kd16:17.1.43"
msgid "Bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.45"
msgid "“saccaṃ kira, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.47"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.49"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.51"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd16:17.1.52"
msgid "“na, bhikkhave, piṇḍanikkhepanamattena navakammaṃ dātabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.1.54"
msgid "na kuṭṭalepanamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.55"
msgid "Na dvāraṭṭhapanamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.56"
msgid "Na aggaḷavaṭṭikaraṇamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.57"
msgid "Na ālokasandhikaraṇamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.58"
msgid "Na setavaṇṇakaraṇamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.59"
msgid "Na kāḷavaṇṇakaraṇamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.60"
msgid "Na gerukaparikammakaraṇamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.61"
msgid "Na chādanamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.62"
msgid "Na bandhanamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.63"
msgid "Na bhaṇḍikāṭṭhapanamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.64"
msgid "Na khaṇḍaphullapaṭisaṅkharaṇamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.65"
msgid "Na paribhaṇḍakaraṇamattena navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.66"
msgid "Na vīsativassikaṃ navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.67"
msgid "Na tiṃsavassikaṃ navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.68"
msgid "Na yāvajīvikaṃ navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.69"
msgid "Na dhūmakālikampi pariyositaṃ vihāraṃ navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.1.70"
msgid "Yo dadeyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd16:17.1.71"
msgid "Anujānāmi, bhikkhave, akataṃ vā vippakataṃ vā navakammaṃ dātuṃ, khuddake vihāre kammaṃ oloketvā chappañcavassikaṃ navakammaṃ dātuṃ, aḍḍhayoge kammaṃ oloketvā sattaṭṭhavassikaṃ navakammaṃ dātuṃ, mahallake vihāre pāsāde vā kammaṃ oloketvā dasadvādasavassikaṃ navakammaṃ dātun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc142 
msgctxt "pli-tv-kd16:17.2.1"
msgid "Tena kho pana samayena bhikkhū sabbe vihāre navakammaṃ denti."
msgstr ""

msgctxt "pli-tv-kd16:17.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.3"
msgid "“Na, bhikkhave, sabbe vihāre navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.4"
msgid "Yo dadeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc143 
msgctxt "pli-tv-kd16:17.2.5"
msgid "Tena kho pana samayena bhikkhū ekassa dve denti."
msgstr ""

msgctxt "pli-tv-kd16:17.2.6"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.7"
msgid "“Na, bhikkhave, ekassa dve dātabbā."
msgstr ""

msgctxt "pli-tv-kd16:17.2.8"
msgid "Yo dadeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc144, pts-vp-pli2.173 
msgctxt "pli-tv-kd16:17.2.9"
msgid "Tena kho pana samayena bhikkhū navakammaṃ gahetvā aññaṃ vāsenti."
msgstr ""

msgctxt "pli-tv-kd16:17.2.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.11"
msgid "“Na, bhikkhave, navakammaṃ gahetvā añño vāsetabbo."
msgstr ""

msgctxt "pli-tv-kd16:17.2.12"
msgid "Yo vāseyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc145 
msgctxt "pli-tv-kd16:17.2.13"
msgid "Tena kho pana samayena bhikkhū navakammaṃ gahetvā saṃghikaṃ paṭibāhenti."
msgstr ""

msgctxt "pli-tv-kd16:17.2.14"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.15"
msgid "“Na, bhikkhave, navakammaṃ gahetvā saṃghikaṃ paṭibāhitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.16"
msgid "Yo paṭibāheyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd16:17.2.17"
msgid "Anujānāmi, bhikkhave, ekaṃ varaseyyaṃ gahetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc146 
msgctxt "pli-tv-kd16:17.2.18"
msgid "Tena kho pana samayena bhikkhū nissīme ṭhitassa navakammaṃ denti."
msgstr ""

msgctxt "pli-tv-kd16:17.2.19"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.20"
msgid "“Na, bhikkhave, nissīme ṭhitassa navakammaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.21"
msgid "Yo dadeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc147 
msgctxt "pli-tv-kd16:17.2.22"
msgid "Tena kho pana samayena bhikkhū navakammaṃ gahetvā sabbakālaṃ paṭibāhanti."
msgstr ""

msgctxt "pli-tv-kd16:17.2.23"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.24"
msgid "“Na, bhikkhave, navakammaṃ gahetvā sabbakālaṃ paṭibāhitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:17.2.25"
msgid "Yo paṭibāheyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd16:17.2.26"
msgid "Anujānāmi, bhikkhave, vassānaṃ temāsaṃ paṭibāhituṃ, utukālaṃ pana na paṭibāhitun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc148 
msgctxt "pli-tv-kd16:17.3.1"
msgid "Tena kho pana samayena bhikkhū navakammaṃ gahetvā pakkamantipi, vibbhamantipi, kālampi karonti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.2"
msgid "sāmaṇerāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.3"
msgid "sikkhaṃ paccakkhātakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.4"
msgid "antimavatthuṃ ajjhāpannakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.5"
msgid "ummattakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.6"
msgid "khittacittāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.7"
msgid "vedanāṭṭāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.8"
msgid "āpattiyā adassane ukkhittakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.9"
msgid "āpattiyā appaṭikamme ukkhittakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.10"
msgid "pāpikāya diṭṭhiyā appaṭinissagge ukkhittakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.11"
msgid "paṇḍakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.12"
msgid "theyyasaṃvāsakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.13"
msgid "titthiyapakkantakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.14"
msgid "tiracchānagatāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.15"
msgid "mātughātakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.16"
msgid "pitughātakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.17"
msgid "arahantaghātakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.18"
msgid "bhikkhunidūsakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.19"
msgid "saṃghabhedakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.20"
msgid "lohituppādakāpi paṭijānanti;"
msgstr ""

msgctxt "pli-tv-kd16:17.3.21"
msgid "ubhatobyañjanakāpi paṭijānanti."
msgstr ""

msgctxt "pli-tv-kd16:17.3.22"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc149 
msgctxt "pli-tv-kd16:17.3.23"
msgid "“Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā pakkamati—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.24"
msgid "mā saṃghassa hāyīti aññassa dātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc150 
msgctxt "pli-tv-kd16:17.3.25"
msgid "Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā vibbhamati … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.3.27"
msgid "kālaṃ karoti, sāmaṇero paṭijānāti, sikkhaṃ paccakkhātako paṭijānāti, antimavatthuṃ ajjhāpannako paṭijānāti, ummattako paṭijānāti, khittacitto paṭijānāti, vedanāṭṭo paṭijānāti, āpattiyā adassane ukkhittako paṭijānāti, āpattiyā appaṭikamme ukkhittako paṭijānāti, pāpikāya diṭṭhiyā appaṭinissagge ukkhittako paṭijānāti, paṇḍako paṭijānāti, theyyasaṃvāsako paṭijānāti, titthiyapakkantako paṭijānāti, tiracchānagato paṭijānāti, mātughātako paṭijānāti, pitughātako paṭijānāti, arahantaghātako paṭijānāti, bhikkhunidūsako paṭijānāti, saṃghabhedako paṭijānāti, lohituppādako paṭijānāti, ubhatobyañjanako paṭijānāti—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.28"
msgid "mā saṃghassa hāyīti aññassa dātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc151 
msgctxt "pli-tv-kd16:17.3.29"
msgid "Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā vippakate pakkamati—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.30"
msgid "mā saṃghassa hāyīti aññassa dātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc152 
msgctxt "pli-tv-kd16:17.3.31"
msgid "Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā vippakate vibbhamati … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.3.33"
msgid "ubhatobyañjanako paṭijānāti—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.34"
msgid "mā saṃghassa hāyīti aññassa dātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc153 
msgctxt "pli-tv-kd16:17.3.35"
msgid "Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā pariyosite pakkamati—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.36"
msgid "tassevetaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc154 
msgctxt "pli-tv-kd16:17.3.37"
msgid "Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā pariyosite vibbhamati … pe …"
msgstr ""

msgctxt "pli-tv-kd16:17.3.39"
msgid "kālaṃ karoti, sāmaṇero paṭijānāti, sikkhaṃ paccakkhātako paṭijānāti, antimavatthuṃ ajjhāpannako paṭijānāti—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.40"
msgid "saṃgho sāmī."
msgstr ""

#. HTML: </p><p>
#. REF: sc155, pts-vp-pli2.174 
msgctxt "pli-tv-kd16:17.3.41"
msgid "Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā pariyosite ummattako paṭijānāti, khittacitto paṭijānāti, vedanāṭṭo paṭijānāti, āpattiyā adassane ukkhittako paṭijānāti, āpattiyā appaṭikamme ukkhittako paṭijānāti, pāpikāya diṭṭhiyā appaṭinissagge ukkhittako paṭijānāti—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.42"
msgid "tassevetaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc156 
msgctxt "pli-tv-kd16:17.3.43"
msgid "Idha pana, bhikkhave, bhikkhu navakammaṃ gahetvā pariyosite paṇḍako paṭijānāti, theyyasaṃvāsako paṭijānāti, titthiyapakkantako paṭijānāti, tiracchānagato paṭijānāti, mātughātako paṭijānāti, pitughātako paṭijānāti, arahantaghātako paṭijānāti, bhikkhunidūsako paṭijānāti, saṃghabhedako paṭijānāti, lohituppādako paṭijānāti, ubhatobyañjanako paṭijānāti—"
msgstr ""

msgctxt "pli-tv-kd16:17.3.44"
msgid "saṃgho sāmī”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd16:18.1.0"
msgid "4. Aññatraparibhogapaṭikkhepādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc157 
msgctxt "pli-tv-kd16:18.1.1"
msgid "Tena kho pana samayena bhikkhū aññatarassa upāsakassa vihāraparibhogaṃ senāsanaṃ aññatra paribhuñjanti."
msgstr ""

msgctxt "pli-tv-kd16:18.1.2"
msgid "Atha kho so upāsako ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-kd16:18.1.3"
msgid "“kathañhi nāma bhadantā aññatra paribhogaṃ aññatra paribhuñjissantī”ti."
msgstr ""

msgctxt "pli-tv-kd16:18.1.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:18.1.5"
msgid "“Na, bhikkhave, aññatra paribhogo aññatra paribhuñjitabbo."
msgstr ""

msgctxt "pli-tv-kd16:18.1.6"
msgid "Yo paribhuñjeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc158 
msgctxt "pli-tv-kd16:18.1.7"
msgid "Tena kho pana samayena bhikkhū uposathaggampi sannisajjampi harituṃ kukkuccāyantā chamāya nisīdanti."
msgstr ""

msgctxt "pli-tv-kd16:18.1.8"
msgid "Gattānipi cīvarānipi paṃsukitāni honti."
msgstr ""

msgctxt "pli-tv-kd16:18.1.9"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:18.1.10"
msgid "“Anujānāmi, bhikkhave, tāvakālikaṃ haritun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc159 
msgctxt "pli-tv-kd16:18.1.11"
msgid "Tena kho pana samayena saṃghassa mahāvihāro undriyati."
msgstr ""

#. VAR: nātiharanti → nābhiharanti (s1-3, mr)
msgctxt "pli-tv-kd16:18.1.12"
msgid "Bhikkhū kukkuccāyantā senāsanaṃ nātiharanti."
msgstr ""

msgctxt "pli-tv-kd16:18.1.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:18.1.14"
msgid "“Anujānāmi, bhikkhave, guttatthāya haritun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc160 
msgctxt "pli-tv-kd16:19.1.1"
msgid "Tena kho pana samayena saṃghassa senāsanaparikkhāriko mahaggho kambalo uppanno hoti."
msgstr ""

msgctxt "pli-tv-kd16:19.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:19.1.3"
msgid "“Anujānāmi, bhikkhave, phātikammatthāya parivattetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc161 
msgctxt "pli-tv-kd16:19.1.4"
msgid "Tena kho pana samayena saṃghassa senāsanaparikkhārikaṃ mahagghaṃ dussaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd16:19.1.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:19.1.6"
msgid "“Anujānāmi, bhikkhave, phātikammatthāya parivattetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc162 
msgctxt "pli-tv-kd16:19.1.7"
msgid "Tena kho pana samayena saṃghassa acchacammaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd16:19.1.8"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:19.1.9"
msgid "“Anujānāmi, bhikkhave, pādapuñchaniṃ kātun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc163 
msgctxt "pli-tv-kd16:19.1.10"
msgid "Tena kho pana samayena saṃghassa cakkalikaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd16:19.1.11"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:19.1.12"
msgid "“Anujānāmi, bhikkhave, pādapuñchaniṃ kātun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc164 
msgctxt "pli-tv-kd16:19.1.13"
msgid "Tena kho pana samayena saṃghassa coḷakaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd16:19.1.14"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:19.1.15"
msgid "“Anujānāmi, bhikkhave, pādapuñchaniṃ kātun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc165 
msgctxt "pli-tv-kd16:20.1.1"
msgid "Tena kho pana samayena bhikkhū adhotehi pādehi senāsanaṃ akkamanti."
msgstr ""

msgctxt "pli-tv-kd16:20.1.2"
msgid "Senāsanaṃ dussati."
msgstr ""

msgctxt "pli-tv-kd16:20.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.1.4"
msgid "“Na, bhikkhave, adhotehi pādehi senāsanaṃ akkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.1.5"
msgid "Yo akkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc166, pts-vp-pli2.175 
msgctxt "pli-tv-kd16:20.2.1"
msgid "Tena kho pana samayena bhikkhū allehi pādehi senāsanaṃ akkamanti."
msgstr ""

msgctxt "pli-tv-kd16:20.2.2"
msgid "Senāsanaṃ dussati."
msgstr ""

msgctxt "pli-tv-kd16:20.2.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.4"
msgid "“Na, bhikkhave, allehi pādehi senāsanaṃ akkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.5"
msgid "Yo akkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc167 
msgctxt "pli-tv-kd16:20.2.6"
msgid "Tena kho pana samayena bhikkhū saupāhanā senāsanaṃ akkamanti."
msgstr ""

msgctxt "pli-tv-kd16:20.2.7"
msgid "Senāsanaṃ dussati."
msgstr ""

msgctxt "pli-tv-kd16:20.2.8"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.9"
msgid "“Na, bhikkhave, saupāhanena senāsanaṃ akkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.10"
msgid "Yo akkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc168 
msgctxt "pli-tv-kd16:20.2.11"
msgid "Tena kho pana samayena bhikkhū parikammakatāya bhūmiyā niṭṭhubhanti."
msgstr ""

msgctxt "pli-tv-kd16:20.2.12"
msgid "Vaṇṇo dussati."
msgstr ""

msgctxt "pli-tv-kd16:20.2.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.14"
msgid "“Na, bhikkhave, parikammakatāya bhūmiyā niṭṭhubhitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.15"
msgid "Yo niṭṭhubheyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd16:20.2.16"
msgid "Anujānāmi, bhikkhave, kheḷamallakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc169 
msgctxt "pli-tv-kd16:20.2.17"
msgid "Tena kho pana samayena mañcapādāpi pīṭhapādāpi parikammakataṃ bhūmiṃ vilikhanti."
msgstr ""

msgctxt "pli-tv-kd16:20.2.18"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.19"
msgid "“Anujānāmi, bhikkhave, coḷakena paliveṭhetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc170 
msgctxt "pli-tv-kd16:20.2.20"
msgid "Tena kho pana samayena bhikkhū parikammakataṃ bhittiṃ apassenti."
msgstr ""

msgctxt "pli-tv-kd16:20.2.21"
msgid "Vaṇṇo dussati."
msgstr ""

msgctxt "pli-tv-kd16:20.2.22"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.23"
msgid "“Na, bhikkhave, parikammakatā bhitti apassetabbā."
msgstr ""

msgctxt "pli-tv-kd16:20.2.24"
msgid "Yo apasseyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd16:20.2.25"
msgid "Anujānāmi, bhikkhave, apassenaphalakan”ti."
msgstr ""

msgctxt "pli-tv-kd16:20.2.26"
msgid "Apassenaphalakaṃ heṭṭhato bhūmiṃ vilikhati, uparito bhittiñca."
msgstr ""

msgctxt "pli-tv-kd16:20.2.27"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.28"
msgid "“Anujānāmi, bhikkhave, heṭṭhato ca uparito ca coḷakena paliveṭhetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc171 
msgctxt "pli-tv-kd16:20.2.29"
msgid "Tena kho pana samayena bhikkhū dhotapādakā nipajjituṃ kukkuccāyanti."
msgstr ""

msgctxt "pli-tv-kd16:20.2.30"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:20.2.31"
msgid "“Anujānāmi, bhikkhave, paccattharitvā nipajjitun”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd16:21.1.0"
msgid "5. Saṃghabhattādianujānana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc172 
msgctxt "pli-tv-kd16:21.1.1"
msgid "Atha kho bhagavā āḷaviyaṃ yathābhirantaṃ viharitvā yena rājagahaṃ tena cārikaṃ pakkāmi."
msgstr ""

msgctxt "pli-tv-kd16:21.1.2"
msgid "Anupubbena cārikaṃ caramāno yena rājagahaṃ tadavasari."
msgstr ""

msgctxt "pli-tv-kd16:21.1.3"
msgid "Tatra sudaṃ bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pli-tv-kd16:21.1.4"
msgid "Tena kho pana samayena rājagahaṃ dubbhikkhaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd16:21.1.5"
msgid "Manussā na sakkonti saṃghabhattaṃ kātuṃ;"
msgstr ""

msgctxt "pli-tv-kd16:21.1.6"
msgid "icchanti uddesabhattaṃ nimantanaṃ salākabhattaṃ pakkhikaṃ uposathikaṃ pāṭipadikaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.1.7"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.1.8"
msgid "“Anujānāmi, bhikkhave, saṃghabhattaṃ uddesabhattaṃ nimantanaṃ salākabhattaṃ pakkhikaṃ uposathikaṃ pāṭipadikan”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd16:21.1.9"
msgid "6. Bhattuddesakasammuti"
msgstr ""

#. HTML: </h2><p>
#. REF: sc173, pts-vp-pli2.176 
msgctxt "pli-tv-kd16:21.1.10"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū attano varabhattāni gahetvā lāmakāni bhattāni bhikkhūnaṃ denti."
msgstr ""

msgctxt "pli-tv-kd16:21.1.11"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.1.12"
msgid "“Anujānāmi, bhikkhave, pañcahaṅgehi samannāgataṃ bhikkhuṃ bhattuddesakaṃ sammannituṃ—"
msgstr ""

msgctxt "pli-tv-kd16:21.1.13"
msgid "yo na chandāgatiṃ gaccheyya, na dosāgatiṃ gaccheyya, na mohāgatiṃ gaccheyya, na bhayāgatiṃ gaccheyya, uddiṭṭhānuddiṭṭhañca jāneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.1.14"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pli-tv-kd16:21.1.15"
msgid "Paṭhamaṃ bhikkhu yācitabbo, yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc174 
msgctxt "pli-tv-kd16:21.1.16"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.1.17"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuṃ bhattuddesakaṃ sammanneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.1.18"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc175 
msgctxt "pli-tv-kd16:21.1.19"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.1.20"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ bhattuddesakaṃ sammannati."
msgstr ""

msgctxt "pli-tv-kd16:21.1.21"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno bhattuddesakassa sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd16:21.1.22"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc176 
msgctxt "pli-tv-kd16:21.1.23"
msgid "Sammato saṃghena itthannāmo bhikkhu bhattuddesako."
msgstr ""

msgctxt "pli-tv-kd16:21.1.24"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc177 
msgctxt "pli-tv-kd16:21.1.25"
msgid "Atha kho bhattuddesakānaṃ bhikkhūnaṃ etadahosi—"
msgstr ""

msgctxt "pli-tv-kd16:21.1.26"
msgid "“kathaṃ nu kho bhattaṃ uddisitabban”ti?"
msgstr ""

msgctxt "pli-tv-kd16:21.1.27"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. VAR: paṭṭikāya vā → paṭikāya vā (s1-3)
msgctxt "pli-tv-kd16:21.1.28"
msgid "“Anujānāmi, bhikkhave, salākāya vā paṭṭikāya vā upanibandhitvā opuñjitvā bhattaṃ uddisitun”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd16:21.2.0"
msgid "7. Senāsanapaññāpakādisammuti"
msgstr ""

#. HTML: </h2><p>
#. REF: sc178 
msgctxt "pli-tv-kd16:21.2.1"
msgid "Tena kho pana samayena saṃghassa senāsanapaññāpako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.2.3"
msgid "bhaṇḍāgāriko na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.2.5"
msgid "cīvarappaṭiggāhako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.2.7"
msgid "cīvarabhājako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.2.9"
msgid "yāgubhājako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.2.11"
msgid "phalabhājako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.2.13"
msgid "khajjakabhājako na hoti."
msgstr ""

msgctxt "pli-tv-kd16:21.2.14"
msgid "Khajjakaṃ abhājiyamānaṃ nassati."
msgstr ""

msgctxt "pli-tv-kd16:21.2.15"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.2.16"
msgid "“Anujānāmi, bhikkhave, pañcahaṅgehi samannāgataṃ bhikkhuṃ khajjakabhājakaṃ sammannituṃ—"
msgstr ""

msgctxt "pli-tv-kd16:21.2.17"
msgid "yo na chandāgatiṃ gaccheyya, na dosāgatiṃ gaccheyya, na mohāgatiṃ gaccheyya, na bhayāgatiṃ gaccheyya, bhājitābhājitañca jāneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.2.18"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pli-tv-kd16:21.2.19"
msgid "Paṭhamaṃ bhikkhu yācitabbo, yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc179 
msgctxt "pli-tv-kd16:21.2.20"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.2.21"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuṃ khajjakabhājakaṃ sammanneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.2.22"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc180 
msgctxt "pli-tv-kd16:21.2.23"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.2.24"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ khajjakabhājakaṃ sammannati."
msgstr ""

msgctxt "pli-tv-kd16:21.2.25"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno khajjakabhājakassa sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd16:21.2.26"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc181 
msgctxt "pli-tv-kd16:21.2.27"
msgid "Sammato saṃghena itthannāmo bhikkhu khajjakabhājako."
msgstr ""

msgctxt "pli-tv-kd16:21.2.28"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd16:21.3.0"
msgid "8. Appamattakavissajjakasammuti"
msgstr ""

#. HTML: </h2><p>
#. VAR: uppanno → ussanno (s1-3)
#. REF: sc182, pts-vp-pli2.177
msgctxt "pli-tv-kd16:21.3.1"
msgid "Tena kho pana samayena saṃghassa bhaṇḍāgāre appamattako parikkhāro uppanno hoti."
msgstr ""

msgctxt "pli-tv-kd16:21.3.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.3.3"
msgid "“Anujānāmi, bhikkhave, pañcahaṅgehi samannāgataṃ bhikkhuṃ appamattakavissajjakaṃ sammannituṃ—"
msgstr ""

msgctxt "pli-tv-kd16:21.3.4"
msgid "yo na chandāgatiṃ gaccheyya, na dosāgatiṃ gaccheyya, na mohāgatiṃ gaccheyya, na bhayāgatiṃ gaccheyya, vissajjitāvissajjitañca jāneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.3.5"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pli-tv-kd16:21.3.6"
msgid "Paṭhamaṃ bhikkhu yācitabbo, yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc183 
msgctxt "pli-tv-kd16:21.3.7"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.3.8"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuṃ appamattakavissajjakaṃ sammanneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.3.9"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc184 
msgctxt "pli-tv-kd16:21.3.10"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.3.11"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ appamattakavissajjakaṃ sammannati."
msgstr ""

msgctxt "pli-tv-kd16:21.3.12"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno appamattakavissajjakassa sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd16:21.3.13"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc185 
msgctxt "pli-tv-kd16:21.3.14"
msgid "Sammato saṃghena itthannāmo bhikkhu appamattakavissajjako."
msgstr ""

msgctxt "pli-tv-kd16:21.3.15"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. VAR: ekā → ekekā (bj, pts1)
#. REF: sc186
msgctxt "pli-tv-kd16:21.3.16"
msgid "Tena appamattakavissajjakena bhikkhunā ekā sūci dātabbā, satthakaṃ dātabbaṃ, upāhanā dātabbā, kāyabandhanaṃ dātabbaṃ, aṃsabaddhako dātabbo, parissāvanaṃ dātabbaṃ, dhammakaraṇo dātabbo, kusi dātabbā, aḍḍhakusi dātabbā, maṇḍalaṃ dātabbaṃ, aḍḍhamaṇḍalaṃ dātabbaṃ, anuvāto dātabbo, paribhaṇḍaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.3.17"
msgid "Sace hoti saṃghassa sappi vā telaṃ vā madhu vā phāṇitaṃ vā, sakiṃ paṭisāyituṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.3.18"
msgid "Sace punapi attho hoti, punapi dātabbaṃ."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd16:21.3.19"
msgid "9. Sāṭiyaggāhāpakādisammuti"
msgstr ""

#. HTML: </h2><p>
#. REF: sc187 
msgctxt "pli-tv-kd16:21.3.20"
msgid "Tena kho pana samayena saṃghassa sāṭiyaggāhāpako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.3.22"
msgid "pattaggāhāpako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.3.24"
msgid "ārāmikapesako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd16:21.3.26"
msgid "sāmaṇerapesako na hoti."
msgstr ""

msgctxt "pli-tv-kd16:21.3.27"
msgid "Sāmaṇerā apesiyamānā kammaṃ na karonti."
msgstr ""

msgctxt "pli-tv-kd16:21.3.28"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:21.3.29"
msgid "“Anujānāmi, bhikkhave, pañcahaṅgehi samannāgataṃ bhikkhuṃ sāmaṇerapesakaṃ sammannituṃ—"
msgstr ""

msgctxt "pli-tv-kd16:21.3.30"
msgid "yo na chandāgatiṃ gaccheyya, na dosāgatiṃ gaccheyya, na mohāgatiṃ gaccheyya, na bhayāgatiṃ gaccheyya, pesitāpesitañca jāneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.3.31"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pli-tv-kd16:21.3.32"
msgid "Paṭhamaṃ bhikkhu yācitabbo, yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc188 
msgctxt "pli-tv-kd16:21.3.33"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.3.34"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuṃ sāmaṇerapesakaṃ sammanneyya."
msgstr ""

msgctxt "pli-tv-kd16:21.3.35"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc189 
msgctxt "pli-tv-kd16:21.3.36"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd16:21.3.37"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ sāmaṇerapesakaṃ sammannati."
msgstr ""

msgctxt "pli-tv-kd16:21.3.38"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno sāmaṇerapesakassa sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd16:21.3.39"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc190 
msgctxt "pli-tv-kd16:21.3.40"
msgid "Sammato saṃghena itthannāmo bhikkhu sāmaṇerapesako."
msgstr ""

msgctxt "pli-tv-kd16:21.3.41"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc191 
msgctxt "pli-tv-kd16:21.3.42"
msgid "Tatiyabhāṇavāro niṭṭhito."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:21.3.43"
msgid "Senāsanakkhandhako chaṭṭho."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc192 
msgctxt "pli-tv-kd16:22.1.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc193 
msgctxt "pli-tv-kd16:22.1.2"
msgid "Vihāraṃ buddhaseṭṭhena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.3"
msgid "apaññattaṃ tadā ahu;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.4"
msgid "Tahaṃ tahaṃ nikkhamanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.5"
msgid "vāsā te jinasāvakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc194 
msgctxt "pli-tv-kd16:22.1.6"
msgid "Seṭṭhī gahapati disvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.7"
msgid "bhikkhūnaṃ idamabravi;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.8"
msgid "Kārāpeyyaṃ vaseyyātha,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.9"
msgid "paṭipucchiṃsu nāyakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc195 
msgctxt "pli-tv-kd16:22.1.10"
msgid "Vihāraṃ aḍḍhayogañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.11"
msgid "pāsādaṃ hammiyaṃ guhaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.12"
msgid "Pañcaleṇaṃ anuññāsi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.13"
msgid "vihāre seṭṭhi kārayi."
msgstr ""

#. HTML: </p><p>
#. REF: sc196 
msgctxt "pli-tv-kd16:22.1.14"
msgid "Jano vihāraṃ kāreti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.15"
msgid "akavāṭaṃ asaṃvutaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.16"
msgid "Kavāṭaṃ piṭṭhasaṅghāṭaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.17"
msgid "udukkhalañca uttari."
msgstr ""

#. HTML: </p><p>
#. REF: sc197, pts-vp-pli2.178 
msgctxt "pli-tv-kd16:22.1.18"
msgid "Āviñchanacchiddaṃ rajjuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.19"
msgid "vaṭṭiñca kapisīsakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.20"
msgid "Sūcighaṭitāḷacchiddaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.21"
msgid "lohakaṭṭhavisāṇakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc198 
msgctxt "pli-tv-kd16:22.1.22"
msgid "Yantakaṃ sūcikañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.23"
msgid "chadanaṃ ullittāvalittaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.24"
msgid "Vedijālasalākañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.25"
msgid "cakkali santharena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc199 
msgctxt "pli-tv-kd16:22.1.26"
msgid "Miḍḍhi bidalamañcañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.27"
msgid "sosānikamasārako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.28"
msgid "Bundikuḷirapādañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.29"
msgid "āhaccāsandi uccake."
msgstr ""

#. HTML: </p><p>
#. REF: sc200 
msgctxt "pli-tv-kd16:22.1.30"
msgid "Sattaṅgo ca bhaddapīṭhaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.31"
msgid "pīṭhakeḷakapādakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.32"
msgid "Āmalāphalakā kocchā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.33"
msgid "palālapīṭhameva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc201 
msgctxt "pli-tv-kd16:22.1.34"
msgid "Uccāhipaṭipādakā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.35"
msgid "aṭṭhaṅguli ca pādakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.36"
msgid "Suttaṃ aṭṭhapadaṃ coḷaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.37"
msgid "tūlikaṃ aḍḍhakāyikaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc202 
msgctxt "pli-tv-kd16:22.1.38"
msgid "Giraggo bhisiyo cāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.39"
msgid "dussaṃ senāsanampi ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.40"
msgid "Onaddhaṃ heṭṭhā patati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.41"
msgid "uppāṭetvā haranti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc203 
msgctxt "pli-tv-kd16:22.1.42"
msgid "Bhattiñca hatthabhattiñca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.43"
msgid "anuññāsi tathāgato;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.44"
msgid "Titthiyā vihāre cāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.45"
msgid "thusaṃ saṇhañca mattikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc204 
msgctxt "pli-tv-kd16:22.1.46"
msgid "Ikkāsaṃ pāṇikaṃ kuṇḍaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.47"
msgid "sāsapaṃ sitthatelakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.48"
msgid "Ussanne paccuddharituṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.49"
msgid "pharusaṃ gaṇḍumattikaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc205 
msgctxt "pli-tv-kd16:22.1.50"
msgid "Ikkāsaṃ paṭibhānañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.51"
msgid "nīcā cayo ca āruhaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.52"
msgid "Paripatanti āḷakā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.53"
msgid "aḍḍhakuṭṭaṃ tayo puna."
msgstr ""

#. HTML: </p><p>
#. REF: sc206 
msgctxt "pli-tv-kd16:22.1.54"
msgid "Khuddake kuṭṭapādo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.55"
msgid "ovassati saraṃ khilaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.56"
msgid "Cīvaravaṃsaṃ rajjuñca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.57"
msgid "āḷindaṃ kiṭikena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc207 
msgctxt "pli-tv-kd16:22.1.58"
msgid "Ālambanaṃ tiṇacuṇṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.59"
msgid "heṭṭhāmagge nayaṃ kare;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.60"
msgid "Ajjhokāse otappati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.61"
msgid "sālaṃ heṭṭhā ca bhājanaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc208 
msgctxt "pli-tv-kd16:22.1.62"
msgid "Vihāro koṭṭhako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.63"
msgid "pariveṇaggisālakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.64"
msgid "Ārāme ca puna koṭṭhe,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.65"
msgid "heṭṭhaññeva nayaṃ kare."
msgstr ""

#. HTML: </p><p>
#. REF: sc209 
msgctxt "pli-tv-kd16:22.1.66"
msgid "Sudhaṃ anāthapiṇḍi ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.67"
msgid "saddho sītavanaṃ agā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.68"
msgid "Diṭṭhadhammo nimantesi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.69"
msgid "saha saṃghena nāyakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc210 
msgctxt "pli-tv-kd16:22.1.70"
msgid "Āṇāpesantarāmagge,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.71"
msgid "ārāmaṃ kārayī gaṇo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.72"
msgid "Vesāliyaṃ navakammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.73"
msgid "purato ca pariggahi."
msgstr ""

#. HTML: </p><p>
#. REF: sc211 
msgctxt "pli-tv-kd16:22.1.74"
msgid "Ko arahati bhattagge,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.75"
msgid "tittirañca avandiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.76"
msgid "Pariggahitantaragharā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.77"
msgid "tūlo sāvatthi osari."
msgstr ""

#. HTML: </p><p>
#. REF: sc212 
msgctxt "pli-tv-kd16:22.1.78"
msgid "Patiṭṭhāpesi ārāmaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.79"
msgid "bhattagge ca kolāhalaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.80"
msgid "Gilānā varaseyyā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.81"
msgid "lesā sattarasā tahiṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc213 
msgctxt "pli-tv-kd16:22.1.82"
msgid "Kena nu kho kathaṃ nu kho,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.83"
msgid "vihāraggena bhājayi;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.84"
msgid "Pariveṇaṃ anubhāgañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.85"
msgid "akāmā bhāgaṃ no dade."
msgstr ""

#. HTML: </p><p>
#. REF: sc214 
msgctxt "pli-tv-kd16:22.1.86"
msgid "Nissīmaṃ sabbakālañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.87"
msgid "gāhā senāsane tayo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.88"
msgid "Upanando ca vaṇṇesi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.89"
msgid "ṭhitakā samakāsanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc215 
msgctxt "pli-tv-kd16:22.1.90"
msgid "Samānāsanikā bhindiṃsu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.91"
msgid "tivaggā ca duvaggikaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.92"
msgid "Asamānāsanikā dīghaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.93"
msgid "sāḷindaṃ paribhuñjituṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc216, pts-vp-pli2.179 
msgctxt "pli-tv-kd16:22.1.94"
msgid "Ayyikā ca avidūre,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.95"
msgid "bhājitañca kīṭāgire;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.96"
msgid "Āḷavī piṇḍakakuṭṭehi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.97"
msgid "dvāraaggaḷavaṭṭikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc217 
msgctxt "pli-tv-kd16:22.1.98"
msgid "Ālokasetakāḷañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.99"
msgid "geruchādanabandhanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.100"
msgid "Bhaṇḍikhaṇḍaparibhaṇḍaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.101"
msgid "vīsa tiṃsā ca kālikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc218 
msgctxt "pli-tv-kd16:22.1.102"
msgid "Osite akataṃ vippaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.103"
msgid "khudde chappañcavassikaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.104"
msgid "Aḍḍhayoge ca sattaṭṭha,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.105"
msgid "mahalle dasa dvādasa."
msgstr ""

#. HTML: </p><p>
#. REF: sc219 
msgctxt "pli-tv-kd16:22.1.106"
msgid "Sabbaṃ vihāraṃ ekassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.107"
msgid "aññaṃ vāsenti saṃghikaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.108"
msgid "Nissīmaṃ sabbakālañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.109"
msgid "pakkami vibbhamanti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc220 
msgctxt "pli-tv-kd16:22.1.110"
msgid "Kālañca sāmaṇerañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.111"
msgid "sikkhāpaccakkhaantimaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.112"
msgid "Ummattakhittacittā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.113"
msgid "vedanāpattidassanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc221 
msgctxt "pli-tv-kd16:22.1.114"
msgid "Appaṭikammadiṭṭhiyā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.115"
msgid "paṇḍakā theyyatitthiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.116"
msgid "Tiracchānamātupitu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.117"
msgid "arahantā ca dūsakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc222 
msgctxt "pli-tv-kd16:22.1.118"
msgid "Bhedakā lohituppādā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.119"
msgid "ubhato cāpi byañjanakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.120"
msgid "Mā saṃghassa parihāyi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.121"
msgid "kammaṃ aññassa dātave."
msgstr ""

#. HTML: </p><p>
#. REF: sc223 
msgctxt "pli-tv-kd16:22.1.122"
msgid "Vippakate ca aññassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.123"
msgid "kate tasseva pakkame;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.124"
msgid "Vibbhamati kālaṅkato,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.125"
msgid "sāmaṇero ca jāyati."
msgstr ""

#. HTML: </p><p>
#. REF: sc224 
msgctxt "pli-tv-kd16:22.1.126"
msgid "Paccakkhāto ca sikkhāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.127"
msgid "antimajjhāpannako yadi;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.128"
msgid "Saṃghova sāmiko hoti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.129"
msgid "ummattakhittavedanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc225 
msgctxt "pli-tv-kd16:22.1.130"
msgid "Adassanāppaṭikamme,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.131"
msgid "diṭṭhi tasseva hoti taṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.132"
msgid "Paṇḍako theyyatitthī ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.133"
msgid "tiracchānamātupettikaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc226 
msgctxt "pli-tv-kd16:22.1.134"
msgid "Ghātako dūsako cāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.135"
msgid "bhedalohitabyañjanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.136"
msgid "Paṭijānāti yadi so,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.137"
msgid "saṃghova hoti sāmiko."
msgstr ""

#. HTML: </p><p>
#. REF: sc227 
msgctxt "pli-tv-kd16:22.1.138"
msgid "Harantaññatra kukkuccaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.139"
msgid "undriyati ca kambalaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.140"
msgid "Dussañca cammacakkalī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.141"
msgid "coḷakaṃ akkamanti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc228 
msgctxt "pli-tv-kd16:22.1.142"
msgid "Allā upāhanāniṭṭhu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.143"
msgid "likhanti apassenti ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.144"
msgid "Apassenaṃ likhateva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.145"
msgid "dhotapaccattharena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc229 
msgctxt "pli-tv-kd16:22.1.146"
msgid "Rājagahe na sakkonti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.147"
msgid "lāmakaṃ bhattuddesakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.148"
msgid "Kathaṃ nu kho paññāpakaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.149"
msgid "bhaṇḍāgārikasammuti."
msgstr ""

#. HTML: </p><p>
#. REF: sc230 
msgctxt "pli-tv-kd16:22.1.150"
msgid "Paṭiggāhabhājako cāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.151"
msgid "yāgu ca phalabhājako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.152"
msgid "Khajjakabhājako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.153"
msgid "appamattakavissajje."
msgstr ""

#. HTML: </p><p>
#. REF: sc231 
msgctxt "pli-tv-kd16:22.1.154"
msgid "Sāṭiyaggāhāpako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.155"
msgid "tatheva pattaggāhako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.156"
msgid "Ārāmikasāmaṇera,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:22.1.157"
msgid "pesakassa ca sammuti."
msgstr ""

#. HTML: </p><p>
