#. HTML: </p><h2>
msgctxt "pli-tv-kd16:16.1.0"
msgid "2. Avebhaṅgiyavatthu"
msgstr ""

#. HTML: </h2><p>
#. REF: sc132, pts-vp-pli2.171 
msgctxt "pli-tv-kd16:16.1.1"
msgid "Atha kho bhagavā sāvatthiyaṃ yathābhirantaṃ viharitvā yena kīṭāgiri tena cārikaṃ pakkāmi mahatā bhikkhusaṃghena saddhiṃ pañcamattehi bhikkhusatehi sāriputtamoggallānehi ca."
msgstr ""

msgctxt "pli-tv-kd16:16.1.2"
msgid "Assosuṃ kho assajipunabbasukā bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd16:16.1.3"
msgid "“bhagavā kira kīṭāgiriṃ āgacchati mahatā bhikkhusaṃghena saddhiṃ pañcamattehi bhikkhusatehi sāriputtamoggallānehi ca."
msgstr ""

msgctxt "pli-tv-kd16:16.1.4"
msgid "Handa mayaṃ, āvuso, sabbaṃ saṃghikaṃ senāsanaṃ bhājema."
msgstr ""

msgctxt "pli-tv-kd16:16.1.5"
msgid "Pāpicchā sāriputtamoggallānā, pāpikānaṃ icchānaṃ vasaṃ gatā;"
msgstr ""

msgctxt "pli-tv-kd16:16.1.6"
msgid "na mayaṃ tesaṃ senāsanaṃ paññapessāmā”ti, te sabbaṃ saṃghikaṃ senāsanaṃ bhājesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc133 
msgctxt "pli-tv-kd16:16.1.7"
msgid "Atha kho bhagavā anupubbena cārikaṃ caramāno yena kīṭāgiri tadavasari."
msgstr ""

msgctxt "pli-tv-kd16:16.1.8"
msgid "Atha kho bhagavā sambahule bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd16:16.1.9"
msgid "“gacchatha tumhe, bhikkhave;"
msgstr ""

msgctxt "pli-tv-kd16:16.1.10"
msgid "assajipunabbasuke bhikkhū upasaṅkamitvā evaṃ vadetha—‘"
msgstr ""

msgctxt "pli-tv-kd16:16.1.11"
msgid "bhagavā, āvuso, āgacchati mahatā bhikkhusaṃghena saddhiṃ pañcamattehi bhikkhusatehi sāriputtamoggallānehi ca."
msgstr ""

msgctxt "pli-tv-kd16:16.1.12"
msgid "Bhagavato ca, āvuso, senāsanaṃ paññapetha, bhikkhusaṃghassa ca, sāriputtamoggallānānañcā’”ti."
msgstr ""

msgctxt "pli-tv-kd16:16.1.13"
msgid "“Evaṃ, bhante”ti kho te bhikkhū bhagavato paṭissutvā yena assajipunabbasukā bhikkhū tenupasaṅkamiṃsu, upasaṅkamitvā assajipunabbasuke bhikkhū etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd16:16.1.14"
msgid "“bhagavā, āvuso, āgacchati mahatā bhikkhusaṃghena saddhiṃ pañcamattehi bhikkhusatehi sāriputtamoggallānehi ca."
msgstr ""

msgctxt "pli-tv-kd16:16.1.15"
msgid "Bhagavato ca, āvuso, senāsanaṃ paññapetha, bhikkhusaṃghassa ca, sāriputtamoggallānānañcā”ti."
msgstr ""

msgctxt "pli-tv-kd16:16.1.16"
msgid "“Natthāvuso, saṃghikaṃ senāsanaṃ."
msgstr ""

msgctxt "pli-tv-kd16:16.1.17"
msgid "Sabbaṃ amhehi bhājitaṃ."
msgstr ""

msgctxt "pli-tv-kd16:16.1.18"
msgid "Svāgataṃ, āvuso, bhagavato."
msgstr ""

msgctxt "pli-tv-kd16:16.1.19"
msgid "Yasmiṃ vihāre bhagavā icchissati tasmiṃ vihāre vasissati."
msgstr ""

msgctxt "pli-tv-kd16:16.1.20"
msgid "Pāpicchā sāriputtamoggallānā, pāpikānaṃ icchānaṃ vasaṃ gatā."
msgstr ""

msgctxt "pli-tv-kd16:16.1.21"
msgid "Na mayaṃ tesaṃ senāsanaṃ paññapessāmā”ti."
msgstr ""

msgctxt "pli-tv-kd16:16.2.1"
msgid "“Kiṃ pana tumhe, āvuso, saṃghikaṃ senāsanaṃ bhājitthā”ti?"
msgstr ""

msgctxt "pli-tv-kd16:16.2.2"
msgid "“Evamāvuso”ti."
msgstr ""

