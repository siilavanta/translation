#. HTML: <section class="sutta" id="pli-tv-kd16"><article><div class="hgroup"><p class="collection">
msgctxt "pli-tv-kd16:0.1"
msgid "Theravāda Vinayapiṭaka"
msgstr ""

#. HTML: </p><p class="division">
msgctxt "pli-tv-kd16:0.2"
msgid "Cūḷavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-kd16:0.3"
msgid "Senāsanakkhandhaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-kd16:1.1.0"
msgid "1. Vihārānujānana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli2.146 
msgctxt "pli-tv-kd16:1.1.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pli-tv-kd16:1.1.2"
msgid "Tena kho pana samayena bhagavatā bhikkhūnaṃ senāsanaṃ apaññattaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd16:1.1.3"
msgid "Te ca bhikkhū tahaṃ tahaṃ viharanti—"
msgstr ""

msgctxt "pli-tv-kd16:1.1.4"
msgid "araññe, rukkhamūle, pabbate, kandarāyaṃ, giriguhāyaṃ, susāne, vanapatthe, ajjhokāse, palālapuñje."
msgstr ""

msgctxt "pli-tv-kd16:1.1.5"
msgid "Te kālasseva tato tato upanikkhamanti—"
msgstr ""

msgctxt "pli-tv-kd16:1.1.6"
msgid "araññā rukkhamūlā pabbatā kandarā giriguhā susānā vanapatthā ajjhokāsā palālapuñjā, pāsādikena abhikkantena paṭikkantena, ālokitena vilokitena, samiñjitena pasāritena, okkhittacakkhū, iriyāpathasampannā."
msgstr ""

#. VAR: seṭṭhī → seṭṭhi (pts1, mr)
msgctxt "pli-tv-kd16:1.2.1"
msgid "Tena kho pana samayena rājagahako seṭṭhī kālasseva uyyānaṃ agamāsi."
msgstr ""

msgctxt "pli-tv-kd16:1.2.2"
msgid "Addasā kho rājagahako seṭṭhī te bhikkhū kālasseva tato tato upanikkhamante—"
msgstr ""

msgctxt "pli-tv-kd16:1.2.3"
msgid "araññā rukkhamūlā pabbatā kandarā giriguhā susānā vanapatthā ajjhokāsā palālapuñjā, pāsādikena abhikkantena paṭikkantena, ālokitena vilokitena, samiñjitena pasāritena, okkhittacakkhū, iriyāpathasampanne."
msgstr ""

msgctxt "pli-tv-kd16:1.2.4"
msgid "Disvānassa cittaṃ pasīdi."
msgstr ""

msgctxt "pli-tv-kd16:1.2.5"
msgid "Atha kho rājagahako seṭṭhī yena te bhikkhū tenupasaṅkami, upasaṅkamitvā te bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd16:1.2.6"
msgid "“sacāhaṃ, bhante, vihāre kārāpeyyaṃ, vaseyyātha me vihāresū”ti?"
msgstr ""

msgctxt "pli-tv-kd16:1.2.7"
msgid "“Na kho, gahapati, bhagavatā vihārā anuññātā”ti."
msgstr ""

msgctxt "pli-tv-kd16:1.2.8"
msgid "“Tena hi, bhante, bhagavantaṃ paṭipucchitvā mama āroceyyāthā”ti."
msgstr ""

msgctxt "pli-tv-kd16:1.2.9"
msgid "“Evaṃ, gahapatī”ti kho te bhikkhū rājagahakassa seṭṭhissa paṭissutvā yena bhagavā tenupasaṅkamiṃsu, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdiṃsu."
msgstr ""

msgctxt "pli-tv-kd16:1.2.10"
msgid "Ekamantaṃ nisinnā kho te bhikkhū bhagavantaṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd16:1.2.11"
msgid "“rājagahako, bhante, seṭṭhī vihāre kārāpetukāmo."
msgstr ""

#. VAR: bhante, amhehi → bhante (bj, pts1, mr)
msgctxt "pli-tv-kd16:1.2.12"
msgid "Kathaṃ nu kho, bhante, amhehi paṭipajjitabban”ti?"
msgstr ""

msgctxt "pli-tv-kd16:1.2.13"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—“"
msgstr ""

#. VAR: pañca leṇāni → pañca senāsanāni (s1-3) | pañca lenāni (pts1)
msgctxt "pli-tv-kd16:1.2.14"
msgid "anujānāmi, bhikkhave, pañca leṇāni—"
msgstr ""

msgctxt "pli-tv-kd16:1.2.15"
msgid "vihāraṃ, aḍḍhayogaṃ, pāsādaṃ, hammiyaṃ, guhan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli2.147 
msgctxt "pli-tv-kd16:1.3.1"
msgid "Atha kho te bhikkhū yena rājagahako seṭṭhī tenupasaṅkamiṃsu, upasaṅkamitvā rājagahakaṃ seṭṭhiṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd16:1.3.2"
msgid "“anuññātā kho, gahapati, bhagavatā vihārā;"
msgstr ""

msgctxt "pli-tv-kd16:1.3.3"
msgid "yassadāni kālaṃ maññasī”ti."
msgstr ""

msgctxt "pli-tv-kd16:1.3.4"
msgid "Atha kho rājagahako seṭṭhī ekāheneva saṭṭhivihāre patiṭṭhāpesi."
msgstr ""

msgctxt "pli-tv-kd16:1.3.5"
msgid "Atha kho rājagahako seṭṭhī te saṭṭhivihāre pariyosāpetvā yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd16:1.3.6"
msgid "Ekamantaṃ nisinno kho rājagahako seṭṭhī bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd16:1.3.7"
msgid "“adhivāsetu me, bhante, bhagavā svātanāya bhattaṃ saddhiṃ bhikkhusaṃghenā”ti."
msgstr ""

msgctxt "pli-tv-kd16:1.3.8"
msgid "Adhivāsesi bhagavā tuṇhībhāvena."
msgstr ""

msgctxt "pli-tv-kd16:1.3.9"
msgid "Atha kho rājagahako seṭṭhī bhagavato adhivāsanaṃ viditvā uṭṭhāyāsanā bhagavantaṃ abhivādetvā padakkhiṇaṃ katvā pakkāmi."
msgstr ""

#. HTML: </p><p>
#. REF: sc3 
msgctxt "pli-tv-kd16:1.4.1"
msgid "Atha kho rājagahako seṭṭhī tassā rattiyā accayena paṇītaṃ khādanīyaṃ bhojanīyaṃ paṭiyādāpetvā bhagavato kālaṃ ārocāpesi—"
msgstr ""

msgctxt "pli-tv-kd16:1.4.2"
msgid "“kālo, bhante, niṭṭhitaṃ bhattan”ti."
msgstr ""

msgctxt "pli-tv-kd16:1.4.3"
msgid "Atha kho bhagavā pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya yena rājagahakassa seṭṭhissa nivesanaṃ tenupasaṅkami, upasaṅkamitvā paññatte āsane nisīdi saddhiṃ bhikkhusaṃghena."
msgstr ""

msgctxt "pli-tv-kd16:1.4.4"
msgid "Atha kho rājagahako seṭṭhī buddhappamukhaṃ bhikkhusaṃghaṃ paṇītena khādanīyena bhojanīyena sahatthā santappetvā sampavāretvā, bhagavantaṃ bhuttāviṃ onītapattapāṇiṃ, ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd16:1.4.5"
msgid "Ekamantaṃ nisinno kho rājagahako seṭṭhī bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd16:1.4.6"
msgid "“ete me, bhante, saṭṭhivihārā puññatthikena saggatthikena kārāpitā."
msgstr ""

msgctxt "pli-tv-kd16:1.4.7"
msgid "Kathāhaṃ, bhante, tesu vihāresu paṭipajjāmī”ti?"
msgstr ""

msgctxt "pli-tv-kd16:1.4.8"
msgid "“Tena hi tvaṃ, gahapati, te saṭṭhivihāre āgatānāgatassa cātuddisassa saṃghassa patiṭṭhāpehī”ti."
msgstr ""

msgctxt "pli-tv-kd16:1.4.9"
msgid "“Evaṃ, bhante”ti kho rājagahako seṭṭhī bhagavato paṭissutvā te saṭṭhivihāre āgatānāgatassa cātuddisassa saṃghassa patiṭṭhāpesi."
msgstr ""

#. HTML: </p><p>
#. REF: sc4 
msgctxt "pli-tv-kd16:1.5.1"
msgid "Atha kho bhagavā rājagahakaṃ seṭṭhiṃ imāhi gāthāhi anumodi—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. VAR: paṭihanti → paṭihanati (mr)
#. REF: sc5
msgctxt "pli-tv-kd16:1.5.2"
msgid "“Sītaṃ uṇhaṃ paṭihanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.3"
msgid "tato vāḷamigāni ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.4"
msgid "Sarīsape ca makase,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.5"
msgid "sisire cāpi vuṭṭhiyo."
msgstr ""

#. HTML: </p><p>
#. REF: sc6 
msgctxt "pli-tv-kd16:1.5.6"
msgid "Tato vātātapo ghoro,"
msgstr ""

#. HTML: <br>
#. VAR: sañjāto → sañjāte (mr saddanīti)
msgctxt "pli-tv-kd16:1.5.7"
msgid "sañjāto paṭihaññati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.8"
msgid "Leṇatthañca sukhatthañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.9"
msgid "jhāyituñca vipassituṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc7 
msgctxt "pli-tv-kd16:1.5.10"
msgid "Vihāradānaṃ saṃghassa,"
msgstr ""

#. HTML: <br>
#. VAR: buddhena → buddhehi (s1-3)
msgctxt "pli-tv-kd16:1.5.11"
msgid "Aggaṃ buddhena vaṇṇitaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.12"
msgid "Tasmā hi paṇḍito poso,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.13"
msgid "Sampassaṃ atthamattano."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli2.148 
msgctxt "pli-tv-kd16:1.5.14"
msgid "Vihāre kāraye ramme,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.15"
msgid "vāsayettha bahussute;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.16"
msgid "Tesaṃ annañca pānañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.17"
msgid "vatthasenāsanāni ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc9 
msgctxt "pli-tv-kd16:1.5.18"
msgid "Dadeyya ujubhūtesu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.19"
msgid "Vippasannena cetasā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.20"
msgid "Te tassa dhammaṃ desenti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.21"
msgid "Sabbadukkhāpanūdanaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.22"
msgid "Yaṃ so dhammaṃ idhaññāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd16:1.5.23"
msgid "Parinibbāti anāsavo”ti."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc10 
msgctxt "pli-tv-kd16:1.5.24"
msgid "Atha kho bhagavā rājagahakaṃ seṭṭhiṃ imāhi gāthāhi anumoditvā uṭṭhāyāsanā pakkāmi."
msgstr ""

#. HTML: </p><p>
#. REF: sc11 
msgctxt "pli-tv-kd16:2.1.1"
msgid "Assosuṃ kho manussā—“"
msgstr ""

#. VAR: anuññātā”ti sakkaccaṃ → te sakkaccaṃ (s1-3, km)
msgctxt "pli-tv-kd16:2.1.2"
msgid "bhagavatā kira vihārā anuññātā”ti sakkaccaṃ vihāre kārāpenti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.3"
msgid "Te vihārā akavāṭakā honti;"
msgstr ""

msgctxt "pli-tv-kd16:2.1.4"
msgid "ahīpi vicchikāpi satapadiyopi pavisanti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.6"
msgid "“Anujānāmi, bhikkhave, kavāṭan”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.7"
msgid "Bhittichiddaṃ karitvā valliyāpi rajjuyāpi kavāṭaṃ bandhanti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.8"
msgid "Undūrehipi upacikāhipi khajjanti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.9"
msgid "Khayitabandhanāni kavāṭāni patanti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.11"
msgid "“Anujānāmi, bhikkhave, piṭṭhasaṅghāṭaṃ udukkhalikaṃ uttarapāsakan”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.12"
msgid "Kavāṭā na phusīyanti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.14"
msgid "“Anujānāmi, bhikkhave, āviñchanacchiddaṃ āviñchanarajjun”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.15"
msgid "Kavāṭā na thakiyanti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.16"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.17"
msgid "“Anujānāmi, bhikkhave, aggaḷavaṭṭiṃ kapisīsakaṃ sūcikaṃ ghaṭikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc12 
msgctxt "pli-tv-kd16:2.1.18"
msgid "Tena kho pana samayena bhikkhū na sakkonti kavāṭaṃ apāpurituṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.19"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.20"
msgid "“Anujānāmi, bhikkhave, tāḷacchiddaṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.21"
msgid "Tīṇi tāḷāni—"
msgstr ""

msgctxt "pli-tv-kd16:2.1.22"
msgid "lohatāḷaṃ, kaṭṭhatāḷaṃ, visāṇatāḷan”ti."
msgstr ""

#. VAR: ugghāṭetvā pavisanti → yehi te ugghāṭetvā pavisantīti (mr)
#. VAR: Yehi → yepi (bj, s1-3, pts1)
msgctxt "pli-tv-kd16:2.1.23"
msgid "Yehi te ugghāṭetvā pavisanti, vihārā aguttā honti."
msgstr ""

msgctxt "pli-tv-kd16:2.1.24"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.1.25"
msgid "“Anujānāmi, bhikkhave, yantakaṃ sūcikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13 
msgctxt "pli-tv-kd16:2.2.1"
msgid "Tena kho pana samayena vihārā tiṇacchadanā honti;"
msgstr ""

msgctxt "pli-tv-kd16:2.2.2"
msgid "sītakāle sītā, uṇhakāle uṇhā."
msgstr ""

msgctxt "pli-tv-kd16:2.2.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.2.4"
msgid "“Anujānāmi, bhikkhave, ogumphetvā ullittāvalittaṃ kātun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14 
msgctxt "pli-tv-kd16:2.2.5"
msgid "Tena kho pana samayena vihārā avātapānakā honti acakkhussā duggandhā."
msgstr ""

msgctxt "pli-tv-kd16:2.2.6"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.2.7"
msgid "“Anujānāmi, bhikkhave, tīṇi vātapānāni—"
msgstr ""

msgctxt "pli-tv-kd16:2.2.8"
msgid "vedikāvātapānaṃ, jālavātapānaṃ, salākavātapānan”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.2.9"
msgid "Vātapānantarikāya kāḷakāpi vagguliyopi pavisanti."
msgstr ""

msgctxt "pli-tv-kd16:2.2.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.2.11"
msgid "“Anujānāmi, bhikkhave, vātapānacakkalikan”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.2.12"
msgid "Cakkalikantarikāyapi kāḷakāpi vagguliyopi pavisanti."
msgstr ""

msgctxt "pli-tv-kd16:2.2.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.2.14"
msgid "“Anujānāmi, bhikkhave, vātapānakavāṭakaṃ vātapānabhisikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc15, pts-vp-pli2.149 
msgctxt "pli-tv-kd16:2.3.1"
msgid "Tena kho pana samayena bhikkhū chamāyaṃ sayanti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.2"
msgid "Gattānipi cīvarānipi paṃsukitāni honti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.3.4"
msgid "“Anujānāmi, bhikkhave, tiṇasanthārakan”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.5"
msgid "Tiṇasanthārako undūrehipi upacikāhipi khajjati."
msgstr ""

msgctxt "pli-tv-kd16:2.3.6"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. VAR: miḍḍhin”ti → mīḍhanti (bj) | miḍhinti (s1-3, pts1)
msgctxt "pli-tv-kd16:2.3.7"
msgid "“Anujānāmi, bhikkhave, miḍḍhin”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.8"
msgid "Miḍḍhiyā gattāni dukkhā honti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.9"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.3.10"
msgid "“Anujānāmi, bhikkhave, bidalamañcakan”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd16:2.3.11"
msgid "2. Mañcapīṭhādianujānana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc16 
msgctxt "pli-tv-kd16:2.3.12"
msgid "Tena kho pana samayena saṃghassa sosāniko masārako mañco uppanno hoti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.3.14"
msgid "“Anujānāmi, bhikkhave, masārakaṃ mañcan”ti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.15"
msgid "Masārakaṃ pīṭhaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd16:2.3.16"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd16:2.3.17"
