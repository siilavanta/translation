msgstr ""

msgctxt "pli-tv-kd16:17.1.10"
msgid "setavaṇṇakaraṇamattenapi navakammaṃ denti;"
msgstr ""

msgctxt "pli-tv-kd16:17.1.11"
msgid "kāḷavaṇṇakaraṇamattenapi navakammaṃ denti;"
msgstr ""

msgctxt "pli-tv-kd16:17.1.12"
msgid "gerukaparikammakaraṇamattenapi navakammaṃ denti;"
msgstr ""

msgctxt "pli-tv-kd16:17.1.13"
