msgctxt "pli-tv-kd13:3.2.3"
msgid "ahaṃ, bhante, ekaṃ āpattiṃ āpajjiṃ sañcetanikaṃ sukkavissaṭṭhiṃ ekāhappaṭicchannaṃ."
msgstr ""

msgctxt "pli-tv-kd13:3.2.4"
msgid "Sohaṃ, bhante, saṃghaṃ ekissā āpattiyā sañcetanikāya sukkavissaṭṭhiyā ekāhappaṭicchannāya ekāhaparivāsaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd13:3.2.5"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd13:3.2.6"
msgid "Tatiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd13:3.3.1"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
