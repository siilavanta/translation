#. HTML: </p><h3>
msgctxt "pli-tv-kd13:26.1.0"
msgid "2.5. Suddhantaparivāsa"
msgstr ""

#. HTML: </h3><p>
#. REF: sc160, pts-vp-pli2.59
msgctxt "pli-tv-kd13:26.1.1"
msgid "Tena kho pana samayena aññataro bhikkhu sambahulā saṃghādisesā āpattiyo āpanno hoti."
msgstr ""

msgctxt "pli-tv-kd13:26.1.2"
msgid "So āpattipariyantaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.1.3"
msgid "rattipariyantaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.1.4"
msgid "āpattipariyantaṃ nassarati, rattipariyantaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.1.5"
msgid "āpattipariyante vematiko, rattipariyante vematiko."
msgstr ""

msgctxt "pli-tv-kd13:26.1.6"
msgid "So bhikkhūnaṃ ārocesi—"
msgstr ""

msgctxt "pli-tv-kd13:26.1.7"
msgid "“ahaṃ, āvuso, sambahulā saṃghādisesā āpattiyo āpajjiṃ;"
msgstr ""

msgctxt "pli-tv-kd13:26.1.8"
msgid "āpattipariyantaṃ na jānāmi, rattipariyantaṃ na jānāmi;"
msgstr ""

msgctxt "pli-tv-kd13:26.1.9"
msgid "āpattipariyantaṃ nassarāmi, rattipariyantaṃ nassarāmi;"
msgstr ""

msgctxt "pli-tv-kd13:26.1.10"
msgid "āpattipariyante vematiko, rattipariyante vematiko."
msgstr ""

msgctxt "pli-tv-kd13:26.1.11"
msgid "Kathaṃ nu kho mayā paṭipajjitabban”ti?"
msgstr ""

msgctxt "pli-tv-kd13:26.1.12"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd13:26.1.13"
msgid "“Tena hi, bhikkhave, saṃgho tassa bhikkhuno tāsaṃ āpattīnaṃ suddhantaparivāsaṃ detu."
msgstr ""

msgctxt "pli-tv-kd13:26.2.1"
msgid "Evañca pana, bhikkhave, dātabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc161
msgctxt "pli-tv-kd13:26.2.2"
msgid "Tena, bhikkhave, bhikkhunā saṃghaṃ upasaṅkamitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd13:26.2.4"
msgid "evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-kd13:26.2.5"
msgid "‘ahaṃ, bhante, sambahulā saṃghādisesā āpattiyo āpajjiṃ."
msgstr ""

msgctxt "pli-tv-kd13:26.2.6"
msgid "Āpattipariyantaṃ na jānāmi, rattipariyantaṃ na jānāmi;"
msgstr ""

msgctxt "pli-tv-kd13:26.2.7"
msgid "āpattipariyantaṃ nassarāmi, rattipariyantaṃ nassarāmi;"
msgstr ""

msgctxt "pli-tv-kd13:26.2.8"
msgid "āpattipariyante vematiko, rattipariyante vematiko."
msgstr ""

msgctxt "pli-tv-kd13:26.2.9"
msgid "Sohaṃ, bhante, saṃghaṃ tāsaṃ āpattīnaṃ suddhantaparivāsaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd13:26.2.10"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd13:26.2.11"
msgid "Tatiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd13:26.2.12"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc162
msgctxt "pli-tv-kd13:26.2.13"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd13:26.2.14"
msgid "Ayaṃ itthannāmo bhikkhu sambahulā saṃghādisesā āpattiyo āpajji."
msgstr ""

msgctxt "pli-tv-kd13:26.2.15"
msgid "Āpattipariyantaṃ na jānāti, rattipariyantaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.2.16"
msgid "āpattipariyantaṃ nassarati, rattipariyantaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.2.17"
msgid "āpattipariyante vematiko, rattipariyante vematiko."
msgstr ""

msgctxt "pli-tv-kd13:26.2.18"
msgid "So saṃghaṃ tāsaṃ āpattīnaṃ suddhantaparivāsaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd13:26.2.19"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmassa bhikkhuno tāsaṃ āpattīnaṃ suddhantaparivāsaṃ dadeyya."
msgstr ""

msgctxt "pli-tv-kd13:26.2.20"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc163
msgctxt "pli-tv-kd13:26.2.21"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd13:26.2.22"
msgid "Ayaṃ itthannāmo bhikkhu sambahulā saṃghādisesā āpattiyo āpajji."
msgstr ""

msgctxt "pli-tv-kd13:26.2.23"
msgid "Āpattipariyantaṃ na jānāti, rattipariyantaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.2.24"
msgid "āpattipariyantaṃ nassarati, rattipariyantaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.2.25"
msgid "āpattipariyante vematiko, rattipariyante vematiko."
msgstr ""

msgctxt "pli-tv-kd13:26.2.26"
msgid "So saṃghaṃ tāsaṃ āpattīnaṃ suddhantaparivāsaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd13:26.2.27"
msgid "Saṃgho itthannāmassa bhikkhuno tāsaṃ āpattīnaṃ suddhantaparivāsaṃ deti."
msgstr ""

msgctxt "pli-tv-kd13:26.2.28"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno tāsaṃ āpattīnaṃ suddhantaparivāsassa dānaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd13:26.2.29"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc164
msgctxt "pli-tv-kd13:26.2.30"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-kd13:26.2.32"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-kd13:26.2.33"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc165
msgctxt "pli-tv-kd13:26.2.34"
msgid "Dinno saṃghena itthannāmassa bhikkhuno tāsaṃ āpattīnaṃ suddhantaparivāso."
msgstr ""

msgctxt "pli-tv-kd13:26.2.35"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc166
msgctxt "pli-tv-kd13:26.3.1"
msgid "Evaṃ kho, bhikkhave, suddhantaparivāso dātabbo;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.2"
msgid "evaṃ parivāso dātabbo."
msgstr ""

msgctxt "pli-tv-kd13:26.3.3"
msgid "Kathañca, bhikkhave, suddhantaparivāso dātabbo?"
msgstr ""

msgctxt "pli-tv-kd13:26.3.4"
msgid "Āpattipariyantaṃ na jānāti, rattipariyantaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.5"
msgid "āpattipariyantaṃ nassarati, rattipariyantaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.6"
msgid "āpattipariyante vematiko, rattipariyante vematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.3.7"
msgid "suddhantaparivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc167
msgctxt "pli-tv-kd13:26.3.8"
msgid "Āpattipariyantaṃ jānāti, rattipariyantaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.9"
msgid "āpattipariyantaṃ sarati, rattipariyantaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.10"
msgid "āpattipariyante nibbematiko, rattipariyante vematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.3.11"
msgid "suddhantaparivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc168
msgctxt "pli-tv-kd13:26.3.12"
msgid "Āpattipariyantaṃ ekaccaṃ jānāti, ekaccaṃ na jānāti, rattipariyantaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.13"
msgid "āpattipariyantaṃ ekaccaṃ sarati, ekaccaṃ nassarati, rattipariyantaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.14"
msgid "āpattipariyante ekacce vematiko, ekacce nibbematiko, rattipariyante vematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.3.15"
msgid "suddhantaparivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc169
msgctxt "pli-tv-kd13:26.3.16"
msgid "Āpattipariyantaṃ na jānāti, rattipariyantaṃ ekaccaṃ jānāti, ekaccaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.17"
msgid "āpattipariyantaṃ nassarati, rattipariyantaṃ ekaccaṃ sarati, ekaccaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.18"
msgid "āpattipariyante vematiko, rattipariyante ekacce vematiko, ekacce nibbematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.3.19"
msgid "suddhantaparivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc170, pts-vp-pli2.60
msgctxt "pli-tv-kd13:26.3.20"
msgid "Āpattipariyantaṃ jānāti, rattipariyantaṃ ekaccaṃ jānāti, ekaccaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.21"
msgid "āpattipariyantaṃ sarati, rattipariyantaṃ ekaccaṃ sarati, ekaccaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.22"
msgid "āpattipariyante nibbematiko, rattipariyante ekacce vematiko, ekacce nibbematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.3.23"
msgid "suddhantaparivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc171
msgctxt "pli-tv-kd13:26.3.24"
msgid "Āpattipariyantaṃ ekaccaṃ jānāti, ekaccaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.25"
msgid "rattipariyantaṃ ekaccaṃ jānāti, ekaccaṃ na jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.26"
msgid "āpattipariyantaṃ ekaccaṃ sarati, ekaccaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.27"
msgid "rattipariyantaṃ ekaccaṃ sarati, ekaccaṃ nassarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.28"
msgid "āpattipariyante ekacce vematiko, ekacce nibbematiko;"
msgstr ""

msgctxt "pli-tv-kd13:26.3.29"
msgid "rattipariyante ekacce vematiko, ekacce nibbematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.3.30"
msgid "suddhantaparivāso dātabbo."
msgstr ""

msgctxt "pli-tv-kd13:26.3.31"
msgid "Evaṃ kho, bhikkhave, suddhantaparivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc172
msgctxt "pli-tv-kd13:26.4.1"
msgid "Kathañca, bhikkhave, parivāso dātabbo?"
msgstr ""

msgctxt "pli-tv-kd13:26.4.2"
msgid "Āpattipariyantaṃ jānāti, rattipariyantaṃ jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.4.3"
msgid "āpattipariyantaṃ sarati, rattipariyantaṃ sarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.4.4"
msgid "āpattipariyante nibbematiko, rattipariyante nibbematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.4.5"
msgid "parivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc173
msgctxt "pli-tv-kd13:26.4.6"
msgid "Āpattipariyantaṃ na jānāti, rattipariyantaṃ jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.4.7"
msgid "āpattipariyantaṃ nassarati, rattipariyantaṃ sarati, āpattipariyante vematiko, rattipariyante nibbematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.4.8"
msgid "parivāso dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc174
msgctxt "pli-tv-kd13:26.4.9"
msgid "Āpattipariyantaṃ ekaccaṃ jānāti, ekaccaṃ na jānāti, rattipariyantaṃ jānāti;"
msgstr ""

msgctxt "pli-tv-kd13:26.4.10"
msgid "āpattipariyantaṃ ekaccaṃ sarati, ekaccaṃ nassarati, rattipariyantaṃ sarati;"
msgstr ""

msgctxt "pli-tv-kd13:26.4.11"
msgid "āpattipariyante ekacce vematiko, ekacce nibbematiko, rattipariyante nibbematiko—"
msgstr ""

msgctxt "pli-tv-kd13:26.4.12"
msgid "parivāso dātabbo."
msgstr ""

msgctxt "pli-tv-kd13:26.4.13"
msgid "Evaṃ kho, bhikkhave, parivāso dātabbo”."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc175
msgctxt "pli-tv-kd13:26.4.14"
msgid "Parivāso niṭṭhito."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd13:27.1.0"
msgid "3. Cattālīsaka"
msgstr ""

#. HTML: </h2><p>
#. REF: sc176
msgctxt "pli-tv-kd13:27.1.1"
msgid "Tena kho pana samayena aññataro bhikkhu parivasanto vibbhami."
msgstr ""

msgctxt "pli-tv-kd13:27.1.2"
msgid "So puna paccāgantvā bhikkhū upasampadaṃ yāci."
msgstr ""

msgctxt "pli-tv-kd13:27.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc177
msgctxt "pli-tv-kd13:27.1.4"
msgid "“Idha pana, bhikkhave, bhikkhu parivasanto vibbhamati."
msgstr ""

msgctxt "pli-tv-kd13:27.1.5"
msgid "Vibbhantakassa, bhikkhave, parivāso na ruhati."
msgstr ""

msgctxt "pli-tv-kd13:27.1.6"
msgid "So ce puna upasampajjati, tassa tadeva purimaṃ parivāsadānaṃ."
msgstr ""

msgctxt "pli-tv-kd13:27.1.7"
msgid "Yo parivāso dinno sudinno, yo parivuttho suparivuttho, avaseso parivasitabbo."
msgstr ""

msgctxt "pli-tv-kd13:27.1.8"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
