#. HTML: </p><h3>
msgctxt "pli-tv-kd13:3.1.0"
msgid "1.3. Ekāhappaṭicchannaparivāsa"
msgstr ""

#. HTML: </h3><p>
#. REF: sc17
msgctxt "pli-tv-kd13:3.1.1"
msgid "Tena kho pana samayena āyasmā udāyī ekaṃ āpattiṃ āpanno hoti sañcetanikaṃ sukkavissaṭṭhiṃ ekāhappaṭicchannaṃ."
msgstr ""

msgctxt "pli-tv-kd13:3.1.2"
msgid "So bhikkhūnaṃ ārocesi—"
msgstr ""

msgctxt "pli-tv-kd13:3.1.3"
msgid "“ahaṃ, āvuso, ekaṃ āpattiṃ āpajjiṃ sañcetanikaṃ sukkavissaṭṭhiṃ ekāhappaṭicchannaṃ."
msgstr ""

msgctxt "pli-tv-kd13:3.1.4"
msgid "Kathaṃ nu kho mayā paṭipajjitabban”ti?"
msgstr ""

msgctxt "pli-tv-kd13:3.1.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd13:3.1.6"
msgid "“Tena hi, bhikkhave, saṃgho udāyissa bhikkhuno ekissā āpattiyā sañcetanikāya sukkavissaṭṭhiyā ekāhappaṭicchannāya ekāhaparivāsaṃ detu."
