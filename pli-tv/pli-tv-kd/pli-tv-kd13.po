#. REF: sc155
msgctxt "pli-tv-kd13:25.3.1"
msgid "Idha pana, bhikkhave, bhikkhu dve saṃghādisesā āpattiyo āpajjati dvemāsappaṭicchannāyo;"
msgstr ""

msgctxt "pli-tv-kd13:25.3.2"
msgid "ekaṃ māsaṃ sarati, ekaṃ māsaṃ nassarati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.3"
msgid "So saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ yaṃ māsaṃ sarati taṃ māsaṃ parivāsaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.4"
msgid "Tassa saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ yaṃ māsaṃ sarati taṃ māsaṃ parivāsaṃ deti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.5"
msgid "So parivasanto itarampi māsaṃ sarati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.6"
msgid "Tassa evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd13:25.3.7"
msgid "ahaṃ kho dve saṃghādisesā āpattiyo āpajjiṃ dvemāsappaṭicchannāyo;"
msgstr ""

msgctxt "pli-tv-kd13:25.3.8"
msgid "ekaṃ māsaṃ sariṃ, ekaṃ māsaṃ nassariṃ."
msgstr ""

msgctxt "pli-tv-kd13:25.3.9"
msgid "Sohaṃ saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ yaṃ māsaṃ sariṃ taṃ māsaṃ parivāsaṃ yāciṃ."
msgstr ""

msgctxt "pli-tv-kd13:25.3.10"
msgid "Tassa me saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ yaṃ māsaṃ sariṃ taṃ māsaṃ parivāsaṃ adāsi."
msgstr ""

msgctxt "pli-tv-kd13:25.3.11"
msgid "Sohaṃ parivasanto itarampi māsaṃ sarāmi."
msgstr ""

msgctxt "pli-tv-kd13:25.3.12"
msgid "Yannūnāhaṃ saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ itarampi māsaṃ parivāsaṃ yāceyyan’ti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.13"
msgid "So saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ itarampi māsaṃ parivāsaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.14"
msgid "Tassa saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ itarampi māsaṃ parivāsaṃ deti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.15"
msgid "Tena, bhikkhave, bhikkhunā purimaṃ upādāya dve māsā parivasitabbā."
msgstr ""

#. HTML: </p><p>
#. REF: sc156, pts-vp-pli2.58
msgctxt "pli-tv-kd13:25.3.16"
msgid "Idha pana, bhikkhave, bhikkhu dve saṃghādisesā āpattiyo āpajjati dvemāsappaṭicchannāyo;"
msgstr ""

msgctxt "pli-tv-kd13:25.3.17"
msgid "ekaṃ māsaṃ nibbematiko, ekaṃ māsaṃ vematiko."
msgstr ""

msgctxt "pli-tv-kd13:25.3.18"
msgid "So saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ yaṃ māsaṃ nibbematiko taṃ māsaṃ parivāsaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.19"
msgid "Tassa saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ yaṃ māsaṃ nibbematiko taṃ māsaṃ parivāsaṃ deti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.20"
msgid "So parivasanto itarampi māsaṃ nibbematiko hoti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.21"
msgid "Tassa evaṃ hoti—‘"
msgstr ""

msgctxt "pli-tv-kd13:25.3.22"
msgid "ahaṃ kho dve saṃghādisesā āpattiyo āpajjiṃ dvemāsappaṭicchannāyo;"
msgstr ""

msgctxt "pli-tv-kd13:25.3.23"
msgid "ekaṃ māsaṃ nibbematiko, ekaṃ māsaṃ vematiko."
msgstr ""

msgctxt "pli-tv-kd13:25.3.24"
msgid "Sohaṃ saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ yaṃ māsaṃ nibbematiko taṃ māsaṃ parivāsaṃ yāciṃ."
msgstr ""

msgctxt "pli-tv-kd13:25.3.25"
msgid "Tassa me saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchānnānaṃ yaṃ māsaṃ nibbematiko taṃ māsaṃ parivāsaṃ adāsi."
msgstr ""

msgctxt "pli-tv-kd13:25.3.26"
msgid "Sohaṃ parivasanto itarampi māsaṃ nibbematiko."
msgstr ""

msgctxt "pli-tv-kd13:25.3.27"
msgid "Yannūnāhaṃ saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ itarampi māsaṃ parivāsaṃ yāceyyan’ti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.28"
msgid "So saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ itarampi māsaṃ parivāsaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.29"
msgid "Tassa saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ itarampi māsaṃ parivāsaṃ deti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.30"
msgid "Tena, bhikkhave, bhikkhunā purimaṃ upādāya dve māsā parivasitabbā."
msgstr ""

#. HTML: </p><p>
#. REF: sc157
msgctxt "pli-tv-kd13:25.3.31"
msgid "Idha pana, bhikkhave, bhikkhu dve saṃghādisesā āpattiyo āpajjati dvemāsappaṭicchannāyo;"
msgstr ""

msgctxt "pli-tv-kd13:25.3.32"
msgid "eko māso jānappaṭicchanno, eko māso ajānappaṭicchanno."
msgstr ""

msgctxt "pli-tv-kd13:25.3.33"
msgid "So saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ dvemāsaparivāsaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.34"
msgid "Tassa saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ dvemāsaparivāsaṃ deti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.35"
msgid "Tassa parivasantassa añño bhikkhu āgacchati bahussuto āgatāgamo dhammadharo vinayadharo mātikādharo paṇḍito viyatto medhāvī lajjī kukkuccako sikkhākāmo."
msgstr ""

msgctxt "pli-tv-kd13:25.3.36"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd13:25.3.37"
msgid "kiṃ ayaṃ, āvuso, bhikkhu āpanno?"
msgstr ""

msgctxt "pli-tv-kd13:25.3.38"
msgid "Kissāyaṃ bhikkhu parivasatī’ti?"
msgstr ""

msgctxt "pli-tv-kd13:25.3.39"
msgid "Te evaṃ vadenti—‘"
msgstr ""

msgctxt "pli-tv-kd13:25.3.40"
msgid "ayaṃ, āvuso, bhikkhu dve saṃghādisesā āpattiyo āpajji dvemāsappaṭicchannāyo."
msgstr ""

msgctxt "pli-tv-kd13:25.3.41"
msgid "Eko māso jānappaṭicchanno, eko māso ajānappaṭicchanno."
msgstr ""

msgctxt "pli-tv-kd13:25.3.42"
msgid "So saṃghaṃ dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ dvemāsaparivāsaṃ yāci."
msgstr ""

msgctxt "pli-tv-kd13:25.3.43"
msgid "Tassa saṃgho dvinnaṃ āpattīnaṃ dvemāsappaṭicchannānaṃ dvemāsaparivāsaṃ adāsi."
msgstr ""

msgctxt "pli-tv-kd13:25.3.44"
msgid "Tāyo ayaṃ, āvuso, bhikkhu āpanno tāsāyaṃ bhikkhu parivasatī’ti."
msgstr ""

msgctxt "pli-tv-kd13:25.3.45"
msgid "So evaṃ vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd13:25.3.46"
msgid "yvāyaṃ, āvuso, māso jānappaṭicchanno dhammikaṃ tassa māsassa parivāsadānaṃ;"
msgstr ""

msgctxt "pli-tv-kd13:25.3.47"
msgid "dhammattā ruhati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.48"
msgid "Yo ca khvāyaṃ, āvuso, māso ajānappaṭicchanno adhammikaṃ tassa māsassa parivāsadānaṃ;"
msgstr ""

msgctxt "pli-tv-kd13:25.3.49"
msgid "adhammattā na ruhati."
msgstr ""

msgctxt "pli-tv-kd13:25.3.50"
msgid "Ekassa, āvuso, māsassa bhikkhu mānattāraho’ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc158
