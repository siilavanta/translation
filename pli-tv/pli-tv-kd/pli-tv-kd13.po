#. HTML: </h2><h3>
msgctxt "pli-tv-kd13:20.1.0"
msgid "2.1. Agghasamodhānaparivāsa"
msgstr ""

#. HTML: </h3><p>
#. REF: sc118
msgctxt "pli-tv-kd13:20.1.1"
msgid "Tena kho pana samayena aññataro bhikkhu sambahulā saṃghādisesā āpattiyo āpanno hoti—"
msgstr ""

msgctxt "pli-tv-kd13:20.1.2"
msgid "ekā āpatti ekāhappaṭicchannā, ekā āpatti dvīhappaṭicchannā, ekā āpatti tīhappaṭicchannā, ekā āpatti catūhappaṭicchannā, ekā āpatti pañcāhappaṭicchannā, ekā āpatti chāhappaṭicchannā, ekā āpatti sattāhappaṭicchannā, ekā āpatti aṭṭhāhappaṭicchannā, ekā āpatti navāhappaṭicchannā, ekā āpatti dasāhappaṭicchannā."
msgstr ""

msgctxt "pli-tv-kd13:20.1.3"
msgid "So bhikkhūnaṃ ārocesi—"
msgstr ""

msgctxt "pli-tv-kd13:20.1.4"
msgid "“ahaṃ, āvuso, sambahulā saṃghādisesā āpattiyo āpajjiṃ—"
msgstr ""

msgctxt "pli-tv-kd13:20.1.5"
msgid "ekā āpatti ekāhappaṭicchannā … pe …"
msgstr ""

msgctxt "pli-tv-kd13:20.1.7"
msgid "ekā āpatti dasāhappaṭicchannā."
msgstr ""

msgctxt "pli-tv-kd13:20.1.8"
msgid "Kathaṃ nu kho mayā paṭipajjitabban”ti?"
msgstr ""

msgctxt "pli-tv-kd13:20.1.9"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd13:20.2.1"
msgid "“Tena hi, bhikkhave, saṃgho tassa bhikkhuno tāsaṃ āpattīnaṃ yā āpatti dasāhappaṭicchannā tassā agghena samodhānaparivāsaṃ detu."
msgstr ""

msgctxt "pli-tv-kd13:20.2.2"
msgid "Evañca pana, bhikkhave, dātabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc119
msgctxt "pli-tv-kd13:20.2.3"
msgid "Tena, bhikkhave, bhikkhunā saṃghaṃ upasaṅkamitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd13:20.2.5"
msgid "evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-kd13:20.2.6"
msgid "‘ahaṃ, bhante, sambahulā saṃghādisesā āpattiyo āpajjiṃ—"
msgstr ""

msgctxt "pli-tv-kd13:20.2.7"
msgid "ekā āpatti ekāhappaṭicchannā … pe …"
msgstr ""

msgctxt "pli-tv-kd13:20.2.9"
msgid "ekā āpatti dasāhappaṭicchannā."
msgstr ""

msgctxt "pli-tv-kd13:20.2.10"
msgid "Sohaṃ, bhante, saṃghaṃ tāsaṃ āpattīnaṃ yā āpatti dasāhappaṭicchannā tassā agghena samodhānaparivāsaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd13:20.2.11"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd13:20.2.12"
msgid "Tatiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd13:20.2.13"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

