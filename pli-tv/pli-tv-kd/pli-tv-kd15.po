#. HTML: <section class="sutta" id="pli-tv-kd15"><article><div class="hgroup"><p class="collection">
msgctxt "pli-tv-kd15:0.1"
msgid "Theravāda Vinayapiṭaka"
msgstr ""

#. HTML: </p><p class="division">
msgctxt "pli-tv-kd15:0.2"
msgid "Cūḷavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-kd15:0.3"
msgid "Khuddakavatthukkhandhaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-kd15:0.4"
msgid "1. Khuddakavatthu"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli2.105
msgctxt "pli-tv-kd15:1.1.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pli-tv-kd15:1.1.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū nahāyamānā rukkhe kāyaṃ ugghaṃsenti, ūrumpi bāhumpi urampi piṭṭhimpi."
msgstr ""

msgctxt "pli-tv-kd15:1.1.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

#. VAR: gāmamoddavā”ti → gāmapoddavāti (bj, pts1) | gāmapūṭavā (s1-3)
msgctxt "pli-tv-kd15:1.1.4"
msgid "“kathañhi nāma samaṇā sakyaputtiyā nahāyamānā rukkhe kāyaṃ ugghaṃsessanti, ūrumpi bāhumpi urampi piṭṭhimpi, seyyathāpi mallamuṭṭhikā gāmamoddavā”ti."
msgstr ""

msgctxt "pli-tv-kd15:1.1.5"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.1.6"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.1.7"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā bhikkhū paṭipucchi—"
msgstr ""

msgctxt "pli-tv-kd15:1.1.8"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhū nahāyamānā rukkhe kāyaṃ ugghaṃsenti, ūrumpi bāhumpi urampi piṭṭhimpī”ti?"
msgstr ""

msgctxt "pli-tv-kd15:1.1.9"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd15:1.1.10"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-kd15:1.1.11"
msgid "“ananucchavikaṃ, bhikkhave, tesaṃ moghapurisānaṃ ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.1.12"
msgid "Kathañhi nāma te, bhikkhave, moghapurisā nahāyamānā rukkhe kāyaṃ ugghaṃsessanti, ūrumpi bāhumpi urampi piṭṭhimpi?"
msgstr ""

msgctxt "pli-tv-kd15:1.1.13"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.1.14"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.1.15"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:1.1.16"
msgid "“na, bhikkhave, nahāyamānena bhikkhunā rukkhe kāyo ugghaṃsetabbo."
msgstr ""

msgctxt "pli-tv-kd15:1.1.17"
msgid "Yo ugghaṃseyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-kd15:1.1.18"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū nahāyamānā thambhe kāyaṃ ugghaṃsenti, ūrumpi bāhumpi urampi piṭṭhimpi."
msgstr ""

msgctxt "pli-tv-kd15:1.1.19"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:1.1.20"
msgid "“kathañhi nāma samaṇā sakyaputtiyā nahāyamānā thambhe kāyaṃ ugghaṃsessanti, ūrumpi bāhumpi urampi piṭṭhimpi, seyyathāpi mallamuṭṭhikā gāmamoddavā”ti."
msgstr ""

msgctxt "pli-tv-kd15:1.1.21"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.1.22"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.1.23"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.1.24"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.1.25"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:1.1.26"
msgid "“na, bhikkhave, nahāyamānena bhikkhunā thambhe kāyo ugghaṃsetabbo."
msgstr ""

msgctxt "pli-tv-kd15:1.1.27"
msgid "Yo ugghaṃseyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
#. VAR: kuṭṭe → kuḍḍe (bj, s1-3, pts1)
msgctxt "pli-tv-kd15:1.1.28"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū nahāyamānā kuṭṭe kāyaṃ ugghaṃsenti, ūrumpi bāhumpi urampi piṭṭhimpi."
msgstr ""

msgctxt "pli-tv-kd15:1.1.29"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:1.1.30"
msgid "“kathañhi nāma samaṇā sakyaputtiyā nahāyamānā kuṭṭe kāyaṃ ugghaṃsessanti, ūrumpi bāhumpi urampi piṭṭhimpi, seyyathāpi mallamuṭṭhikā gāmamoddavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.1.31"
msgid "“na, bhikkhave, nahāyamānena bhikkhunā kuṭṭe kāyo ugghaṃsetabbo."
msgstr ""

msgctxt "pli-tv-kd15:1.1.32"
msgid "Yo ugghaṃseyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4, pts-vp-pli2.106
#. VAR: aṭṭāne → aṭṭhāne (bj, s1-3)
msgctxt "pli-tv-kd15:1.3.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū aṭṭāne nahāyanti."
msgstr ""

msgctxt "pli-tv-kd15:1.3.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.3.3"
msgid "seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:1.3.4"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.3.5"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.3.6"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.3.7"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.3.8"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:1.3.9"
msgid "“na, bhikkhave, aṭṭāne nahāyitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.3.10"
msgid "Yo nahāyeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-kd15:1.3.11"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū gandhabbahatthakena nahāyanti."
msgstr ""

msgctxt "pli-tv-kd15:1.3.12"
msgid "Manussā ujjhāyanti khiyyanti vipācenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.3.13"
msgid "seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:1.3.14"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.3.15"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.3.16"
msgid "“na, bhikkhave, gandhabbahatthakena nahāyitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.3.17"
msgid "Yo nahāyeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-kd15:1.3.18"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū kuruvindakasuttiyā nahāyanti."
msgstr ""

msgctxt "pli-tv-kd15:1.3.19"
msgid "Manussā ujjhāyanti khiyyanti vipācenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.3.20"
msgid "seyyathāpi gihī kāmabhoginoti."
msgstr ""

msgctxt "pli-tv-kd15:1.3.21"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.3.22"
msgid "“Na, bhikkhave, kuruvindakasuttiyā nahāyitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.3.23"
msgid "Yo nahāyeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-kd15:1.4.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū viggayha parikammaṃ kārāpenti."
msgstr ""

msgctxt "pli-tv-kd15:1.4.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.4.3"
msgid "seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:1.4.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.4.5"
msgid "“Na, bhikkhave, viggayha parikammaṃ kārāpetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.4.6"
msgid "Yo kārāpeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-kd15:1.4.7"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū mallakena nahāyanti."
msgstr ""

msgctxt "pli-tv-kd15:1.4.8"
msgid "Manussā ujjhāyanti khiyyanti vipācenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:1.4.9"
msgid "seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:1.4.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.4.11"
msgid "“Na, bhikkhave, mallakena nahāyitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.4.12"
msgid "Yo nahāyeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-kd15:1.4.13"
msgid "Tena kho pana samayena aññatarassa bhikkhuno kacchurogābādho hoti."
msgstr ""

msgctxt "pli-tv-kd15:1.4.14"
msgid "Na tassa vinā mallakena phāsu hoti."
msgstr ""

msgctxt "pli-tv-kd15:1.4.15"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.4.16"
msgid "“Anujānāmi, bhikkhave, gilānassa akatamallakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-kd15:1.5.1"
msgid "Tena kho pana samayena aññataro bhikkhu jarādubbalo nahāyamāno na sakkoti attano kāyaṃ ugghaṃsetuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.5.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.5.3"
msgid "“Anujānāmi, bhikkhave, ukkāsikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-kd15:1.5.4"
msgid "Tena kho pana samayena bhikkhū piṭṭhiparikammaṃ kātuṃ kukkuccāyanti."
msgstr ""

msgctxt "pli-tv-kd15:1.5.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:1.5.6"
msgid "“Anujānāmi, bhikkhave, puthupāṇikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-kd15:2.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū vallikaṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.2"
msgid "pāmaṅgaṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.3"
msgid "kaṇṭhasuttakaṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.4"
msgid "kaṭisuttakaṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.5"
msgid "ovaṭṭikaṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.6"
msgid "kāyuraṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.7"
msgid "hatthābharaṇaṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.8"
msgid "aṅgulimuddikaṃ dhārenti."
msgstr ""

msgctxt "pli-tv-kd15:2.1.9"
msgid "Manussā ujjhāyanti khiyyanti vipācenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.10"
msgid "seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:2.1.11"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.1.12"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.1.13"
msgid "“Saccaṃ kira, bhikkhave, chabbaggiyā bhikkhū vallikaṃ dhārenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.14"
msgid "pāmaṅgaṃ dhārenti, kaṇṭhasuttakaṃ dhārenti, kaṭisuttakaṃ dhārenti, ovaṭṭikaṃ dhārenti, kāyuraṃ dhārenti, hatthābharaṇaṃ dhārenti, aṅgulimuddikaṃ dhārentī”ti?"
msgstr ""

msgctxt "pli-tv-kd15:2.1.15"
msgid "“Saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.16"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.17"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:2.1.18"
msgid "“na, bhikkhave, vallikā dhāretabbā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.19"
msgid "na pāmaṅgo dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.20"
msgid "na kaṇṭhasuttakaṃ dhāretabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.21"
msgid "na kaṭisuttakaṃ dhāretabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.22"
msgid "na ovaṭṭikaṃ dhāretabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.23"
msgid "na kāyuraṃ dhāretabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.24"
msgid "na hatthābharaṇaṃ dhāretabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:2.1.25"
msgid "na aṅgulimuddikā dhāretabbā."
msgstr ""

msgctxt "pli-tv-kd15:2.1.26"
msgid "Yo dhāreyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13, pts-vp-pli2.107
msgctxt "pli-tv-kd15:2.2.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū dīghe kese dhārenti."
msgstr ""

msgctxt "pli-tv-kd15:2.2.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:2.2.3"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:2.2.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.2.5"
msgid "“Na, bhikkhave, dīghā kesā dhāretabbā."
msgstr ""

msgctxt "pli-tv-kd15:2.2.6"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:2.2.7"
msgid "Anujānāmi, bhikkhave, dumāsikaṃ vā duvaṅgulaṃ vā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
#. VAR: osaṇṭhenti → osaṇhenti (bj, s1-3, pts1)
msgctxt "pli-tv-kd15:2.3.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū kocchena kese osaṇṭhenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.3.2"
msgid "phaṇakena kese osaṇṭhenti, hatthaphaṇakena kese osaṇṭhenti, sitthatelakena kese osaṇṭhenti, udakatelakena kese osaṇṭhenti."
msgstr ""

msgctxt "pli-tv-kd15:2.3.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:2.3.4"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:2.3.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.3.6"
msgid "“Na, bhikkhave, kocchena kesā osaṇṭhetabbā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.3.7"
msgid "na sitthatelakena kesā osaṇṭhetabbā …"
msgstr ""

msgctxt "pli-tv-kd15:2.3.8"
msgid "na udakatelakena kesā osaṇṭhetabbā."
msgstr ""

msgctxt "pli-tv-kd15:2.3.9"
msgid "Yo osaṇṭheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-kd15:2.4.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū ādāsepi udakapattepi mukhanimittaṃ olokenti."
msgstr ""

msgctxt "pli-tv-kd15:2.4.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:2.4.3"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:2.4.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.4.5"
msgid "“Na, bhikkhave, ādāse vā udakapatte vā mukhanimittaṃ oloketabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.4.6"
msgid "Yo olokeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-kd15:2.4.7"
msgid "Tena kho pana samayena aññatarassa bhikkhuno mukhe vaṇo hoti."
msgstr ""

msgctxt "pli-tv-kd15:2.4.8"
msgid "So bhikkhū pucchi—"
msgstr ""

msgctxt "pli-tv-kd15:2.4.9"
msgid "“kīdiso me, āvuso, vaṇo”ti?"
msgstr ""

msgctxt "pli-tv-kd15:2.4.10"
msgid "Bhikkhū evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-kd15:2.4.11"
msgid "“ediso te, āvuso vaṇo”ti."
msgstr ""

msgctxt "pli-tv-kd15:2.4.12"
msgid "So na saddahati."
msgstr ""

msgctxt "pli-tv-kd15:2.4.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.4.14"
msgid "“Anujānāmi, bhikkhave, ābādhappaccayā ādāse vā udakapatte vā mukhanimittaṃ oloketun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-kd15:2.5.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū mukhaṃ ālimpanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.5.2"
msgid "mukhaṃ ummaddenti, mukhaṃ cuṇṇenti, manosilikāya mukhaṃ lañchenti, aṅgarāgaṃ karonti, mukharāgaṃ karonti, aṅgarāgamukharāgaṃ karonti."
msgstr ""

msgctxt "pli-tv-kd15:2.5.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:2.5.4"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:2.5.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.5.6"
msgid "“Na, bhikkhave, mukhaṃ ālimpitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:2.5.7"
msgid "na mukhaṃ ummadditabbaṃ, na mukhaṃ cuṇṇetabbaṃ, na manosilikāya mukhaṃ lañchetabbaṃ, na aṅgarāgo kātabbo, na mukharāgo kātabbo, na aṅgarāgamukharāgo kātabbo."
msgstr ""

msgctxt "pli-tv-kd15:2.5.8"
msgid "Yo kareyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-kd15:2.5.9"
msgid "Tena kho pana samayena aññatarassa bhikkhuno cakkhurogābādho hoti."
msgstr ""

msgctxt "pli-tv-kd15:2.5.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.5.11"
msgid "“Anujānāmi, bhikkhave, ābādhappaccayā mukhaṃ ālimpitun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc19, pts-vp-pli2.108
#. VAR: giraggasamajjo → samajjā (abhidhānaganthe)
msgctxt "pli-tv-kd15:2.6.1"
msgid "Tena kho pana samayena rājagahe giraggasamajjo hoti."
msgstr ""

msgctxt "pli-tv-kd15:2.6.2"
msgid "Chabbaggiyā bhikkhū giraggasamajjaṃ dassanāya agamaṃsu."
msgstr ""

msgctxt "pli-tv-kd15:2.6.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:2.6.4"
msgid "“kathañhi nāma samaṇā sakyaputtiyā naccampi gītampi vāditampi dassanāya gacchissanti, seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:2.6.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.6.6"
msgid "“Na, bhikkhave, naccaṃ vā gītaṃ vā vāditaṃ vā dassanāya gantabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:2.6.7"
msgid "Yo gaccheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-kd15:3.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū āyatakena gītassarena dhammaṃ gāyanti."
msgstr ""

msgctxt "pli-tv-kd15:3.1.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—“"
msgstr ""

#. VAR: yatheva → yathā ca (mr)
msgctxt "pli-tv-kd15:3.1.3"
msgid "yatheva mayaṃ gāyāma, evamevime samaṇā sakyaputtiyā āyatakena gītassarena dhammaṃ gāyantī”ti."
msgstr ""

msgctxt "pli-tv-kd15:3.1.4"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd15:3.1.5"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:3.1.6"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:3.1.7"
msgid "“kathañhi nāma chabbaggiyā bhikkhū āyatakena gītassarena dhammaṃ gāyissantī”ti."
msgstr ""

msgctxt "pli-tv-kd15:3.1.8"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:3.1.9"
msgid "“Saccaṃ kira, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd15:3.1.10"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:3.1.11"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:3.1.12"
msgid "“pañcime, bhikkhave, ādīnavā āyatakena gītassarena dhammaṃ gāyantassa."
msgstr ""

msgctxt "pli-tv-kd15:3.1.13"
msgid "Attanāpi tasmiṃ sare sārajjati, parepi tasmiṃ sare sārajjanti, gahapatikāpi ujjhāyanti, sarakuttimpi nikāmayamānassa samādhissa bhaṅgo hoti, pacchimā janatā diṭṭhānugatiṃ āpajjati—"
msgstr ""

msgctxt "pli-tv-kd15:3.1.14"
msgid "ime kho, bhikkhave, pañca ādīnavā āyatakena gītassarena dhammaṃ gāyantassa."
msgstr ""

msgctxt "pli-tv-kd15:3.1.15"
msgid "Na, bhikkhave, āyatakena gītassarena dhammo gāyitabbo."
msgstr ""

msgctxt "pli-tv-kd15:3.1.16"
msgid "Yo gāyeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-kd15:3.2.1"
msgid "Tena kho pana samayena bhikkhū sarabhaññe kukkuccāyanti."
msgstr ""

msgctxt "pli-tv-kd15:3.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:3.2.3"
msgid "“Anujānāmi, bhikkhave, sarabhaññan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc22
#. VAR: bāhiralomiṃ → bāhiyalomiṃ (mr)
msgctxt "pli-tv-kd15:4.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū bāhiralomiṃ uṇṇiṃ dhārenti."
msgstr ""

msgctxt "pli-tv-kd15:4.1.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:4.1.3"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:4.1.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. VAR: bāhiyalomi uṇṇi → bāhiralomī uṇṇī (s1-3, pts1)
msgctxt "pli-tv-kd15:4.1.5"
msgid "“Na, bhikkhave, bāhiyalomi uṇṇi dhāretabbā."
msgstr ""

msgctxt "pli-tv-kd15:4.1.6"
msgid "Yo dhāreyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc23, pts-vp-pli2.109
msgctxt "pli-tv-kd15:5.1.1"
msgid "Tena kho pana samayena rañño māgadhassa seniyassa bimbisārassa ārāme ambā phalino honti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.2"
msgid "Raññā māgadhena seniyena bimbisārena anuññātaṃ hoti—"
msgstr ""

msgctxt "pli-tv-kd15:5.1.3"
msgid "“yathāsukhaṃ ayyā ambaṃ paribhuñjantū”ti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.4"
msgid "Chabbaggiyā bhikkhū taruṇaññeva ambaṃ pātāpetvā paribhuñjiṃsu."
msgstr ""

msgctxt "pli-tv-kd15:5.1.5"
msgid "Rañño ca māgadhassa seniyassa bimbisārassa ambena attho hoti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.6"
msgid "Atha kho rājā māgadho seniyo bimbisāro manusse āṇāpesi—"
msgstr ""

msgctxt "pli-tv-kd15:5.1.7"
msgid "“gacchatha, bhaṇe, ārāmaṃ gantvā ambaṃ āharathā”ti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.8"
msgid "“Evaṃ, devā”ti kho te manussā rañño māgadhassa seniyassa bimbisārassa paṭissutvā ārāmaṃ gantvā ārāmapālaṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd15:5.1.9"
msgid "“devassa, bhaṇe, ambena attho, ambaṃ dethā”ti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.10"
msgid "“Natthāyyā, ambaṃ."
msgstr ""

msgctxt "pli-tv-kd15:5.1.11"
msgid "Taruṇaññeva ambaṃ pātāpetvā bhikkhū paribhuñjiṃsū”ti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.12"
msgid "Atha kho te manussā rañño māgadhassa seniyassa bimbisārassa etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:5.1.13"
msgid "“Suparibhuttaṃ, bhaṇe, ayyehi ambaṃ, api ca bhagavatā mattā vaṇṇitā”ti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.14"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:5.1.15"
msgid "“kathañhi nāma samaṇā sakyaputtiyā na mattaṃ jānitvā rañño ambaṃ paribhuñjissantī”ti."
msgstr ""

msgctxt "pli-tv-kd15:5.1.16"
msgid "Assosuṃ kho bhikkhū tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-kd15:5.1.17"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:5.1.18"
msgid "“Na, bhikkhave, ambaṃ paribhuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:5.1.19"
msgid "Yo paribhuñjeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-kd15:5.2.1"
msgid "Tena kho pana samayena aññatarassa pūgassa saṃghabhattaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:5.2.2"
msgid "Sūpe ambapesikāyo pakkhittā honti."
msgstr ""

msgctxt "pli-tv-kd15:5.2.3"
msgid "Bhikkhū kukkuccāyantā nappaṭiggaṇhanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:5.2.4"
msgid "“paṭiggaṇhatha, bhikkhave, paribhuñjatha."
msgstr ""

msgctxt "pli-tv-kd15:5.2.5"
msgid "Anujānāmi, bhikkhave, ambapesikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-kd15:5.2.6"
msgid "Tena kho pana samayena aññatarassa pūgassa saṃghabhattaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:5.2.7"
msgid "Te na pariyāpuṇiṃsu ambapesikaṃ kātuṃ, bhattagge sakaleheva ambehi denti."
msgstr ""

msgctxt "pli-tv-kd15:5.2.8"
msgid "Bhikkhū kukkuccāyantā nappaṭiggaṇhanti."
msgstr ""

msgctxt "pli-tv-kd15:5.2.9"
msgid "“Paṭiggaṇhatha, bhikkhave, paribhuñjatha."
msgstr ""

msgctxt "pli-tv-kd15:5.2.10"
msgid "Anujānāmi, bhikkhave, pañcahi samaṇakappehi phalaṃ paribhuñjituṃ—"
msgstr ""

#. VAR: nibbattabījaññeva → nibbaṭṭabījaññeva (bj, s1-3) | nivattabījaññeva (pts1)
#. VAR: aggiparicitaṃ, satthaparicitaṃ, nakhaparicitaṃ → aggiparijitaṃ satthaparijitaṃ nakhaparijitaṃ (^pi-tv-pvr7#63 [Ekuttarikanaya/ Pañcakavāra])
msgctxt "pli-tv-kd15:5.2.11"
msgid "aggiparicitaṃ, satthaparicitaṃ, nakhaparicitaṃ, abījaṃ, nibbattabījaññeva pañcamaṃ."
msgstr ""

msgctxt "pli-tv-kd15:5.2.12"
msgid "Anujānāmi, bhikkhave, imehi pañcahi samaṇakappehi phalaṃ paribhuñjitun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc26, pts-vp-pli2.110
msgctxt "pli-tv-kd15:6.1.1"
msgid "Tena kho pana samayena aññataro bhikkhu ahinā daṭṭho kālaṅkato hoti."
msgstr ""

msgctxt "pli-tv-kd15:6.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:6.1.3"
msgid "“Na hi nūna so, bhikkhave, bhikkhu imāni cattāri ahirājakulāni mettena cittena phari."
msgstr ""

msgctxt "pli-tv-kd15:6.1.4"
msgid "Sace hi so, bhikkhave, bhikkhu imāni cattāri ahirājakulāni mettena cittena phareyya, na hi so, bhikkhave, bhikkhu ahinā daṭṭho kālaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd15:6.1.5"
msgid "Katamāni cattāri ahirājakulāni?"
msgstr ""

msgctxt "pli-tv-kd15:6.1.6"
msgid "Virūpakkhaṃ ahirājakulaṃ, erāpathaṃ ahirājakulaṃ, chabyāputtaṃ ahirājakulaṃ, kaṇhāgotamaṃ ahirājakulaṃ."
msgstr ""

msgctxt "pli-tv-kd15:6.1.7"
msgid "Na hi nūna so, bhikkhave, bhikkhu imāni cattāri ahirājakulāni mettena cittena phari."
msgstr ""

msgctxt "pli-tv-kd15:6.1.8"
msgid "Sace hi so, bhikkhave, bhikkhu imāni cattāri ahirājakulāni mettena cittena phareyya, na hi so, bhikkhave, bhikkhu ahinā daṭṭho kālaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd15:6.1.9"
msgid "Anujānāmi, bhikkhave, imāni cattāri ahirājakulāni mettena cittena pharituṃ, attaguttiyā attarakkhāya attaparittaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-kd15:6.1.10"
msgid "Evañca pana, bhikkhave, kātabbaṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc27
msgctxt "pli-tv-kd15:6.1.11"
msgid "‘Virūpakkhehi me mettaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.12"
msgid "Mettaṃ erāpathehi me;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.13"
msgid "Chabyāputtehi me mettaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.14"
msgid "Mettaṃ kaṇhāgotamakehi ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc28
msgctxt "pli-tv-kd15:6.1.15"
msgid "Apādakehi me mettaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.16"
msgid "mettaṃ dvipādakehi me;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.17"
msgid "Catuppadehi me mettaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.18"
msgid "mettaṃ bahuppadehi me."
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-kd15:6.1.19"
msgid "Mā maṃ apādako hiṃsi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.20"
msgid "mā maṃ hiṃsi dvipādako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.21"
msgid "Mā maṃ catuppado hiṃsi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.22"
msgid "mā maṃ hiṃsi bahuppado."
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-kd15:6.1.23"
msgid "Sabbe sattā sabbe pāṇā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.24"
msgid "sabbe bhūtā ca kevalā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.25"
msgid "Sabbe bhadrāni passantu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.26"
msgid "mā kiñci pāpamāgamā."
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-kd15:6.1.27"
msgid "Appamāṇo buddho,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.28"
msgid "Appamāṇo dhammo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.29"
msgid "Appamāṇo saṃgho,"
msgstr ""

#. HTML: <br>
#. VAR: sarīsapāni → siriṃsapāni (bj, s1-3, pts1)
msgctxt "pli-tv-kd15:6.1.30"
msgid "Pamāṇavantāni sarīsapāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-kd15:6.1.31"
msgid "Ahi vicchikā satapadī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.32"
msgid "Uṇṇanābhi sarabū mūsikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.33"
msgid "Katā me rakkhā kataṃ me parittaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.34"
msgid "Paṭikkamantu bhūtāni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.35"
msgid "Sohaṃ namo bhagavato,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd15:6.1.36"
msgid "Namo sattannaṃ sammāsambuddhānan’”ti."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc33
msgctxt "pli-tv-kd15:7.1.1"
msgid "Tena kho pana samayena aññataro bhikkhu anabhiratiyā pīḷito attano aṅgajātaṃ chindi."
msgstr ""

msgctxt "pli-tv-kd15:7.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:7.1.3"
msgid "“Aññamhi so, bhikkhave, moghapuriso chetabbamhi, aññaṃ chindi."
msgstr ""

msgctxt "pli-tv-kd15:7.1.4"
msgid "Na, bhikkhave, attano aṅgajātaṃ chetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:7.1.5"
msgid "Yo chindeyya, āpatti thullaccayassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc34, pts-vp-pli2.111
#. VAR: candanassa → candanasārassa (bj, s1-3, pts1)
msgctxt "pli-tv-kd15:8.1.1"
msgid "Tena kho pana samayena rājagahakassa seṭṭhissa mahagghassa candanassa candanagaṇṭhi uppannā hoti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.2"
msgid "Atha kho rājagahakassa seṭṭhissa etadahosi—"
msgstr ""

msgctxt "pli-tv-kd15:8.1.3"
msgid "“yannūnāhaṃ imāya candanagaṇṭhiyā pattaṃ lekhāpeyyaṃ."
msgstr ""

msgctxt "pli-tv-kd15:8.1.4"
msgid "Lekhañca me paribhogaṃ bhavissati, pattañca dānaṃ dassāmī”ti."
msgstr ""

#. VAR: bandhitvā → vāhitvā (s1-3)
#. VAR: uḍḍitvā → vāhitvā (bj) | pakkhipitvā (pts1)
msgctxt "pli-tv-kd15:8.1.5"
msgid "Atha kho rājagahako seṭṭhi tāya candanagaṇṭhiyā pattaṃ lekhāpetvā sikkāya uḍḍitvā veḷagge ālaggetvā veḷuparamparāya bandhitvā evamāha—"
msgstr ""

msgctxt "pli-tv-kd15:8.1.6"
msgid "“yo samaṇo vā brāhmaṇo vā arahā ceva iddhimā ca dinnaṃyeva pattaṃ oharatū”ti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.7"
msgid "Atha kho pūraṇo kassapo yena rājagahako seṭṭhi tenupasaṅkami, upasaṅkamitvā rājagahakaṃ seṭṭhiṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:8.1.8"
msgid "“ahañhi, gahapati, arahā ceva iddhimā ca, dehi me pattan”ti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.9"
msgid "“Sace, bhante, āyasmā arahā ceva iddhimā ca dinnaṃyeva pattaṃ oharatū”ti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.10"
msgid "Atha kho makkhali gosālo …"
msgstr ""

msgctxt "pli-tv-kd15:8.1.11"
msgid "ajito kesakambalo …"
msgstr ""

msgctxt "pli-tv-kd15:8.1.12"
msgid "pakudho kaccāyano …"
msgstr ""

#. VAR: sañcayo belaṭṭhaputto → sañjayo bellaṭṭhiputto (bj, pts1) | sañjayo Veḷaṭṭhaputto (s1-3)
msgctxt "pli-tv-kd15:8.1.13"
msgid "sañcayo belaṭṭhaputto …"
msgstr ""

#. VAR: nāṭaputto → nātaputto (bj, pts1)
#. VAR: nigaṇṭho → niggaṇṭho (s1-3)
msgctxt "pli-tv-kd15:8.1.14"
msgid "nigaṇṭho nāṭaputto yena rājagahako seṭṭhi tenupasaṅkami, upasaṅkamitvā rājagahakaṃ seṭṭhiṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:8.1.15"
msgid "“ahañhi, gahapati, arahā ceva iddhimā ca, dehi me pattan”ti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.16"
msgid "“Sace, bhante, āyasmā arahā ceva iddhimā ca, dinnaṃyeva pattaṃ oharatū”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-kd15:8.1.17"
msgid "Tena kho pana samayena āyasmā ca mahāmoggallāno āyasmā ca piṇḍolabhāradvājo pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya rājagahaṃ piṇḍāya pavisiṃsu."
msgstr ""

msgctxt "pli-tv-kd15:8.1.18"
msgid "Atha kho āyasmā piṇḍolabhāradvājo āyasmantaṃ mahāmoggallānaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:8.1.19"
msgid "“āyasmā kho mahāmoggallāno arahā ceva iddhimā ca."
msgstr ""

msgctxt "pli-tv-kd15:8.1.20"
msgid "Gacchāvuso, moggallāna, etaṃ pattaṃ ohara."
msgstr ""

msgctxt "pli-tv-kd15:8.1.21"
msgid "Tuyheso patto”ti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.22"
msgid "“Āyasmā kho bhāradvājo arahā ceva iddhimā ca."
msgstr ""

msgctxt "pli-tv-kd15:8.1.23"
msgid "Gacchāvuso, bhāradvāja, etaṃ pattaṃ ohara."
msgstr ""

msgctxt "pli-tv-kd15:8.1.24"
msgid "Tuyheso patto”ti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.25"
msgid "Atha kho āyasmā piṇḍolabhāradvājo vehāsaṃ abbhuggantvā taṃ pattaṃ gahetvā tikkhattuṃ rājagahaṃ anupariyāyi."
msgstr ""

#. HTML: </p><p>
#. REF: sc36
msgctxt "pli-tv-kd15:8.1.26"
msgid "Tena kho pana samayena rājagahako seṭṭhi saputtadāro sake nivesane ṭhito hoti pañjaliko namassamāno—"
msgstr ""

msgctxt "pli-tv-kd15:8.1.27"
msgid "idheva, bhante, ayyo bhāradvājo amhākaṃ nivesane patiṭṭhātūti."
msgstr ""

msgctxt "pli-tv-kd15:8.1.28"
msgid "Atha kho āyasmā piṇḍolabhāradvājo rājagahakassa seṭṭhissa nivesane patiṭṭhāsi."
msgstr ""

msgctxt "pli-tv-kd15:8.1.29"
msgid "Atha kho rājagahako seṭṭhi āyasmato piṇḍolabhāradvājassa hatthato pattaṃ gahetvā mahagghassa khādanīyassa pūretvā āyasmato piṇḍolabhāradvājassa adāsi."
msgstr ""

msgctxt "pli-tv-kd15:8.1.30"
msgid "Atha kho āyasmā piṇḍolabhāradvājo taṃ pattaṃ gahetvā ārāmaṃ agamāsi."
msgstr ""

msgctxt "pli-tv-kd15:8.2.1"
msgid "Assosuṃ kho manussā—"
msgstr ""

msgctxt "pli-tv-kd15:8.2.2"
msgid "ayyena kira piṇḍolabhāradvājena rājagahakassa seṭṭhissa patto ohāritoti."
msgstr ""

msgctxt "pli-tv-kd15:8.2.3"
msgid "Te ca manussā uccāsaddā mahāsaddā āyasmantaṃ piṇḍolabhāradvājaṃ piṭṭhito piṭṭhito anubandhiṃsu."
msgstr ""

#. HTML: </p><p>
#. REF: sc37
msgctxt "pli-tv-kd15:8.2.4"
msgid "Assosi kho bhagavā uccāsaddaṃ mahāsaddaṃ;"
msgstr ""

msgctxt "pli-tv-kd15:8.2.5"
msgid "sutvāna āyasmantaṃ ānandaṃ āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:8.2.6"
msgid "“kiṃ nu kho so, ānanda, uccāsaddo mahāsaddo”ti?"
msgstr ""

msgctxt "pli-tv-kd15:8.2.7"
msgid "“Āyasmatā, bhante, piṇḍolabhāradvājena rājagahakassa seṭṭhissa patto ohārito."
msgstr ""

msgctxt "pli-tv-kd15:8.2.8"
msgid "Assosuṃ kho, bhante, manussā—"
msgstr ""

msgctxt "pli-tv-kd15:8.2.9"
msgid "ayyena kira piṇḍolabhāradvājena rājagahakassa seṭṭhissa patto ohāritoti."
msgstr ""

msgctxt "pli-tv-kd15:8.2.10"
msgid "Te ca, bhante, manussā uccāsaddā mahāsaddā āyasmantaṃ piṇḍolabhāradvājaṃ piṭṭhito piṭṭhito anubandhā."
msgstr ""

msgctxt "pli-tv-kd15:8.2.11"
msgid "So eso, bhante, bhagavā uccāsaddo mahāsaddo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc38, pts-vp-pli2.112
msgctxt "pli-tv-kd15:8.2.12"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ piṇḍolabhāradvājaṃ paṭipucchi—"
msgstr ""

msgctxt "pli-tv-kd15:8.2.13"
msgid "“saccaṃ kira tayā, bhāradvāja, rājagahakassa seṭṭhissa patto ohārito”ti?"
msgstr ""

msgctxt "pli-tv-kd15:8.2.14"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd15:8.2.15"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-kd15:8.2.16"
msgid "“ananucchavikaṃ, bhāradvāja, ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd15:8.2.17"
msgid "Kathañhi nāma tvaṃ, bhāradvāja, chavassa dārupattassa kāraṇā gihīnaṃ uttari manussadhammaṃ iddhipāṭihāriyaṃ dassessasi."
msgstr ""

msgctxt "pli-tv-kd15:8.2.18"
msgid "Seyyathāpi, bhāradvāja, mātugāmo chavassa māsakarūpassa kāraṇā kopinaṃ dasseti;"
msgstr ""

msgctxt "pli-tv-kd15:8.2.19"
msgid "evameva kho tayā, bhāradvāja, chavassa dārupattassa kāraṇā gihīnaṃ uttari manussadhammaṃ iddhipāṭihāriyaṃ dassitaṃ."
msgstr ""

msgctxt "pli-tv-kd15:8.2.20"
msgid "Netaṃ, bhāradvāja, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-kd15:8.2.21"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:8.2.22"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:8.2.23"
msgid "“na, bhikkhave, gihīnaṃ uttarimanussadhammaṃ iddhipāṭihāriyaṃ dassetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:8.2.24"
msgid "Yo dasseyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:8.2.25"
msgid "Bhindathetaṃ, bhikkhave, dārupattaṃ sakalikaṃ sakalikaṃ katvā, bhikkhūnaṃ añjanupapisanaṃ detha."
msgstr ""

msgctxt "pli-tv-kd15:8.2.26"
msgid "Na ca, bhikkhave, dārupatto dhāretabbo."
msgstr ""

msgctxt "pli-tv-kd15:8.2.27"
msgid "Yo dhāreyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc39
msgctxt "pli-tv-kd15:9.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū uccāvace patte dhārenti sovaṇṇamayaṃ rūpiyamayaṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.1.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:9.1.3"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.1.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.1.5"
msgid "“Na, bhikkhave, sovaṇṇamayo patto dhāretabbo … pe …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.6"
msgid "na rūpiyamayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.7"
msgid "na maṇimayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.8"
msgid "na veḷuriyamayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.9"
msgid "na phalikamayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.10"
msgid "na kaṃsamayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.11"
msgid "na kācamayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.12"
msgid "na tipumayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.13"
msgid "na sīsamayo patto dhāretabbo …"
msgstr ""

msgctxt "pli-tv-kd15:9.1.14"
msgid "na tambalohamayo patto dhāretabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.1.15"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:9.1.16"
msgid "Anujānāmi, bhikkhave, dve patte—"
msgstr ""

msgctxt "pli-tv-kd15:9.1.17"
msgid "ayopattaṃ, mattikāpattan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-kd15:9.2.1"
msgid "Tena kho pana samayena pattamūlaṃ ghaṃsiyati."
msgstr ""

msgctxt "pli-tv-kd15:9.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.2.3"
msgid "“Anujānāmi, bhikkhave, pattamaṇḍalan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc41, pts-vp-pli2.113
msgctxt "pli-tv-kd15:9.2.4"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū uccāvacāni pattamaṇḍalāni dhārenti sovaṇṇamayaṃ rūpiyamayaṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.2.5"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:9.2.6"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.2.7"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.2.8"
msgid "“Na, bhikkhave, uccāvacāni pattamaṇḍalāni dhāretabbāni."
msgstr ""

msgctxt "pli-tv-kd15:9.2.9"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:9.2.10"
msgid "Anujānāmi, bhikkhave, dve pattamaṇḍalāni—"
msgstr ""

msgctxt "pli-tv-kd15:9.2.11"
msgid "tipumayaṃ, sīsamayan”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.2.12"
msgid "Bahalāni maṇḍalāni na acchupiyanti."
msgstr ""

msgctxt "pli-tv-kd15:9.2.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.2.14"
msgid "“Anujānāmi, bhikkhave, likhitun”ti."
msgstr ""

#. VAR: Valī → valiṃ (pts1) | calī (mr)
msgctxt "pli-tv-kd15:9.2.15"
msgid "Valī honti."
msgstr ""

msgctxt "pli-tv-kd15:9.2.16"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.2.17"
msgid "“Anujānāmi, bhikkhave, makaradantakaṃ chinditun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc42
msgctxt "pli-tv-kd15:9.2.18"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū citrāni pattamaṇḍalāni dhārenti rūpakākiṇṇāni bhittikammakatāni."
msgstr ""

msgctxt "pli-tv-kd15:9.2.19"
msgid "Tāni rathikāyapi dassentā āhiṇḍanti."
msgstr ""

msgctxt "pli-tv-kd15:9.2.20"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:9.2.21"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.2.22"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.2.23"
msgid "“Na, bhikkhave, citrāni pattamaṇḍalāni dhāretabbāni rūpakākiṇṇāni bhittikammakatāni."
msgstr ""

msgctxt "pli-tv-kd15:9.2.24"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:9.2.25"
msgid "Anujānāmi, bhikkhave, pakatimaṇḍalan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc43
msgctxt "pli-tv-kd15:9.3.1"
msgid "Tena kho pana samayena bhikkhū sodakaṃ pattaṃ paṭisāmenti."
msgstr ""

msgctxt "pli-tv-kd15:9.3.2"
msgid "Patto dussati."
msgstr ""

msgctxt "pli-tv-kd15:9.3.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.3.4"
msgid "“Na, bhikkhave, sodako patto paṭisāmetabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.3.5"
msgid "Yo paṭisāmeyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:9.3.6"
msgid "Anujānāmi, bhikkhave, otāpetvā pattaṃ paṭisāmetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc44
#. VAR: sodakaṃ → saudakaṃ (s1-3, pts1, mr)
msgctxt "pli-tv-kd15:9.3.7"
msgid "Tena kho pana samayena bhikkhū sodakaṃ pattaṃ otāpenti."
msgstr ""

msgctxt "pli-tv-kd15:9.3.8"
msgid "Patto duggandho hoti."
msgstr ""

msgctxt "pli-tv-kd15:9.3.9"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.3.10"
msgid "“Na, bhikkhave, sodako patto otāpetabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.3.11"
msgid "Yo otāpeyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:9.3.12"
msgid "Anujānāmi, bhikkhave, vodakaṃ katvā otāpetvā pattaṃ paṭisāmetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-kd15:9.3.13"
msgid "Tena kho pana samayena bhikkhū uṇhe pattaṃ nidahanti."
msgstr ""

msgctxt "pli-tv-kd15:9.3.14"
msgid "Pattassa vaṇṇo dussati."
msgstr ""

msgctxt "pli-tv-kd15:9.3.15"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.3.16"
msgid "“Na, bhikkhave, uṇhe patto nidahitabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.3.17"
msgid "Yo nidaheyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:9.3.18"
msgid "Anujānāmi, bhikkhave, muhuttaṃ uṇhe otāpetvā pattaṃ paṭisāmetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc46
msgctxt "pli-tv-kd15:9.4.1"
msgid "Tena kho pana samayena sambahulā pattā ajjhokāse anādhārā nikkhittā honti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.2"
msgid "Vātamaṇḍalikāya āvaṭṭetvā pattā bhijjiṃsu."
msgstr ""

msgctxt "pli-tv-kd15:9.4.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.4"
msgid "“Anujānāmi, bhikkhave, pattādhārakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc47
msgctxt "pli-tv-kd15:9.4.5"
msgid "Tena kho pana samayena bhikkhū miḍḍhante pattaṃ nikkhipanti."
msgstr ""

#. VAR: Paripatitvā → parivaṭṭitvā (s1-3)
msgctxt "pli-tv-kd15:9.4.6"
msgid "Paripatitvā patto bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:9.4.7"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.8"
msgid "“Na, bhikkhave, miḍḍhante patto nikkhipitabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.4.9"
msgid "Yo nikkhipeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-kd15:9.4.10"
msgid "Tena kho pana samayena bhikkhū paribhaṇḍante pattaṃ nikkhipanti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.11"
msgid "Paripatitvā patto bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:9.4.12"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.13"
msgid "“Na, bhikkhave, paribhaṇḍante patto nikkhipitabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.4.14"
msgid "Yo nikkhipeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc49, pts-vp-pli2.114
msgctxt "pli-tv-kd15:9.4.15"
msgid "Tena kho pana samayena bhikkhū chamāya pattaṃ nikkujjanti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.16"
msgid "Oṭṭho ghaṃsiyati."
msgstr ""

msgctxt "pli-tv-kd15:9.4.17"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.18"
msgid "“Anujānāmi, bhikkhave, tiṇasanthārakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.19"
msgid "Tiṇasanthārako upacikāhi khajjati."
msgstr ""

msgctxt "pli-tv-kd15:9.4.20"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.21"
msgid "“Anujānāmi, bhikkhave, coḷakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.22"
msgid "Coḷakaṃ upacikāhi khajjati."
msgstr ""

msgctxt "pli-tv-kd15:9.4.23"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.24"
msgid "“Anujānāmi, bhikkhave, pattamāḷakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.25"
msgid "Pattamāḷako paripatitvā patto bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:9.4.26"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.27"
msgid "“Anujānāmi, bhikkhave, pattakuṇḍolikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.28"
msgid "Pattakuṇḍolikāya patto ghaṃsiyati."
msgstr ""

msgctxt "pli-tv-kd15:9.4.29"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.30"
msgid "“Anujānāmi, bhikkhave, pattatthavikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.31"
msgid "Aṃsabaddhako na hoti."
msgstr ""

msgctxt "pli-tv-kd15:9.4.32"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.4.33"
msgid "“Anujānāmi, bhikkhave, aṃsabaddhakaṃ bandhanasuttakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc50
msgctxt "pli-tv-kd15:9.5.1"
msgid "Tena kho pana samayena bhikkhū bhittikhilepi nāgadantakepi pattaṃ laggenti."
msgstr ""

msgctxt "pli-tv-kd15:9.5.2"
msgid "Paripatitvā patto bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:9.5.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.5.4"
msgid "“Na, bhikkhave, patto laggetabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.5.5"
msgid "Yo laggeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc51
msgctxt "pli-tv-kd15:9.5.6"
msgid "Tena kho pana samayena bhikkhū mañce pattaṃ nikkhipanti, satisammosā nisīdantā ottharitvā pattaṃ bhindenti."
msgstr ""

msgctxt "pli-tv-kd15:9.5.7"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.5.8"
msgid "“Na, bhikkhave, mañce patto nikkhipitabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.5.9"
msgid "Yo nikkhipeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc52
msgctxt "pli-tv-kd15:9.5.10"
msgid "Tena kho pana samayena bhikkhū pīṭhe pattaṃ nikkhipanti, satisammosā nisīdantā ottharitvā pattaṃ bhindenti."
msgstr ""

msgctxt "pli-tv-kd15:9.5.11"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.5.12"
msgid "“Na, bhikkhave, pīṭhe patto nikkhipitabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.5.13"
msgid "Yo nikkhipeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc53
msgctxt "pli-tv-kd15:9.5.14"
msgid "Tena kho pana samayena bhikkhū aṅke pattaṃ nikkhipanti, satisammosā uṭṭhahanti."
msgstr ""

msgctxt "pli-tv-kd15:9.5.15"
msgid "Paripatitvā patto bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:9.5.16"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.5.17"
msgid "“Na, bhikkhave, aṅke patto nikkhipitabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.5.18"
msgid "Yo nikkhipeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc54
msgctxt "pli-tv-kd15:9.5.19"
msgid "Tena kho pana samayena bhikkhū chatte pattaṃ nikkhipanti."
msgstr ""

msgctxt "pli-tv-kd15:9.5.20"
msgid "Vātamaṇḍalikāya chattaṃ ukkhipiyati paripatitvā patto bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:9.5.21"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.5.22"
msgid "“Na, bhikkhave, chatte patto nikkhipitabbo."
msgstr ""

msgctxt "pli-tv-kd15:9.5.23"
msgid "Yo nikkhipeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-kd15:9.5.24"
msgid "Tena kho pana samayena bhikkhū pattahatthā kavāṭaṃ paṇāmenti."
msgstr ""

msgctxt "pli-tv-kd15:9.5.25"
msgid "Kavāṭo āvaṭṭitvā patto bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:9.5.26"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. VAR: kavāṭaṃ paṇāmetabbaṃ → kavāṭo paṇāmetabbo (bj, mr)
msgctxt "pli-tv-kd15:9.5.27"
msgid "“Na, bhikkhave, pattahatthena kavāṭaṃ paṇāmetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:9.5.28"
msgid "Yo paṇāmeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-kd15:10.1.1"
msgid "Tena kho pana samayena bhikkhū tumbakaṭāhe piṇḍāya caranti."
msgstr ""

msgctxt "pli-tv-kd15:10.1.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:10.1.3"
msgid "“seyyathāpi titthiyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:10.1.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.1.5"
msgid "“Na, bhikkhave, tumbakaṭāhe piṇḍāya caritabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.1.6"
msgid "Yo careyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc57, pts-vp-pli2.115
#. VAR: ghaṭikaṭāhe → ghaṭikaṭāhena (s1-3)
msgctxt "pli-tv-kd15:10.1.7"
msgid "Tena kho pana samayena bhikkhū ghaṭikaṭāhe piṇḍāya caranti."
msgstr ""

msgctxt "pli-tv-kd15:10.1.8"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:10.1.9"
msgid "“seyyathāpi titthiyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:10.1.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.1.11"
msgid "“Na, bhikkhave, ghaṭikaṭāhe piṇḍāya caritabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.1.12"
msgid "Yo careyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-kd15:10.2.1"
msgid "Tena kho pana samayena aññataro bhikkhu sabbapaṃsukūliko hoti."
msgstr ""

msgctxt "pli-tv-kd15:10.2.2"
msgid "So chavasīsassa pattaṃ dhāreti."
msgstr ""

msgctxt "pli-tv-kd15:10.2.3"
msgid "Aññatarā itthī passitvā bhītā vissaramakāsi—"
msgstr ""

msgctxt "pli-tv-kd15:10.2.4"
msgid "“abhuṃ me pisāco vatāyan”ti."
msgstr ""

msgctxt "pli-tv-kd15:10.2.5"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:10.2.6"
msgid "“kathañhi nāma samaṇā sakyaputtiyā chavasīsassa pattaṃ dhāressanti, seyyathāpi pisācillikā”ti."
msgstr ""

msgctxt "pli-tv-kd15:10.2.7"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.2.8"
msgid "“Na, bhikkhave, chavasīsassa patto dhāretabbo."
msgstr ""

msgctxt "pli-tv-kd15:10.2.9"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:10.2.10"
msgid "Na ca, bhikkhave, sabbapaṃsukūlikena bhavitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.2.11"
msgid "Yo bhaveyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-kd15:10.3.1"
msgid "Tena kho pana samayena bhikkhū calakānipi aṭṭhikānipi ucchiṭṭhodakampi pattena nīharanti."
msgstr ""

msgctxt "pli-tv-kd15:10.3.2"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:10.3.3"
msgid "“yasmiṃ yevime samaṇā sakyaputtiyā bhuñjanti sova nesaṃ paṭiggaho”ti."
msgstr ""

msgctxt "pli-tv-kd15:10.3.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.3.5"
msgid "“Na bhikkhave, calakāni vā aṭṭhikāni vā ucchiṭṭhodakaṃ vā pattena nīharitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:10.3.6"
msgid "Yo nīhareyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:10.3.7"
msgid "Anujānāmi, bhikkhave, paṭiggahan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-kd15:11.1.1"
msgid "Tena kho pana samayena bhikkhū hatthena vipphāḷetvā cīvaraṃ sibbenti."
msgstr ""

msgctxt "pli-tv-kd15:11.1.2"
msgid "Cīvaraṃ vilomikaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:11.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.1.4"
msgid "“Anujānāmi, bhikkhave, satthakaṃ namatakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc61
msgctxt "pli-tv-kd15:11.1.5"
msgid "Tena kho pana samayena saṃghassa daṇḍasatthakaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:11.1.6"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.1.7"
msgid "“Anujānāmi, bhikkhave, daṇḍasatthakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-kd15:11.1.8"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū uccāvace satthakadaṇḍe dhārenti sovaṇṇamayaṃ rūpiyamayaṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.1.9"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:11.1.10"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.1.11"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.1.12"
msgid "“Na, bhikkhave, uccāvacā satthakadaṇḍā dhāretabbā."
msgstr ""

msgctxt "pli-tv-kd15:11.1.13"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:11.1.14"
msgid "Anujānāmi, bhikkhave, aṭṭhimayaṃ dantamayaṃ visāṇamayaṃ naḷamayaṃ veḷumayaṃ kaṭṭhamayaṃ jatumayaṃ phalamayaṃ lohamayaṃ saṅkhanābhimayan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc63, pts-vp-pli2.116
msgctxt "pli-tv-kd15:11.2.1"
msgid "Tena kho pana samayena bhikkhū kukkuṭapattenapi veḷupesikāyapi cīvaraṃ sibbenti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.2"
msgid "Cīvaraṃ dussibbitaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.2.4"
msgid "“Anujānāmi, bhikkhave, sūcin”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.5"
msgid "Sūciyo kaṇṇakitāyo honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.2.6"
msgid "“anujānāmi, bhikkhave, sūcināḷikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.7"
msgid "Sūcināḷikāyapi kaṇṇakitāyo honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.2.8"
msgid "“anujānāmi, bhikkhave, kiṇṇena pūretun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.9"
msgid "Kiṇṇepi kaṇṇakitāyo honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.2.10"
msgid "“anujānāmi, bhikkhave, sattuyā pūretun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.11"
msgid "Sattuyāpi kaṇṇakitāyo honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.2.12"
msgid "“anujānāmi, bhikkhave, saritakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.13"
msgid "Saritakepi kaṇṇakitāyo honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.2.14"
msgid "“anujānāmi, bhikkhave, madhusitthakena sāretun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.2.15"
msgid "Saritakaṃ paribhijjati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.2.16"
msgid "“anujānāmi, bhikkhave, saritakasipāṭikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-kd15:11.3.1"
msgid "Tena kho pana samayena bhikkhū tattha tattha khilaṃ nikkhaṇitvā sambandhitvā cīvaraṃ sibbenti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.2"
msgid "Cīvaraṃ vikaṇṇaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. VAR: kathinaṃ kathinarajjuṃ → kaṭhinaṃ kaṭhinarajjuṃ (bj, s1-3, pts1)
msgctxt "pli-tv-kd15:11.3.4"
msgid "“Anujānāmi, bhikkhave, kathinaṃ kathinarajjuṃ tattha tattha obandhitvā cīvaraṃ sibbetun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.5"
msgid "Visame kathinaṃ pattharanti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.6"
msgid "Kathinaṃ paribhijjati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.3.7"
msgid "“na, bhikkhave, visame kathinaṃ pattharitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.3.8"
msgid "Yo patthareyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-kd15:11.3.9"
msgid "Chamāya kathinaṃ pattharanti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.10"
msgid "Kathinaṃ paṃsukitaṃ hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.3.11"
msgid "“anujānāmi, bhikkhave, tiṇasanthārakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.12"
msgid "Kathinassa anto jīrati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.3.13"
msgid "“anujānāmi, bhikkhave, anuvātaṃ paribhaṇḍaṃ āropetun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.14"
msgid "Kathinaṃ nappahoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.3.15"
msgid "“anujānāmi, bhikkhave, daṇḍakathinaṃ bidalakaṃ salākaṃ vinandhanarajjuṃ vinandhanasuttaṃ vinandhitvā cīvaraṃ sibbetun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.16"
msgid "Suttantarikāyo visamā honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.3.17"
msgid "“anujānāmi, bhikkhave, kaḷimbhakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.3.18"
msgid "Suttā vaṅkā honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.3.19"
msgid "“anujānāmi, bhikkhave, moghasuttakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-kd15:11.4.1"
msgid "Tena kho pana samayena bhikkhū adhotehi pādehi kathinaṃ akkamanti."
msgstr ""

msgctxt "pli-tv-kd15:11.4.2"
msgid "Kathinaṃ dussati."
msgstr ""

msgctxt "pli-tv-kd15:11.4.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.4.4"
msgid "“Na, bhikkhave, adhotehi pādehi kathinaṃ akkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.4.5"
msgid "Yo akkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-kd15:11.4.6"
msgid "Tena kho pana samayena bhikkhū allehi pādehi kathinaṃ akkamanti."
msgstr ""

msgctxt "pli-tv-kd15:11.4.7"
msgid "Kathinaṃ dussati."
msgstr ""

msgctxt "pli-tv-kd15:11.4.8"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.4.9"
msgid "“Na, bhikkhave, allehi pādehi kathinaṃ akkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.4.10"
msgid "Yo akkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-kd15:11.4.11"
msgid "Tena kho pana samayena bhikkhū saupāhanā kathinaṃ akkamanti."
msgstr ""

msgctxt "pli-tv-kd15:11.4.12"
msgid "Kathinaṃ dussati."
msgstr ""

msgctxt "pli-tv-kd15:11.4.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.4.14"
msgid "“Na, bhikkhave, saupāhanena kathinaṃ akkamitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.4.15"
msgid "Yo akkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc69
msgctxt "pli-tv-kd15:11.5.1"
msgid "Tena kho pana samayena bhikkhū cīvaraṃ sibbantā aṅguliyā paṭiggaṇhanti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.2"
msgid "Aṅguliyo dukkhā honti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.5.4"
msgid "“Anujānāmi, bhikkhave, paṭiggahan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc70, pts-vp-pli2.117
msgctxt "pli-tv-kd15:11.5.5"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū uccāvace paṭiggahe dhārenti sovaṇṇamayaṃ rūpiyamayaṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.5.6"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:11.5.7"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.8"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.5.9"
msgid "“Na, bhikkhave, uccāvacā paṭiggahā dhāretabbā."
msgstr ""

msgctxt "pli-tv-kd15:11.5.10"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:11.5.11"
msgid "Anujānāmi, bhikkhave, aṭṭhimayaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.5.12"
msgid "saṅkhanābhimayan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-kd15:11.5.13"
msgid "Tena kho pana samayena sūciyopi satthakāpi paṭiggahāpi nassanti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.14"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.5.15"
msgid "“Anujānāmi, bhikkhave, āvesanavitthakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.16"
msgid "Āvesanavitthake samākulā honti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.17"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.5.18"
msgid "“Anujānāmi, bhikkhave, paṭiggahathavikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.19"
msgid "Aṃsabaddhako na hoti."
msgstr ""

msgctxt "pli-tv-kd15:11.5.20"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.5.21"
msgid "“Anujānāmi, bhikkhave, aṃsabaddhakaṃ bandhanasuttakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-kd15:11.6.1"
msgid "Tena kho pana samayena bhikkhū abbhokāse cīvaraṃ sibbantā sītenapi uṇhenapi kilamanti."
msgstr ""

msgctxt "pli-tv-kd15:11.6.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.6.3"
msgid "“Anujānāmi, bhikkhave, kathinasālaṃ kathinamaṇḍapan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.6.4"
msgid "Kathinasālā nīcavatthukā hoti, udakena otthariyyati."
msgstr ""

msgctxt "pli-tv-kd15:11.6.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.6.6"
msgid "“Anujānāmi, bhikkhave, uccavatthukaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.6.7"
msgid "Cayo paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.6.8"
msgid "“anujānāmi, bhikkhave, cinituṃ tayo caye—"
msgstr ""

msgctxt "pli-tv-kd15:11.6.9"
msgid "iṭṭhakācayaṃ, silācayaṃ, dārucayan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.6.10"
msgid "Ārohantā vihaññanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.6.11"
msgid "“anujānāmi, bhikkhave, tayo sopāne—"
msgstr ""

msgctxt "pli-tv-kd15:11.6.12"
msgid "iṭṭhakāsopānaṃ, silāsopānaṃ, dārusopānan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.6.13"
msgid "Ārohantā paripatanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.6.14"
msgid "“anujānāmi, bhikkhave, ālambanabāhan”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.6.15"
msgid "Kathinasālāya tiṇacuṇṇaṃ paripatati … pe …"
msgstr ""

#. VAR: ogumphetvā → ogumbetvā (bj, s1-3)
msgctxt "pli-tv-kd15:11.6.16"
msgid "“anujānāmi, bhikkhave, ogumphetvā ullittāvalittaṃ kātuṃ—"
msgstr ""

msgctxt "pli-tv-kd15:11.6.17"
msgid "setavaṇṇaṃ kāḷavaṇṇaṃ gerukaparikammaṃ mālākammaṃ latākammaṃ makaradantakaṃ pañcapaṭikaṃ cīvaravaṃsaṃ cīvararajjukan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-kd15:11.7.1"
msgid "Tena kho pana samayena bhikkhū cīvaraṃ sibbetvā tattheva kathinaṃ ujjhitvā pakkamanti, undūrehipi upacikāhipi khajjati."
msgstr ""

msgctxt "pli-tv-kd15:11.7.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.7.3"
msgid "“Anujānāmi, bhikkhave, kathinaṃ saṅgharitun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.7.4"
msgid "Kathinaṃ paribhijjati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.7.5"
msgid "“anujānāmi, bhikkhave, goghaṃsikāya kathinaṃ saṅgharitun”ti."
msgstr ""

msgctxt "pli-tv-kd15:11.7.6"
msgid "Kathinaṃ viniveṭhiyati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:11.7.7"
msgid "“anujānāmi, bhikkhave, bandhanarajjun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc74
msgctxt "pli-tv-kd15:11.7.8"
msgid "Tena kho pana samayena bhikkhū kuṭṭepi thambhepi kathinaṃ ussāpetvā pakkamanti."
msgstr ""

msgctxt "pli-tv-kd15:11.7.9"
msgid "Paripatitvā kathinaṃ bhijjati."
msgstr ""

msgctxt "pli-tv-kd15:11.7.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:11.7.11"
msgid "“Anujānāmi, bhikkhave, bhittikhile vā nāgadante vā laggetun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc75, pts-vp-pli2.118
msgctxt "pli-tv-kd15:12.1.1"
msgid "Atha kho bhagavā rājagahe yathābhirantaṃ viharitvā yena vesālī tena cārikaṃ pakkāmi."
msgstr ""

msgctxt "pli-tv-kd15:12.1.2"
msgid "Tena kho pana samayena bhikkhū sūcikampi satthakampi bhesajjampi pattena ādāya gacchanti."
msgstr ""

msgctxt "pli-tv-kd15:12.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:12.1.4"
msgid "“Anujānāmi, bhikkhave, bhesajjatthavikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:12.1.5"
msgid "Aṃsabaddhako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:12.1.6"
msgid "“anujānāmi, bhikkhave, aṃsabaddhakaṃ bandhanasuttakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-kd15:12.1.7"
msgid "Tena kho pana samayena aññataro bhikkhu upāhanāyo kāyabandhanena bandhitvā gāmaṃ piṇḍāya pāvisi."
msgstr ""

msgctxt "pli-tv-kd15:12.1.8"
msgid "Aññataro upāsako taṃ bhikkhuṃ abhivādento upāhanāyo sīsena ghaṭṭeti."
msgstr ""

msgctxt "pli-tv-kd15:12.1.9"
msgid "So bhikkhu maṅku ahosi."
msgstr ""

msgctxt "pli-tv-kd15:12.1.10"
msgid "Atha kho so bhikkhu ārāmaṃ gantvā bhikkhūnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-kd15:12.1.11"
msgid "Bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:12.1.12"
msgid "“Anujānāmi, bhikkhave, upāhanatthavikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:12.1.13"
msgid "Aṃsabaddhako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:12.1.14"
msgid "“anujānāmi, bhikkhave, aṃsabaddhakaṃ bandhanasuttakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-kd15:13.1.1"
msgid "Tena kho pana samayena antarāmagge udakaṃ akappiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:13.1.2"
msgid "Parissāvanaṃ na hoti."
msgstr ""

msgctxt "pli-tv-kd15:13.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.1.4"
msgid "“Anujānāmi, bhikkhave, parissāvanan”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.1.5"
msgid "Coḷakaṃ nappahoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:13.1.6"
msgid "“anujānāmi, bhikkhave, kaṭacchuparissāvanan”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.1.7"
msgid "Coḷakaṃ nappahoti … pe …"
msgstr ""

#. VAR: dhammakaraṇan”ti → dhammakarakanti (bj, s1-3, pts1)
msgctxt "pli-tv-kd15:13.1.8"
msgid "“anujānāmi, bhikkhave, dhammakaraṇan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc78, pts-vp-pli2.119
msgctxt "pli-tv-kd15:13.2.1"
msgid "Tena kho pana samayena dve bhikkhū kosalesu janapade addhānamaggappaṭipannā honti."
msgstr ""

msgctxt "pli-tv-kd15:13.2.2"
msgid "Eko bhikkhu anācāraṃ ācarati."
msgstr ""

msgctxt "pli-tv-kd15:13.2.3"
msgid "Dutiyo bhikkhu taṃ bhikkhuṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:13.2.4"
msgid "“mā, āvuso, evarūpaṃ akāsi."
msgstr ""

msgctxt "pli-tv-kd15:13.2.5"
msgid "Netaṃ kappatī”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.2.6"
msgid "So tasmiṃ upanandhi."
msgstr ""

msgctxt "pli-tv-kd15:13.2.7"
msgid "Atha kho so bhikkhu pipāsāya pīḷito upanaddhaṃ bhikkhuṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:13.2.8"
msgid "“dehi me, āvuso, parissāvanaṃ, pānīyaṃ pivissāmī”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.2.9"
msgid "Upanaddho bhikkhu na adāsi."
msgstr ""

msgctxt "pli-tv-kd15:13.2.10"
msgid "So bhikkhu pipāsāya pīḷito kālamakāsi."
msgstr ""

msgctxt "pli-tv-kd15:13.2.11"
msgid "Atha kho so bhikkhu ārāmaṃ gantvā bhikkhūnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-kd15:13.2.12"
msgid "“Kiṃ pana tvaṃ, āvuso, parissāvanaṃ yāciyamāno na adāsī”ti?"
msgstr ""

msgctxt "pli-tv-kd15:13.2.13"
msgid "“Evamāvuso”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.2.14"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:13.2.15"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:13.2.16"
msgid "“kathañhi nāma bhikkhu parissāvanaṃ yāciyamāno na dassatī”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.2.17"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.2.18"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā taṃ bhikkhuṃ paṭipucchi—"
msgstr ""

msgctxt "pli-tv-kd15:13.2.19"
msgid "“saccaṃ kira tvaṃ, bhikkhu, parissāvanaṃ yāciyamāno na adāsī”ti?"
msgstr ""

msgctxt "pli-tv-kd15:13.2.20"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.2.21"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-kd15:13.2.22"
msgid "“ananucchavikaṃ, moghapurisa, ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.2.23"
msgid "Kathañhi nāma tvaṃ, moghapurisa, parissāvanaṃ yāciyamāno na dassasi."
msgstr ""

msgctxt "pli-tv-kd15:13.2.24"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-kd15:13.2.25"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:13.2.26"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:13.2.27"
msgid "“na, bhikkhave, addhānamaggappaṭipannena bhikkhunā parissāvanaṃ yāciyamānena na dātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.2.28"
msgid "Yo na dadeyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:13.2.29"
msgid "Na ca, bhikkhave, aparissāvanakena addhāno paṭipajjitabbo."
msgstr ""

msgctxt "pli-tv-kd15:13.2.30"
msgid "Yo paṭipajjeyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:13.2.31"
msgid "Sace na hoti parissāvanaṃ vā dhammakaraṇo vā, saṅghāṭikaṇṇopi adhiṭṭhātabbo—"
msgstr ""

msgctxt "pli-tv-kd15:13.2.32"
msgid "iminā parissāvetvā pivissāmī”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.3.1"
msgid "Atha kho bhagavā anupubbena cārikaṃ caramāno yena vesālī tadavasari."
msgstr ""

msgctxt "pli-tv-kd15:13.3.2"
msgid "Tatra sudaṃ bhagavā vesāliyaṃ viharati mahāvane kūṭāgārasālāyaṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.3.3"
msgid "Tena kho pana samayena bhikkhū navakammaṃ karonti."
msgstr ""

msgctxt "pli-tv-kd15:13.3.4"
msgid "Parissāvanaṃ na sammati."
msgstr ""

msgctxt "pli-tv-kd15:13.3.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.3.6"
msgid "“Anujānāmi, bhikkhave, daṇḍaparissāvanan”ti."
msgstr ""

msgctxt "pli-tv-kd15:13.3.7"
msgid "Daṇḍaparissāvanaṃ na sammati."
msgstr ""

msgctxt "pli-tv-kd15:13.3.8"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.3.9"
msgid "“Anujānāmi, bhikkhave, ottharakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc79
msgctxt "pli-tv-kd15:13.3.10"
msgid "Tena kho pana samayena bhikkhū makasehi ubbāḷhā honti."
msgstr ""

msgctxt "pli-tv-kd15:13.3.11"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:13.3.12"
msgid "“Anujānāmi, bhikkhave, makasakuṭikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc80
msgctxt "pli-tv-kd15:14.1.1"
msgid "Tena kho pana samayena vesāliyaṃ paṇītānaṃ bhattānaṃ bhattapaṭipāṭi aṭṭhitā hoti."
msgstr ""

msgctxt "pli-tv-kd15:14.1.2"
msgid "Bhikkhū paṇītāni bhojanāni bhuñjitvā abhisannakāyā honti bahvābādhā."
msgstr ""

msgctxt "pli-tv-kd15:14.1.3"
msgid "Atha kho jīvako komārabhacco vesāliṃ agamāsi kenacideva karaṇīyena."
msgstr ""

msgctxt "pli-tv-kd15:14.1.4"
msgid "Addasā kho jīvako komārabhacco bhikkhū abhisannakāye bahvābādhe."
msgstr ""

msgctxt "pli-tv-kd15:14.1.5"
msgid "Disvāna yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd15:14.1.6"
msgid "Ekamantaṃ nisinno kho jīvako komārabhacco bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:14.1.7"
msgid "“etarahi, bhante, bhikkhū abhisannakāyā bahvābādhā."
msgstr ""

msgctxt "pli-tv-kd15:14.1.8"
msgid "Sādhu, bhante, bhagavā bhikkhūnaṃ caṅkamañca jantāgharañca anujānātu."
msgstr ""

msgctxt "pli-tv-kd15:14.1.9"
msgid "Evaṃ bhikkhū appābādhā bhavissantī”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.1.10"
msgid "Atha kho bhagavā jīvakaṃ komārabhaccaṃ dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṃsesi."
msgstr ""

msgctxt "pli-tv-kd15:14.1.11"
msgid "Atha kho jīvako komārabhacco bhagavatā dhammiyā kathāya sandassito samādapito samuttejito sampahaṃsito uṭṭhāyāsanā bhagavantaṃ abhivādetvā padakkhiṇaṃ katvā pakkāmi."
msgstr ""

msgctxt "pli-tv-kd15:14.1.12"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:14.1.13"
msgid "“anujānāmi, bhikkhave, caṅkamañca jantāgharañcā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc81, pts-vp-pli2.120
msgctxt "pli-tv-kd15:14.2.1"
msgid "Tena kho pana samayena bhikkhū visame caṅkame caṅkamanti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.2"
msgid "Pādā dukkhā honti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:14.2.4"
msgid "“Anujānāmi, bhikkhave, samaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.5"
msgid "Caṅkamo nīcavatthuko hoti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.6"
msgid "Udakena otthariyyati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.7"
msgid "“anujānāmi, bhikkhave, uccavatthukaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.8"
msgid "Cayo paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.9"
msgid "“anujānāmi, bhikkhave, cinituṃ tayo caye—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.10"
msgid "iṭṭhakācayaṃ, silācayaṃ, dārucayan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.11"
msgid "Ārohantā vihaññanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.12"
msgid "“anujānāmi, bhikkhave, tayo sopāne—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.13"
msgid "iṭṭhakāsopānaṃ, silāsopānaṃ, dārusopānan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.14"
msgid "Ārohantā paripatanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.15"
msgid "“anujānāmi, bhikkhave, ālambanabāhan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc82
msgctxt "pli-tv-kd15:14.2.16"
msgid "Tena kho pana samayena bhikkhū caṅkame caṅkamantā paripatanti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.17"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:14.2.18"
msgid "“Anujānāmi, bhikkhave, caṅkamanavedikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-kd15:14.2.19"
msgid "Tena kho pana samayena bhikkhū ajjhokāse caṅkamantā sītenapi uṇhenapi kilamanti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.20"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:14.2.21"
msgid "“Anujānāmi, bhikkhave, caṅkamanasālan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.22"
msgid "Caṅkamanasālāyaṃ tiṇacuṇṇaṃ paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.23"
msgid "“anujānāmi, bhikkhave, ogumphetvā ullittāvalittaṃ kātuṃ—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.24"
msgid "setavaṇṇaṃ kāḷavaṇṇaṃ gerukaparikammaṃ mālākammaṃ latākammaṃ makaradantakaṃ pañcapaṭikaṃ cīvaravaṃsaṃ cīvararajjun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.25"
msgid "Jantāgharaṃ nīcavatthukaṃ hoti, udakena otthariyyati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.26"
msgid "“anujānāmi, bhikkhave, uccavatthukaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.27"
msgid "Cayo paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.28"
msgid "“anujānāmi, bhikkhave, cinituṃ tayo caye—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.29"
msgid "iṭṭhakācayaṃ, silācayaṃ, dārucayan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.30"
msgid "Ārohantā vihaññanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.31"
msgid "“anujānāmi, bhikkhave, tayo sopāne—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.32"
msgid "iṭṭhakāsopānaṃ, silāsopānaṃ, dārusopānan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.33"
msgid "Ārohantā paripatanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.34"
msgid "“anujānāmi, bhikkhave, ālambanabāhan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.35"
msgid "Jantāgharassa kavāṭaṃ na hoti … pe …"
msgstr ""

#. VAR: piṭṭhasaṅghāṭaṃ → piṭṭhasaṃghāṭaṃ (pts1) | pīṭhasaṃghāṭaṃ (mr)
#. VAR: aggaḷavaṭṭiṃ → aggalavaṭṭiṃ (pts1) | aggaḷavaṭikaṃ (c1)
msgctxt "pli-tv-kd15:14.2.36"
msgid "“anujānāmi, bhikkhave, kavāṭaṃ piṭṭhasaṅghāṭaṃ udukkhalikaṃ uttarapāsakaṃ aggaḷavaṭṭiṃ kapisīsakaṃ sūcikaṃ ghaṭikaṃ tāḷacchiddaṃ āviñchanachiddaṃ āviñchanarajjun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.37"
msgid "Jantāgharassa kuṭṭapādo jīrati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.38"
msgid "“anujānāmi, bhikkhave, maṇḍalikaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.39"
msgid "Jantāgharassa dhūmanettaṃ na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.40"
msgid "“anujānāmi, bhikkhave, dhūmanettan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc84
msgctxt "pli-tv-kd15:14.2.41"
msgid "Tena kho pana samayena bhikkhū khuddake jantāghare majjhe aggiṭṭhānaṃ karonti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.42"
msgid "Upacāro na hoti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.43"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:14.2.44"
msgid "“Anujānāmi, bhikkhave, khuddake jantāghare ekamantaṃ aggiṭṭhānaṃ kātuṃ, mahallake majjhe”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.45"
msgid "Jantāghare aggi mukhaṃ ḍahati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.46"
msgid "“anujānāmi, bhikkhave, mukhamattikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.47"
msgid "Hatthe mattikaṃ tementi … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.48"
msgid "“anujānāmi, bhikkhave, mattikādoṇikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.49"
msgid "Mattikā duggandhā hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.50"
msgid "“anujānāmi, bhikkhave, vāsetun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.51"
msgid "Jantāghare aggi kāyaṃ ḍahati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.52"
msgid "“anujānāmi, bhikkhave, udakaṃ atiharitun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.53"
msgid "Pātiyāpi pattenapi udakaṃ atiharanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.54"
msgid "“anujānāmi, bhikkhave, udakaṭṭhānaṃ, udakasarāvakan”ti."
msgstr ""

#. VAR: tiṇacchadanaṃ na sedeti → tiṇacchadanaṃ sādeti (bj) | tiṇacchādanena chādeti (mr)
msgctxt "pli-tv-kd15:14.2.55"
msgid "Jantāgharaṃ tiṇacchadanaṃ na sedeti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.56"
msgid "“anujānāmi, bhikkhave, ogumphetvā ullittāvalittaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.57"
msgid "Jantāgharaṃ cikkhallaṃ hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.58"
msgid "“anujānāmi, bhikkhave, santharituṃ tayo santhare—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.59"
msgid "iṭṭhakāsantharaṃ, silāsantharaṃ, dārusantharan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.60"
msgid "Cikkhallaṃyeva hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.61"
msgid "“anujānāmi, bhikkhave, dhovitun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.62"
msgid "Udakaṃ santiṭṭhati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.63"
msgid "“anujānāmi, bhikkhave, udakaniddhamanan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc85, pts-vp-pli2.121
msgctxt "pli-tv-kd15:14.2.64"
msgid "Tena kho pana samayena bhikkhū jantāghare chamāya nisīdanti, gattāni kaṇḍūvanti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.65"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:14.2.66"
msgid "“Anujānāmi, bhikkhave, jantāgharapīṭhan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.67"
msgid "Tena kho pana samayena jantāgharaṃ aparikkhittaṃ hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.68"
msgid "“anujānāmi, bhikkhave, parikkhipituṃ tayo pākāre—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.69"
msgid "iṭṭhakāpākāraṃ, silāpākāraṃ, dārupākāran”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.70"
msgid "Koṭṭhako na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.71"
msgid "“anujānāmi, bhikkhave, koṭṭhakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.72"
msgid "Koṭṭhako nīcavatthuko hoti, udakena otthariyyati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.73"
msgid "“anujānāmi, bhikkhave, uccavatthukaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.74"
msgid "Cayo paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.75"
msgid "“anujānāmi, bhikkhave, cinituṃ tayo caye—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.76"
msgid "iṭṭhakācayaṃ, silācayaṃ, dārucayan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.77"
msgid "Ārohantā vihaññanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.78"
msgid "“anujānāmi, bhikkhave, tayo sopāne—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.79"
msgid "iṭṭhakāsopānaṃ, silāsopānaṃ, dārusopānan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.80"
msgid "Ārohantā paripatanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.81"
msgid "“anujānāmi, bhikkhave, ālambanabāhan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.82"
msgid "Koṭṭhakassa kavāṭaṃ na hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.83"
msgid "“anujānāmi, bhikkhave, kavāṭaṃ piṭṭhasaṅghāṭaṃ udukkhalikaṃ uttarapāsakaṃ aggaḷavaṭṭiṃ kapisīsakaṃ sūcikaṃ ghaṭikaṃ tāḷacchiddaṃ āviñchanachiddaṃ āviñchanarajjun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.84"
msgid "Koṭṭhake tiṇacuṇṇaṃ paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.85"
msgid "“anujānāmi, bhikkhave, ogumphetvā ullittāvalittaṃ kātuṃ—"
msgstr ""

msgctxt "pli-tv-kd15:14.2.86"
msgid "setavaṇṇaṃ kāḷavaṇṇaṃ gerukaparikammaṃ mālākammaṃ latākammaṃ makaradantakaṃ pañcapaṭikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.87"
msgid "Pariveṇaṃ cikkhallaṃ hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.88"
msgid "“anujānāmi, bhikkhave, marumbaṃ upakiritun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.89"
msgid "Na pariyāpuṇanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.90"
msgid "“anujānāmi, bhikkhave, padarasilaṃ nikkhipitun”ti."
msgstr ""

msgctxt "pli-tv-kd15:14.2.91"
msgid "Udakaṃ santiṭṭhati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:14.2.92"
msgid "“anujānāmi, bhikkhave, udakaniddhamanan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc86
msgctxt "pli-tv-kd15:15.1.1"
msgid "Tena kho pana samayena bhikkhū naggā naggaṃ abhivādenti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.2"
msgid "naggā naggaṃ abhivādāpenti, naggā naggassa parikammaṃ karonti, naggā naggassa parikammaṃ kārāpenti, naggā naggassa denti, naggā paṭiggaṇhanti, naggā khādanti, naggā bhuñjanti, naggā sāyanti, naggā pivanti."
msgstr ""

msgctxt "pli-tv-kd15:15.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. VAR: naggena → idaṃ padaṃ pts1, mr potthakesu
msgctxt "pli-tv-kd15:15.1.4"
msgid "“Na, bhikkhave, naggena naggo abhivādetabbo … pe …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.5"
msgid "na naggena abhivādetabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.6"
msgid "na naggena naggo abhivādāpetabbo …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.7"
msgid "na naggena abhivādāpetabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.8"
msgid "na naggena naggassa parikammaṃ kātabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.9"
msgid "na naggena naggassa parikammaṃ kārāpetabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.10"
msgid "na naggena naggassa dātabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.11"
msgid "na naggena paṭiggahetabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.12"
msgid "na naggena khāditabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.13"
msgid "na naggena bhuñjitabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.14"
msgid "na naggena sāyitabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:15.1.15"
msgid "na naggena pātabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:15.1.16"
msgid "Yo piveyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc87, pts-vp-pli2.122
msgctxt "pli-tv-kd15:16.1.1"
msgid "Tena kho pana samayena bhikkhū jantāghare chamāya cīvaraṃ nikkhipanti."
msgstr ""

msgctxt "pli-tv-kd15:16.1.2"
msgid "Cīvaraṃ paṃsukitaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:16.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:16.1.4"
msgid "“Anujānāmi, bhikkhave, cīvaravaṃsaṃ cīvararajjun”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.1.5"
msgid "Deve vassante cīvaraṃ ovassati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.6"
msgid "“anujānāmi, bhikkhave, jantāgharasālan”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.1.7"
msgid "Jantāgharasālā nīcavatthukā hoti, udakena otthariyyati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.8"
msgid "“anujānāmi, bhikkhave, uccavatthukaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.1.9"
msgid "Cayo paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.10"
msgid "“anujānāmi, bhikkhave, cinituṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.11"
msgid "ārohantā vihaññanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.12"
msgid "ārohantā paripatanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.13"
msgid "“anujānāmi, bhikkhave, ālambanabāhan”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.1.14"
msgid "Jantāgharasālāya tiṇacuṇṇaṃ paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.15"
msgid "“anujānāmi, bhikkhave, ogumphetvā ullittāvalittaṃ kātuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.1.16"
msgid "cīvaravaṃsaṃ cīvararajjun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc88
msgctxt "pli-tv-kd15:16.2.1"
msgid "Tena kho pana samayena bhikkhū jantāgharepi udakepi parikammaṃ kātuṃ kukkuccāyanti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:16.2.3"
msgid "“Anujānāmi, bhikkhave, tisso paṭicchādiyo—"
msgstr ""

msgctxt "pli-tv-kd15:16.2.4"
msgid "jantāgharapaṭicchādiṃ, udakapaṭicchādiṃ, vatthapaṭicchādin”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc89
msgctxt "pli-tv-kd15:16.2.5"
msgid "Tena kho pana samayena jantāghare udakaṃ na hoti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.6"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:16.2.7"
msgid "“Anujānāmi, bhikkhave, udapānan”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.8"
msgid "Udapānassa kūlaṃ lujjati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.9"
msgid "“anujānāmi, bhikkhave, cinituṃ tayo caye—"
msgstr ""

msgctxt "pli-tv-kd15:16.2.10"
msgid "iṭṭhakācayaṃ, silācayaṃ, dārucayan”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.11"
msgid "Udapāno nīcavatthuko hoti, udakena otthariyyati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.12"
msgid "“anujānāmi, bhikkhave, uccavatthukaṃ kātun”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.13"
msgid "Cayo paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.14"
msgid "ārohantā vihaññanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.15"
msgid "ārohantā paripatanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.16"
msgid "“anujānāmi, bhikkhave, ālambanabāhan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc90
#. VAR: vāhenti → vāhanti (pts1, mr)
msgctxt "pli-tv-kd15:16.2.17"
msgid "Tena kho pana samayena bhikkhū vallikāyapi kāyabandhanenapi udakaṃ vāhenti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.18"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:16.2.19"
msgid "“Anujānāmi, bhikkhave, udakavāhanarajjun”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.20"
msgid "Hatthā dukkhā honti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.21"
msgid "“anujānāmi, bhikkhave, tulaṃ karakaṭakaṃ cakkavaṭṭakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.22"
msgid "Bhājanā bahū bhijjanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.23"
msgid "“anujānāmi, bhikkhave, tayo vārake—"
msgstr ""

msgctxt "pli-tv-kd15:16.2.24"
msgid "lohavārakaṃ, dāruvārakaṃ, cammakkhaṇḍan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc91
msgctxt "pli-tv-kd15:16.2.25"
msgid "Tena kho pana samayena bhikkhū ajjhokāse udakaṃ vāhentā sītenapi uṇhenapi kilamanti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.26"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:16.2.27"
msgid "“Anujānāmi, bhikkhave, udapānasālan”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.28"
msgid "Udapānasālāya tiṇacuṇṇaṃ paripatati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.29"
msgid "“anujānāmi, bhikkhave, ogumphetvā ullittāvalittaṃ kātuṃ—"
msgstr ""

msgctxt "pli-tv-kd15:16.2.30"
msgid "setavaṇṇaṃ kāḷavaṇṇaṃ gerukaparikammaṃ mālākammaṃ latākammaṃ makaradantakaṃ pañcapaṭikaṃ cīvaravaṃsaṃ cīvararajjun”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.31"
msgid "Udapāno apāruto hoti, tiṇacuṇṇehipi paṃsukehipi okiriyyati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.32"
msgid "“anujānāmi, bhikkhave, apidhānan”ti."
msgstr ""

msgctxt "pli-tv-kd15:16.2.33"
msgid "Udakabhājanaṃ na saṃvijjati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:16.2.34"
msgid "“anujānāmi, bhikkhave, udakadoṇiṃ udakakaṭāhan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc92
msgctxt "pli-tv-kd15:17.1.1"
msgid "Tena kho pana samayena bhikkhū ārāme tahaṃ tahaṃ nahāyanti."
msgstr ""

msgctxt "pli-tv-kd15:17.1.2"
msgid "Ārāmo cikkhallo hoti."
msgstr ""

msgctxt "pli-tv-kd15:17.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:17.1.4"
msgid "“Anujānāmi, bhikkhave, candanikan”ti."
msgstr ""

msgctxt "pli-tv-kd15:17.1.5"
msgid "Candanikā pākaṭā hoti."
msgstr ""

msgctxt "pli-tv-kd15:17.1.6"
msgid "Bhikkhū hiriyanti nahāyituṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.1.7"
msgid "“anujānāmi, bhikkhave, parikkhipituṃ tayo pākāre—"
msgstr ""

msgctxt "pli-tv-kd15:17.1.8"
msgid "iṭṭhakāpākāraṃ, silāpākāraṃ, dārupākāran”ti."
msgstr ""

msgctxt "pli-tv-kd15:17.1.9"
msgid "Candanikā cikkhallā hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.1.10"
msgid "“anujānāmi, bhikkhave, santharituṃ tayo santhare—"
msgstr ""

msgctxt "pli-tv-kd15:17.1.11"
msgid "iṭṭhakāsantharaṃ, silāsantharaṃ, dārusantharan”ti."
msgstr ""

msgctxt "pli-tv-kd15:17.1.12"
msgid "Udakaṃ santiṭṭhati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.1.13"
msgid "“anujānāmi, bhikkhave, udakaniddhamanan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc93
msgctxt "pli-tv-kd15:17.1.14"
msgid "Tena kho pana samayena bhikkhūnaṃ gattāni sītigatāni honti."
msgstr ""

msgctxt "pli-tv-kd15:17.1.15"
msgid "Bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.1.16"
msgid "“anujānāmi, bhikkhave, udakapuñchaniṃ coḷakenapi paccuddharitun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc94, pts-vp-pli2.123
msgctxt "pli-tv-kd15:17.2.1"
msgid "Tena kho pana samayena aññataro upāsako saṃghassa atthāya pokkharaṇiṃ kāretukāmo hoti."
msgstr ""

msgctxt "pli-tv-kd15:17.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.2.3"
msgid "“anujānāmi, bhikkhave, pokkharaṇin”ti."
msgstr ""

msgctxt "pli-tv-kd15:17.2.4"
msgid "Pokkharaṇiyā kūlaṃ lujjati … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.2.5"
msgid "“anujānāmi, bhikkhave, cinituṃ tayo caye—"
msgstr ""

msgctxt "pli-tv-kd15:17.2.6"
msgid "iṭṭhakācayaṃ, silācayaṃ, dārucayan”ti."
msgstr ""

msgctxt "pli-tv-kd15:17.2.7"
msgid "Ārohantā vihaññanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.2.8"
msgid "“anujānāmi, bhikkhave, tayo sopāne—"
msgstr ""

msgctxt "pli-tv-kd15:17.2.9"
msgid "iṭṭhakāsopānaṃ, silāsopānaṃ, dārusopānan”ti."
msgstr ""

msgctxt "pli-tv-kd15:17.2.10"
msgid "Ārohantā paripatanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.2.11"
msgid "“anujānāmi, bhikkhave, ālambanabāhan”ti."
msgstr ""

msgctxt "pli-tv-kd15:17.2.12"
msgid "Pokkharaṇiyā udakaṃ purāṇaṃ hoti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.2.13"
msgid "“anujānāmi, bhikkhave, udakamātikaṃ udakaniddhamanan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc95
msgctxt "pli-tv-kd15:17.2.14"
msgid "Tena kho pana samayena aññataro bhikkhu saṃghassa atthāya nillekhaṃ jantāgharaṃ kattukāmo hoti."
msgstr ""

msgctxt "pli-tv-kd15:17.2.15"
msgid "Bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:17.2.16"
msgid "“anujānāmi, bhikkhave, nillekhaṃ jantāgharan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc96
msgctxt "pli-tv-kd15:18.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū cātumāsaṃ nisīdanena vippavasanti."
msgstr ""

msgctxt "pli-tv-kd15:18.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:18.1.3"
msgid "“na, bhikkhave, cātumāsaṃ nisīdanena vippavasitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:18.1.4"
msgid "Yo vippavaseyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc97
msgctxt "pli-tv-kd15:18.1.5"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū pupphābhikiṇṇesu sayanesu sayanti."
msgstr ""

msgctxt "pli-tv-kd15:18.1.6"
msgid "Manussā vihāracārikaṃ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:18.1.7"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:18.1.8"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:18.1.9"
msgid "“Na, bhikkhave, pupphābhikiṇṇesu sayanesu sayitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:18.1.10"
msgid "Yo sayeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc98
msgctxt "pli-tv-kd15:18.1.11"
msgid "Tena kho pana samayena manussā gandhampi mālampi ādāya ārāmaṃ āgacchanti."
msgstr ""

msgctxt "pli-tv-kd15:18.1.12"
msgid "Bhikkhū kukkuccāyantā na paṭiggaṇhanti."
msgstr ""

msgctxt "pli-tv-kd15:18.1.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:18.1.14"
msgid "“Anujānāmi, bhikkhave, gandhaṃ gahetvā kavāṭe pañcaṅgulikaṃ dātuṃ, pupphaṃ gahetvā vihāre ekamantaṃ nikkhipitun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc99
msgctxt "pli-tv-kd15:19.1.1"
msgid "Tena kho pana samayena saṃghassa namatakaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:19.1.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:19.1.3"
msgid "“Anujānāmi, bhikkhave, namatakan”ti."
msgstr ""

msgctxt "pli-tv-kd15:19.1.4"
msgid "Atha kho bhikkhūnaṃ etadahosi—"
msgstr ""

msgctxt "pli-tv-kd15:19.1.5"
msgid "“namatakaṃ adhiṭṭhātabbaṃ nu kho udāhu vikappetabban”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:19.1.6"
msgid "“na, bhikkhave, namatakaṃ adhiṭṭhātabbaṃ, na vikappetabban”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc100, pts-vp-pli2.124
msgctxt "pli-tv-kd15:19.1.7"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū āsittakūpadhāne bhuñjanti."
msgstr ""

msgctxt "pli-tv-kd15:19.1.8"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:19.1.9"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:19.1.10"
msgid "Bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:19.1.11"
msgid "“na, bhikkhave, āsittakūpadhāne bhuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:19.1.12"
msgid "Yo bhuñjeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc101
msgctxt "pli-tv-kd15:19.1.13"
msgid "Tena kho pana samayena aññataro bhikkhu gilāno hoti."
msgstr ""

msgctxt "pli-tv-kd15:19.1.14"
msgid "So bhuñjamāno na sakkoti hatthena pattaṃ sandhāretuṃ."
msgstr ""

msgctxt "pli-tv-kd15:19.1.15"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:19.1.16"
msgid "“Anujānāmi, bhikkhave, maḷorikan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc102
msgctxt "pli-tv-kd15:19.2.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū ekabhājanepi bhuñjanti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:19.2.2"
msgid "ekathālakepi pivanti, ekamañcepi tuvaṭṭenti, ekattharaṇāpi tuvaṭṭenti, ekapāvuraṇāpi tuvaṭṭenti, ekattharaṇapāvuraṇāpi tuvaṭṭenti."
msgstr ""

msgctxt "pli-tv-kd15:19.2.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:19.2.4"
msgid "“seyyathāpi gihī kāmabhogino”ti."
msgstr ""

msgctxt "pli-tv-kd15:19.2.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:19.2.6"
msgid "“Na, bhikkhave, ekabhājane bhuñjitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:19.2.7"
msgid "na ekathālake pātabbaṃ …"
msgstr ""

msgctxt "pli-tv-kd15:19.2.8"
msgid "na ekamañce tuvaṭṭitabbaṃ …"
msgstr ""

#. VAR: na ekattharaṇā → na ekattharaṇe (s1-3)
msgctxt "pli-tv-kd15:19.2.9"
msgid "na ekattharaṇā tuvaṭṭitabbaṃ …"
msgstr ""

#. VAR: na ekapāvuraṇā → na ekapāvuraṇe (s1-3)
msgctxt "pli-tv-kd15:19.2.10"
msgid "na ekapāvuraṇā tuvaṭṭitabbaṃ …"
msgstr ""

#. VAR: na ekattharaṇapāvuraṇā → na ekattharaṇapāvuraṇe (s1-3)
msgctxt "pli-tv-kd15:19.2.11"
msgid "na ekattharaṇapāvuraṇā tuvaṭṭitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:19.2.12"
msgid "Yo tuvaṭṭeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc103
msgctxt "pli-tv-kd15:20.1.1"
msgid "Tena kho pana samayena vaḍḍho licchavī mettiyabhūmajakānaṃ bhikkhūnaṃ sahāyo hoti."
msgstr ""

msgctxt "pli-tv-kd15:20.1.2"
msgid "Atha kho vaḍḍho licchavī yena mettiyabhūmajakā bhikkhū tenupasaṅkami, upasaṅkamitvā mettiyabhūmajake bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:20.1.3"
msgid "“vandāmi, ayyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.1.4"
msgid "Evaṃ vutte, mettiyabhūmajakā bhikkhū nālapiṃsu."
msgstr ""

msgctxt "pli-tv-kd15:20.1.5"
msgid "Dutiyampi kho vaḍḍho licchavī mettiyabhūmajake bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:20.1.6"
msgid "“vandāmi, ayyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.1.7"
msgid "Dutiyampi kho mettiyabhūmajakā bhikkhū nālapiṃsu."
msgstr ""

msgctxt "pli-tv-kd15:20.1.8"
msgid "Tatiyampi kho vaḍḍho licchavī mettiyabhūmajake bhikkhū etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:20.1.9"
msgid "“vandāmi, ayyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.1.10"
msgid "Tatiyampi kho mettiyabhūmajakā bhikkhū nālapiṃsu."
msgstr ""

msgctxt "pli-tv-kd15:20.1.11"
msgid "“Kyāhaṃ ayyānaṃ aparajjhāmi, kissa maṃ ayyā nālapantī”ti?"
msgstr ""

msgctxt "pli-tv-kd15:20.1.12"
msgid "“Tathā hi pana tvaṃ, āvuso vaḍḍha, amhe dabbena mallaputtena viheṭhiyamāne ajjhupekkhasī”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.1.13"
msgid "“Kyāhaṃ, ayyā, karomī”ti?"
msgstr ""

msgctxt "pli-tv-kd15:20.1.14"
msgid "“Sace kho tvaṃ, āvuso vaḍḍha, iccheyyāsi, ajjeva bhagavā āyasmantaṃ dabbaṃ mallaputtaṃ nāsāpeyyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.1.15"
msgid "“Kyāhaṃ, ayyā, karomi, kiṃ mayā sakkā kātun”ti?"
msgstr ""

msgctxt "pli-tv-kd15:20.1.16"
msgid "“Ehi tvaṃ, āvuso vaḍḍha, yena bhagavā tenupasaṅkama, upasaṅkamitvā bhagavantaṃ evaṃ vadehi—‘"
msgstr ""

msgctxt "pli-tv-kd15:20.1.17"
msgid "idaṃ, bhante, nacchannaṃ nappatirūpaṃ."
msgstr ""

msgctxt "pli-tv-kd15:20.1.18"
msgid "Yāyaṃ, bhante, disā abhayā anītikā anupaddavā sāyaṃ disā sabhayā saītikā saupaddavā;"
msgstr ""

msgctxt "pli-tv-kd15:20.1.19"
msgid "yato nivātaṃ tato savātaṃ;"
msgstr ""

msgctxt "pli-tv-kd15:20.1.20"
msgid "udakaṃ maññe ādittaṃ;"
msgstr ""

msgctxt "pli-tv-kd15:20.1.21"
msgid "ayyena me dabbena mallaputtena pajāpati dūsitā’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc104, pts-vp-pli2.125
msgctxt "pli-tv-kd15:20.2.1"
msgid "“Evaṃ, ayyā”ti kho vaḍḍho licchavī mettiyabhūmajakānaṃ bhikkhūnaṃ paṭissutvā yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd15:20.2.2"
msgid "Ekamantaṃ nisinno kho vaḍḍho licchavī bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:20.2.3"
msgid "“idaṃ, bhante, nacchannaṃ nappatirūpaṃ."
msgstr ""

msgctxt "pli-tv-kd15:20.2.4"
msgid "Yāyaṃ, bhante, disā abhayā anītikā anupaddavā sāyaṃ disā sabhayā saītikā saupaddavā;"
msgstr ""

msgctxt "pli-tv-kd15:20.2.5"
msgid "yato nivātaṃ tato savātaṃ;"
msgstr ""

msgctxt "pli-tv-kd15:20.2.6"
msgid "udakaṃ maññe ādittaṃ;"
msgstr ""

msgctxt "pli-tv-kd15:20.2.7"
msgid "ayyena me dabbena mallaputtena pajāpati dūsitā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc105
msgctxt "pli-tv-kd15:20.2.8"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ dabbaṃ mallaputtaṃ paṭipucchi—"
msgstr ""

msgctxt "pli-tv-kd15:20.2.9"
msgid "“sarasi tvaṃ, dabba, evarūpaṃ kattā yathāyaṃ vaḍḍho āhā”ti?"
msgstr ""

msgctxt "pli-tv-kd15:20.2.10"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.2.11"
msgid "Dutiyampi kho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:20.2.12"
msgid "tatiyampi kho bhagavā āyasmantaṃ dabbaṃ mallaputtaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:20.2.13"
msgid "“sarasi tvaṃ, dabba, evarūpaṃ kattā yathāyaṃ vaḍḍho āhā”ti?"
msgstr ""

msgctxt "pli-tv-kd15:20.2.14"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.2.15"
msgid "“Na kho, dabba, dabbā evaṃ nibbeṭhenti."
msgstr ""

msgctxt "pli-tv-kd15:20.2.16"
msgid "Sace tayā kataṃ, katanti vadehi;"
msgstr ""

msgctxt "pli-tv-kd15:20.2.17"
msgid "sace akataṃ, akatanti vadehī”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.2.18"
msgid "“Yato ahaṃ, bhante, jāto nābhijānāmi supinantenapi methunaṃ dhammaṃ paṭisevitā, pageva jāgaro”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.3.1"
msgid "Atha kho bhagavā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:20.3.2"
msgid "“tena hi, bhikkhave, saṃgho vaḍḍhassa licchavissa pattaṃ nikkujjatu, asambhogaṃ saṃghena karotu."
msgstr ""

#. HTML: </p><p>
#. REF: sc106
msgctxt "pli-tv-kd15:20.3.3"
msgid "Aṭṭhahi, bhikkhave, aṅgehi samannāgatassa upāsakassa patto nikkujjitabbo—"
msgstr ""

#. VAR: avāsāya → anāvāsāya (s1-3)
msgctxt "pli-tv-kd15:20.3.4"
msgid "bhikkhūnaṃ alābhāya parisakkati, bhikkhūnaṃ anatthāya parisakkati, bhikkhūnaṃ avāsāya parisakkati, bhikkhū akkosati paribhāsati, bhikkhū bhikkhūhi bhedeti, buddhassa avaṇṇaṃ bhāsati, dhammassa avaṇṇaṃ bhāsati, saṃghassa avaṇṇaṃ bhāsati."
msgstr ""

msgctxt "pli-tv-kd15:20.3.5"
msgid "Anujānāmi, bhikkhave, imehi aṭṭhahaṅgehi samannāgatassa upāsakassa pattaṃ nikkujjituṃ."
msgstr ""

msgctxt "pli-tv-kd15:20.4.1"
msgid "Evañca pana, bhikkhave, nikkujjitabbo."
msgstr ""

msgctxt "pli-tv-kd15:20.4.2"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc107
msgctxt "pli-tv-kd15:20.4.3"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd15:20.4.4"
msgid "Vaḍḍho licchavī āyasmantaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃseti."
msgstr ""

msgctxt "pli-tv-kd15:20.4.5"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho vaḍḍhassa licchavissa pattaṃ nikkujjeyya, asambhogaṃ saṃghena kareyya."
msgstr ""

msgctxt "pli-tv-kd15:20.4.6"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc108
msgctxt "pli-tv-kd15:20.4.7"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd15:20.4.8"
msgid "Vaḍḍho licchavī āyasmantaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃseti."
msgstr ""

msgctxt "pli-tv-kd15:20.4.9"
msgid "Saṃgho vaḍḍhassa licchavissa pattaṃ nikkujjati, asambhogaṃ saṃghena karoti."
msgstr ""

msgctxt "pli-tv-kd15:20.4.10"
msgid "Yassāyasmato khamati vaḍḍhassa licchavissa pattassa nikkujjanā, asambhogaṃ saṃghena karaṇaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd15:20.4.11"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc109
msgctxt "pli-tv-kd15:20.4.12"
msgid "Nikkujjito saṃghena vaḍḍhassa licchavissa patto, asambhogo saṃghena."
msgstr ""

msgctxt "pli-tv-kd15:20.4.13"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc110, pts-vp-pli2.126
msgctxt "pli-tv-kd15:20.5.1"
msgid "Atha kho āyasmā ānando pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya yena vaḍḍhassa licchavissa nivesanaṃ tenupasaṅkami, upasaṅkamitvā vaḍḍhaṃ licchaviṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:20.5.2"
msgid "“saṃghena te, āvuso vaḍḍha, patto nikkujjito."
msgstr ""

msgctxt "pli-tv-kd15:20.5.3"
msgid "Asambhogosi saṃghenā”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.5.4"
msgid "Atha kho vaḍḍho licchavī—"
msgstr ""

msgctxt "pli-tv-kd15:20.5.5"
msgid "saṃghena kira me patto nikkujjito, asambhogomhi kira saṃghenāti—"
msgstr ""

msgctxt "pli-tv-kd15:20.5.6"
msgid "tattheva mucchito papato."
msgstr ""

msgctxt "pli-tv-kd15:20.5.7"
msgid "Atha kho vaḍḍhassa licchavissa mittāmaccā ñātisālohitā vaḍḍhaṃ licchaviṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd15:20.5.8"
msgid "“alaṃ, āvuso vaḍḍha, mā soci, mā paridevi."
msgstr ""

msgctxt "pli-tv-kd15:20.5.9"
msgid "Mayaṃ bhagavantaṃ pasādessāma bhikkhusaṃghañcā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc111
msgctxt "pli-tv-kd15:20.5.10"
msgid "Atha kho vaḍḍho licchavī saputtadāro samittāmacco sañātisālohito allavattho allakeso yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavato pādesu sirasā nipatitvā bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:20.5.11"
msgid "“accayo maṃ, bhante, accagamā yathābālaṃ yathāmūḷhaṃ yathāakusalaṃ, yohaṃ ayyaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsesiṃ."
msgstr ""

msgctxt "pli-tv-kd15:20.5.12"
msgid "Tassa me, bhante, bhagavā accayaṃ accayato paṭiggaṇhātu āyatiṃ saṃvarāyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.5.13"
msgid "“Taggha tvaṃ, āvuso vaḍḍha, accayo accagamā yathābālaṃ yathāmūḷhaṃ yathāakusalaṃ, yaṃ tvaṃ dabbaṃ mallaputtaṃ amūlikāya sīlavipattiyā anuddhaṃsesi."
msgstr ""

msgctxt "pli-tv-kd15:20.5.14"
msgid "Yato ca kho tvaṃ, āvuso vaḍḍha, accayaṃ accayato disvā yathādhammaṃ paṭikarosi, taṃ te mayaṃ paṭiggaṇhāma."
msgstr ""

msgctxt "pli-tv-kd15:20.5.15"
msgid "Vuḍḍhihesā, āvuso vaḍḍha, ariyassa vinaye yo accayaṃ accayato disvā yathādhammaṃ paṭikaroti, āyatiṃ saṃvaraṃ āpajjatī”ti."
msgstr ""

msgctxt "pli-tv-kd15:20.6.1"
msgid "Atha kho bhagavā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:20.6.2"
msgid "“tena hi, bhikkhave, saṃgho vaḍḍhassa licchavissa pattaṃ ukkujjatu, sambhogaṃ saṃghena karotu."
msgstr ""

#. HTML: </p><p>
#. REF: sc112, pts-vp-pli2.127
msgctxt "pli-tv-kd15:20.6.3"
msgid "Aṭṭhahi, bhikkhave, aṅgehi samannāgatassa upāsakassa patto ukkujjitabbo—"
msgstr ""

msgctxt "pli-tv-kd15:20.6.4"
msgid "na bhikkhūnaṃ alābhāya parisakkati, na bhikkhūnaṃ anatthāya parisakkati, na bhikkhūnaṃ avāsāya parisakkati, na bhikkhū akkosati paribhāsati, na bhikkhū bhikkhūhi bhedeti, na buddhassa avaṇṇaṃ bhāsati, na dhammassa avaṇṇaṃ bhāsati, na saṃghassa avaṇṇaṃ bhāsati."
msgstr ""

msgctxt "pli-tv-kd15:20.6.5"
msgid "Anujānāmi, bhikkhave, imehi aṭṭhahaṅgehi samannāgatassa upāsakassa pattaṃ ukkujjituṃ."
msgstr ""

msgctxt "pli-tv-kd15:20.7.1"
msgid "Evañca pana, bhikkhave, ukkujjitabbo."
msgstr ""

msgctxt "pli-tv-kd15:20.7.2"
msgid "Tena, bhikkhave, vaḍḍhena licchavinā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā bhikkhūnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd15:20.7.3"
msgid "saṃghena me, bhante, patto nikkujjito, asambhogomhi saṃghena."
msgstr ""

msgctxt "pli-tv-kd15:20.7.4"
msgid "Sohaṃ, bhante, sammā vattāmi, lomaṃ pātemi, netthāraṃ vattāmi, saṃghaṃ pattukkujjanaṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd15:20.7.5"
msgid "Dutiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd15:20.7.6"
msgid "Tatiyampi yācitabbo."
msgstr ""

msgctxt "pli-tv-kd15:20.7.7"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc113
msgctxt "pli-tv-kd15:20.7.8"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd15:20.7.9"
msgid "Saṃghena vaḍḍhassa licchavissa patto nikkujjito, asambhogo saṃghena."
msgstr ""

msgctxt "pli-tv-kd15:20.7.10"
msgid "So sammā vattati, lomaṃ pāteti, netthāraṃ vattati, saṃghaṃ pattukkujjanaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd15:20.7.11"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho vaḍḍhassa licchavissa pattaṃ ukkujjeyya, sambhogaṃ saṃghena kareyya."
msgstr ""

msgctxt "pli-tv-kd15:20.7.12"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc114
msgctxt "pli-tv-kd15:20.7.13"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd15:20.7.14"
msgid "Saṃghena vaḍḍhassa licchavissa patto nikkujjito, asambhogo saṃghena."
msgstr ""

msgctxt "pli-tv-kd15:20.7.15"
msgid "So sammā vattati, lomaṃ pāteti, netthāraṃ vattati, saṃghaṃ pattukkujjanaṃ yācati."
msgstr ""

msgctxt "pli-tv-kd15:20.7.16"
msgid "Saṃgho vaḍḍhassa licchavissa pattaṃ ukkujjati, sambhogaṃ saṃghena karoti."
msgstr ""

msgctxt "pli-tv-kd15:20.7.17"
msgid "Yassāyasmato khamati vaḍḍhassa licchavissa pattassa ukkujjanā, sambhogaṃ saṃghena karaṇaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd15:20.7.18"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc115
msgctxt "pli-tv-kd15:20.7.19"
msgid "Ukkujjito saṃghena vaḍḍhassa licchavissa patto, sambhogo saṃghena."
msgstr ""

msgctxt "pli-tv-kd15:20.7.20"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc116, pts-vp-pli2.128
msgctxt "pli-tv-kd15:21.1.1"
msgid "Atha kho bhagavā vesāliyaṃ yathābhirantaṃ viharitvā yena bhaggā tena cārikaṃ pakkāmi."
msgstr ""

msgctxt "pli-tv-kd15:21.1.2"
msgid "Anupubbena cārikaṃ caramāno yena bhaggā tadavasari."
msgstr ""

#. VAR: susumāragire → suṃsumāragire (bj, s1-3, pts1) | saṃsumāragire (mr)
msgctxt "pli-tv-kd15:21.1.3"
msgid "Tatra sudaṃ bhagavā bhaggesu viharati susumāragire bhesakaḷāvane migadāye."
msgstr ""

#. VAR: kokanado → kokanudo (s1-3, mr)
msgctxt "pli-tv-kd15:21.1.4"
msgid "Tena kho pana samayena bodhissa rājakumārassa kokanado nāma pāsādo acirakārito hoti, anajjhāvuttho samaṇena vā brāhmaṇena vā kenaci vā manussabhūtena."
msgstr ""

msgctxt "pli-tv-kd15:21.1.5"
msgid "Atha kho bodhi rājakumāro sañcikāputtaṃ māṇavaṃ āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:21.1.6"
msgid "“ehi tvaṃ, samma sañcikāputta, yena bhagavā tenupasaṅkama, upasaṅkamitvā mama vacanena bhagavato pāde sirasā vanda;"
msgstr ""

msgctxt "pli-tv-kd15:21.1.7"
msgid "appābādhaṃ appātaṅkaṃ lahuṭṭhānaṃ balaṃ phāsuvihāraṃ puccha—‘"
msgstr ""

msgctxt "pli-tv-kd15:21.1.8"
msgid "bodhi, bhante, rājakumāro bhagavato pāde sirasā vandati, appābādhaṃ appātaṅkaṃ lahuṭṭhānaṃ balaṃ phāsuvihāraṃ pucchatī’ti."
msgstr ""

msgctxt "pli-tv-kd15:21.1.9"
msgid "Evañca vadehi—"
msgstr ""

msgctxt "pli-tv-kd15:21.1.10"
msgid "‘adhivāsetu kira, bhante, bhagavā bodhissa rājakumārassa svātanāya bhattaṃ saddhiṃ bhikkhusaṃghenā’”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.1.11"
msgid "“Evaṃ, bho”ti kho sañcikāputto māṇavo bodhissa rājakumārassa paṭissutvā yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavatā saddhiṃ sammodi, sammodanīyaṃ kathaṃ sāraṇīyaṃ vītisāretvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd15:21.1.12"
msgid "Ekamantaṃ nisinno kho sañcikāputto māṇavo bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:21.1.13"
msgid "“bodhi kho rājakumāro bhoto gotamassa pāde sirasā vandati, appābādhaṃ appātaṅkaṃ lahuṭṭhānaṃ balaṃ phāsuvihāraṃ pucchati."
msgstr ""

msgctxt "pli-tv-kd15:21.1.14"
msgid "Evañca vadeti—‘"
msgstr ""

msgctxt "pli-tv-kd15:21.1.15"
msgid "adhivāsetu kira bhavaṃ gotamo bodhissa rājakumārassa svātanāya bhattaṃ saddhiṃ bhikkhusaṃghenā’”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.1.16"
msgid "Adhivāsesi bhagavā tuṇhībhāvena."
msgstr ""

msgctxt "pli-tv-kd15:21.2.1"
msgid "Atha kho sañcikāputto māṇavo bhagavato adhivāsanaṃ viditvā uṭṭhāyāsanā yena bodhi rājakumāro tenupasaṅkami, upasaṅkamitvā bodhiṃ rājakumāraṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:21.2.2"
msgid "“avocumha kho mayaṃ bhoto vacanena taṃ bhavantaṃ gotamaṃ—‘"
msgstr ""

msgctxt "pli-tv-kd15:21.2.3"
msgid "bodhi kho rājakumāro bhoto gotamassa pāde sirasā vandati, appābādhaṃ appātaṅkaṃ lahuṭṭhānaṃ balaṃ phāsuvihāraṃ pucchati."
msgstr ""

msgctxt "pli-tv-kd15:21.2.4"
msgid "Evañca vadeti—"
msgstr ""

msgctxt "pli-tv-kd15:21.2.5"
msgid "adhivāsetu kira bhavaṃ gotamo bodhissa rājakumārassa svātanāya bhattaṃ saddhiṃ bhikkhusaṃghenā’ti."
msgstr ""

msgctxt "pli-tv-kd15:21.2.6"
msgid "Adhivutthañca pana samaṇena gotamenā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc117
msgctxt "pli-tv-kd15:21.3.1"
msgid "Atha kho bodhi rājakumāro tassā rattiyā accayena paṇītaṃ khādanīyaṃ bhojanīyaṃ paṭiyādāpetvā, kokanadañca pāsādaṃ odātehi dussehi santharāpetvā yāva pacchimasopānakaḷevarā, sañcikāputtaṃ māṇavaṃ āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:21.3.2"
msgid "“ehi tvaṃ, samma sañcikāputta, yena bhagavā tenupasaṅkama, upasaṅkamitvā bhagavato kālaṃ ārocehi—‘"
msgstr ""

msgctxt "pli-tv-kd15:21.3.3"
msgid "kālo, bhante, niṭṭhitaṃ bhattan’”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.3.4"
msgid "“Evaṃ, bho”ti kho sañcikāputto māṇavo bodhissa rājakumārassa paṭissutvā yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavato kālaṃ ārocesi—"
msgstr ""

msgctxt "pli-tv-kd15:21.3.5"
msgid "“kālo, bho gotama, niṭṭhitaṃ bhattan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc118
msgctxt "pli-tv-kd15:21.3.6"
msgid "Atha kho bhagavā pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya yena bodhissa rājakumārassa nivesanaṃ tenupasaṅkami."
msgstr ""

msgctxt "pli-tv-kd15:21.3.7"
msgid "Tena kho pana samayena bodhi rājakumāro bahidvārakoṭṭhake ṭhito hoti, bhagavantaṃ āgamayamāno."
msgstr ""

msgctxt "pli-tv-kd15:21.3.8"
msgid "Addasā kho bodhi rājakumāro bhagavantaṃ dūratova āgacchantaṃ."
msgstr ""

msgctxt "pli-tv-kd15:21.3.9"
msgid "Disvāna tato paccuggantvā bhagavantaṃ abhivādetvā purekkhatvā yena kokanado pāsādo tenupasaṅkami."
msgstr ""

msgctxt "pli-tv-kd15:21.3.10"
msgid "Atha kho bhagavā pacchimasopānakaḷevaraṃ nissāya aṭṭhāsi."
msgstr ""

msgctxt "pli-tv-kd15:21.3.11"
msgid "Atha kho bodhi rājakumāro bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:21.3.12"
msgid "“akkamatu, bhante, bhagavā dussāni, akkamatu sugato dussāni, yaṃ mama assa dīgharattaṃ hitāya sukhāyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.3.13"
msgid "Evaṃ vutte, bhagavā tuṇhī ahosi."
msgstr ""

msgctxt "pli-tv-kd15:21.3.14"
msgid "Dutiyampi kho … pe …"
msgstr ""

msgctxt "pli-tv-kd15:21.3.15"
msgid "tatiyampi kho bodhi rājakumāro bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:21.3.16"
msgid "“akkamatu, bhante, bhagavā dussāni, akkamatu sugato dussāni, yaṃ mama assa dīgharattaṃ hitāya sukhāyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.3.17"
msgid "Atha kho bhagavā āyasmantaṃ ānandaṃ apalokesi."
msgstr ""

msgctxt "pli-tv-kd15:21.3.18"
msgid "Atha kho āyasmā ānando bodhiṃ rājakumāraṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:21.3.19"
msgid "“saṃharantu, rājakumāra, dussāni."
msgstr ""

#. VAR: celapaṭikaṃ → celapattikaṃ (bj, pts1) | ceḷapaṭikaṃ (s1-3)
msgctxt "pli-tv-kd15:21.3.20"
msgid "Na bhagavā celapaṭikaṃ akkamissati pacchimaṃ janataṃ tathāgato anukampatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc119, pts-vp-pli2.129
msgctxt "pli-tv-kd15:21.3.21"
msgid "Atha kho bodhi rājakumāro dussāni saṃharāpetvā uparikokanade pāsāde āsanaṃ paññapesi."
msgstr ""

msgctxt "pli-tv-kd15:21.3.22"
msgid "Atha kho bhagavā kokanadaṃ pāsādaṃ abhiruhitvā paññatte āsane nisīdi saddhiṃ bhikkhusaṃghena."
msgstr ""

msgctxt "pli-tv-kd15:21.3.23"
msgid "Atha kho bodhi rājakumāro buddhappamukhaṃ bhikkhusaṃghaṃ paṇītena khādanīyena bhojanīyena sahatthā santappetvā sampavāretvā, bhagavantaṃ bhuttāviṃ onītapattapāṇiṃ, ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd15:21.3.24"
msgid "Ekamantaṃ nisinnaṃ kho bodhiṃ rājakumāraṃ bhagavā dhammiyā kathāya sandassetvā samādapetvā samuttejetvā sampahaṃsetvā uṭṭhāyāsanā pakkāmi."
msgstr ""

msgctxt "pli-tv-kd15:21.3.25"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:21.3.26"
msgid "“na, bhikkhave, celapaṭikā akkamitabbā."
msgstr ""

msgctxt "pli-tv-kd15:21.3.27"
msgid "Yo akkameyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc120
msgctxt "pli-tv-kd15:21.4.1"
msgid "Tena kho pana samayena aññatarā itthī apagatagabbhā bhikkhū nimantetvā dussaṃ paññapetvā etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:21.4.2"
msgid "“akkamatha, bhante, dussan”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.4.3"
msgid "Bhikkhū kukkuccāyantā na akkamanti."
msgstr ""

msgctxt "pli-tv-kd15:21.4.4"
msgid "“Akkamatha, bhante, dussaṃ maṅgalatthāyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.4.5"
msgid "Bhikkhū kukkuccāyantā na akkamiṃsu."
msgstr ""

msgctxt "pli-tv-kd15:21.4.6"
msgid "Atha kho sā itthī ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-kd15:21.4.7"
msgid "“kathañhi nāma ayyā maṅgalatthāya yāciyamānā celapaṭikaṃ na akkamissantī”ti."
msgstr ""

msgctxt "pli-tv-kd15:21.4.8"
msgid "Assosuṃ kho bhikkhū tassā itthiyā ujjhāyantiyā khiyyantiyā vipācentiyā."
msgstr ""

msgctxt "pli-tv-kd15:21.4.9"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:21.4.10"
msgid "“Gihī, bhikkhave, maṅgalikā."
msgstr ""

msgctxt "pli-tv-kd15:21.4.11"
msgid "Anujānāmi, bhikkhave, gihīnaṃ maṅgalatthāya yāciyamānena celapaṭikaṃ akkamitun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc121
msgctxt "pli-tv-kd15:21.4.12"
msgid "Tena kho pana samayena bhikkhū dhotapādakaṃ akkamituṃ kukkuccāyanti."
msgstr ""

msgctxt "pli-tv-kd15:21.4.13"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:21.4.14"
msgid "“Anujānāmi, bhikkhave, dhotapādakaṃ akkamitun”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc122
msgctxt "pli-tv-kd15:21.4.15"
msgid "Dutiyabhāṇavāro niṭṭhito."
msgstr ""

#. HTML: </p><p>
#. REF: sc123
msgctxt "pli-tv-kd15:22.1.1"
msgid "Atha kho bhagavā bhaggesu yathābhirantaṃ viharitvā yena sāvatthi tena cārikaṃ pakkāmi."
msgstr ""

msgctxt "pli-tv-kd15:22.1.2"
msgid "Anupubbena cārikaṃ caramāno yena sāvatthi tadavasari."
msgstr ""

msgctxt "pli-tv-kd15:22.1.3"
msgid "Tatra sudaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-kd15:22.1.4"
msgid "Atha kho visākhā migāramātā ghaṭakañca katakañca sammajjaniñca ādāya yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd15:22.1.5"
msgid "Ekamantaṃ nisinnā kho visākhā migāramātā bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:22.1.6"
msgid "“paṭiggaṇhātu me, bhante, bhagavā ghaṭakañca katakañca sammajjaniñca, yaṃ mama assa dīgharattaṃ hitāya sukhāyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:22.1.7"
msgid "Paṭiggahesi bhagavā ghaṭakañca sammajjaniñca."
msgstr ""

msgctxt "pli-tv-kd15:22.1.8"
msgid "Na bhagavā katakaṃ paṭiggahesi."
msgstr ""

msgctxt "pli-tv-kd15:22.1.9"
msgid "Atha kho bhagavā visākhaṃ migāramātaraṃ dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṃsesi."
msgstr ""

msgctxt "pli-tv-kd15:22.1.10"
msgid "Atha kho visākhā migāramātā bhagavatā dhammiyā kathāya sandassitā samādapitā samuttejitā sampahaṃsitā uṭṭhāyāsanā bhagavantaṃ abhivādetvā padakkhiṇaṃ katvā pakkāmi."
msgstr ""

#. HTML: </p><p>
#. REF: sc124, pts-vp-pli2.130
msgctxt "pli-tv-kd15:22.1.11"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:22.1.12"
msgid "“anujānāmi, bhikkhave, ghaṭakañca sammajjaniñca."
msgstr ""

msgctxt "pli-tv-kd15:22.1.13"
msgid "Na, bhikkhave, katakaṃ paribhuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:22.1.14"
msgid "Yo paribhuñjeyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:22.1.15"
msgid "Anujānāmi, bhikkhave, tisso pādaghaṃsaniyo—"
msgstr ""

msgctxt "pli-tv-kd15:22.1.16"
msgid "sakkharaṃ, kathalaṃ, samuddapheṇakan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc125
#. VAR: tālavaṇṭañca → tālavaṇḍañca (mr)
msgctxt "pli-tv-kd15:22.2.1"
msgid "Atha kho visākhā migāramātā vidhūpanañca tālavaṇṭañca ādāya yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-kd15:22.2.2"
msgid "Ekamantaṃ nisinnā kho visākhā migāramātā bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd15:22.2.3"
msgid "“paṭiggaṇhātu me, bhante, bhagavā vidhūpanañca tālavaṇṭañca, yaṃ mama assa dīgharattaṃ hitāya sukhāyā”ti."
msgstr ""

msgctxt "pli-tv-kd15:22.2.4"
msgid "Paṭiggahesi bhagavā vidhūpanañca tālavaṇṭañca."
msgstr ""

#. HTML: </p><p>
#. REF: sc126
msgctxt "pli-tv-kd15:22.2.5"
msgid "Atha kho bhagavā visākhaṃ migāramātaraṃ dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṃsesi … pe …"
msgstr ""

msgctxt "pli-tv-kd15:22.2.6"
msgid "padakkhiṇaṃ katvā pakkāmi."
msgstr ""

msgctxt "pli-tv-kd15:22.2.7"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:23.1.1"
msgid "“anujānāmi, bhikkhave, vidhūpanañca tālavaṇṭañcā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc127
msgctxt "pli-tv-kd15:23.1.2"
msgid "Tena kho pana samayena saṃghassa makasabījanī uppannā hoti."
msgstr ""

msgctxt "pli-tv-kd15:23.1.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:23.1.4"
msgid "“Anujānāmi, bhikkhave, makasabījanin”ti."
msgstr ""

msgctxt "pli-tv-kd15:23.1.5"
msgid "Cāmaribījanī uppannā hoti."
msgstr ""

msgctxt "pli-tv-kd15:23.1.6"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:23.1.7"
msgid "“Na, bhikkhave, cāmaribījanī dhāretabbā."
msgstr ""

msgctxt "pli-tv-kd15:23.1.8"
msgid "Yo dhāreyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd15:23.1.9"
msgid "Anujānāmi, bhikkhave, tisso bījaniyo—"
msgstr ""

msgctxt "pli-tv-kd15:23.1.10"
msgid "vākamayaṃ, usīramayaṃ, morapiñchamayan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc128
msgctxt "pli-tv-kd15:23.2.1"
msgid "Tena kho pana samayena saṃghassa chattaṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-kd15:23.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:23.2.3"
msgid "“Anujānāmi, bhikkhave, chattan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc129, pts-vp-pli2.131
#. VAR: chattappaggahitā → chattapaggahitā (s1-3) | chattaṃ paggahetvā (pts1, mr)
msgctxt "pli-tv-kd15:23.2.4"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū chattappaggahitā āhiṇḍanti."
msgstr ""

msgctxt "pli-tv-kd15:23.2.5"
msgid "Tena kho pana samayena aññataro upāsako sambahulehi ājīvakasāvakehi saddhiṃ uyyānaṃ agamāsi."
msgstr ""

msgctxt "pli-tv-kd15:23.2.6"
msgid "Addasāsuṃ kho te ājīvakasāvakā chabbaggiye bhikkhū dūratova chattappaggahite āgacchante."
msgstr ""

msgctxt "pli-tv-kd15:23.2.7"
msgid "Disvāna taṃ upāsakaṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-kd15:23.2.8"
msgid "“ete kho ayyā tumhākaṃ bhadantā chattappaggahitā āgacchanti, seyyathāpi gaṇakamahāmattā”ti."
msgstr ""

msgctxt "pli-tv-kd15:23.2.9"
msgid "“Nāyyā ete bhikkhū, paribbājakā”ti."
msgstr ""

msgctxt "pli-tv-kd15:23.2.10"
msgid "“Bhikkhū na bhikkhū”ti abbhutaṃ akaṃsu."
msgstr ""

msgctxt "pli-tv-kd15:23.2.11"
msgid "Atha kho so upāsako upagate sañjānitvā ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-kd15:23.2.12"
msgid "“kathañhi nāma bhadantā chattappaggahitā āhiṇḍissantī”ti."
msgstr ""

msgctxt "pli-tv-kd15:23.2.13"
msgid "Assosuṃ kho bhikkhū tassa upāsakassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pli-tv-kd15:23.2.14"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:23.2.15"
msgid "“Saccaṃ kira, bhikkhave … pe …"
msgstr ""

msgctxt "pli-tv-kd15:23.2.16"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:23.2.17"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:23.2.18"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:23.2.19"
msgid "“na, bhikkhave, chattaṃ dhāretabbaṃ."
msgstr ""

msgctxt "pli-tv-kd15:23.2.20"
msgid "Yo dhāreyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc130
msgctxt "pli-tv-kd15:23.3.1"
msgid "Tena kho pana samayena aññataro bhikkhu gilāno hoti."
msgstr ""

msgctxt "pli-tv-kd15:23.3.2"
msgid "Tassa bhikkhuno vinā chattaṃ na phāsu hoti."
msgstr ""

msgctxt "pli-tv-kd15:23.3.3"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:23.3.4"
msgid "“Anujānāmi, bhikkhave, gilānassa chattaṃ dhāretun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc131
msgctxt "pli-tv-kd15:23.3.5"
msgid "Tena kho pana samayena bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd15:23.3.6"
msgid "gilānasseva bhagavatā chattaṃ anuññātaṃ no agilānassāti—"
msgstr ""

msgctxt "pli-tv-kd15:23.3.7"
msgid "ārāme ārāmūpacāre chattaṃ dhāretuṃ kukkuccāyanti."
msgstr ""

msgctxt "pli-tv-kd15:23.3.8"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:23.3.9"
msgid "“Anujānāmi, bhikkhave, agilānenapi ārāme ārāmūpacāre chattaṃ dhāretun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc132
msgctxt "pli-tv-kd15:24.1.1"
msgid "Tena kho pana samayena aññataro bhikkhu sikkāya pattaṃ uṭṭitvā daṇḍe ālaggetvā vikāle aññatarena gāmadvārena atikkamati."
msgstr ""

msgctxt "pli-tv-kd15:24.1.2"
msgid "Manussā—"
msgstr ""

msgctxt "pli-tv-kd15:24.1.3"
msgid "“esayyo coro gacchati, asissa vijjotalatī”ti anupatitvā gahetvā sañjānitvā muñciṃsu."
msgstr ""

msgctxt "pli-tv-kd15:24.1.4"
msgid "Atha kho so bhikkhu ārāmaṃ gantvā bhikkhūnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-kd15:24.1.5"
msgid "“Kiṃ pana tvaṃ, āvuso, daṇḍasikkaṃ dhāresī”ti?"
msgstr ""

msgctxt "pli-tv-kd15:24.1.6"
msgid "“Evamāvuso”ti."
msgstr ""

msgctxt "pli-tv-kd15:24.1.7"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:24.1.8"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd15:24.1.9"
msgid "“kathañhi nāma bhikkhu daṇḍasikkaṃ dhāressasī”ti."
msgstr ""

msgctxt "pli-tv-kd15:24.1.10"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd15:24.1.11"
msgid "“saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd15:24.1.12"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd15:24.1.13"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd15:24.1.14"
msgid "“na, bhikkhave, daṇḍasikkā dhāretabbā."
msgstr ""

msgctxt "pli-tv-kd15:24.1.15"
msgid "Yo dhāreyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc133, pts-vp-pli2.132
msgctxt "pli-tv-kd15:24.2.1"
msgid "Tena kho pana samayena aññataro bhikkhu gilāno hoti, na sakkoti vinā daṇḍena āhiṇḍituṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.2.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.2.3"
msgid "“Anujānāmi, bhikkhave, gilānassa bhikkhuno daṇḍasammutiṃ dātuṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.2.4"
msgid "Evañca pana, bhikkhave, dātabbā."
msgstr ""

msgctxt "pli-tv-kd15:24.2.5"
msgid "Tena gilānena bhikkhunā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā vuḍḍhānaṃ bhikkhūnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd15:24.2.6"
msgid "ahaṃ, bhante, gilāno;"
msgstr ""

msgctxt "pli-tv-kd15:24.2.7"
msgid "na sakkomi vinā daṇḍena āhiṇḍituṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.2.8"
msgid "Sohaṃ, bhante, saṃghaṃ daṇḍasammutiṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd15:24.2.9"
msgid "Dutiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-kd15:24.2.10"
msgid "Tatiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-kd15:24.2.11"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc134
msgctxt "pli-tv-kd15:24.2.12"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd15:24.2.13"
msgid "Ayaṃ itthannāmo bhikkhu gilāno, na sakkoti vinā daṇḍena āhiṇḍituṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.2.14"
msgid "So saṃghaṃ daṇḍasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-kd15:24.2.15"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmassa bhikkhuno daṇḍasammutiṃ dadeyya."
msgstr ""

msgctxt "pli-tv-kd15:24.2.16"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc135
msgctxt "pli-tv-kd15:24.2.17"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd15:24.2.18"
msgid "Ayaṃ itthannāmo bhikkhu gilāno, na sakkoti vinā daṇḍena āhiṇḍituṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.2.19"
msgid "So saṃghaṃ daṇḍasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-kd15:24.2.20"
msgid "Saṃgho itthannāmassa bhikkhuno daṇḍasammutiṃ deti."
msgstr ""

msgctxt "pli-tv-kd15:24.2.21"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno daṇḍasammutiyā dānaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd15:24.2.22"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc136
msgctxt "pli-tv-kd15:24.2.23"
msgid "Dinnā saṃghena itthannāmassa bhikkhuno daṇḍasammuti."
msgstr ""

msgctxt "pli-tv-kd15:24.2.24"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc137
msgctxt "pli-tv-kd15:24.3.1"
msgid "Tena kho pana samayena aññataro bhikkhu gilāno hoti, na sakkoti vinā sikkāya pattaṃ pariharituṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.3.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.3.3"
msgid "“Anujānāmi, bhikkhave, gilānassa bhikkhuno sikkāsammutiṃ dātuṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.3.4"
msgid "Evañca pana, bhikkhave, dātabbā."
msgstr ""

msgctxt "pli-tv-kd15:24.3.5"
msgid "Tena gilānena bhikkhunā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā vuḍḍhānaṃ bhikkhūnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—‘"
msgstr ""

msgctxt "pli-tv-kd15:24.3.6"
msgid "ahaṃ, bhante, gilāno;"
msgstr ""

msgctxt "pli-tv-kd15:24.3.7"
msgid "na sakkomi vinā sikkāya pattaṃ pariharituṃ."
msgstr ""

msgctxt "pli-tv-kd15:24.3.8"
msgid "Sohaṃ, bhante, saṃghaṃ sikkāsammutiṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-kd15:24.3.9"
msgid "Dutiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-kd15:24.3.10"
msgid "Tatiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-kd15:24.3.11"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc138
