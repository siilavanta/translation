#. HTML: <section class="sutta" id="pli-tv-kd19"><article><div class="hgroup"><p class="collection">
msgctxt "pli-tv-kd19:0.1"
msgid "Theravāda Vinayapiṭaka"
msgstr ""

#. HTML: </p><p class="division">
msgctxt "pli-tv-kd19:0.2"
msgid "Cūḷavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-kd19:0.3"
msgid "Pātimokkhaṭṭhapanakkhandhaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-kd19:0.4"
msgid "1. Pātimokkhuddesayācana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli2.236
msgctxt "pli-tv-kd19:1.1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati pubbārāme migāramātu pāsāde."
msgstr ""

msgctxt "pli-tv-kd19:1.1.2"
msgid "Tena kho pana samayena bhagavā tadahuposathe bhikkhusaṃghaparivuto nisinno hoti."
msgstr ""

msgctxt "pli-tv-kd19:1.1.3"
msgid "Atha kho āyasmā ānando abhikkantāya rattiyā nikkhante paṭhame yāme uṭṭhāyāsanā ekaṃsaṃ uttarāsaṅgaṃ karitvā yena bhagavā tenañjaliṃ paṇāmetvā bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd19:1.1.4"
msgid "“abhikkantā, bhante, ratti, nikkhanto paṭhamo yāmo, ciranisinno bhikkhusaṃgho."
msgstr ""

msgctxt "pli-tv-kd19:1.1.5"
msgid "Uddisatu, bhante, bhagavā bhikkhūnaṃ pātimokkhan”ti."
msgstr ""

msgctxt "pli-tv-kd19:1.1.6"
msgid "Evaṃ vutte, bhagavā tuṇhī ahosi."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-kd19:1.1.7"
msgid "Dutiyampi kho āyasmā ānando abhikkantāya rattiyā nikkhante majjhime yāme uṭṭhāyāsanā ekaṃsaṃ uttarāsaṅgaṃ karitvā yena bhagavā tenañjaliṃ paṇāmetvā bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd19:1.1.8"
msgid "“abhikkantā, bhante, ratti, nikkhanto majjhimo yāmo, ciranisinno bhikkhusaṃgho."
msgstr ""

msgctxt "pli-tv-kd19:1.1.9"
msgid "Uddisatu, bhante, bhagavā bhikkhūnaṃ pātimokkhan”ti."
msgstr ""

msgctxt "pli-tv-kd19:1.1.10"
msgid "Dutiyampi kho bhagavā tuṇhī ahosi."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-kd19:1.1.11"
msgid "Tatiyampi kho āyasmā ānando abhikkantāya rattiyā nikkhante pacchime yāme uddhaste aruṇe nandimukhiyā rattiyā uṭṭhāyāsanā ekaṃsaṃ uttarāsaṅgaṃ karitvā yena bhagavā tenañjaliṃ paṇāmetvā bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd19:1.1.12"
msgid "“abhikkantā, bhante, ratti, nikkhanto pacchimo yāmo, uddhastaṃ aruṇaṃ nandimukhi ratti, ciranisinno bhikkhusaṃgho."
msgstr ""

msgctxt "pli-tv-kd19:1.1.13"
msgid "Uddisatu, bhante, bhagavā bhikkhūnaṃ pātimokkhan”ti."
msgstr ""

msgctxt "pli-tv-kd19:1.1.14"
msgid "“Aparisuddhā, ānanda, parisā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4, pts-vp-pli2.237
msgctxt "pli-tv-kd19:1.2.1"
msgid "Atha kho āyasmato mahāmoggallānassa etadahosi—"
msgstr ""

msgctxt "pli-tv-kd19:1.2.2"
msgid "“kaṃ nu kho bhagavā puggalaṃ sandhāya evamāha—‘"
msgstr ""

msgctxt "pli-tv-kd19:1.2.3"
msgid "aparisuddhā, ānanda, parisā’”ti?"
msgstr ""

msgctxt "pli-tv-kd19:1.2.4"
msgid "Atha kho āyasmā mahāmoggallāno sabbāvantaṃ bhikkhusaṃghaṃ cetasā ceto paricca manasākāsi."
msgstr ""

#. VAR: kasambujātaṃ → kasambukajātaṃ (s1-3, pts1, mr)
msgctxt "pli-tv-kd19:1.2.5"
msgid "Addasā kho āyasmā mahāmoggallāno taṃ puggalaṃ dussīlaṃ pāpadhammaṃ asucisaṅkassarasamācāraṃ paṭicchannakammantaṃ assamaṇaṃ samaṇapaṭiññaṃ abrahmacāriṃ brahmacāripaṭiññaṃ antopūtiṃ avassutaṃ kasambujātaṃ majjhe bhikkhusaṃghassa nisinnaṃ."
msgstr ""

msgctxt "pli-tv-kd19:1.2.6"
msgid "Disvāna yena so puggalo tenupasaṅkami, upasaṅkamitvā taṃ puggalaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd19:1.2.7"
msgid "“uṭṭhehi, āvuso, diṭṭhosi bhagavatā;"
msgstr ""

msgctxt "pli-tv-kd19:1.2.8"
msgid "natthi te bhikkhūhi saddhiṃ saṃvāso”ti."
msgstr ""

msgctxt "pli-tv-kd19:1.2.9"
msgid "Evaṃ vutte, so puggalo tuṇhī ahosi."
msgstr ""

msgctxt "pli-tv-kd19:1.2.10"
msgid "Dutiyampi kho āyasmā mahāmoggallāno taṃ puggalaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd19:1.2.11"
msgid "“uṭṭhehi, āvuso, diṭṭhosi bhagavatā;"
msgstr ""

msgctxt "pli-tv-kd19:1.2.12"
msgid "natthi te bhikkhūhi saddhiṃ saṃvāso”ti."
msgstr ""

msgctxt "pli-tv-kd19:1.2.13"
msgid "Dutiyampi kho so puggalo tuṇhī ahosi."
msgstr ""

msgctxt "pli-tv-kd19:1.2.14"
msgid "Tatiyampi kho āyasmā mahāmoggallāno taṃ puggalaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd19:1.2.15"
msgid "“uṭṭhehi, āvuso, diṭṭhosi bhagavatā;"
msgstr ""

msgctxt "pli-tv-kd19:1.2.16"
msgid "natthi te bhikkhūhi saddhiṃ saṃvāso”ti."
msgstr ""

msgctxt "pli-tv-kd19:1.2.17"
msgid "Tatiyampi kho so puggalo tuṇhī ahosi."
msgstr ""

msgctxt "pli-tv-kd19:1.2.18"
msgid "Atha kho āyasmā mahāmoggallāno taṃ puggalaṃ bāhāyaṃ gahetvā bahidvārakoṭṭhakā nikkhāmetvā sūcighaṭikaṃ datvā yena bhagavā tenupasaṅkami, upasaṅkamitvā bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-kd19:1.2.19"
msgid "“nikkhāmito so, bhante, puggalo mayā;"
msgstr ""

msgctxt "pli-tv-kd19:1.2.20"
msgid "suddhā parisā;"
msgstr ""

msgctxt "pli-tv-kd19:1.2.21"
msgid "uddisatu, bhante, bhagavā bhikkhūnaṃ pātimokkhan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-kd19:1.2.22"
msgid "“Acchariyaṃ, moggallāna, abbhutaṃ, moggallāna, yāva bāhāgahaṇāpi nāma so moghapuriso āgamessatī”ti."
msgstr ""

msgctxt "pli-tv-kd19:1.3.1"
msgid "Atha kho bhagavā bhikkhū āmantesi—"
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd19:1.3.2"
msgid "2. Mahāsamuddeaṭṭhacchariya"
msgstr ""

#. HTML: </h2><p>
#. REF: sc6
msgctxt "pli-tv-kd19:1.3.3"
msgid "“Aṭṭhime, bhikkhave, mahāsamudde acchariyā abbhutā dhammā, ye disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

msgctxt "pli-tv-kd19:1.3.4"
msgid "Katame aṭṭha?"
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-kd19:1.3.5"
msgid "Mahāsamuddo, bhikkhave, anupubbaninno anupubbapoṇo anupubbapabbhāro na āyatakeneva papāto."
msgstr ""

msgctxt "pli-tv-kd19:1.3.6"
msgid "Yampi, bhikkhave, mahāsamuddo anupubbaninno anupubbapoṇo anupubbapabbhāro na āyatakeneva papāto—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.7"
msgid "ayaṃ, bhikkhave, mahāsamudde paṭhamo acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-kd19:1.3.8"
msgid "Puna caparaṃ, bhikkhave, mahāsamuddo ṭhitadhammo velaṃ nātivattati."
msgstr ""

msgctxt "pli-tv-kd19:1.3.9"
msgid "Yampi, bhikkhave, mahāsamuddo ṭhitadhammo velaṃ nātivattati—"
msgstr ""

#. VAR: ayaṃ → ayampi (s1-3)
msgctxt "pli-tv-kd19:1.3.10"
msgid "ayaṃ, bhikkhave, mahāsamudde dutiyo acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-kd19:1.3.11"
msgid "Puna caparaṃ, bhikkhave, mahāsamuddo na matena kuṇapena saṃvasati."
msgstr ""

msgctxt "pli-tv-kd19:1.3.12"
msgid "Yaṃ hoti mahāsamudde mataṃ kuṇapaṃ, taṃ khippaññeva tīraṃ vāheti, thalaṃ ussāreti."
msgstr ""

msgctxt "pli-tv-kd19:1.3.13"
msgid "Yampi, bhikkhave, mahāsamuddo na matena kuṇapena saṃvasati, yaṃ hoti mahāsamudde mataṃ kuṇapaṃ, taṃ khippaññeva tīraṃ vāheti, thalaṃ ussāreti—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.14"
msgid "ayaṃ, bhikkhave, mahāsamudde tatiyo acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10, pts-vp-pli2.238
msgctxt "pli-tv-kd19:1.3.15"
msgid "Puna caparaṃ, bhikkhave, yā kāci mahānadiyo, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.16"
msgid "gaṅgā, yamunā, aciravatī, sarabhū, mahī, tā mahāsamuddaṃ pattā jahanti purimāni nāmagottāni, mahāsamuddotveva saṅkhaṃ gacchanti."
msgstr ""

msgctxt "pli-tv-kd19:1.3.17"
msgid "Yampi, bhikkhave, yā kāci mahānadiyo, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.18"
msgid "gaṅgā, yamunā, aciravatī, sarabhū, mahī, tā mahāsamuddaṃ pattā jahanti purimāni nāmagottāni, mahāsamuddotveva saṅkhaṃ gacchanti—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.19"
msgid "ayaṃ, bhikkhave, mahāsamudde catuttho acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
#. VAR: ca → yā kāci (s1-3)
msgctxt "pli-tv-kd19:1.3.20"
msgid "Puna caparaṃ, bhikkhave, yā ca loke savantiyo mahāsamuddaṃ appenti, yā ca antalikkhā dhārā papatanti, na tena mahāsamuddassa ūnattaṃ vā pūrattaṃ vā paññāyati."
msgstr ""

msgctxt "pli-tv-kd19:1.3.21"
msgid "Yampi, bhikkhave, yā ca loke savantiyo mahāsamuddaṃ appenti, yā ca antalikkhā dhārā papatanti, na tena mahāsamuddassa ūnattaṃ vā pūrattaṃ vā paññāyati—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.22"
msgid "ayaṃ, bhikkhave, mahāsamudde pañcamo acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-kd19:1.3.23"
msgid "Puna caparaṃ, bhikkhave, mahāsamuddo ekaraso loṇaraso."
msgstr ""

msgctxt "pli-tv-kd19:1.3.24"
msgid "Yampi, bhikkhave, mahāsamuddo ekaraso loṇaraso—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.25"
msgid "ayaṃ, bhikkhave, mahāsamudde chaṭṭho acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
#. VAR: bahuratano → pahūtaratano (mr)
msgctxt "pli-tv-kd19:1.3.26"
msgid "Puna caparaṃ, bhikkhave, mahāsamuddo bahuratano anekaratano."
msgstr ""

msgctxt "pli-tv-kd19:1.3.27"
msgid "Tatrimāni ratanāni, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.28"
msgid "muttā, maṇi, veḷuriyo, saṅkho, silā, pavāḷaṃ, rajataṃ, jātarūpaṃ, lohitako, masāragallaṃ."
msgstr ""

msgctxt "pli-tv-kd19:1.3.29"
msgid "Yampi, bhikkhave, mahāsamuddo bahuratano anekaratano, tatrimāni ratanāni, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.30"
msgid "muttā, maṇi, veḷuriyo, saṅkho, silā, pavāḷaṃ, rajataṃ, jātarūpaṃ, lohitako, masāragallaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.31"
msgid "ayaṃ, bhikkhave, mahāsamudde sattamo acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-kd19:1.3.32"
msgid "Puna caparaṃ, bhikkhave, mahāsamuddo mahataṃ bhūtānaṃ āvāso."
msgstr ""

msgctxt "pli-tv-kd19:1.3.33"
msgid "Tatrime bhūtā—"
msgstr ""

#. VAR: timitimiṅgalo → timirapiṅgalo (bj) | timitimiṅgalo mahātimiṅgalo (s1-3, km)
msgctxt "pli-tv-kd19:1.3.34"
msgid "timi, timiṅgalo, timitimiṅgalo, asurā, nāgā, gandhabbā."
msgstr ""

msgctxt "pli-tv-kd19:1.3.35"
msgid "Santi mahāsamudde yojanasatikāpi attabhāvā, dviyojanasatikāpi attabhāvā, tiyojanasatikāpi attabhāvā, catuyojanasatikāpi attabhāvā, pañcayojanasatikāpi attabhāvā."
msgstr ""

msgctxt "pli-tv-kd19:1.3.36"
msgid "Yampi, bhikkhave, mahāsamuddo mahataṃ bhūtānaṃ āvāso, tatrime bhūtā—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.37"
msgid "timi, timiṅgalo, timitimiṅgalo, asurā, nāgā, gandhabbā;"
msgstr ""

msgctxt "pli-tv-kd19:1.3.38"
msgid "santi mahāsamudde, yojanasatikāpi attabhāvā … pe …"
msgstr ""

msgctxt "pli-tv-kd19:1.3.39"
msgid "pañcayojanasatikāpi attabhāvā—"
msgstr ""

msgctxt "pli-tv-kd19:1.3.40"
msgid "ayaṃ, bhikkhave, mahāsamudde aṭṭhamo acchariyo abbhuto dhammo, yaṃ disvā disvā asurā mahāsamudde abhiramanti."
msgstr ""

msgctxt "pli-tv-kd19:1.3.41"
msgid "Ime kho, bhikkhave, mahāsamudde aṭṭha acchariyā abbhutā dhammā, ye disvā disvā asurā mahāsamudde abhiraman”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd19:1.4.0"
msgid "3. Imasmiṃdhammavinayeaṭṭhacchariya"
msgstr ""

#. HTML: </h2><p>
#. REF: sc15
msgctxt "pli-tv-kd19:1.4.1"
msgid "Evameva kho, bhikkhave, imasmiṃ dhammavinaye aṭṭha acchariyā abbhutā dhammā, ye disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

msgctxt "pli-tv-kd19:1.4.2"
msgid "Katame aṭṭha?"
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-kd19:1.4.3"
msgid "Seyyathāpi, bhikkhave, mahāsamuddo anupubbaninno anupubbapoṇo anupubbapabbhāro na āyatakeneva papāto;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.4"
msgid "evameva kho, bhikkhave, imasmiṃ dhammavinaye anupubbasikkhā anupubbakiriyā anupubbapaṭipadā na āyatakeneva aññāpaṭivedho."
msgstr ""

msgctxt "pli-tv-kd19:1.4.5"
msgid "Yampi, bhikkhave, imasmiṃ dhammavinaye anupubbasikkhā anupubbakiriyā anupubbapaṭipadā na āyatakeneva aññāpaṭivedho—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.6"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye paṭhamo acchariyo abbhuto dhammo, yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc17, pts-vp-pli2.239
msgctxt "pli-tv-kd19:1.4.7"
msgid "Seyyathāpi, bhikkhave, mahāsamuddo ṭhitadhammo velaṃ nātivattati;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.8"
msgid "evameva kho, bhikkhave, yaṃ mayā mama sāvakānaṃ sikkhāpadaṃ paññattaṃ, taṃ mama sāvakā jīvitahetupi nātikkamanti."
msgstr ""

msgctxt "pli-tv-kd19:1.4.9"
msgid "Yampi, bhikkhave, mayā mama sāvakānaṃ sikkhāpadaṃ paññattaṃ, taṃ mama sāvakā jīvitahetupi nātikkamanti—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.10"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye dutiyo acchariyo abbhuto dhammo, yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-kd19:1.4.11"
msgid "Seyyathāpi, bhikkhave, mahāsamuddo na matena kuṇapena saṃvasati, yaṃ hoti mahāsamudde mataṃ kuṇapaṃ taṃ khippameva tīraṃ vāheti, thalaṃ ussāreti;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.12"
msgid "evameva kho, bhikkhave, yo so puggalo dussīlo pāpadhammo asucisaṅkassarasamācāro paṭicchannakammanto assamaṇo samaṇapaṭiñño abrahmacārī brahmacāripaṭiñño antopūti avassuto kasambujāto, na tena saṃgho saṃvasati, khippameva naṃ sannipatitvā ukkhipati, kiñcāpi kho so hoti majjhe bhikkhusaṃghassa nisinno."
msgstr ""

msgctxt "pli-tv-kd19:1.4.13"
msgid "Atha kho so ārakāva saṃghamhā, saṃgho ca tena."
msgstr ""

msgctxt "pli-tv-kd19:1.4.14"
msgid "Yampi, bhikkhave, yo so puggalo dussīlo pāpadhammo asucisaṅkassarasamācāro paṭicchannakammanto assamaṇo samaṇapaṭiñño abrahmacārī brahmacāripaṭiñño antopūti avassuto kasambujāto, na tena saṃgho saṃvasati, khippameva naṃ sannipatitvā ukkhipati, kiñcāpi kho so hoti majjhe bhikkhusaṃghassa nisinno, atha kho so ārakāva saṃghamhā, saṃgho ca tena—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.15"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye tatiyo acchariyo abbhuto dhammo, yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-kd19:1.4.16"
msgid "Seyyathāpi, bhikkhave, yā kāci mahānadiyo, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.17"
msgid "gaṅgā, yamunā, aciravatī, sarabhū, mahī, tā mahāsamuddaṃ pattā jahanti purimāni nāmagottāni, mahāsamuddotveva saṅkhaṃ gacchanti;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.18"
msgid "evameva kho, bhikkhave, cattārome vaṇṇā—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.19"
msgid "khattiyā, brāhmaṇā, vessā, suddā."
msgstr ""

#. VAR: pabbajitvā → pabbajitā (bj)
msgctxt "pli-tv-kd19:1.4.20"
msgid "Te tathāgatappavedite dhammavinaye agārasmā anagāriyaṃ pabbajitvā jahanti purimāni nāmagottāni, samaṇā sakyaputtiyātveva saṅkhaṃ gacchanti."
msgstr ""

msgctxt "pli-tv-kd19:1.4.21"
msgid "Yampi, bhikkhave, cattārome vaṇṇā khattiyā brāhmaṇā vessā suddā, te tathāgatappavedite dhammavinaye agārasmā anagāriyaṃ pabbajitvā jahanti purimāni nāmagottāni, samaṇā sakyaputtiyātveva saṅkhaṃ gacchanti—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.22"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye catuttho acchariyo abbhuto dhammo, yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-kd19:1.4.23"
msgid "Seyyathāpi, bhikkhave, yā ca loke savantiyo mahāsamuddaṃ appenti, yā ca antalikkhā dhārā papatanti, na tena mahāsamuddassa ūnattaṃ vā pūrattaṃ vā paññāyati;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.24"
msgid "evameva kho, bhikkhave, bahū cepi bhikkhū anupādisesāya nibbānadhātuyā parinibbāyanti, na tena nibbānadhātuyā ūnattaṃ vā pūrattaṃ vā paññāyati."
msgstr ""

msgctxt "pli-tv-kd19:1.4.25"
msgid "Yampi, bhikkhave, bahū cepi bhikkhū anupādisesāya nibbānadhātuyā parinibbāyanti, na tena nibbānadhātuyā ūnattaṃ vā pūrattaṃ vā paññāyati—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.26"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye pañcamo acchariyo abbhuto dhammo yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-kd19:1.4.27"
msgid "Seyyathāpi, bhikkhave, mahāsamuddo ekaraso loṇaraso;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.28"
msgid "evameva kho, bhikkhave, ayaṃ dhammavinayo ekaraso vimuttiraso."
msgstr ""

msgctxt "pli-tv-kd19:1.4.29"
msgid "Yampi, bhikkhave, ayaṃ dhammavinayo ekaraso vimuttiraso—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.30"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye chaṭṭho acchariyo abbhuto dhammo, yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc22, pts-vp-pli2.240
msgctxt "pli-tv-kd19:1.4.31"
msgid "Seyyathāpi, bhikkhave, mahāsamuddo bahuratano anekaratano, tatrimāni ratanāni, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.32"
msgid "muttā, maṇi, veḷuriyo, saṅkho, silā, pavāḷaṃ, rajataṃ, jātarūpaṃ, lohitako, masāragallaṃ;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.33"
msgid "evameva kho, bhikkhave, ayaṃ dhammavinayo bahuratano anekaratano."
msgstr ""

msgctxt "pli-tv-kd19:1.4.34"
msgid "Tatrimāni ratanāni, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.35"
msgid "cattāro satipaṭṭhānā, cattāro sammappadhānā, cattāro iddhipādā, pañcindriyāni, pañca balāni, satta bojjhaṅgā, ariyo aṭṭhaṅgiko maggo."
msgstr ""

msgctxt "pli-tv-kd19:1.4.36"
msgid "Yampi, bhikkhave, ayaṃ dhammavinayo bahuratano anekaratano, tatrimāni ratanāni, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.37"
msgid "cattāro satipaṭṭhānā … pe …"
msgstr ""

msgctxt "pli-tv-kd19:1.4.38"
msgid "ariyo aṭṭhaṅgiko maggo—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.39"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye sattamo acchariyo abbhuto dhammo, yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-kd19:1.4.40"
msgid "Seyyathāpi, bhikkhave, mahāsamuddo mahataṃ bhūtānaṃ āvāso, tatrime bhūtā—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.41"
msgid "timi, timiṅgalo, timitimiṅgalo, asurā, nāgā, gandhabbā, santi mahāsamudde yojanasatikāpi attabhāvā, dviyojanasatikāpi attabhāvā, tiyojanasatikāpi attabhāvā, catuyojanasatikāpi attabhāvā, pañcayojanasatikāpi attabhāvā;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.42"
msgid "evameva kho, bhikkhave, ayaṃ dhammavinayo mahataṃ bhūtānaṃ āvāso."
msgstr ""

msgctxt "pli-tv-kd19:1.4.43"
msgid "Tatrime bhūtā—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.44"
msgid "sotāpanno, sotāpattiphalasacchikiriyāya paṭipanno;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.45"
msgid "sakadāgāmī, sakadāgāmiphalasacchikiriyāya paṭipanno;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.46"
msgid "anāgāmī, anāgāmiphalasacchikiriyāya paṭipanno;"
msgstr ""

msgctxt "pli-tv-kd19:1.4.47"
msgid "arahā, arahattaphalasacchikiriyāya paṭipanno."
msgstr ""

msgctxt "pli-tv-kd19:1.4.48"
msgid "Yampi, bhikkhave, ayaṃ dhammavinayo mahataṃ bhūtānaṃ āvāso, tatrime bhūtā—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.49"
msgid "sotāpanno, sotāpattiphalasacchikiriyāya paṭipanno … pe …"
msgstr ""

msgctxt "pli-tv-kd19:1.4.50"
msgid "arahā, arahattaphalasacchikiriyāya paṭipanno—"
msgstr ""

msgctxt "pli-tv-kd19:1.4.51"
msgid "ayaṃ, bhikkhave, imasmiṃ dhammavinaye aṭṭhamo acchariyo abbhuto dhammo, yaṃ disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramanti."
msgstr ""

msgctxt "pli-tv-kd19:1.4.52"
msgid "Ime kho, bhikkhave, imasmiṃ dhammavinaye aṭṭha acchariyā abbhutā dhammā, ye disvā disvā bhikkhū imasmiṃ dhammavinaye abhiramantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-kd19:1.4.53"
msgid "Atha kho bhagavā etamatthaṃ viditvā tāyaṃ velāyaṃ imaṃ udānaṃ udānesi—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc25
#. VAR: Channamativassati → suchannamativassati (mr)
msgctxt "pli-tv-kd19:1.4.54"
msgid "“Channamativassati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd19:1.4.55"
msgid "vivaṭaṃ nātivassati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd19:1.4.56"
msgid "Tasmā channaṃ vivaretha,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd19:1.4.57"
msgid "evaṃ taṃ nātivassatī”ti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-kd19:2.1.0"
msgid "4. Pātimokkhasavanāraha"
msgstr ""

#. HTML: </h2><p>
#. REF: sc26, pts-vp-pli2.241
msgctxt "pli-tv-kd19:2.1.1"
msgid "Atha kho bhagavā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd19:2.1.2"
msgid "“nadānāhaṃ, bhikkhave, ito paraṃ uposathaṃ karissāmi, pātimokkhaṃ uddisissāmi."
msgstr ""

msgctxt "pli-tv-kd19:2.1.3"
msgid "Tumhevadāni, bhikkhave, ito paraṃ uposathaṃ kareyyātha, pātimokkhaṃ uddiseyyātha."
msgstr ""

msgctxt "pli-tv-kd19:2.1.4"
msgid "Aṭṭhānametaṃ, bhikkhave, anavakāso yaṃ tathāgato aparisuddhāya parisāya uposathaṃ kareyya, pātimokkhaṃ uddiseyya."
msgstr ""

#. VAR: Na ca, bhikkhave → na ca bhikkhave bhikkhunā (s1-3, km)
msgctxt "pli-tv-kd19:2.1.5"
msgid "Na ca, bhikkhave, sāpattikena pātimokkhaṃ sotabbaṃ."
msgstr ""

msgctxt "pli-tv-kd19:2.1.6"
msgid "Yo suṇeyya, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-kd19:2.1.7"
msgid "Anujānāmi, bhikkhave, yo sāpattiko pātimokkhaṃ suṇāti, tassa pātimokkhaṃ ṭhapetuṃ."
msgstr ""

msgctxt "pli-tv-kd19:2.1.8"
msgid "Evañca pana, bhikkhave, ṭhapetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd19:2.1.9"
msgid "Tadahuposathe cātuddase vā pannarase vā tasmiṃ puggale sammukhībhūte saṃghamajjhe udāharitabbaṃ—"
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-kd19:2.1.10"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd19:2.1.11"
msgid "Itthannāmo puggalo sāpattiko, tassa pātimokkhaṃ ṭhapemi, na tasmiṃ sammukhībhūte pātimokkhaṃ uddisitabban’ti ṭhapitaṃ hoti pātimokkhan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc28
msgctxt "pli-tv-kd19:3.1.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.2"
msgid "“nāmhe koci jānātī”ti sāpattikāva pātimokkhaṃ suṇanti."
msgstr ""

msgctxt "pli-tv-kd19:3.1.3"
msgid "Therā bhikkhū paracittaviduno bhikkhūnaṃ ārocenti—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.4"
msgid "“itthannāmo ca itthannāmo ca, āvuso, chabbaggiyā bhikkhū—‘"
msgstr ""

msgctxt "pli-tv-kd19:3.1.5"
msgid "nāmhe koci jānātī’ti sāpattikāva pātimokkhaṃ suṇantī”ti."
msgstr ""

msgctxt "pli-tv-kd19:3.1.6"
msgid "Assosuṃ kho chabbaggiyā bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.7"
msgid "“therā kira bhikkhū paracittaviduno amhe bhikkhūnaṃ ārocenti—‘"
msgstr ""

msgctxt "pli-tv-kd19:3.1.8"
msgid "itthannāmo ca itthannāmo ca, āvuso, chabbaggiyā bhikkhū—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.9"
msgid "“nāmhe koci jānātī”ti sāpattikāva pātimokkhaṃ suṇantī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-kd19:3.1.10"
msgid "Te—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.11"
msgid "“puramhākaṃ pesalā bhikkhū pātimokkhaṃ ṭhapentī”ti—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.12"
msgid "paṭikacceva suddhānaṃ bhikkhūnaṃ anāpattikānaṃ avatthusmiṃ akāraṇe pātimokkhaṃ ṭhapenti."
msgstr ""

msgctxt "pli-tv-kd19:3.1.13"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd19:3.1.14"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.15"
msgid "“kathañhi nāma chabbaggiyā bhikkhū suddhānaṃ bhikkhūnaṃ anāpattikānaṃ avatthusmiṃ akāraṇe pātimokkhaṃ ṭhapessantī”ti."
msgstr ""

msgctxt "pli-tv-kd19:3.1.16"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-kd19:3.1.17"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhū suddhānaṃ bhikkhūnaṃ anāpattikānaṃ avatthusmiṃ akāraṇe pātimokkhaṃ ṭhapentī”ti?"
msgstr ""

msgctxt "pli-tv-kd19:3.1.18"
msgid "“Saccaṃ, bhagavā”ti … pe …"
msgstr ""

msgctxt "pli-tv-kd19:3.1.19"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-kd19:3.1.20"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd19:3.1.21"
msgid "“na, bhikkhave, suddhānaṃ bhikkhūnaṃ anāpattikānaṃ avatthusmiṃ akāraṇe pātimokkhaṃ ṭhapetabbaṃ."
msgstr ""

msgctxt "pli-tv-kd19:3.1.22"
msgid "Yo ṭhapeyya, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd19:3.2.0"
msgid "5. Dhammikādhammikapātimokkhaṭṭhapana"
msgstr ""

#. HTML: </h2><p>
#. REF: sc30
#. VAR: pātimokkhaṭṭhapanaṃ → idaṃ padaṃ bj, s1-3 potthakesu
msgctxt "pli-tv-kd19:3.2.1"
msgid "Ekaṃ, bhikkhave, adhammikaṃ pātimokkhaṭṭhapanaṃ, ekaṃ dhammikaṃ;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.2"
msgid "pātimokkhaṭṭhapanaṃ, dve adhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.3"
msgid "pātimokkhaṭṭhapanāni, dve dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.4"
msgid "tīṇi adhammikāni pātimokkhaṭṭhapanāni, tīṇi dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.5"
msgid "cattāri adhammikāni pātimokkhaṭṭhapanāni, cattāri dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.6"
msgid "pañca adhammikāni pātimokkhaṭṭhapanāni, pañca dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.7"
msgid "cha adhammikāni pātimokkhaṭṭhapanāni, cha dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.8"
msgid "satta adhammikāni pātimokkhaṭṭhapanāni, satta dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.9"
msgid "aṭṭha adhammikāni pātimokkhaṭṭhapanāni, aṭṭha dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.10"
msgid "nava adhammikāni pātimokkhaṭṭhapanāni, nava dhammikāni;"
msgstr ""

msgctxt "pli-tv-kd19:3.2.11"
msgid "dasa adhammikāni pātimokkhaṭṭhapanāni, dasa dhammikāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-kd19:3.3.1"
msgid "Katamaṃ ekaṃ adhammikaṃ pātimokkhaṭṭhapanaṃ?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.2"
msgid "Amūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.3"
msgid "idaṃ ekaṃ adhammikaṃ pātimokkhaṭṭhapanaṃ."
msgstr ""

msgctxt "pli-tv-kd19:3.3.4"
msgid "Katamaṃ ekaṃ dhammikaṃ pātimokkhaṭṭhapanaṃ?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.5"
msgid "Samūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.6"
msgid "idaṃ ekaṃ dhammikaṃ pātimokkhaṭṭhapanaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc32, pts-vp-pli2.242
msgctxt "pli-tv-kd19:3.3.7"
msgid "Katamāni dve adhammikāni pātimokkhaṭṭhapanāni?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.8"
msgid "Amūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti, amūlikāya ācāravipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.9"
msgid "imāni dve adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-kd19:3.3.10"
msgid "Katamāni dve dhammikāni pātimokkhaṭṭhapanāni?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.11"
msgid "Samūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti, samūlikāya ācāravipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.12"
msgid "imāni dve dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc33
msgctxt "pli-tv-kd19:3.3.13"
msgid "Katamāni tīṇi adhammikāni pātimokkhaṭṭhapanāni?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.14"
msgid "Amūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti, amūlikāya ācāravipattiyā pātimokkhaṃ ṭhapeti, amūlikāya diṭṭhivipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.15"
msgid "imāni tīṇi adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-kd19:3.3.16"
msgid "Katamāni tīṇi dhammikāni pātimokkhaṭṭhapanāni?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.17"
msgid "Samūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti, samūlikāya ācāravipattiyā pātimokkhaṃ ṭhapeti, samūlikāya diṭṭhivipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.18"
msgid "imāni tīṇi dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-kd19:3.3.19"
msgid "Katamāni cattāri adhammikāni pātimokkhaṭṭhapanāni?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.20"
msgid "Amūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti, amūlikāya ācāravipattiyā pātimokkhaṃ ṭhapeti, amūlikāya diṭṭhivipattiyā pātimokkhaṃ ṭhapeti, amūlikāya ājīvavipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.21"
msgid "imāni cattāri adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-kd19:3.3.22"
msgid "Katamāni cattāri dhammikāni pātimokkhaṭṭhapanāni?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.23"
msgid "Samūlikāya sīlavipattiyā pātimokkhaṃ ṭhapeti, samūlikāya ācāravipattiyā pātimokkhaṃ ṭhapeti, samūlikāya diṭṭhivipattiyā pātimokkhaṃ ṭhapeti, samūlikāya ājīvavipattiyā pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.24"
msgid "imāni cattāri dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-kd19:3.3.25"
msgid "Katamāni pañca adhammikāni pātimokkhaṭṭhapanāni?"
msgstr ""

msgctxt "pli-tv-kd19:3.3.26"
msgid "Amūlakena pārājikena pātimokkhaṃ ṭhapeti, amūlakena saṃghādisesena … pe …"
msgstr ""

msgctxt "pli-tv-kd19:3.3.27"
msgid "amūlakena pācittiyena …"
msgstr ""

msgctxt "pli-tv-kd19:3.3.28"
msgid "amūlakena pāṭidesanīyena …"
msgstr ""

msgctxt "pli-tv-kd19:3.3.29"
msgid "amūlakena dukkaṭena pātimokkhaṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-kd19:3.3.30"
