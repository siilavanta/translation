msgctxt "pli-tv-kd7:12.1.32"
msgid "“amukaṃ nāma āvāsaṃ gamissāmi;"
msgstr ""

msgctxt "pli-tv-kd7:12.1.33"
msgid "tattha me phāsu bhavissati vasissāmi, no ce me phāsu bhavissati, amukaṃ nāma āvāsaṃ gamissāmi;"
msgstr ""

msgctxt "pli-tv-kd7:12.1.34"
msgid "tattha me phāsu bhavissati vasissāmi, no ce me phāsu bhavissati, amukaṃ nāma āvāsaṃ gamissāmi;"
msgstr ""

msgctxt "pli-tv-kd7:12.1.35"
msgid "tattha me phāsu bhavissati vasissāmi, no ce me phāsu bhavissati, paccessan”ti."
msgstr ""

msgctxt "pli-tv-kd7:12.1.36"
msgid "So bahisīmagato taṃ cīvaraṃ kāreti."
msgstr ""

msgctxt "pli-tv-kd7:12.1.37"
msgid "So katacīvaro—"
msgstr ""

msgctxt "pli-tv-kd7:12.1.38"
msgid "“paccessaṃ paccessan”ti bahiddhā kathinuddhāraṃ vītināmeti."
msgstr ""

msgctxt "pli-tv-kd7:12.1.39"
msgid "Tassa bhikkhuno sīmātikkantiko kathinuddhāro."
msgstr ""

msgctxt "pli-tv-kd7:12.1.40"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc135
msgctxt "pli-tv-kd7:12.1.41"
msgid "Bhikkhu atthatakathino phāsuvihāriko cīvaraṃ ādāya pakkamati—"
msgstr ""

msgctxt "pli-tv-kd7:12.1.42"
msgid "“amukaṃ nāma āvāsaṃ gamissāmi;"
msgstr ""

msgctxt "pli-tv-kd7:12.1.43"
msgid "tattha me phāsu bhavissati vasissāmi, no ce me phāsu bhavissati, amukaṃ nāma āvāsaṃ gamissāmi;"
msgstr ""

msgctxt "pli-tv-kd7:12.1.44"
msgid "tattha me phāsu bhavissati vasissāmi, no ce me phāsu bhavissati, amukaṃ nāma āvāsaṃ gamissāmi;"
msgstr ""

msgctxt "pli-tv-kd7:12.1.45"
msgid "tattha me phāsu bhavissati vasissāmi, no ce me phāsu bhavissati, paccessan”ti."
msgstr ""

msgctxt "pli-tv-kd7:12.1.46"
msgid "So bahisīmagato taṃ cīvaraṃ kāreti."
msgstr ""

msgctxt "pli-tv-kd7:12.1.47"
msgid "So katacīvaro “paccessaṃ paccessan”ti sambhuṇāti kathinuddhāraṃ."
msgstr ""

msgctxt "pli-tv-kd7:12.1.48"
msgid "Tassa bhikkhuno saha bhikkhūhi kathinuddhāro."
msgstr ""

msgctxt "pli-tv-kd7:12.1.49"
msgid "(5)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc136
msgctxt "pli-tv-kd7:12.1.50"
msgid "Phāsuvihārapañcakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-kd7:13.1.0"
msgid "14. Palibodhāpalibodhakathā"
msgstr ""

#. HTML: </h2><p>
#. REF: sc137
msgctxt "pli-tv-kd7:13.1.1"
msgid "Dveme, bhikkhave, kathinassa palibodhā, dve apalibodhā."
msgstr ""

msgctxt "pli-tv-kd7:13.1.2"
msgid "Katame ca, bhikkhave, dve kathinassa palibodhā?"
msgstr ""

msgctxt "pli-tv-kd7:13.1.3"
msgid "Āvāsapalibodho ca cīvarapalibodho ca."
msgstr ""

msgctxt "pli-tv-kd7:13.1.4"
msgid "Kathañca, bhikkhave, āvāsapalibodho hoti?"
msgstr ""

msgctxt "pli-tv-kd7:13.1.5"
msgid "Idha, bhikkhave, bhikkhu vasati vā tasmiṃ āvāse, sāpekkho vā pakkamati “paccessan”ti."
msgstr ""

msgctxt "pli-tv-kd7:13.1.6"
msgid "Evaṃ kho, bhikkhave, āvāsapalibodho hoti."
msgstr ""

msgctxt "pli-tv-kd7:13.1.7"
msgid "Kathañca, bhikkhave, cīvarapalibodho hoti?"
msgstr ""

msgctxt "pli-tv-kd7:13.1.8"
msgid "Idha, bhikkhave, bhikkhuno cīvaraṃ akataṃ vā hoti vippakataṃ vā, cīvarāsā vā anupacchinnā."
msgstr ""

msgctxt "pli-tv-kd7:13.1.9"
msgid "Evaṃ kho, bhikkhave, cīvarapalibodho hoti."
msgstr ""

msgctxt "pli-tv-kd7:13.1.10"
msgid "Ime kho, bhikkhave, dve kathinassa palibodhā."
msgstr ""

#. HTML: </p><p>
#. REF: sc138
msgctxt "pli-tv-kd7:13.2.1"
msgid "Katame ca, bhikkhave, dve kathinassa apalibodhā?"
msgstr ""

msgctxt "pli-tv-kd7:13.2.2"
msgid "Āvāsaapalibodho ca cīvaraapalibodho ca."
msgstr ""

msgctxt "pli-tv-kd7:13.2.3"
msgid "Kathañca, bhikkhave, āvāsaapalibodho hoti?"
msgstr ""

#. VAR: anapekkho → anapekho (bj) | anapekkhena (s1-3, pts1, mr)
msgctxt "pli-tv-kd7:13.2.4"
msgid "Idha, bhikkhave, bhikkhu pakkamati tamhā āvāsā cattena vantena muttena anapekkho “na paccessan”ti."
msgstr ""

msgctxt "pli-tv-kd7:13.2.5"
msgid "Evaṃ kho, bhikkhave, āvāsaapalibodho hoti."
msgstr ""

msgctxt "pli-tv-kd7:13.2.6"
msgid "Kathañca, bhikkhave, cīvaraapalibodho hoti?"
msgstr ""

msgctxt "pli-tv-kd7:13.2.7"
msgid "Idha, bhikkhave, bhikkhuno cīvaraṃ kataṃ vā hoti, naṭṭhaṃ vā vinaṭṭhaṃ vā daḍḍhaṃ vā, cīvarāsā vā upacchinnā."
msgstr ""

msgctxt "pli-tv-kd7:13.2.8"
msgid "Evaṃ kho, bhikkhave, cīvaraapalibodho hoti."
msgstr ""

msgctxt "pli-tv-kd7:13.2.9"
msgid "Ime kho, bhikkhave, dve kathinassa apalibodhā”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc139
msgctxt "pli-tv-kd7:13.2.10"
msgid "Kathinakkhandhako niṭṭhito sattamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc140
msgctxt "pli-tv-kd7:13.2.11"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc141
msgctxt "pli-tv-kd7:13.2.12"
msgid "Tiṃsa pāveyyakā bhikkhū,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.13"
msgid "sāketukkaṇṭhitā vasuṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.14"
msgid "Vassaṃvuṭṭhokapuṇṇehi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.15"
msgid "agamuṃ jinadassanaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc142
msgctxt "pli-tv-kd7:13.2.16"
msgid "Idaṃ vatthu kathinassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.17"
msgid "kappissanti ca pañcakā;"
msgstr ""

#. HTML: <br>
#. VAR: Anāmantā asamācārā → anāmantāsamādānaṃ (bj)
msgctxt "pli-tv-kd7:13.2.18"
msgid "Anāmantā asamācārā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.19"
msgid "tatheva gaṇabhojanaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc143
msgctxt "pli-tv-kd7:13.2.20"
msgid "Yāvadatthañca uppādo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.21"
msgid "atthatānaṃ bhavissati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.22"
msgid "Ñatti evatthatañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.23"
msgid "evañceva anatthataṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc144
msgctxt "pli-tv-kd7:13.2.24"
msgid "Ullikhi dhovanā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.25"
msgid "vicāraṇañca chedanaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.26"
msgid "Bandhano vaṭṭi kaṇḍusa,"
msgstr ""

#. HTML: <br>
#. VAR: daḷhīkammānuvātikā → ... vātakā (bj) | daḷhikammānuvātikā (s1-3, pts1)
msgctxt "pli-tv-kd7:13.2.27"
msgid "daḷhīkammānuvātikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc145, pts-vp-pli1.266
#. VAR: ovaddheyyaṃ → ovaṭṭeyyaṃ (bj, s1-3, pts1)
msgctxt "pli-tv-kd7:13.2.28"
msgid "Paribhaṇḍaṃ ovaddheyyaṃ,"
msgstr ""

#. HTML: <br>
#. VAR: nimittaṃ kathā → nimittakathā (bj, s1-3, pts1)
msgctxt "pli-tv-kd7:13.2.29"
msgid "maddanā nimittaṃ kathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.30"
msgid "Kukku sannidhi nissaggi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.31"
msgid "na kappaññatra te tayo."
msgstr ""

#. HTML: </p><p>
#. REF: sc146
msgctxt "pli-tv-kd7:13.2.32"
msgid "Aññatra pañcātireke,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.33"
msgid "sañchinnena samaṇḍalī;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.34"
msgid "Nāññatra puggalā sammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.35"
msgid "nissīmaṭṭhonumodati."
msgstr ""

#. HTML: </p><p>
#. REF: sc147
msgctxt "pli-tv-kd7:13.2.36"
msgid "Kathinānatthataṃ hoti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.37"
msgid "evaṃ buddhena desitaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.38"
msgid "Ahatākappapiloti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.39"
msgid "paṃsu pāpaṇikāya ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc148
msgctxt "pli-tv-kd7:13.2.40"
msgid "Animittāparikathā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.41"
msgid "akukku ca asannidhi;"
msgstr ""

#. HTML: <br>
#. VAR: kappakate → kappakato (bj)
msgctxt "pli-tv-kd7:13.2.42"
msgid "Anissaggi kappakate,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.43"
msgid "tathā ticīvarena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc149
msgctxt "pli-tv-kd7:13.2.44"
msgid "Pañcake vātireke vā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.45"
msgid "chinne samaṇḍalīkate;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.46"
msgid "Puggalassatthārā sammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.47"
msgid "sīmaṭṭho anumodati."
msgstr ""

#. HTML: </p><p>
#. REF: sc150
msgctxt "pli-tv-kd7:13.2.48"
msgid "Evaṃ kathinattharaṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.49"
msgid "ubbhārassaṭṭhamātikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.50"
msgid "Pakkamananti niṭṭhānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.51"
msgid "sanniṭṭhānañca nāsanaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc151
msgctxt "pli-tv-kd7:13.2.52"
msgid "Savanaṃ āsāvacchedi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.53"
msgid "sīmā sahubbhāraṭṭhamī;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.54"
msgid "Katacīvaramādāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.55"
msgid "“na paccessan”ti gacchati."
msgstr ""

#. HTML: </p><p>
#. REF: sc152
msgctxt "pli-tv-kd7:13.2.56"
msgid "Tassa taṃ kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.57"
msgid "hoti pakkamanantiko;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.58"
msgid "Ādāya cīvaraṃ yāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.59"
msgid "nissīme idaṃ cintayi."
msgstr ""

#. HTML: </p><p>
#. REF: sc153
msgctxt "pli-tv-kd7:13.2.60"
msgid "“Kāressaṃ na paccessan”ti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.61"
msgid "niṭṭhāne kathinuddhāro;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.62"
msgid "Ādāya nissīmaṃ neva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.63"
msgid "“na paccessan”ti mānaso."
msgstr ""

#. HTML: </p><p>
#. REF: sc154
msgctxt "pli-tv-kd7:13.2.64"
msgid "Tassa taṃ kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.65"
msgid "sanniṭṭhānantiko bhave;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.66"
msgid "Ādāya cīvaraṃ yāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.67"
msgid "nissīme idaṃ cintayi."
msgstr ""

#. HTML: </p><p>
#. REF: sc155
msgctxt "pli-tv-kd7:13.2.68"
msgid "“Kāressaṃ na paccessan”ti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.69"
msgid "kayiraṃ tassa nassati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.70"
msgid "Tassa taṃ kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.71"
msgid "bhavati nāsanantiko."
msgstr ""

#. HTML: </p><p>
#. REF: sc156
msgctxt "pli-tv-kd7:13.2.72"
msgid "Ādāya yāti “paccessaṃ”,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.73"
msgid "bahi kāreti cīvaraṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.74"
msgid "Katacīvaro suṇāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.75"
msgid "ubbhataṃ kathinaṃ tahiṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc157
msgctxt "pli-tv-kd7:13.2.76"
msgid "Tassa taṃ kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.77"
msgid "bhavati savanantiko;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.78"
msgid "Ādāya yāti “paccessaṃ”,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.79"
msgid "bahi kāreti cīvaraṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc158
msgctxt "pli-tv-kd7:13.2.80"
msgid "Katacīvaro bahiddhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.81"
msgid "nāmeti kathinuddhāraṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.82"
msgid "Tassa taṃ kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.83"
msgid "sīmātikkantiko bhave."
msgstr ""

#. HTML: </p><p>
#. REF: sc159
msgctxt "pli-tv-kd7:13.2.84"
msgid "Ādāya yāti “paccessaṃ”,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.85"
msgid "bahi kāreti cīvaraṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.86"
msgid "Katacīvaro paccessaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.87"
msgid "sambhoti kathinuddhāraṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc160
msgctxt "pli-tv-kd7:13.2.88"
msgid "Tassa taṃ kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.89"
msgid "saha bhikkhūhi jāyati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.90"
msgid "Ādāya ca samādāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.91"
msgid "sattasattavidhā gati."
msgstr ""

#. HTML: </p><p>
#. REF: sc161
msgctxt "pli-tv-kd7:13.2.92"
msgid "Pakkamanantikā natthi,"
msgstr ""

#. HTML: <br>
#. VAR: vippakate → vippakatā (bj, pts1, mr)
#. VAR: chakke → chaṭṭhe (si) | chaccā (pts1, mr)
msgctxt "pli-tv-kd7:13.2.93"
msgid "chakke vippakate gati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.94"
msgid "Ādāya nissīmagataṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.95"
msgid "kāressaṃ iti jāyati."
msgstr ""

#. HTML: </p><p>
#. REF: sc162
msgctxt "pli-tv-kd7:13.2.96"
msgid "Niṭṭhānaṃ sanniṭṭhānañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.97"
msgid "nāsanañca ime tayo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.98"
msgid "Ādāya “na paccessan”ti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.99"
msgid "bahisīme karomiti."
msgstr ""

#. HTML: </p><p>
#. REF: sc163
msgctxt "pli-tv-kd7:13.2.100"
msgid "Niṭṭhānaṃ sanniṭṭhānampi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.101"
msgid "nāsanampi idaṃ tayo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.102"
msgid "Anadhiṭṭhitena nevassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.103"
msgid "heṭṭhā tīṇi nayāvidhi."
msgstr ""

#. HTML: </p><p>
#. REF: sc164, pts-vp-pli1.267
msgctxt "pli-tv-kd7:13.2.104"
msgid "Ādāya yāti paccessaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.105"
msgid "bahisīme karomiti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.106"
msgid "“Na paccessan”ti kāreti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.107"
msgid "niṭṭhāne kathinuddhāro."
msgstr ""

#. HTML: </p><p>
#. REF: sc165
msgctxt "pli-tv-kd7:13.2.108"
msgid "Sanniṭṭhānaṃ nāsanañca,"
msgstr ""

#. HTML: <br>
#. VAR: savanasīmātikkamā → savaṇaṃ sīmātikkamo (bj) | savanaṃ sīmatikkamā (s1-3)
msgctxt "pli-tv-kd7:13.2.109"
msgid "savanasīmātikkamā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.110"
msgid "Saha bhikkhūhi jāyetha,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.111"
msgid "evaṃ pannarasaṃ gati."
msgstr ""

#. HTML: </p><p>
#. REF: sc166
msgctxt "pli-tv-kd7:13.2.112"
msgid "Samādāya vippakatā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.113"
msgid "samādāya punā tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-kd7:13.2.114"
msgid "Ime te caturo vārā,"
msgstr ""

#. HTML: <br>
