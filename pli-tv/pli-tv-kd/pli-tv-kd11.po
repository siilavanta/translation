msgctxt "pli-tv-kd11:4.2.1"
msgid "Tiṇṇaṃ, bhikkhave, bhikkhūnaṃ, ākaṅkhamāno saṃgho, tajjanīyakammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd11:4.2.2"
msgid "Eko bhaṇḍanakārako hoti kalahakārako vivādakārako bhassakārako saṃghe adhikaraṇakārako;"
msgstr ""

msgctxt "pli-tv-kd11:4.2.3"
msgid "eko bālo hoti abyatto āpattibahulo anapadāno;"
msgstr ""

msgctxt "pli-tv-kd11:4.2.4"
msgid "eko gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi—"
msgstr ""

msgctxt "pli-tv-kd11:4.2.5"
msgid "imesaṃ kho, bhikkhave, tiṇṇaṃ bhikkhūnaṃ, ākaṅkhamāno saṃgho, tajjanīyakammaṃ kareyya. #4"
msgstr ""

