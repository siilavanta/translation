#. HTML: <section class="sutta" id="pli-tv-kd11"><article><div class="hgroup"><p class="collection">
msgctxt "pli-tv-kd11:0.1"
msgid "Theravāda Vinayapiṭaka"
msgstr ""

#. HTML: </p><p class="division">
msgctxt "pli-tv-kd11:0.2"
msgid "Cūḷavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-kd11:0.3"
msgid "Kammakkhandhaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-kd11:0.4"
msgid "1. Tajjanīyakamma"
msgstr ""

#. HTML: </h2><p class="namo">
#. REF: sc1 
msgctxt "pli-tv-kd11:0.5"
msgid "Namo tassa Bhagavato Arahato Sammāsambuddhassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli2.1 
msgctxt "pli-tv-kd11:1.1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-kd11:1.1.2"
msgid "Tena kho pana samayena paṇḍukalohitakā bhikkhū attanā bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd11:1.1.3"
msgid "“mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.1.4"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

#. VAR: alamattatarā ca → alamatthatarā ca (s1-3, pts1, mr)
msgctxt "pli-tv-kd11:1.1.5"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.1.6"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.1.7"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā”ti."
msgstr ""

msgctxt "pli-tv-kd11:1.1.8"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattanti."
msgstr ""

msgctxt "pli-tv-kd11:1.2.1"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.2.2"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-kd11:1.2.3"
msgid "“kathañhi nāma paṇḍukalohitakā bhikkhū attanā bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vakkhanti—‘"
msgstr ""

msgctxt "pli-tv-kd11:1.2.4"
msgid "mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.2.5"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

msgctxt "pli-tv-kd11:1.2.6"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.2.7"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.2.8"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā’ti."
msgstr ""

msgctxt "pli-tv-kd11:1.2.9"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3, pts-vp-pli2.2 
msgctxt "pli-tv-kd11:1.2.10"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-kd11:1.2.11"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā bhikkhū paṭipucchi—"
msgstr ""

msgctxt "pli-tv-kd11:1.2.12"
msgid "“saccaṃ kira, bhikkhave, paṇḍukalohitakā bhikkhū attanā bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vadanti—‘"
msgstr ""

msgctxt "pli-tv-kd11:1.2.13"
msgid "mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.2.14"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

msgctxt "pli-tv-kd11:1.2.15"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.2.16"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.2.17"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā’ti."
msgstr ""

msgctxt "pli-tv-kd11:1.2.18"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti, uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattantī”ti?"
msgstr ""

msgctxt "pli-tv-kd11:1.2.19"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-kd11:1.2.20"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-kd11:1.2.21"
msgid "“ananucchavikaṃ, bhikkhave, tesaṃ moghapurisānaṃ ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-kd11:1.2.22"
msgid "Kathañhi nāma te, bhikkhave, moghapurisā attanā bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.2.23"
msgid "saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.2.24"
msgid "saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vakkhanti—‘"
msgstr ""

msgctxt "pli-tv-kd11:1.2.25"
msgid "mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.2.26"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

msgctxt "pli-tv-kd11:1.2.27"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.2.28"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.2.29"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā’ti?"
msgstr ""

msgctxt "pli-tv-kd11:1.2.30"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti, uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattanti."
msgstr ""

msgctxt "pli-tv-kd11:1.2.31"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya pasannānaṃ vā bhiyyobhāvāya."
msgstr ""

msgctxt "pli-tv-kd11:1.2.32"
msgid "Atha khvetaṃ, bhikkhave, appasannānañceva appasādāya pasannānañca ekaccānaṃ aññathattāyā”ti."
msgstr ""

#. HTML: </p><p>
#. VAR: asantuṭṭhitāya → asantuṭṭhiyā (bj, pts1) | asantuṭṭhatāya (s1-3)
#. VAR: vīriyārambhassa → viriyārambhassa (bj, s1-3, pts1) | vīriyārabbhassa (mr)
#. VAR: Atha kho bhagavā te → paṇḍukalohitake (s1-3)
#. REF: sc4
msgctxt "pli-tv-kd11:1.3.1"
msgid "Atha kho bhagavā te bhikkhū anekapariyāyena vigarahitvā dubbharatāya dupposatāya mahicchatāya asantuṭṭhitāya saṅgaṇikāya kosajjassa avaṇṇaṃ bhāsitvā anekapariyāyena subharatāya suposatāya appicchassa santuṭṭhassa sallekhassa dhutassa pāsādikassa apacayassa vīriyārambhassa vaṇṇaṃ bhāsitvā bhikkhūnaṃ tadanucchavikaṃ tadanulomikaṃ dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-kd11:1.3.2"
msgid "“tena hi, bhikkhave, saṃgho paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyakammaṃ karotu."
msgstr ""

msgctxt "pli-tv-kd11:1.4.1"
msgid "Evañca pana, bhikkhave, kātabbaṃ."
msgstr ""

#. VAR: āpattiṃ → āpatti (bj, s1-3)
msgctxt "pli-tv-kd11:1.4.2"
msgid "Paṭhamaṃ paṇḍukalohitakā bhikkhū codetabbā, codetvā sāretabbā, sāretvā āpattiṃ āropetabbā, āpattiṃ āropetvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc5 
msgctxt "pli-tv-kd11:1.4.3"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd11:1.4.4"
msgid "Ime paṇḍukalohitakā bhikkhū attanā bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā kalahakārakā vivādakārakā bhassakārakā saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd11:1.4.5"
msgid "“mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.4.6"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.7"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.4.8"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.9"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā”ti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.10"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti, uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattanti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.11"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyakammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd11:1.4.12"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6 
msgctxt "pli-tv-kd11:1.4.13"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd11:1.4.14"
msgid "Ime paṇḍukalohitakā bhikkhū attanā bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.4.15"
msgid "saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.4.16"
msgid "saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd11:1.4.17"
msgid "“mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.4.18"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.19"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.4.20"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.21"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā”ti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.22"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti, uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattanti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.23"
msgid "Saṃgho paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyakammaṃ karoti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.24"
msgid "Yassāyasmato khamati paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyassa kammassa karaṇaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd11:1.4.25"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc7 
msgctxt "pli-tv-kd11:1.4.26"
msgid "Dutiyampi etamatthaṃ vadāmi—"
msgstr ""

msgctxt "pli-tv-kd11:1.4.27"
msgid "suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd11:1.4.28"
msgid "Ime paṇḍukalohitakā bhikkhū attanā bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.4.29"
msgid "saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.4.30"
msgid "saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd11:1.4.31"
msgid "“mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.4.32"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.33"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.4.34"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.35"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā”ti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.36"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti, uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattanti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.37"
msgid "Saṃgho paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyakammaṃ karoti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.38"
msgid "Yassāyasmato khamati paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyassa kammassa karaṇaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd11:1.4.39"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc8 
msgctxt "pli-tv-kd11:1.4.40"
msgid "Tatiyampi etamatthaṃ vadāmi—"
msgstr ""

msgctxt "pli-tv-kd11:1.4.41"
msgid "suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd11:1.4.42"
msgid "Ime paṇḍukalohitakā bhikkhū attanā bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.4.43"
msgid "saṃghe adhikaraṇakārakā, yepi caññe bhikkhū bhaṇḍanakārakā … pe …"
msgstr ""

msgctxt "pli-tv-kd11:1.4.44"
msgid "saṃghe adhikaraṇakārakā te upasaṅkamitvā evaṃ vadanti—"
msgstr ""

msgctxt "pli-tv-kd11:1.4.45"
msgid "“mā kho tumhe, āyasmanto, eso ajesi."
msgstr ""

msgctxt "pli-tv-kd11:1.4.46"
msgid "Balavābalavaṃ paṭimantetha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.47"
msgid "Tumhe tena paṇḍitatarā ca byattatarā ca bahussutatarā ca alamattatarā ca."
msgstr ""

msgctxt "pli-tv-kd11:1.4.48"
msgid "Mā cassa bhāyittha."
msgstr ""

msgctxt "pli-tv-kd11:1.4.49"
msgid "Mayampi tumhākaṃ pakkhā bhavissāmā”ti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.50"
msgid "Tena anuppannāni ceva bhaṇḍanāni uppajjanti, uppannāni ca bhaṇḍanāni bhiyyobhāvāya vepullāya saṃvattanti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.51"
msgid "Saṃgho paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyakammaṃ karoti."
msgstr ""

msgctxt "pli-tv-kd11:1.4.52"
msgid "Yassāyasmato khamati paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyassa kammassa karaṇaṃ, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-kd11:1.4.53"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc9 
msgctxt "pli-tv-kd11:1.4.54"
msgid "Kataṃ saṃghena paṇḍukalohitakānaṃ bhikkhūnaṃ tajjanīyakammaṃ."
msgstr ""

msgctxt "pli-tv-kd11:1.4.55"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-kd11:2.1.0"
msgid "1.1. Adhammakammadvādasaka"
msgstr ""

#. HTML: </h3><p>
#. VAR: adhammakammañca → adhammakammañceva (s1-3)
#. REF: sc10, pts-vp-pli2.3
msgctxt "pli-tv-kd11:2.1.1"
msgid "“Tīhi, bhikkhave, aṅgehi samannāgataṃ tajjanīyakammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca."
msgstr ""

msgctxt "pli-tv-kd11:2.1.2"
msgid "Asammukhā kataṃ hoti, appaṭipucchā kataṃ hoti, appaṭiññāya kataṃ hoti—"
msgstr ""

msgctxt "pli-tv-kd11:2.1.3"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgataṃ tajjanīyakammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca. #1"
msgstr ""

#. HTML: </p><p>
#. REF: sc11 
msgctxt "pli-tv-kd11:2.1.4"
msgid "Aparehipi, bhikkhave, tīhaṅgehi samannāgataṃ tajjanīyakammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca."
msgstr ""

msgctxt "pli-tv-kd11:2.1.5"
msgid "Anāpattiyā kataṃ hoti, adesanāgāminiyā āpattiyā kataṃ hoti, desitāya āpattiyā kataṃ hoti—"
msgstr ""

msgctxt "pli-tv-kd11:2.1.6"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgataṃ tajjanīyakammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca. #2"
msgstr ""

#. HTML: </p><p>
#. REF: sc12 
msgctxt "pli-tv-kd11:2.1.7"
msgid "Aparehipi, bhikkhave, tīhaṅgehi samannāgataṃ tajjanīyakammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca."
msgstr ""

msgctxt "pli-tv-kd11:2.1.8"
msgid "Acodetvā kataṃ hoti, asāretvā kataṃ hoti, āpattiṃ anāropetvā kataṃ hoti—"
msgstr ""

msgctxt "pli-tv-kd11:2.1.9"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgataṃ tajjanīyakammaṃ adhammakammañca hoti, avinayakammañca, duvūpasantañca. #3"
msgstr ""

#. HTML: </p><p>
#. REF: sc13 
