msgid "1.3. Ākaṅkhamānachakka"
msgstr ""

#. HTML: </h3><p>
#. REF: sc36 
msgctxt "pli-tv-kd11:4.1.1"
msgid "Tīhi, bhikkhave, aṅgehi samannāgatassa bhikkhuno, ākaṅkhamāno saṃgho, tajjanīyakammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd11:4.1.2"
msgid "Bhaṇḍanakārako hoti kalahakārako vivādakārako bhassakārako saṃghe adhikaraṇakārako;"
msgstr ""

msgctxt "pli-tv-kd11:4.1.3"
msgid "bālo hoti abyatto āpattibahulo anapadāno;"
msgstr ""

msgctxt "pli-tv-kd11:4.1.4"
msgid "gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi—"
msgstr ""

msgctxt "pli-tv-kd11:4.1.5"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgatassa bhikkhuno, ākaṅkhamāno saṃgho, tajjanīyakammaṃ kareyya. #1"
msgstr ""

#. HTML: </p><p>
#. REF: sc37 
msgctxt "pli-tv-kd11:4.1.6"
msgid "Aparehipi, bhikkhave, tīhaṅgehi samannāgatassa bhikkhuno, ākaṅkhamāno saṃgho, tajjanīyakammaṃ kareyya."
msgstr ""

msgctxt "pli-tv-kd11:4.1.7"
msgid "Adhisīle sīlavipanno hoti, ajjhācāre ācāravipanno hoti, atidiṭṭhiyā diṭṭhivipanno hoti—"
msgstr ""

msgctxt "pli-tv-kd11:4.1.8"
msgid "imehi kho, bhikkhave, tīhaṅgehi samannāgatassa bhikkhuno, ākaṅkhamāno saṃgho, tajjanīyakammaṃ kareyya. #2"
