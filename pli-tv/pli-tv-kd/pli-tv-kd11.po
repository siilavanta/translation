msgctxt "pli-tv-kd11:13.7.20"
msgid "tatiyampi etamatthaṃ vadāmi."
msgstr ""

msgctxt "pli-tv-kd11:13.7.21"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-kd11:13.7.22"
msgid "Ime assajipunabbasukā bhikkhū kuladūsakā pāpasamācārā."
msgstr ""

msgctxt "pli-tv-kd11:13.7.23"
msgid "Imesaṃ pāpakā samācārā dissanti ceva suyyanti ca."
msgstr ""

msgctxt "pli-tv-kd11:13.7.24"
msgid "Kulāni ca imehi duṭṭhāni dissanti ceva suyyanti ca."
msgstr ""

msgctxt "pli-tv-kd11:13.7.25"
