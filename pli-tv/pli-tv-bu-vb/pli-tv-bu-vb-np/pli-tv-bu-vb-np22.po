msgctxt "pli-tv-bu-vb-np22:2.2.64a"
msgid ""
msgstr "Non-offenses"

#. HTML: </h2><p>
#. REF: sc33, ms1V_2305
msgctxt "pli-tv-bu-vb-np22:2.2.65"
msgid "Anāpatti—"
msgstr "There is no offense:"

msgctxt "pli-tv-bu-vb-np22:2.2.66"
msgid "naṭṭhapattassa,"
msgstr "if his bowl is lost;"

msgctxt "pli-tv-bu-vb-np22:2.2.67"
msgid "bhinnapattassa,"
msgstr "if his bowl is broken;"

msgctxt "pli-tv-bu-vb-np22:2.2.68"
msgid "ñātakānaṃ"
msgstr "if it is from relatives;"

msgctxt "pli-tv-bu-vb-np22:2.2.69"
msgid "pavāritānaṃ,"
msgstr "if it is from those who have given an invitation;"

msgctxt "pli-tv-bu-vb-np22:2.2.70"
msgid "aññassatthāya,"
msgstr "if it is for the benefit of someone else;"

msgctxt "pli-tv-bu-vb-np22:2.2.71"
msgid "attano dhanena,"
msgstr "if it is by means of his own property;"

msgctxt "pli-tv-bu-vb-np22:2.2.72"
msgid "ummattakassa,"
msgstr "if he is insane;"

msgctxt "pli-tv-bu-vb-np22:2.2.73"
msgid "ādikammikassāti."
msgstr "if he is the first offender."

#. HTML: </p><hr><p class="endsutta">
