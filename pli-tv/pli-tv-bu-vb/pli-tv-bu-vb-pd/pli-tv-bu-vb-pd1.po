#. HTML: <section class="sutta" id="pli-tv-bu-vb-pd1"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pd1:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pd1:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pd1:3.1"
msgid "Pāṭidesanīyakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pd1:4.1"
msgid "1. Paṭhamapāṭidesanīyasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.175
msgctxt "pli-tv-bu-vb-pd1:5.1"
msgid "Ime kho panāyasmanto cattāro pāṭidesanīyā dhammā uddesaṃ āgacchanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pd1:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.2"
msgid "Tena kho pana samayena aññatarā bhikkhunī sāvatthiyaṃ piṇḍāya caritvā paṭikkamanakāle aññataraṃ bhikkhuṃ passitvā etadavoca—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.3"
msgid "“handāyya, bhikkhaṃ paṭiggaṇhā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.4"
msgid "“Suṭṭhu, bhaginī”ti sabbeva aggahesi."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.5"
msgid "Sā upakaṭṭhe kāle nāsakkhi piṇḍāya carituṃ, chinnabhattā ahosi."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.6"
msgid "Atha kho sā bhikkhunī dutiyampi divasaṃ … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.7"
msgid "tatiyampi divasaṃ sāvatthiyaṃ piṇḍāya caritvā paṭikkamanakāle taṃ bhikkhuṃ passitvā etadavoca—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.8"
msgid "“handāyya, bhikkhaṃ paṭiggaṇhā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.9"
msgid "“Suṭṭhu, bhaginī”ti sabbeva aggahesi."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.10"
msgid "Sā upakaṭṭhe kāle nāsakkhi piṇḍāya carituṃ, chinnabhattā ahosi."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.11"
msgid "Atha kho sā bhikkhunī catutthe divase rathikāya pavedhentī gacchati."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.12"
msgid "Seṭṭhi gahapati rathena paṭipathaṃ āgacchanto taṃ bhikkhuniṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.13"
msgid "“apehāyye”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.14"
msgid "Sā vokkamantī tattheva paripati."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.15"
msgid "Seṭṭhi gahapati taṃ bhikkhuniṃ khamāpesi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.16"
msgid "“khamāhāyye, mayāsi pātitā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.17"
msgid "“Nāhaṃ, gahapati, tayā pātitā."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.18"
msgid "Api ca ahameva dubbalā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.19"
msgid "“Kissa pana tvaṃ, ayye, dubbalā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.20"
msgid "Atha kho sā bhikkhunī seṭṭhissa gahapatissa etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.21"
msgid "Seṭṭhi gahapati taṃ bhikkhuniṃ gharaṃ netvā bhojetvā ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.22"
msgid "“kathañhi nāma bhadantā bhikkhuniyā hatthato āmisaṃ paṭiggahessanti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:6.23"
msgid "Kicchalābho mātugāmo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3, pts-vp-pli4.176
msgctxt "pli-tv-bu-vb-pd1:7.1"
msgid "Assosuṃ kho bhikkhū tassa seṭṭhissa gahapatissa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.3"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.4"
msgid "“kathañhi nāma bhikkhu bhikkhuniyā hatthato āmisaṃ paṭiggahessatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.5"
msgid "“saccaṃ kira tvaṃ, bhikkhu, bhikkhuniyā hatthato āmisaṃ paṭiggahesī”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.7"
msgid "“Ñātikā te, bhikkhu, aññātikā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.8"
msgid "“Aññātikā, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.9"
msgid "“Aññātako, moghapurisa, aññātikāya na jānāti patirūpaṃ vā appatirūpaṃ vā santaṃ vā asantaṃ vā."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.10"
msgid "Kathañhi nāma tvaṃ, moghapurisa, aññātikāya bhikkhuniyā hatthato āmisaṃ paṭiggahessasi."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.11"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:7.12"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc4
msgctxt "pli-tv-bu-vb-pd1:8.1"
msgid "“Yo pana bhikkhu aññātikāya bhikkhuniyā antaragharaṃ paviṭṭhāya hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādeyya vā bhuñjeyya vā, paṭidesetabbaṃ tena bhikkhunā—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:8.2"
msgid "‘gārayhaṃ, āvuso, dhammaṃ āpajjiṃ asappāyaṃ pāṭidesanīyaṃ, taṃ paṭidesemī’”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc5
msgctxt "pli-tv-bu-vb-pd1:9.1"
msgid "Yo panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pd1:9.2"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bu-vb-pd1:9.3"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pd1:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:9.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bu-vb-pd1:10.1"
msgid "Aññātikā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pd1:10.2"
msgid "mātito vā."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:10.3"
msgid "Pitito vā yāva sattamā pitāmahayugā asambaddhā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bu-vb-pd1:11.1"
msgid "Bhikkhunī nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pd1:11.2"
msgid "ubhatosaṃghe upasampannā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bu-vb-pd1:12.1"
msgid "Antaragharaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pd1:12.2"
msgid "rathikā byūhaṃ siṅghāṭakaṃ gharaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bu-vb-pd1:13.1"
msgid "Khādanīyaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pd1:13.2"
msgid "pañca bhojanāni—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:13.3"
msgid "yāmakālikaṃ sattāhakālikaṃ yāvajīvikaṃ ṭhapetvā avasesaṃ khādanīyaṃ nāma."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10
msgctxt "pli-tv-bu-vb-pd1:14.1"
msgid "Bhojanīyaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pd1:14.2"
msgid "pañca bhojanāni—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:14.3"
msgid "odano, kummāso, sattu, maccho, maṃsaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:14.4"
msgid "“Khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:14.5"
msgid "Ajjhohāre ajjhohāre āpatti pāṭidesanīyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bu-vb-pd1:15.1"
msgid "Aññātikāya aññātikasaññī antaragharaṃ paviṭṭhāya hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādati vā bhuñjati vā, āpatti pāṭidesanīyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:15.2"
msgid "Aññātikāya vematiko antaragharaṃ paviṭṭhāya hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādati vā bhuñjati vā, āpatti pāṭidesanīyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:15.3"
msgid "Aññātikāya ñātikasaññī antaragharaṃ paviṭṭhāya hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādati vā bhuñjati vā, āpatti pāṭidesanīyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bu-vb-pd1:16.1"
msgid "Yāmakālikaṃ sattāhakālikaṃ yāvajīvikaṃ āhāratthāya paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:16.2"
msgid "Ajjhohāre ajjhohāre āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:16.3"
msgid "Ekatoupasampannāya hatthato khādanīyaṃ vā bhojanīyaṃ vā—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:16.4"
msgid "“khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:16.5"
msgid "Ajjhohāre ajjhohāre āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:16.6"
msgid "Ñātikāya aññātikasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:16.7"
msgid "Ñātikāya vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:16.8"
msgid "Ñātikāya ñātikasaññī, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13, pts-vp-pli4.177
msgctxt "pli-tv-bu-vb-pd1:17.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pd1:17.2"
msgid "ñātikāya, dāpeti na deti, upanikkhipitvā deti antarārāme, bhikkhunupassaye, titthiyaseyyāya, paṭikkamane, gāmato nīharitvā deti, yāmakālikaṃ sattāhakālikaṃ yāvajīvikaṃ “sati paccaye paribhuñjā”ti deti, sikkhamānāya, sāmaṇeriyā, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc14
msgctxt "pli-tv-bu-vb-pd1:18.1"
msgid "Paṭhamapāṭidesanīyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
