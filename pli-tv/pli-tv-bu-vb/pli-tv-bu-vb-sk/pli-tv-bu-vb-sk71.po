#. HTML: <section class="sutta" id="pli-tv-bu-vb-sk71"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-sk71:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk71:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk71:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk71:4.1"
msgid "Pādukavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk71:5.1"
msgid "71. Pacchatogamanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.205
msgctxt "pli-tv-bu-vb-sk71:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk71:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū pacchato gacchantā purato gacchantassa dhammaṃ desenti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk71:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk71:7.1"
msgid "“Na pacchato gacchanto purato gacchantassa agilānassa dhammaṃ desessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk71:8.1"
msgid "Na pacchato gacchantena purato gacchantassa agilānassa dhammo desetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk71:8.2"
msgid "Yo anādariyaṃ paṭicca pacchato gacchanto purato gacchantassa agilānassa dhammaṃ deseti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk71:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk71:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk71:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk71:10.1"
msgid "Ekādasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
