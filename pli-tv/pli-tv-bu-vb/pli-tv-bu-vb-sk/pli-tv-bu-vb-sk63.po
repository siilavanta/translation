#. HTML: <section class="sutta" id="pli-tv-bu-vb-sk63"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-sk63:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk63:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk63:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk63:4.1"
msgid "Pādukavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk63:5.1"
msgid "63. Yānasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk63:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk63:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū yānagatassa dhammaṃ desenti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk63:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk63:7.1"
msgid "“Na yānagatassa agilānassa dhammaṃ desessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk63:8.1"
msgid "Yānaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-sk63:8.2"
msgid "vayhaṃ ratho sakaṭaṃ sandamānikā sivikā pāṭaṅkī."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk63:9.1"
msgid "Na yānagatassa agilānassa dhammo desetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk63:9.2"
msgid "Yo anādariyaṃ paṭicca yānagatassa agilānassa dhammaṃ deseti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk63:10.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk63:10.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk63:10.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc6
msgctxt "pli-tv-bu-vb-sk63:11.1"
msgid "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
