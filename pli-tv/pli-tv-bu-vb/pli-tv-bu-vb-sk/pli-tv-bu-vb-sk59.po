#. HTML: <section class="sutta" id="pli-tv-bu-vb-sk59"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-sk59:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk59:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk59:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk59:4.1"
msgid "Surusuruvagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk59:5.1"
msgid "59. Satthapāṇisikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk59:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk59:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū satthapāṇissa dhammaṃ desenti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk59:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk59:7.1"
msgid "“Na satthapāṇissa agilānassa dhammaṃ desessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc3, pts-vp-pli4.201
msgctxt "pli-tv-bu-vb-sk59:8.1"
msgid "Satthaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-sk59:8.2"
msgid "ekatodhāraṃ ubhatodhāraṃ paharaṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk59:9.1"
msgid "Na satthapāṇissa agilānassa dhammo desetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk59:9.2"
msgid "Yo anādariyaṃ paṭicca satthapāṇissa agilānassa dhammaṃ deseti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk59:10.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk59:10.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk59:10.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc6
msgctxt "pli-tv-bu-vb-sk59:11.1"
msgid "Navamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
