#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk3:5.1"
msgid "3. Suppaṭicchannasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk3:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk3:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū kāyaṃ vivaritvā antaraghare gacchanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk3:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
#. VAR: Suppaṭicchanno → supaṭicchanno (bj, s1-3, pts1)
msgctxt "pli-tv-bu-vb-sk3:7.1"
msgid "“Suppaṭicchanno antaraghare gamissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk3:8.1"
msgid "Suppaṭicchannena antaraghare gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk3:8.2"
msgid "Yo anādariyaṃ paṭicca kāyaṃ vivaritvā antaraghare gacchati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk3:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk3:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk3:10.1"
msgid "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

