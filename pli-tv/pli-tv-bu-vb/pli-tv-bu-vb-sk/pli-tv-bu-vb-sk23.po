#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk23:5.1"
msgid "23. Oguṇṭhitasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk23:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk23:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū sasīsaṃ pārupitvā antaraghare gacchanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk23:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk23:7.1"
msgid "“Na oguṇṭhito antaraghare gamissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk23:8.1"
msgid "Na oguṇṭhitena antaraghare gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk23:8.2"
msgid "Yo anādariyaṃ paṭicca sasīsaṃ pārupitvā antaraghare gacchati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk23:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk23:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk23:9.4"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk23:10.1"
msgid "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

