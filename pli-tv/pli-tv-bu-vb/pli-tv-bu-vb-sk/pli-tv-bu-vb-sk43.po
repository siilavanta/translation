msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk43:5.1"
msgid "43. Sakabaḷasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk43:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk43:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū sakabaḷena mukhena byāharanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk43:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk43:7.1"
msgid "“Na sakabaḷena mukhena byāharissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk43:8.1"
msgid "Na sakabaḷena mukhena byāharitabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk43:8.2"
msgid "Yo anādariyaṃ paṭicca sakabaḷena mukhena byāharati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk43:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk43:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk43:9.4"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk43:10.1"
msgid "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

