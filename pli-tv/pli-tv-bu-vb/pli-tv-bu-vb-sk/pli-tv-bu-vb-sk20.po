#. HTML: <section class="sutta" id="pli-tv-bu-vb-sk20"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-sk20:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk20:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk20:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk20:4.1"
msgid "Ujjagghikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk20:5.1"
msgid "20. Dutiyasīsappacālakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk20:6.1"
msgid "Tena—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk20:6.2"
msgid "samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk20:6.3"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū sīsappacālakaṃ antaraghare nisīdanti sīsaṃ olambentā …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk20:6.4"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk20:7.1"
msgid "“Na sīsappacālakaṃ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk20:8.1"
msgid "Na sīsappacālakaṃ antaraghare nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk20:8.2"
msgid "Sīsaṃ paggahetvā nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk20:8.3"
msgid "Yo anādariyaṃ paṭicca sīsappacālakaṃ antaraghare nisīdati sīsaṃ olambento, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk20:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk20:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, vāsūpagatassa, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk20:10.1"
msgid "Dasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bu-vb-sk20:10.2"
msgid "Ujjagghikavaggo dutiyo."
msgstr ""

