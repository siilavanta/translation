msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk29:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk29:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk29:4.1"
msgid "Khambhakatavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk29:5.1"
msgid "29. Samasūpakapaṭiggahaṇasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk29:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk29:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū piṇḍapātaṃ paṭiggaṇhantā sūpaññeva bahuṃ paṭiggaṇhanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk29:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk29:7.1"
msgid "“Samasūpakaṃ piṇḍapātaṃ paṭiggahessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk29:8.1"
msgid "Sūpo nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-sk29:8.2"
msgid "dve sūpā—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk29:8.3"
msgid "muggasūpo, māsasūpo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk29:8.4"
msgid "Hatthahāriyo samasūpako piṇḍapāto paṭiggahetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk29:8.5"
msgid "Yo anādariyaṃ paṭicca sūpaññeva bahuṃ paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk29:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk29:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, rasarase, ñātakānaṃ pavāritānaṃ, aññassatthāya, attano dhanena, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk29:10.1"
msgid "Navamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
