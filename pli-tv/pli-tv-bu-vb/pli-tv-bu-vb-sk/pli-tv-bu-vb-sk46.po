#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk46:5.1"
msgid "46. Avagaṇḍakārakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk46:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk46:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū avagaṇḍakārakaṃ bhuñjanti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk46:6.3"
msgid "."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk46:7.1"
msgid "“Na avagaṇḍakārakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk46:8.1"
msgid "Na avagaṇḍakārakaṃ bhuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk46:8.2"
msgid "Yo anādariyaṃ paṭicca ekato vā ubhato vā gaṇḍaṃ katvā bhuñjati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk46:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk46:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, phalāphale, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk46:10.1"
msgid "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
