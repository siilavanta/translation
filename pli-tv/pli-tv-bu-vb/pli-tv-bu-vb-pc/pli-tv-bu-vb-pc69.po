#. HTML: <section class="sutta" id="pli-tv-bu-vb-pc69"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc69:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc69:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc69:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

msgctxt "pli-tv-bu-vb-pc69:0.4"
msgid "Sappāṇakavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc69:0.5"
msgid "69. Ukkhittasambhogasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.137
msgctxt "pli-tv-bu-vb-pc69:1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

#. VAR: ariṭṭhena bhikkhunā → bhikkhunā gaddha­bādhi­pubbena (?)
msgctxt "pli-tv-bu-vb-pc69:1.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū jānaṃ tathāvādinā ariṭṭhena bhikkhunā akaṭānudhammena taṃ diṭṭhiṃ appaṭinissaṭṭhena saddhiṃ sambhuñjantipi saṃvasantipi sahāpi seyyaṃ kappenti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc69:1.3"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:1.4"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

#. VAR: ariṭṭhena bhikkhunā → bhikkhunā gaddha­bādhi­pubbane (?)
msgctxt "pli-tv-bu-vb-pc69:1.5"
msgid "“kathañhi nāma chabbaggiyā bhikkhū jānaṃ tathāvādinā ariṭṭhena bhikkhunā akaṭānudhammena taṃ diṭṭhiṃ appaṭinissaṭṭhena saddhiṃ sambhuñjissantipi saṃvasissantipi sahāpi seyyaṃ kappessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:1.6"
msgid "“saccaṃ kira tumhe, bhikkhave, jānaṃ tathāvādinā ariṭṭhena bhikkhunā akaṭānudhammena taṃ diṭṭhiṃ appaṭinissaṭṭhena saddhiṃ sambhuñjathāpi saṃvasathāpi sahāpi seyyaṃ kappethā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:1.7"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:1.8"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:1.9"
msgid "kathañhi nāma tumhe, moghapurisā, jānaṃ tathāvādinā ariṭṭhena bhikkhunā akaṭānudhammena taṃ diṭṭhiṃ appaṭinissaṭṭhena saddhiṃ sambhuñjissathāpi saṃvasissathāpi sahāpi seyyaṃ kappessatha."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:1.10"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:1.11"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bu-vb-pc69:1.12"
msgid "“Yo pana bhikkhu jānaṃ tathāvādinā bhikkhunā akaṭānudhammena taṃ diṭṭhiṃ appaṭinissaṭṭhena saddhiṃ sambhuñjeyya vā saṃvaseyya vā saha vā seyyaṃ kappeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc69:2.1.1"
msgid "Yo panāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.2"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc69:2.1.3"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.4"
msgid "… pe …"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc69:2.1.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc69:2.1.6"
msgid "Jānāti nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.7"
msgid "sāmaṃ vā jānāti, aññe vā tassa ārocenti, so vā āroceti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc69:2.1.8"
msgid "Tathāvādināti—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc69:2.1.9"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā”ti evaṃ vādinā."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc69:2.1.10"
msgid "Akaṭānudhammo nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.11"
msgid "ukkhitto anosārito."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc69:2.1.12"
msgid "Taṃ diṭṭhiṃ appaṭinissaṭṭhena saddhinti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.13"
msgid "etaṃ diṭṭhiṃ appaṭinissaṭṭhena saddhiṃ."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc9
msgctxt "pli-tv-bu-vb-pc69:2.1.14"
msgid "Sambhuñjeyya vāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.15"
msgid "sambhogo nāma dve sambhogā—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc69:2.1.16"
msgid "āmisasambhogo ca dhammasambhogo ca."
msgstr ""

#. HTML: <b class="term">
msgctxt "pli-tv-bu-vb-pc69:2.1.17"
msgid "Āmisasambhogo nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.18"
msgid "āmisaṃ deti vā paṭiggaṇhāti vā, āpatti pācittiyassa."
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc69:2.1.19"
msgid "Dhammasambhogo nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.20"
msgid "uddisati vā uddisāpeti vā, padena uddisati vā uddisāpeti vā, pade pade āpatti pācittiyassa."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc69:2.1.21"
msgid "Akkharāya uddisati vā uddisāpeti vā, akkharakkharāya āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc10, pts-vp-pli4.138
msgctxt "pli-tv-bu-vb-pc69:2.1.22"
msgid "Saṃvaseyya vāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.23"
msgid "ukkhittakena saddhiṃ uposathaṃ vā pavāraṇaṃ vā saṃghakammaṃ vā karoti, āpatti pācittiyassa."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc69:2.1.24"
msgid "Saha vā seyyaṃ kappeyyāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc69:2.1.25"
msgid "ekacchanne ukkhittake nipanne bhikkhu nipajjati, āpatti pācittiyassa."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc69:2.1.26"
msgid "Bhikkhu nipanne ukkhittako nipajjati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.1.27"
msgid "Ubho vā nipajjanti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.1.28"
msgid "Uṭṭhahitvā punappunaṃ nipajjanti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bu-vb-pc69:2.2.1"
msgid "Ukkhittake ukkhittakasaññī sambhuñjati vā saṃvasati vā saha vā seyyaṃ kappeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.2.2"
msgid "Ukkhittake vematiko sambhuñjati vā saṃvasati vā saha vā seyyaṃ kappeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.2.3"
msgid "Ukkhittake anukkhittakasaññī sambhuñjati vā saṃvasati vā saha vā seyyaṃ kappeti, anāpatti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.2.4"
msgid "Anukkhittake ukkhittakasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.2.5"
msgid "Anukkhittake vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.2.6"
msgid "Anukkhittake anukkhittakasaññī, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bu-vb-pc69:2.3.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc69:2.3.2"
msgid "anukkhittoti jānāti, ukkhitto osāritoti jānāti, taṃ diṭṭhiṃ paṭinissaṭṭhoti jānāti, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc14
msgctxt "pli-tv-bu-vb-pc69:2.3.3"
msgid "Ukkhittasambhogasikkhāpadaṃ niṭṭhitaṃ navamaṃ."
msgstr ""
