#. HTML: <section class="sutta" id="pi-tv-bu-vb-pc75"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc75:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc75:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc75:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"
msgctxt "pli-tv-bu-vb-pc75:0.4"
msgid "Sahadhammikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc75:1.1.0"
msgid "75. Talasattikasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.147
msgctxt "pli-tv-bu-vb-pc75:1.1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū kupitā anattamanā sattarasavaggiyānaṃ bhikkhūnaṃ talasattikaṃ uggiranti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.3"
msgid "Te pahārasamuccitā rodanti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.4"
msgid "Bhikkhū evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.5"
msgid "“kissa tumhe, āvuso, rodathā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.6"
msgid "“Ime, āvuso, chabbaggiyā bhikkhū kupitā anattamanā amhākaṃ talasattikaṃ uggirantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc75:1.1.7"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.8"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.9"
msgid "“kathañhi nāma chabbaggiyā bhikkhū kupitā anattamanā sattarasavaggiyānaṃ bhikkhūnaṃ talasattikaṃ uggirissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.10"
msgid "“saccaṃ kira tumhe, bhikkhave, kupitā anattamanā sattarasavaggiyānaṃ bhikkhūnaṃ talasattikaṃ uggirathā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.11"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.12"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.13"
msgid "kathañhi nāma tumhe, moghapurisā, kupitā anattamanā sattarasavaggiyānaṃ bhikkhūnaṃ talasattikaṃ uggirissatha."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.14"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:1.1.15"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bu-vb-pc75:1.1.16"
msgid "“Yo pana bhikkhu bhikkhussa kupito anattamano talasattikaṃ uggireyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc75:2.1.1"
msgid "Yo panāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc75:2.1.2"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc75:2.1.3"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc75:2.1.4"
msgid "… pe …"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc75:2.1.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc75:2.1.6"
msgid "Bhikkhussāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc75:2.1.7"
msgid "aññassa bhikkhussa."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc75:2.1.8"
msgid "Kupito anattamanoti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc75:2.1.9"
msgid "anabhiraddho āhatacitto khilajāto."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc75:2.1.10"
msgid "Talasattikaṃ uggireyyāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc75:2.1.11"
msgid "kāyaṃ vā kāyapaṭibaddhaṃ vā antamaso uppalapattampi uccāreti, āpatti pācittiyassa."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc75:2.2.1"
msgid "Upasampanne upasampannasaññī kupito anattamano talasattikaṃ uggirati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:2.2.2"
msgid "Upasampanne vematiko kupito anattamano talasattikaṃ uggirati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:2.2.3"
msgid "Upasampanne anupasampannasaññī kupito anattamano talasattikaṃ uggirati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bu-vb-pc75:2.2.4"
msgid "Anupasampannassa kupito anattamano talasattikaṃ uggirati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:2.2.5"
msgid "Anupasampanne upasampannasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:2.2.6"
msgid "Anupasampanne vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:2.2.7"
msgid "Anupasampanne anupasampannasaññī, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bu-vb-pc75:2.2.8"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc75:2.2.9"
msgid "kenaci viheṭhīyamāno mokkhādhippāyo talasattikaṃ uggirati, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc75:2.2.10"
msgid "Talasattikasikkhāpadaṃ niṭṭhitaṃ pañcamaṃ."
msgstr ""

