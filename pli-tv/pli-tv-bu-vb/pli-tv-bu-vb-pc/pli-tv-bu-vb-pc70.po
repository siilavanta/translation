#. HTML: <section class="sutta" id="pli-tv-bu-vb-pc70"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc70:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc70:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc70:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

msgctxt "pli-tv-bu-vb-pc70:0.4"
msgid "Sappāṇakavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc70:0.5"
msgid "70. Kaṇṭakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-pc70:1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

#. VAR: kaṇṭakassa → kaṇḍakassa (s1-3, pts1, mr)
msgctxt "pli-tv-bu-vb-pc70:1.2"
msgid "Tena kho pana samayena kaṇṭakassa nāma samaṇuddesassa evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ hoti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.3"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.4"
msgid "Assosuṃ kho sambahulā bhikkhū kaṇṭakassa nāma kira samaṇuddesassa evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.5"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.6"
msgid "Atha kho te bhikkhū yena kaṇṭako samaṇuddeso tenupasaṅkamiṃsu;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.7"
msgid "upasaṅkamitvā kaṇṭakaṃ samaṇuddesaṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.8"
msgid "“saccaṃ kira te, āvuso kaṇṭaka, evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.9"
msgid "‘tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.10"
msgid "“Evaṃ byā kho ahaṃ, bhante, bhagavatā dhammaṃ desitaṃ ājānāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.11"
msgid "‘yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc70:1.12"
msgid "“Mā, āvuso kaṇṭaka, evaṃ avaca."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.13"
msgid "Mā bhagavantaṃ abbhācikkhi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.14"
msgid "Na hi sādhu bhagavato abbhakkhānaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.15"
msgid "Na hi bhagavā evaṃ vadeyya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.16"
msgid "Anekapariyāyenāvuso kaṇṭaka, antarāyikā dhammā antarāyikā vuttā bhagavatā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.17"
msgid "Alañca pana te paṭisevato antarāyāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.18"
msgid "Appassādā kāmā vuttā bhagavatā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.19"
msgid "evampi kho kaṇṭako samaṇuddeso tehi bhikkhūhi vuccamāno tatheva taṃ pāpakaṃ diṭṭhigataṃ thāmasā parāmāsā abhinivissa voharati—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.20"
msgid "“evaṃ byā kho ahaṃ, bhante, bhagavatā dhammaṃ desitaṃ ājānāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.21"
msgid "‘yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-pc70:1.22"
msgid "Yato ca kho te bhikkhū nāsakkhiṃsu kaṇṭakaṃ samaṇuddesaṃ etasmā pāpakā diṭṭhigatā vivecetuṃ, atha kho te bhikkhū yena bhagavā tenupasaṅkamiṃsu;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.23"
msgid "upasaṅkamitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.24"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā kaṇṭakaṃ samaṇuddesaṃ paṭipucchi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.25"
msgid "“saccaṃ kira te, kaṇṭaka, evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.26"
msgid "‘tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.27"
msgid "“Evaṃ byā kho ahaṃ, bhante, bhagavatā dhammaṃ desitaṃ ājānāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.28"
msgid "‘yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4, pts-vp-pli4.139
msgctxt "pli-tv-bu-vb-pc70:1.29"
msgid "“Kassa nu kho nāma tvaṃ, moghapurisa, mayā evaṃ dhammaṃ desitaṃ ājānāsi?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.30"
msgid "Nanu mayā, moghapurisa, anekapariyāyena antarāyikā dhammā antarāyikā vuttā, alañca pana te paṭisevato antarāyāya?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.31"
msgid "Appassādā kāmā vuttā mayā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.32"
msgid "Aṭṭhikaṅkalūpamā kāmā vuttā mayā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.33"
msgid "sappasirūpamā kāmā vuttā mayā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.34"
msgid "Atha ca pana tvaṃ, moghapurisa, attanā duggahitena amhe ceva abbhācikkhasi attānañca khaṇasi bahuñca apuññaṃ pasavasi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.35"
msgid "Tañhi te, moghapurisa, bhavissati dīgharattaṃ ahitāya dukkhāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.36"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.37"
msgid "pasannānañca ekaccānaṃ aññathattāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.38"
msgid "Vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.39"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.40"
msgid "“tena hi, bhikkhave, saṃgho kaṇṭakaṃ samaṇuddesaṃ nāsetu."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.41"
msgid "Evañca pana, bhikkhave, nāsetabbo—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.42"
msgid "‘ajjatagge te, āvuso kaṇṭaka, na ceva so bhagavā satthā apadisitabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.43"
msgid "Yampi caññe samaṇuddesā labhanti bhikkhūhi saddhiṃ dirattatirattaṃ sahaseyyaṃ sāpi te natthi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.44"
msgid "Cara pire vinassā’”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.45"
msgid "Atha kho saṃgho kaṇṭakaṃ samaṇuddesaṃ nāsesi."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc70:1.46"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū jānaṃ tathānāsitaṃ kaṇṭakaṃ samaṇuddesaṃ upalāpentipi upaṭṭhāpentipi sambhuñjantipi sahāpi seyyaṃ kappenti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.47"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.48"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.49"
msgid "“kathañhi nāma chabbaggiyā bhikkhū jānaṃ tathānāsitaṃ kaṇṭakaṃ samaṇuddesaṃ upalāpessantipi upaṭṭhāpessantipi sambhuñjissantipi sahāpi seyyaṃ kappessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.50"
msgid "“saccaṃ kira tumhe, bhikkhave, jānaṃ tathānāsitaṃ kaṇṭakaṃ samaṇuddesaṃ upalāpethāpi upaṭṭhāpethāpi sambhuñjathāpi sahāpi seyyaṃ kappethā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.51"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.52"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.53"
msgid "kathañhi nāma tumhe, moghapurisā, jānaṃ tathānāsitaṃ kaṇṭakaṃ samaṇuddesaṃ upalāpessathāpi upaṭṭhāpessathāpi sambhuñjissathāpi sahāpi seyyaṃ kappessatha."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.54"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.55"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc70:1.56"
msgid "“Samaṇuddesopi ce evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.57"
msgid "‘tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’ti, so samaṇuddeso bhikkhūhi evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.58"
msgid "‘māvuso samaṇuddesa, evaṃ avaca, mā bhagavantaṃ abbhācikkhi, na hi sādhu bhagavato abbhakkhānaṃ, na hi bhagavā evaṃ vadeyya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.59"
msgid "Anekapariyāyenāvuso samaṇuddesa, antarāyikā dhammā antarāyikā vuttā bhagavatā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.60"
msgid "Alañca pana te paṭisevato antarāyāyā’ti."
msgstr ""

#. VAR: Evañca → evañca pana (bj, pts1, mr)
msgctxt "pli-tv-bu-vb-pc70:1.61"
msgid "Evañca so samaṇuddeso bhikkhūhi vuccamāno tatheva paggaṇheyya, so samaṇuddeso bhikkhūhi evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.62"
msgid "‘ajjatagge te, āvuso samaṇuddesa, na ceva so bhagavā satthā apadisitabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.63"
msgid "Yampi caññe samaṇuddesā labhanti bhikkhūhi saddhiṃ dirattatirattaṃ sahaseyyaṃ sāpi te natthi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.64"
msgid "Cara pire vinassā’ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:1.65"
msgid "Yo pana bhikkhu jānaṃ tathānāsitaṃ samaṇuddesaṃ upalāpeyya vā upaṭṭhāpeyya vā sambhuñjeyya vā saha vā seyyaṃ kappeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc70:2.1.1"
msgid "Samaṇuddeso nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.2"
msgid "sāmaṇero vuccati."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc70:2.1.3"
msgid "Evaṃ vadeyyāti—"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.4"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc9, pts-vp-pli4.140
msgctxt "pli-tv-bu-vb-pc70:2.1.5"
msgid "So samaṇuddesoti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.6"
msgid "yo so evaṃvādī samaṇuddeso."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc10
msgctxt "pli-tv-bu-vb-pc70:2.1.7"
msgid "Bhikkhūhīti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.8"
msgid "aññehi bhikkhūhi, ye passanti ye suṇanti tehi vattabbo—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc70:2.1.9"
msgid "“mā, āvuso samaṇuddesa, evaṃ avaca."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.10"
msgid "Mā bhagavantaṃ abbhācikkhi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.11"
msgid "Na hi sādhu bhagavato abbhakkhānaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.12"
msgid "Na hi bhagavā evaṃ vadeyya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.13"
msgid "Anekapariyāyenāvuso samaṇuddesa, antarāyikā dhammā antarāyikā vuttā bhagavatā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.14"
msgid "Alañca pana te paṭisevato antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.15"
msgid "Dutiyampi vattabbo …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.16"
msgid "tatiyampi vattabbo …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.17"
msgid "sace paṭinissajjati iccetaṃ kusalaṃ, no ce paṭinissajjati so samaṇuddeso bhikkhūhi evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.18"
msgid "“ajjatagge te, āvuso samaṇuddesa, na ceva so bhagavā satthā apadisitabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.19"
msgid "Yampi caññe samaṇuddesā labhanti bhikkhūhi saddhiṃ dirattatirattaṃ sahaseyyaṃ sāpi te natthi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.20"
msgid "Cara pire vinassā”ti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc70:2.1.21"
msgid "Yo panāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.22"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc70:2.1.23"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.24"
msgid "… pe …"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc70:2.1.25"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc12
msgctxt "pli-tv-bu-vb-pc70:2.1.26"
msgid "Jānāti nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.27"
msgid "sāmaṃ vā jānāti, aññe vā tassa ārocenti, so vā āroceti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc13
msgctxt "pli-tv-bu-vb-pc70:2.1.28"
msgid "Tathānāsitanti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.29"
msgid "evaṃ nāsitaṃ."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc14
msgctxt "pli-tv-bu-vb-pc70:2.1.30"
msgid "Samaṇuddeso nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.31"
msgid "sāmaṇero vuccati."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc15
msgctxt "pli-tv-bu-vb-pc70:2.1.32"
msgid "Upalāpeyya vāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.33"
msgid "tassa pattaṃ vā cīvaraṃ vā uddesaṃ vā paripucchaṃ vā dassāmīti upalāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc16
msgctxt "pli-tv-bu-vb-pc70:2.1.34"
msgid "Upaṭṭhāpeyya vāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.35"
msgid "tassa cuṇṇaṃ vā mattikaṃ vā dantakaṭṭhaṃ vā mukhodakaṃ vā sādiyati, āpatti pācittiyassa."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc17
msgctxt "pli-tv-bu-vb-pc70:2.1.36"
msgid "Sambhuñjeyya vāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.37"
msgid "sambhogo nāma dve sambhogā—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc70:2.1.38"
msgid "āmisasambhogo ca dhammasambhogo ca."
msgstr ""

#. HTML: <b class="term">
msgctxt "pli-tv-bu-vb-pc70:2.1.39"
msgid "Āmisasambhogo nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.40"
msgid "āmisaṃ deti vā paṭiggaṇhāti vā, āpatti pācittiyassa."
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc70:2.1.41"
msgid "Dhammasambhogo nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.42"
msgid "uddisati vā uddisāpeti vā, padena uddisati vā uddisāpeti vā, pade pade āpatti pācittiyassa."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc70:2.1.43"
msgid "Akkharāya uddisati vā uddisāpeti vā, akkharakkharāya āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc18
msgctxt "pli-tv-bu-vb-pc70:2.1.44"
msgid "Saha vā seyyaṃ kappeyyāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc70:2.1.45"
msgid "ekacchanne nāsitake samaṇuddese nipanne bhikkhu nipajjati, āpatti pācittiyassa."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc70:2.1.46"
msgid "Bhikkhu nipanne nāsitako samaṇuddeso nipajjati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.47"
msgid "Ubho vā nipajjanti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.1.48"
msgid "Uṭṭhahitvā punappunaṃ nipajjanti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-bu-vb-pc70:2.2.1"
msgid "Nāsitake nāsitakasaññī upalāpeti vā upaṭṭhāpeti vā sambhuñjati vā saha vā seyyaṃ kappeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.2.2"
msgid "Nāsitake vematiko upalāpeti vā upaṭṭhāpeti vā sambhuñjati vā saha vā seyyaṃ kappeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.2.3"
msgid "Nāsitake anāsitakasaññī upalāpeti vā upaṭṭhāpeti vā sambhuñjati vā saha vā seyyaṃ kappeti, anāpatti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.2.4"
msgid "Anāsitake nāsitakasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.2.5"
msgid "Anāsitake vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.2.6"
msgid "Anāsitake anāsitakasaññī, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-bu-vb-pc70:2.3.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc70:2.3.2"
msgid "anāsitakoti jānāti, taṃ diṭṭhiṃ paṭinissaṭṭhoti jānāti, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc21
msgctxt "pli-tv-bu-vb-pc70:2.3.3"
msgid "Kaṇṭakasikkhāpadaṃ niṭṭhitaṃ dasamaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bu-vb-pc70:2.3.4"
msgid "Sappāṇakavaggo sattamo."
msgstr ""

#. HTML: </p><p class="endsection summary">
#. REF: sc22
msgctxt "pli-tv-bu-vb-pc70:2.3.5"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc23
msgctxt "pli-tv-bu-vb-pc70:2.3.6"
msgid "Sañciccavadhasappāṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bu-vb-pc70:2.3.7"
msgid "ukkoṭaṃ duṭṭhullachādanaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bu-vb-pc70:2.3.8"
msgid "Ūnavīsati satthañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bu-vb-pc70:2.3.9"
msgid "saṃvidhānaṃ ariṭṭhakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bu-vb-pc70:2.3.10"
msgid "Ukkhittaṃ kaṇṭakañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bu-vb-pc70:2.3.11"
msgid "dasa sikkhāpadā imeti."
msgstr ""
