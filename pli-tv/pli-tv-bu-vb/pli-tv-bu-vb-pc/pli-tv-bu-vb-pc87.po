#. HTML: <section class="sutta" id="pi-tv-bu-vb-pc87"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc87:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc87:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p> 
msgctxt "pli-tv-bu-vb-pc87:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

#. VAR: Ratanavagga → mañca­sikkhā­padaṃ (bj)
msgctxt "pli-tv-bu-vb-pc87:0.4"
msgid "Ratanavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc87:1.1.0"
msgid "87. Mañcapīṭhasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-pc87:1.1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.2"
msgid "Tena kho pana samayena āyasmā upanando sakyaputto ucce mañce sayati."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.3"
msgid "Atha kho bhagavā sambahulehi bhikkhūhi saddhiṃ senāsanacārikaṃ āhiṇḍanto yenāyasmato upanandassa sakyaputtassa vihāro tenupasaṅkami."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.4"
msgid "Addasā kho āyasmā upanando sakyaputto bhagavantaṃ dūratova āgacchantaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.5"
msgid "Disvāna bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.6"
msgid "“āgacchatu me, bhante, bhagavā sayanaṃ passatū”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.7"
msgid "Atha kho bhagavā tatova paṭinivattitvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.8"
msgid "“āsayato, bhikkhave, moghapuriso veditabbo”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.9"
msgid "Atha kho bhagavā āyasmantaṃ upanandaṃ sakyaputtaṃ anekapariyāyena vigarahitvā dubbharatāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:1.1.10"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc87:1.1.11"
msgid "“Navaṃ pana bhikkhunā mañcaṃ vā pīṭhaṃ vā kārayamānena aṭṭhaṅgulapādakaṃ kāretabbaṃ sugataṅgulena, aññatra heṭṭhimāya aṭaniyā; taṃ atikkāmayato chedanakaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc3
msgctxt "pli-tv-bu-vb-pc87:2.1.1"
msgid "Navaṃ nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc87:2.1.2"
msgid "karaṇaṃ upādāya vuccati."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc87:2.1.3"
msgid "Mañco nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc87:2.1.4"
msgid "cattāro mañcā—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc87:2.1.5"
msgid "masārako, bundikābaddho, kuḷīrapādako, āhaccapādako."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc87:2.1.6"
msgid "Pīṭhaṃ nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc87:2.1.7"
msgid "cattāri pīṭhāni—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc87:2.1.8"
msgid "masārakaṃ, bundikābaddhaṃ, kuḷīrapādakaṃ, āhaccapādakaṃ."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc87:2.1.9"
msgid "Kārayamānenāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc87:2.1.10"
msgid "karonto vā kārāpento vā."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc7, pts-vp-pli4.169
msgctxt "pli-tv-bu-vb-pc87:2.1.11"
msgid "Aṭṭhaṅgulapādakaṃ kāretabbaṃ sugataṅgulena, aññatra heṭṭhimāya aṭaniyāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc87:2.1.12"
msgid "ṭhapetvā heṭṭhimaṃ aṭaniṃ;"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc87:2.1.13"
msgid "taṃ atikkāmetvā karoti vā kārāpeti vā, payoge dukkaṭaṃ, paṭilābhena chinditvā pācittiyaṃ desetabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc87:2.1.14"
msgid "Attanā vippakataṃ attanā pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:2.1.15"
msgid "Attanā vippakataṃ parehi pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:2.1.16"
msgid "Parehi vippakataṃ attanā pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:2.1.17"
msgid "Parehi vippakataṃ parehi pariyosāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bu-vb-pc87:2.1.18"
msgid "Aññassatthāya karoti vā kārāpeti vā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:2.1.19"
msgid "Aññena kataṃ paṭilabhitvā paribhuñjati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bu-vb-pc87:2.2.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc87:2.2.2"
msgid "pamāṇikaṃ karoti, ūnakaṃ karoti, aññena kataṃ pamāṇātikkantaṃ paṭilabhitvā chinditvā paribhuñjati, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc87:2.2.3"
msgid "Mañcapīṭhasikkhāpadaṃ niṭṭhitaṃ pañcamaṃ."
msgstr ""
