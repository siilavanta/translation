#. HTML: <section class="sutta" id="pli-tv-bu-vb-pc66"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc66:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc66:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc66:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

msgctxt "pli-tv-bu-vb-pc66:0.4"
msgid "Sappāṇakavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc66:0.5"
msgid "66. Theyyasatthasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.131
msgctxt "pli-tv-bu-vb-pc66:1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.2"
msgid "Tena kho pana samayena aññataro sattho rājagahā paṭiyālokaṃ gantukāmo hoti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.3"
msgid "Aññataro bhikkhu te manusse etadavoca—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.4"
msgid "“ahampāyasmantehi saddhiṃ gamissāmī”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.5"
msgid "“Mayaṃ kho, bhante, suṅkaṃ pariharissāmā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.6"
msgid "“Pajānāthāvuso”ti."
msgstr ""

#. VAR: kammiyā → kammikā (bj, s1-3, pts1)
msgctxt "pli-tv-bu-vb-pc66:1.7"
msgid "Assosuṃ kho kammiyā—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.8"
msgid "“sattho kira suṅkaṃ pariharissatī”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.9"
msgid "Te magge pariyuṭṭhiṃsu."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.10"
msgid "Atha kho te kammiyā taṃ satthaṃ gahetvā acchinditvā taṃ bhikkhuṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.11"
msgid "“kissa tvaṃ, bhante, jānaṃ theyyasatthena saddhiṃ gacchasī”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.12"
msgid "Palibundhetvā muñciṃsu."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc66:1.13"
msgid "Atha kho so bhikkhu sāvatthiṃ gantvā bhikkhūnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.14"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.15"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.16"
msgid "“kathañhi nāma bhikkhu jānaṃ theyyasatthena saddhiṃ saṃvidhāya ekaddhānamaggaṃ paṭipajjissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.17"
msgid "“saccaṃ kira tvaṃ, bhikkhu, jānaṃ theyyasatthena saddhiṃ saṃvidhāya ekaddhānamaggaṃ paṭipajjasī”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.18"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.19"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.20"
msgid "kathañhi nāma tvaṃ, moghapurisa, jānaṃ theyyasatthena saddhiṃ saṃvidhāya ekaddhānamaggaṃ paṭipajjissasi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.21"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:1.22"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bu-vb-pc66:1.23"
msgid "“Yo pana bhikkhu jānaṃ theyyasatthena saddhiṃ saṃvidhāya ekaddhānamaggaṃ paṭipajjeyya, antamaso gāmantarampi, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc66:2.1.1"
msgid "Yo panāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc66:2.1.2"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc66:2.1.3"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc66:2.1.4"
msgid "… pe …"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc66:2.1.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc66:2.1.6"
msgid "Jānāti nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc66:2.1.7"
msgid "sāmaṃ vā jānāti, aññe vā tassa ārocenti, so vā āroceti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc66:2.1.8"
msgid "Theyyasattho nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc66:2.1.9"
msgid "corā katakammā vā honti akatakammā vā, rājānaṃ vā theyyaṃ gacchanti suṅkaṃ vā pariharanti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc66:2.1.10"
msgid "Saddhinti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc66:2.1.11"
msgid "ekato."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc66:2.1.12"
msgid "Saṃvidhāyāti—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc66:2.1.13"
msgid "“gacchāmāvuso, gacchāma, bhante;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.1.14"
msgid "gacchāma, bhante, gacchāmāvuso, ajja vā hiyyo vā pare vā gacchāmā”ti saṃvidahati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc9
msgctxt "pli-tv-bu-vb-pc66:2.1.15"
msgid "Antamaso gāmantarampīti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc66:2.1.16"
msgid "kukkuṭasampāte gāme gāmantare gāmantare āpatti pācittiyassa."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc66:2.1.17"
msgid "Agāmake araññe addhayojane addhayojane āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10, pts-vp-pli4.132
msgctxt "pli-tv-bu-vb-pc66:2.2.1"
msgid "Theyyasatthe theyyasatthasaññī saṃvidhāya ekaddhānamaggaṃ paṭipajjati, antamaso gāmantarampi, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.2.2"
msgid "Theyyasatthe vematiko saṃvidhāya ekaddhānamaggaṃ paṭipajjati, antamaso gāmantarampi, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.2.3"
msgid "Theyyasatthe atheyyasatthasaññī saṃvidhāya ekaddhānamaggaṃ paṭipajjati, antamaso gāmantarampi, anāpatti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.2.4"
msgid "Bhikkhu saṃvidahati, manussā na saṃvidahanti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.2.5"
msgid "Atheyyasatthe theyyasatthasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.2.6"
msgid "Atheyyasatthe vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.2.7"
msgid "Atheyyasatthe atheyyasatthasaññī anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc66:2.3.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc66:2.3.2"
msgid "asaṃvidahitvā gacchati, manussā saṃvidahanti bhikkhu na saṃvidahati, visaṅketena gacchati, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bu-vb-pc66:2.3.3"
msgid "Theyyasatthasikkhāpadaṃ niṭṭhitaṃ chaṭṭhaṃ."
msgstr ""
