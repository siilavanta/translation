#. HTML: <section class="sutta" id="pi-tv-bu-vb-pc81"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc81:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc81:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc81:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

msgctxt "pli-tv-bu-vb-pc81:0.4"
msgid "Sahadhammikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc81:1.1.0"
msgid "81. Dubbalasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.154
msgctxt "pli-tv-bu-vb-pc81:1.1.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.2"
msgid "Tena kho pana samayena āyasmā dabbo mallaputto saṃghassa senāsanañca paññapeti bhattāni ca uddisati."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.3"
msgid "So cāyasmā dubbalacīvaro hoti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.4"
msgid "Tena kho pana samayena saṃghassa ekaṃ cīvaraṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.5"
msgid "Atha kho saṃgho taṃ cīvaraṃ āyasmato dabbassa mallaputtassa adāsi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.6"
msgid "Chabbaggiyā bhikkhū ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.7"
msgid "“yathāsanthutaṃ bhikkhū saṃghikaṃ lābhaṃ pariṇāmentī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc81:1.1.8"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.9"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.10"
msgid "“kathañhi nāma chabbaggiyā bhikkhū samaggena saṃghena cīvaraṃ datvā pacchā khīyanadhammaṃ āpajjissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.11"
msgid "“saccaṃ kira tumhe, bhikkhave, samaggena saṃghena cīvaraṃ datvā pacchā khīyanadhammaṃ āpajjathā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.12"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.13"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.14"
msgid "kathañhi nāma tumhe, moghapurisā, samaggena saṃghena cīvaraṃ datvā pacchā khīyanadhammaṃ āpajjissatha."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.15"
msgid "Netaṃ, moghapurisā, appasannānaṃ, vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:1.1.16"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bu-vb-pc81:1.1.17"
msgid "“Yo pana bhikkhu samaggena saṃghena cīvaraṃ datvā pacchā khīyanadhammaṃ āpajjeyya ‘yathāsanthutaṃ bhikkhū saṃghikaṃ lābhaṃ pariṇāmentī’ti, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc81:2.1.1"
msgid "Yo panāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.2"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc81:2.1.3"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.4"
msgid "… pe …"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc81:2.1.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc81:2.1.6"
msgid "Samaggo nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.7"
msgid "saṃgho samānasaṃvāsako samānasīmāyaṃ ṭhito."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc81:2.1.8"
msgid "Cīvaraṃ nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.9"
msgid "channaṃ cīvarānaṃ aññataraṃ cīvaraṃ vikappanupagaṃ pacchimaṃ."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc81:2.1.10"
msgid "Datvāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.11"
msgid "sayaṃ datvā."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc81:2.1.12"
msgid "Yathāsanthutaṃ nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.13"
msgid "yathāmittatā yathāsandiṭṭhatā yathāsambhattatā yathāsamānupajjhāyakatā yathāsamānācariyakatā."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc9
msgctxt "pli-tv-bu-vb-pc81:2.1.14"
msgid "Saṃghikaṃ nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.15"
msgid "saṃghassa dinnaṃ hoti pariccattaṃ."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc10
msgctxt "pli-tv-bu-vb-pc81:2.1.16"
msgid "Lābho nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.17"
msgid "cīvarapiṇḍapātasenāsanagilānappaccayabhesajjaparikkhārā, antamaso cuṇṇapiṇḍopi, dantakaṭṭhampi, dasikasuttampi."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc11, pts-vp-pli4.155
msgctxt "pli-tv-bu-vb-pc81:2.1.18"
msgid "Pacchā khīyanadhammaṃ āpajjeyyāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc81:2.1.19"
msgid "upasampannassa saṃghena sammatassa senāsanapaññāpakassa vā bhattuddesakassa vā yāgubhājakassa vā phalabhājakassa vā khajjabhājakassa vā appamattakavissajjakassa vā cīvaraṃ dinne khiyyati, āpatti pācittiyassa."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc12
msgctxt "pli-tv-bu-vb-pc81:2.2.1"
msgid "Dhammakamme dhammakammasaññī cīvaraṃ dinne khiyyati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.2.2"
msgid "Dhammakamme vematiko cīvaraṃ dinne khiyyati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.2.3"
msgid "Dhammakamme adhammakammasaññī cīvaraṃ dinne khiyyati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bu-vb-pc81:2.2.4"
msgid "Aññaṃ parikkhāraṃ dinne khiyyati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.2.5"
msgid "Upasampannassa saṃghena asammatassa senāsanapaññāpakassa vā bhattuddesakassa vā yāgubhājakassa vā phalabhājakassa vā khajjabhājakassa vā appamattakavissajjakassa vā cīvaraṃ vā aññaṃ vā parikkhāraṃ dinne khiyyati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.2.6"
msgid "Anupasampannassa saṃghena sammatassa vā asammatassa vā senāsanapaññāpakassa vā bhattuddesakassa vā yāgubhājakassa vā phalabhājakassa vā khajjabhājakassa vā appamattakavissajjakassa vā cīvaraṃ vā aññaṃ vā parikkhāraṃ dinne khiyyati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.2.7"
msgid "Adhammakamme dhammakammasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.2.8"
msgid "Adhammakamme vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.2.9"
msgid "Adhammakamme adhammakammasaññī, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bu-vb-pc81:2.3.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc81:2.3.2"
msgid "pakatiyā chandā dosā mohā bhayā karontaṃ “kvattho tassa dinnena laddhāpi vinipātessati na sammā upanessatī”ti khiyyati, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc15
msgctxt "pli-tv-bu-vb-pc81:2.3.3"
msgid "Dubbalasikkhāpadaṃ niṭṭhitaṃ ekādasamaṃ."
msgstr ""
