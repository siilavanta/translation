#. HTML: <section class="sutta" id="pi-tv-bu-vb-pc89"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc89:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc89:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc89:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

msgctxt "pli-tv-bu-vb-pc89:0.4"
msgid "Ratanavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc89:1.1.0"
msgid "89. Nisīdanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-pc89:1.1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.2"
msgid "Tena kho pana samayena bhagavatā bhikkhūnaṃ nisīdanaṃ anuññātaṃ hoti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.3"
msgid "Chabbaggiyā bhikkhū—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.4"
msgid "“bhagavatā nisīdanaṃ anuññātan”ti appamāṇikāni nisīdanāni dhārenti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.5"
msgid "Mañcassapi pīṭhassapi puratopi pacchatopi olambenti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc89:1.1.6"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.7"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.8"
msgid "“kathañhi nāma chabbaggiyā bhikkhū appamāṇikāni nisīdanāni dhāressantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.9"
msgid "“saccaṃ kira tumhe, bhikkhave, appamāṇikāni nisīdanāni dhārethā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.10"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.11"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.12"
msgid "kathañhi nāma tumhe, moghapurisā, appamāṇikāni nisīdanāni dhāressatha."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.13"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.14"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bu-vb-pc89:1.1.15"
msgid "“Nisīdanaṃ pana bhikkhunā kārayamānena pamāṇikaṃ kāretabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.16"
msgid "Tatridaṃ pamāṇaṃ—dīghaso dve vidatthiyo, sugatavidatthiyā; tiriyaṃ diyaḍḍhaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:1.1.17"
msgid "Taṃ atikkāmayato chedanakaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc89:1.1.18"
msgid "Evañcidaṃ bhagavatā bhikkhūnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5, pts-vp-pli4.171
msgctxt "pli-tv-bu-vb-pc89:2.1.1"
msgid "Tena kho pana samayena āyasmā udāyī mahākāyo hoti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.2"
msgid "So bhagavato purato nisīdanaṃ paññapetvā samantato samañchamāno nisīdati."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.3"
msgid "Atha kho bhagavā āyasmantaṃ udāyiṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.4"
msgid "“kissa tvaṃ, udāyi, nisīdanaṃ samantato samañchasi;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.5"
msgid "seyyathāpi purāṇāsikoṭṭho”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.6"
msgid "“Tathā hi pana, bhante, bhagavatā bhikkhūnaṃ atikhuddakaṃ nisīdanaṃ anuññātan”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.7"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.8"
msgid "“anujānāmi, bhikkhave, nisīdanassa dasaṃ vidatthiṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.9"
msgid "Evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc89:2.1.10"
msgid "“Nisīdanaṃ pana bhikkhunā kārayamānena pamāṇikaṃ kāretabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.11"
msgid "Tatridaṃ pamāṇaṃ—dīghaso dve vidatthiyo, sugatavidatthiyā; tiriyaṃ diyaḍḍhaṃ. Dasā vidatthi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:2.1.12"
msgid "Taṃ atikkāmayato chedanakaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc89:3.1.1"
msgid "Nisīdanaṃ nāma"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc89:3.1.2"
msgid "sadasaṃ vuccati."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc89:3.1.3"
msgid "Kārayamānenāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc89:3.1.4"
msgid "karonto vā kārāpento vā pamāṇikaṃ kāretabbaṃ."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc89:3.1.5"
msgid "Tatridaṃ pamāṇaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.6"
msgid "dīghaso dve vidatthiyo, sugatavidatthiyā;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.7"
msgid "tiriyaṃ diyaḍḍhaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.8"
msgid "Dasā vidatthi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.9"
msgid "Taṃ atikkāmetvā karoti vā kārāpeti vā, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.10"
msgid "Paṭilābhena chinditvā pācittiyaṃ desetabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bu-vb-pc89:3.1.11"
msgid "Attanā vippakataṃ attanā pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.12"
msgid "Attanā vippakataṃ parehi pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.13"
msgid "Parehi vippakataṃ attanā pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.14"
msgid "Parehi vippakataṃ parehi pariyosāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bu-vb-pc89:3.1.15"
msgid "Aññassatthāya karoti vā kārāpeti vā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.1.16"
msgid "Aññena kataṃ paṭilabhitvā paribhuñjati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc89:3.2.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc89:3.2.2"
msgid "pamāṇikaṃ karoti, ūnakaṃ karoti, aññena kataṃ pamāṇātikkantaṃ paṭilabhitvā chinditvā paribhuñjati, vitānaṃ vā bhūmattharaṇaṃ vā sāṇipākāraṃ vā bhisiṃ vā bibbohanaṃ vā karoti, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bu-vb-pc89:3.2.3"
msgid "Nisīdanasikkhāpadaṃ niṭṭhitaṃ sattamaṃ."
msgstr ""
