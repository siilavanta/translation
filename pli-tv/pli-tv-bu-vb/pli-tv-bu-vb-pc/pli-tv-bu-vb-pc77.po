#. HTML: <section class="sutta" id="pi-tv-bu-vb-pc77"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc77:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc77:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc77:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

msgctxt "pli-tv-bu-vb-pc77:0.4"
msgid "Sahadhammikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc77:1.1.0"
msgid "77. Sañciccasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.149
msgctxt "pli-tv-bu-vb-pc77:1.1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū sattarasavaggiyānaṃ bhikkhūnaṃ sañcicca kukkuccaṃ upadahanti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.3"
msgid "“bhagavatā, āvuso, sikkhāpadaṃ paññattaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.4"
msgid "‘na ūnavīsativasso puggalo upasampādetabbo’ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.5"
msgid "Tumhe ca ūnavīsativassā upasampannā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.6"
msgid "Kacci no tumhe anupasampannā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.7"
msgid "Te rodanti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.8"
msgid "Bhikkhū evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.9"
msgid "“kissa tumhe, āvuso, rodathā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.10"
msgid "“Ime, āvuso, chabbaggiyā bhikkhū amhākaṃ sañcicca kukkuccaṃ upadahantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc77:1.1.11"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.12"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.13"
msgid "“kathañhi nāma chabbaggiyā bhikkhū bhikkhūnaṃ sañcicca kukkuccaṃ upadahissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.14"
msgid "“saccaṃ kira tumhe, bhikkhave, bhikkhūnaṃ sañcicca kukkuccaṃ upadahathā”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.15"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.16"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.17"
msgid "kathañhi nāma tumhe, moghapurisā, bhikkhūnaṃ sañcicca kukkuccaṃ upadahissatha."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.18"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:1.1.19"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
#. VAR: upadaheyya → uppādeyya (itipi)
msgctxt "pli-tv-bu-vb-pc77:1.1.20"
msgid "“Yo pana bhikkhu bhikkhussa sañcicca kukkuccaṃ upadaheyya—‘itissa muhuttampi aphāsu bhavissatī’ti etadeva paccayaṃ karitvā anaññaṃ, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc77:2.1.1"
msgid "Yo panāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc77:2.1.2"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc77:2.1.3"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc77:2.1.4"
msgid "… pe …"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc77:2.1.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc77:2.1.6"
msgid "Bhikkhussāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc77:2.1.7"
msgid "aññassa bhikkhussa."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc77:2.1.8"
msgid "Sañciccāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc77:2.1.9"
msgid "jānanto sañjānanto cecca abhivitaritvā vītikkamo."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc77:2.1.10"
msgid "Kukkuccaṃ upadaheyyāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc77:2.1.11"
msgid "“ūnavīsativasso maññe tvaṃ upasampanno, vikāle maññe tayā bhuttaṃ, majjaṃ maññe tayā pītaṃ, mātugāmena saddhiṃ raho maññe tayā nisinnan”ti kukkuccaṃ upadahati, āpatti pācittiyassa."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc77:2.1.12"
msgid "Etadeva paccayaṃ karitvā, anaññanti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc77:2.1.13"
msgid "na añño koci paccayo hoti kukkuccaṃ upadahituṃ."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc9
msgctxt "pli-tv-bu-vb-pc77:2.2.1"
msgid "Upasampanne upasampannasaññī sañcicca kukkuccaṃ upadahati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:2.2.2"
msgid "Upasampanne vematiko sañcicca kukkuccaṃ upadahati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:2.2.3"
msgid "Upasampanne anupasampannasaññī sañcicca kukkuccaṃ upadahati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bu-vb-pc77:2.2.4"
msgid "Anupasampannassa sañcicca kukkuccaṃ upadahati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:2.2.5"
msgid "Anupasampanne upasampannasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:2.2.6"
msgid "Anupasampanne vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:2.2.7"
msgid "Anupasampanne anupasampannasaññī, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc77:2.3.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc77:2.3.2"
msgid "na kukkuccaṃ upadahitukāmo “ūnavīsativasso maññe tvaṃ upasampanno, vikāle maññe tayā bhuttaṃ, majjaṃ maññe tayā pītaṃ, mātugāmena saddhiṃ raho maññe tayā nisinnaṃ, iṅgha jānāhi, mā te pacchā kukkuccaṃ ahosī”ti bhaṇati, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bu-vb-pc77:2.3.3"
msgid "Sañciccasikkhāpadaṃ niṭṭhitaṃ sattamaṃ."
msgstr ""
