#. HTML: <section class="sutta" id="pli-tv-bi-vb-np4"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-np4:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np4:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np4:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np4:4.1"
msgid "Pattavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-np4:5.1"
msgid "4. Aññaviññāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-np4:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī gilānā hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.3"
msgid "Atha kho aññataro upāsako yena thullanandā bhikkhunī tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.4"
msgid "upasaṅkamitvā thullanandaṃ bhikkhuniṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.5"
msgid "“kiṃ te, ayye, aphāsu, kiṃ āharīyatū”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.6"
msgid "“Sappinā me, āvuso, attho”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.7"
msgid "Atha kho so upāsako aññatarassa āpaṇikassa gharā kahāpaṇassa sappiṃ āharitvā thullanandāya bhikkhuniyā adāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.8"
msgid "Thullanandā bhikkhunī evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.9"
msgid "“na me, āvuso, sappinā attho;"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.10"
msgid "telena me attho”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.11"
msgid "Atha kho so upāsako yena so āpaṇiko tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.12"
msgid "upasaṅkamitvā taṃ āpaṇikaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.13"
msgid "“na kirāyyo, ayyāya sappinā attho, telena attho."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.14"
msgid "Handa te sappiṃ, telaṃ me dehī”ti."
msgstr ""

#. VAR: ādiyissāma → āharissāma (si, pts1, mr)
#. VAR: vikkāyissati → vikkīyissati (?)
msgctxt "pli-tv-bi-vb-np4:6.15"
msgid "“Sace mayaṃ, ayyo, vikkītaṃ bhaṇḍaṃ puna ādiyissāma, kadā amhākaṃ bhaṇḍaṃ vikkāyissati;"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.16"
msgid "sappissa kayena sappi haṭaṃ, telassa kayaṃ āhara, telaṃ harissasī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.17"
msgid "Atha kho so upāsako ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:6.18"
msgid "“kathañhi nāma ayyā thullanandā aññaṃ viññāpetvā aññaṃ viññāpessatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.249
msgctxt "pli-tv-bi-vb-np4:7.1"
msgid "Assosuṃ kho bhikkhuniyo tassa upāsakassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.4"
msgid "atha kho tā bhikkhuniyo bhikkhūnaṃ etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.5"
msgid "Bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

#. VAR: viññāpetī”ti → viññāpesīti (bj, pts1, mr)
msgctxt "pli-tv-bi-vb-np4:7.6"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī aññaṃ viññāpetvā aññaṃ viññāpetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.7"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.8"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.9"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī aññaṃ viññāpetvā aññaṃ viññāpessati."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.10"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:7.11"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-np4:8.1"
msgid "“Yā pana bhikkhunī aññaṃ viññāpetvā aññaṃ viññāpeyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-np4:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np4:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-np4:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np4:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-np4:10.1"
msgid "Aññaṃ viññāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np4:10.2"
msgid "yaṃ kiñci viññāpetvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-np4:11.1"
msgid "Aññaṃ viññāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np4:11.2"
msgid "taṃ ṭhapetvā aññaṃ viññāpeti, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:11.3"
msgid "Paṭilābhena nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:11.4"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā ekabhikkhuniyā vā."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:11.5"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:11.6"
msgid "“idaṃ me, ayye, aññaṃ viññāpetvā aññaṃ viññāpitaṃ nissaggiyaṃ, imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:11.7"
msgid "dadeyyā”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:11.8"
msgid "dadeyyun”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np4:11.9"
msgid "ayyāya dammī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-np4:12.1"
msgid "Aññe aññasaññā aññaṃ viññāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:12.2"
msgid "Aññe vematikā aññaṃ viññāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:12.3"
msgid "Aññe anaññasaññā aññaṃ viññāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-np4:13.1"
msgid "Anaññe aññasaññā anaññaṃ viññāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:13.2"
msgid "Anaññe vematikā anaññaṃ viññāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np4:13.3"
msgid "Anaññe anaññasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-np4:14.1"
msgid "Anāpatti—"
msgstr ""

#. VAR: taññeva → tañceva (s1-3, pts1)
msgctxt "pli-tv-bi-vb-np4:14.2"
msgid "taññeva viññāpeti, aññañca viññāpeti, ānisaṃsaṃ dassetvā viññāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc10
msgctxt "pli-tv-bi-vb-np4:15.1"
msgid "Catutthasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
