#. HTML: <section class="sutta" id="pli-tv-bi-vb-np5"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-np5:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np5:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np5:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np5:4.1"
msgid "Pattavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-np5:5.1"
msgid "5. Aññacetāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.250
msgctxt "pli-tv-bi-vb-np5:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī gilānā hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.3"
msgid "Atha kho aññataro upāsako yena thullanandā bhikkhunī tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.4"
msgid "upasaṅkamitvā thullanandaṃ bhikkhuniṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.5"
msgid "“kacci, ayye, khamanīyaṃ kacci yāpanīyan”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.6"
msgid "“Na me, āvuso, khamanīyaṃ, na yāpanīyan”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.7"
msgid "“Amukassa, ayye, āpaṇikassa ghare kahāpaṇaṃ nikkhipissāmi, tato yaṃ iccheyyāsi taṃ āharāpeyyāsī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.8"
msgid "Thullanandā bhikkhunī aññataraṃ sikkhamānaṃ āṇāpesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.9"
msgid "“gaccha, sikkhamāne, amukassa āpaṇikassa gharā kahāpaṇassa telaṃ āharā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.10"
msgid "Atha kho sā sikkhamānā tassa āpaṇikassa gharā kahāpaṇassa telaṃ āharitvā thullanandāya bhikkhuniyā adāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.11"
msgid "Thullanandā bhikkhunī evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.12"
msgid "“na me, sikkhamāne, telena attho, sappinā me attho”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.13"
msgid "Atha kho sā sikkhamānā yena so āpaṇiko tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.14"
msgid "upasaṅkamitvā taṃ āpaṇikaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.15"
msgid "“na kira, āvuso, ayyāya telena attho, sappinā attho, handa te telaṃ, sappiṃ me dehī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.16"
msgid "“Sace mayaṃ, ayye, vikkītaṃ bhaṇḍaṃ puna ādiyissāma, kadā amhākaṃ bhaṇḍaṃ vikkāyissati."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.17"
msgid "Telassa kayena telaṃ haṭaṃ, sappissa kayaṃ āhara, sappiṃ harissasī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.18"
msgid "Atha kho sā sikkhamānā rodantī aṭṭhāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.19"
msgid "Bhikkhuniyo taṃ sikkhamānaṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:6.20"
msgid "“kissa tvaṃ, sikkhamāne, rodasī”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-np5:7.1"
msgid "Atha kho sā sikkhamānā bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.5"
msgid "“kathañhi nāma ayyā thullanandā aññaṃ cetāpetvā aññaṃ cetāpessatī”ti … pe …"
msgstr ""

#. VAR: cetāpetī”ti → cetāpesīti (mr)
msgctxt "pli-tv-bi-vb-np5:7.7"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī aññaṃ cetāpetvā aññaṃ cetāpetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.11"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī aññaṃ cetāpetvā aññaṃ cetāpessati."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:7.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-np5:8.1"
msgid "“Yā pana bhikkhunī aññaṃ cetāpetvā aññaṃ cetāpeyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-np5:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np5:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-np5:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np5:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-np5:10.1"
msgid "Aññaṃ cetāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np5:10.1"
msgid "yaṃ kiñci cetāpetvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-np5:11.1"
msgid "Aññaṃ cetāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np5:11.1"
msgid "taṃ ṭhapetvā aññaṃ cetāpeti, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:11.2"
msgid "Paṭilābhena nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:11.3"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā ekabhikkhuniyā vā."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:11.4"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:11.6"
msgid "“idaṃ me, ayye, aññaṃ cetāpetvā aññaṃ cetāpitaṃ nissaggiyaṃ, imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:11.8"
msgid "dadeyyāti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:11.10"
msgid "dadeyyunti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np5:11.12"
msgid "ayyāya dammīti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-np5:12.1"
msgid "Aññe aññasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:12.2"
msgid "Aññe vematikā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:12.3"
msgid "Aññe anaññasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-np5:13.1"
msgid "Anaññe aññasaññā anaññaṃ cetāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:13.2"
msgid "Anaññe vematikā anaññaṃ cetāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np5:13.3"
msgid "Anaññe anaññasaññā anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-np5:14.1"
msgid "Anāpatti—"
msgstr ""

#. VAR: taññeva → tañceva (s1-3)
msgctxt "pli-tv-bi-vb-np5:14.2"
msgid "taññeva cetāpeti, aññañca cetāpeti, ānisaṃsaṃ dassetvā cetāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc10
msgctxt "pli-tv-bi-vb-np5:15.1"
msgid "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

