#. HTML: <section class="sutta" id="pli-tv-bi-vb-np7"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-np7:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np7:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np7:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np7:4.1"
msgid "Pattavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-np7:5.1"
msgid "7. Dutiyasaṃghikacetāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.252
msgctxt "pli-tv-bi-vb-np7:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:6.2"
msgid "Tena kho pana samayena upāsakā bhikkhunisaṃghassa cīvaratthāya chandakaṃ saṅgharitvā aññatarassa pāvārikassa ghare parikkhāraṃ nikkhipitvā bhikkhuniyo upasaṅkamitvā etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:6.3"
msgid "“amukassa, ayye, pāvārikassa ghare cīvaratthāya parikkhāro nikkhitto, tato cīvaraṃ āharāpetvā bhājethā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:6.4"
msgid "Bhikkhuniyo tena ca parikkhārena sayampi yācitvā bhesajjaṃ cetāpetvā paribhuñjiṃsu."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-np7:7.1"
msgid "Upāsakā jānitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:7.2"
msgid "“kathañhi nāma bhikkhuniyo aññadatthikena parikkhārena aññuddisikena saṃghikena saññācikena aññaṃ cetāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo aññadatthikena parikkhārena aññuddisikena saṃghikena saññācikena aññaṃ cetāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:7.8"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo aññadatthikena parikkhārena aññuddisikena saṃghikena saññācikena aññaṃ cetāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:7.11"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-np7:8.1"
msgid "“Yā pana bhikkhunī aññadatthikena parikkhārena aññuddisikena saṃghikena saññācikena aññaṃ cetāpeyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-np7:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np7:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-np7:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np7:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-np7:10.1"
msgid "Aññadatthikena parikkhārena aññuddisikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np7:10.1"
msgid "aññassatthāya dinnena."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-np7:11.1"
msgid "Saṃghikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np7:11.1"
msgid "saṃghassa, na gaṇassa, na ekabhikkhuniyā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-np7:12.1"
msgid "Saññācikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np7:12.1"
msgid "sayaṃ yācitvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-np7:13.1"
msgid "Aññaṃ cetāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np7:13.1"
msgid "yaṃatthāya dinnaṃ taṃ ṭhapetvā aññaṃ cetāpeti, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:13.2"
msgid "Paṭilābhena nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:13.3"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā ekabhikkhuniyā vā."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:13.4"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:13.6"
msgid "“idaṃ me, ayye, aññadatthikena parikkhārena aññuddisikena saṃghikena saññācikena aññaṃ cetāpitaṃ nissaggiyaṃ, imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:13.8"
msgid "dadeyyā”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:13.10"
msgid "dadeyyun”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:13.12"
msgid "ayyāya dammī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-np7:14.1"
msgid "Aññadatthike aññadatthikasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:14.2"
msgid "Aññadatthike vematikā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:14.3"
msgid "Aññadatthike anaññadatthikasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:14.4"
msgid "Nissaṭṭhaṃ paṭilabhitvā yathādāne upanetabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-np7:15.1"
msgid "Anaññadatthike aññadatthikasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:15.2"
msgid "Anaññadatthike vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np7:15.3"
msgid "Anaññadatthike anaññadatthikasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-np7:16.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np7:16.2"
msgid "sesakaṃ upaneti, sāmike apaloketvā upaneti, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bi-vb-np7:17.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

