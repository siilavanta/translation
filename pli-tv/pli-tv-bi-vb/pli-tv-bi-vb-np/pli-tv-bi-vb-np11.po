#. HTML: <section class="sutta" id="pli-tv-bi-vb-np11"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-np11:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np11:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np11:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
#. VAR: Cīvaravagga → nissaggiyakaṇḍassa vagganāmā kaṅkhāvitaraṇīaṭṭhakathāya 46--51 piṭṭhesu
msgctxt "pli-tv-bi-vb-np11:4.1"
msgid "Cīvaravagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-np11:5.1"
msgid "11. Garupāvuraṇasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.255
msgctxt "pli-tv-bi-vb-np11:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī bahussutā hoti bhāṇikā visāradā paṭṭā dhammiṃ kathaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.3"
msgid "Atha kho rājā pasenadi kosalo sītakāle mahagghaṃ kambalaṃ pārupitvā yena thullanandā bhikkhunī tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.4"
msgid "upasaṅkamitvā thullanandaṃ bhikkhuniṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.5"
msgid "Ekamantaṃ nisinnaṃ kho rājānaṃ pasenadiṃ kosalaṃ thullanandā bhikkhunī dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṃsesi."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.6"
msgid "Atha kho rājā pasenadi kosalo thullanandāya bhikkhuniyā dhammiyā kathāya sandassito samādapito samuttejito sampahaṃsito thullanandaṃ bhikkhuniṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.7"
msgid "“vadeyyāsi, ayye, yena attho”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.8"
msgid "“Sace me tvaṃ, mahārāja, dātukāmosi, imaṃ kambalaṃ dehī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.9"
msgid "Atha kho rājā pasenadi kosalo thullanandāya bhikkhuniyā kambalaṃ datvā uṭṭhāyāsanā thullanandaṃ bhikkhuniṃ abhivādetvā padakkhiṇaṃ katvā pakkāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.10"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.11"
msgid "“mahicchā imā bhikkhuniyo asantuṭṭhā."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:6.12"
msgid "Kathañhi nāma rājānaṃ kambalaṃ viññāpessantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-np11:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.4"
msgid "“kathañhi nāma ayyā thullanandā rājānaṃ kambalaṃ viññāpessatī”ti … pe …"
msgstr ""

#. VAR: viññāpetī”ti → viññāpesīti (bj, pts1, mr)
msgctxt "pli-tv-bi-vb-np11:7.5"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī rājānaṃ kambalaṃ viññāpetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.8"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī rājānaṃ kambalaṃ viññāpessati."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
#. VAR: Garupāvuraṇaṃ → garupāpuraṇaṃ (bj, s1-3)
#. VAR: uttari → uttariṃ (bj, s1-3)
msgctxt "pli-tv-bi-vb-np11:8.1"
msgid "“Garupāvuraṇaṃ pana bhikkhuniyā cetāpentiyā catukkaṃsaparamaṃ cetāpetabbaṃ. Tato ce uttari cetāpeyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-np11:9.1"
msgid "Garupāvuraṇaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np11:9.2"
msgid "yaṃ kiñci sītakāle pāvuraṇaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-np11:10.1"
msgid "Cetāpentiyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np11:10.2"
msgid "viññāpentiyā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6, pts-vp-pli4.256
msgctxt "pli-tv-bi-vb-np11:11.1"
msgid "Catukkaṃsaparamaṃ cetāpetabbanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np11:11.2"
msgid "soḷasakahāpaṇagghanakaṃ cetāpetabbaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-np11:12.1"
msgid "Tato ce uttari cetāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np11:12.2"
msgid "tatuttari viññāpeti, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:12.3"
msgid "Paṭilābhena nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:12.4"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā ekabhikkhuniyā vā."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:12.5"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:12.6"
msgid "“idaṃ me, ayye, garupāvuraṇaṃ atirekacatukkaṃsaparamaṃ cetāpitaṃ nissaggiyaṃ, imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:12.7"
msgid "dadeyyāti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:12.8"
msgid "dadeyyunti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:12.9"
msgid "ayyāya dammīti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-np11:13.1"
msgid "Atirekacatukkaṃse atirekasaññā cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:13.2"
msgid "Atirekacatukkaṃse vematikā cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:13.3"
msgid "Atirekacatukkaṃse ūnakasaññā cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-np11:14.1"
msgid "Ūnakacatukkaṃse atirekasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:14.2"
msgid "Ūnakacatukkaṃse vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np11:14.3"
msgid "Ūnakacatukkaṃse ūnakasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-np11:15.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np11:15.2"
msgid "catukkaṃsaparamaṃ cetāpeti, ūnakacatukkaṃsaparamaṃ cetāpeti, ñātakānaṃ, pavāritānaṃ, aññassatthāya, attano dhanena, mahagghaṃ cetāpetukāmassa appagghaṃ cetāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-np11:16.1"
msgid "Ekādasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
