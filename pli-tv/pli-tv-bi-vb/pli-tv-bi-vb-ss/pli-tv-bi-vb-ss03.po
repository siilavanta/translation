#. HTML: <section class="sutta" id="pli-tv-bi-vb-ss3"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-ss3:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss3:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss3:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-ss3:4.1"
msgid "3. Ekagāmantaragamanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-ss3:5.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

#. VAR: gāmakaṃ → gāmake (s1-3)
msgctxt "pli-tv-bi-vb-ss3:5.2"
msgid "Tena kho pana samayena bhaddāya kāpilāniyā antevāsinī bhikkhunī bhikkhunīhi saddhiṃ bhaṇḍitvā gāmakaṃ ñātikulaṃ agamāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.3"
msgid "Bhaddā kāpilānī taṃ bhikkhuniṃ apassantī bhikkhuniyo pucchi—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.4"
msgid "“kahaṃ itthannāmā, na dissatī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.5"
msgid "“Bhikkhunīhi saddhiṃ, ayye, bhaṇḍitvā na dissatī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.6"
msgid "“Ammā, amukasmiṃ gāmake etissā ñātikulaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.7"
msgid "Tattha gantvā vicinathā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.8"
msgid "Bhikkhuniyo tattha gantvā taṃ bhikkhuniṃ passitvā etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.9"
msgid "“kissa tvaṃ, ayye, ekikā āgatā, kaccisi appadhaṃsitā”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:5.10"
msgid "“Appadhaṃsitāmhi, ayye”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.228
msgctxt "pli-tv-bi-vb-ss3:6.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.3"
msgid "“kathañhi nāma bhikkhunī ekā gāmantaraṃ gacchissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī ekā gāmantaraṃ gacchatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.7"
msgid "kathañhi nāma, bhikkhave, bhikkhunī ekā gāmantaraṃ gacchissati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:6.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-ss3:7.1"
msgid "“Yā pana bhikkhunī ekā gāmantaraṃ gaccheyya, ayampi bhikkhunī paṭhamāpattikaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc4
msgctxt "pli-tv-bi-vb-ss3:8.1"
msgid "Evañcidaṃ bhagavatā bhikkhunīnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-ss3:9.1"
msgid "Tena kho pana samayena dve bhikkhuniyo sāketā sāvatthiṃ addhānamaggappaṭipannā honti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.2"
msgid "Antarāmagge nadī taritabbā hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.3"
msgid "Atha kho tā bhikkhuniyo nāvike upasaṅkamitvā etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.4"
msgid "“sādhu no, āvuso, tārethā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.5"
msgid "“Nāyye, sakkā ubho sakiṃ tāretun”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.6"
msgid "Eko ekaṃ uttāresi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.7"
msgid "Uttiṇṇo uttiṇṇaṃ dūsesi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.8"
msgid "Anuttiṇṇo anuttiṇṇaṃ dūsesi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.9"
msgid "Tā pacchā samāgantvā pucchiṃsu—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.10"
msgid "“kaccisi, ayye, appadhaṃsitā”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.11"
msgid "“Padhaṃsitāmhi, ayye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.12"
msgid "Tvaṃ pana, ayye, appadhaṃsitā”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:9.13"
msgid "“Padhaṃsitāmhi, ayye”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-ss3:10.1"
msgid "Atha kho tā bhikkhuniyo sāvatthiṃ gantvā bhikkhunīnaṃ etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.4"
msgid "“kathañhi nāma bhikkhunī ekā nadīpāraṃ gacchissatī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.5"
msgid "Atha kho tā bhikkhuniyo bhikkhūnaṃ etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.6"
msgid "Bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.7"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī ekā nadīpāraṃ gacchatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.10"
msgid "kathañhi nāma, bhikkhave, bhikkhunī ekā nadīpāraṃ gacchissati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.11"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:10.12"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc7
msgctxt "pli-tv-bi-vb-ss3:11.1"
msgid "“Yā pana bhikkhunī ekā vā gāmantaraṃ gaccheyya, ekā vā nadīpāraṃ gaccheyya, ayampi bhikkhunī paṭhamāpattikaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-ss3:12.1"
msgid "Evañcidaṃ bhagavatā bhikkhunīnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9, pts-vp-pli4.229
#. VAR: gacchantā → gantvā (pts1, mr)
msgctxt "pli-tv-bi-vb-ss3:13.1"
msgid "Tena kho pana samayena sambahulā bhikkhuniyo kosalesu janapade sāvatthiṃ gacchantā sāyaṃ aññataraṃ gāmaṃ upagacchiṃsu."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.2"
msgid "Tattha aññatarā bhikkhunī abhirūpā hoti dassanīyā pāsādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.3"
msgid "Aññataro puriso tassā bhikkhuniyā saha dassanena paṭibaddhacitto hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.4"
msgid "Atha kho so puriso tāsaṃ bhikkhunīnaṃ seyyaṃ paññapento tassā bhikkhuniyā seyyaṃ ekamantaṃ paññāpesi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.5"
msgid "Atha kho sā bhikkhunī sallakkhetvā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.6"
msgid "“pariyuṭṭhito ayaṃ puriso;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.7"
msgid "sace rattiṃ āgacchissati, vissaro me bhavissatī”ti, bhikkhuniyo anāpucchā aññataraṃ kulaṃ gantvā seyyaṃ kappesi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.8"
msgid "Atha kho so puriso rattiṃ āgantvā taṃ bhikkhuniṃ gavesanto bhikkhuniyo ghaṭṭesi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.9"
msgid "Bhikkhuniyo taṃ bhikkhuniṃ apassantiyo evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:13.10"
msgid "“nissaṃsayaṃ kho sā bhikkhunī purisena saddhiṃ nikkhantā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-ss3:14.1"
msgid "Atha kho sā bhikkhunī tassā rattiyā accayena yena tā bhikkhuniyo tenupasaṅkami."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.2"
msgid "Bhikkhuniyo taṃ bhikkhuniṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.3"
msgid "“kissa tvaṃ, ayye, purisena saddhiṃ nikkhantā”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.4"
msgid "“Nāhaṃ, ayye, purisena saddhiṃ nikkhantā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.5"
msgid "Bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.6"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.7"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.8"
msgid "“kathañhi nāma bhikkhunī ekā rattiṃ vippavasissatī”ti … pe …"
msgstr ""

#. VAR: vippavasatī”ti → vippavasīti (bj, mr, c1) | vippavasati (pts1)
msgctxt "pli-tv-bi-vb-ss3:14.9"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī ekā rattiṃ vippavasatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.10"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.11"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.12"
msgid "kathañhi nāma, bhikkhave, bhikkhunī ekā rattiṃ vippavasissati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.13"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:14.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc11
msgctxt "pli-tv-bi-vb-ss3:15.1"
msgid "“Yā pana bhikkhunī ekā vā gāmantaraṃ gaccheyya, ekā vā nadīpāraṃ gaccheyya, ekā vā rattiṃ vippavaseyya, ayampi bhikkhunī paṭhamāpattikaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-ss3:16.1"
msgid "Evañcidaṃ bhagavatā bhikkhunīnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-ss3:17.1"
msgid "Tena kho pana samayena sambahulā bhikkhuniyo kosalesu janapade sāvatthiṃ addhānamaggappaṭipannā honti."
msgstr ""

#. VAR: ohīyitvā → ohiyitvā (c1)
msgctxt "pli-tv-bi-vb-ss3:17.2"
msgid "Tattha aññatarā bhikkhunī vaccena pīḷitā ekikā ohīyitvā pacchā agamāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:17.3"
msgid "Manussā taṃ bhikkhuniṃ passitvā dūsesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:17.4"
msgid "Atha kho sā bhikkhunī yena tā bhikkhuniyo tenupasaṅkami."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:17.5"
msgid "Bhikkhuniyo taṃ bhikkhuniṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:17.6"
msgid "“kissa tvaṃ, ayye, ekikā ohīnā, kaccisi appadhaṃsitā”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:17.7"
msgid "“Padhaṃsitāmhi, ayye”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bi-vb-ss3:18.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:18.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:18.3"
msgid "kathañhi nāma bhikkhunī ekā gaṇamhā ohīyissatīti … pe …"
msgstr ""

#. VAR: ohīyatī”ti → ohiyīti (bj, c1)
msgctxt "pli-tv-bi-vb-ss3:18.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī ekā gaṇamhā ohīyatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:18.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:18.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:18.7"
msgid "kathañhi nāma, bhikkhave, bhikkhunī ekā gaṇamhā ohīyissati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:18.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:18.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc15, pts-vp-pli4.230
msgctxt "pli-tv-bi-vb-ss3:19.1"
msgid "“Yā pana bhikkhunī ekā vā gāmantaraṃ gaccheyya, ekā vā nadīpāraṃ gaccheyya, ekā vā rattiṃ vippavaseyya, ekā vā gaṇamhā ohīyeyya, ayampi bhikkhunī paṭhamāpattikaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc16
msgctxt "pli-tv-bi-vb-ss3:20.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:20.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-ss3:20.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:20.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:20.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc17
msgctxt "pli-tv-bi-vb-ss3:21.1"
msgid "Ekā vā gāmantaraṃ gaccheyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:21.2"
msgid "parikkhittassa gāmassa parikkhepaṃ paṭhamaṃ pādaṃ atikkāmentiyā āpatti thullaccayassa, dutiyaṃ pādaṃ atikkāmentiyā āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-bi-vb-ss3:22.1"
msgid "Aparikkhittassa gāmassa upacāraṃ paṭhamaṃ pādaṃ atikkāmentiyā āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:22.2"
msgid "Dutiyaṃ pādaṃ atikkāmentiyā āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc19
msgctxt "pli-tv-bi-vb-ss3:23.1"
msgid "Ekā vā nadīpāraṃ gaccheyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:23.2"
msgid "nadī nāma timaṇḍalaṃ paṭicchādetvā yattha katthaci uttarantiyā bhikkhuniyā antaravāsako temiyati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:23.3"
msgid "Paṭhamaṃ pādaṃ uttarantiyā āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:23.4"
msgid "Dutiyaṃ pādaṃ uttarantiyā āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc20
msgctxt "pli-tv-bi-vb-ss3:24.1"
msgid "Ekā vā rattiṃ vippavaseyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:24.2"
msgid "saha aruṇuggamanā dutiyikāya bhikkhuniyā hatthapāsaṃ vijahantiyā āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:24.3"
msgid "Vijahite āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc21
msgctxt "pli-tv-bi-vb-ss3:25.1"
msgid "Ekā vā gaṇamhā ohīyeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:25.2"
msgid "agāmake araññe dutiyikāya bhikkhuniyā dassanūpacāraṃ vā savanūpacāraṃ vā vijahantiyā āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:25.3"
msgid "Vijahite āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc22
msgctxt "pli-tv-bi-vb-ss3:26.1"
msgid "Ayampīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:26.2"
msgid "purimāyo upādāya vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc23
msgctxt "pli-tv-bi-vb-ss3:27.1"
msgid "Paṭhamāpattikanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:27.2"
msgid "saha vatthujjhācārā āpajjati asamanubhāsanāya."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc24
msgctxt "pli-tv-bi-vb-ss3:28.1"
msgid "Nissāraṇīyanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:28.2"
msgid "saṃghamhā nissārīyati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc25
msgctxt "pli-tv-bi-vb-ss3:29.1"
msgid "Saṃghādisesoti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss3:29.2"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss3:29.3"
