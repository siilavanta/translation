#. HTML: <section class="sutta" id="pli-tv-bi-vb-ss5"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-ss5:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss5:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss5:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-ss5:4.1"
msgid "5. Bhojanapaṭiggahaṇasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-ss5:5.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:5.2"
msgid "Tena kho pana samayena sundarīnandā bhikkhunī abhirūpā hoti dassanīyā pāsādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:5.3"
msgid "Manussā bhattagge sundarīnandaṃ bhikkhuniṃ passitvā avassutā avassutāya sundarīnandāya bhikkhuniyā aggamaggāni bhojanāni denti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:5.4"
msgid "Sundarīnandā bhikkhunī yāvadatthaṃ bhuñjati;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:5.5"
msgid "aññā bhikkhuniyo na cittarūpaṃ labhanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.233
msgctxt "pli-tv-bi-vb-ss5:6.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.3"
msgid "“kathañhi nāma ayyā sundarīnandā avassutā avassutassa purisapuggalassa hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādissati bhuñjissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.4"
msgid "“saccaṃ kira, bhikkhave, sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādati bhuñjatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.7"
msgid "kathañhi nāma, bhikkhave, sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādissati bhuñjissati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:6.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-ss5:7.1"
msgid "“Yā pana bhikkhunī avassutā avassutassa purisapuggalassa hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā khādeyya vā bhuñjeyya vā, ayampi bhikkhunī paṭhamāpattikaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-ss5:8.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:8.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-ss5:8.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:8.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:8.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-ss5:9.1"
msgid "Avassutā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:9.2"
msgid "sārattā apekkhavatī paṭibaddhacittā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-ss5:10.1"
msgid "Avassuto nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:10.2"
msgid "sāratto apekkhavā paṭibaddhacitto."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-ss5:11.1"
msgid "Purisapuggalo nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:11.2"
msgid "manussapuriso, na yakkho na peto na tiracchānagato, viññū paṭibalo sārajjituṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-ss5:12.1"
msgid "Khādanīyaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:12.2"
msgid "pañca bhojanāni—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:12.3"
msgid "udakadantaponaṃ ṭhapetvā avasesaṃ khādanīyaṃ nāma."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-ss5:13.1"
msgid "Bhojanīyaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:13.2"
msgid "pañca bhojanāni—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:13.3"
msgid "odano, kummāso, sattu, maccho, maṃsaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-ss5:14.1"
msgid "“Khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:14.2"
msgid "Ajjhohāre ajjhohāre āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc11
msgctxt "pli-tv-bi-vb-ss5:15.1"
msgid "Ayampīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:15.2"
msgid "purimāyo upādāya vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc12
msgctxt "pli-tv-bi-vb-ss5:16.1"
msgid "Paṭhamāpattikanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:16.2"
msgid "saha vatthujjhācārā āpajjati asamanubhāsanāya."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc13
msgctxt "pli-tv-bi-vb-ss5:17.1"
msgid "Nissāraṇīyanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:17.2"
msgid "saṃghamhā nissārīyati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc14
msgctxt "pli-tv-bi-vb-ss5:18.1"
msgid "Saṃghādisesoti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss5:18.2"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:18.3"
msgid "tenapi vuccati saṃghādisesoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-bi-vb-ss5:19.1"
msgid "Udakadantaponaṃ paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:19.2"
msgid "Ekatoavassute “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-bi-vb-ss5:20.1"
msgid "Ajjhohāre ajjhohāre āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:20.2"
msgid "Udakadantaponaṃ paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:20.3"
msgid "Ubhatoavassute yakkhassa vā petassa vā paṇḍakassa vā tiracchānagatamanussaviggahassa vā hatthato “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:20.4"
msgid "Ajjhohāre ajjhohāre āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:20.5"
msgid "Udakadantaponaṃ paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:20.6"
msgid "Ekatoavassute “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:20.7"
msgid "Ajjhohāre ajjhohāre āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:20.8"
msgid "Udakadantaponaṃ paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc17, pts-vp-pli4.234
msgctxt "pli-tv-bi-vb-ss5:21.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss5:21.2"
msgid "ubhatoanavassutā honti, “anavassuto”ti jānantī paṭiggaṇhāti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc18
msgctxt "pli-tv-bi-vb-ss5:22.1"
msgid "Pañcamasaṃghādisesasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
