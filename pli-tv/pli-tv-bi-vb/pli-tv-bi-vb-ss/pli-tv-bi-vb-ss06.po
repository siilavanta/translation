#. HTML: <section class="sutta" id="pli-tv-bi-vb-ss6"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-ss6:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss6:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss6:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-ss6:4.1"
msgid "6. Dutiyabhojanapaṭiggahaṇasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-ss6:5.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.2"
msgid "Tena kho pana samayena sundarīnandā bhikkhunī abhirūpā hoti dassanīyā pāsādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.3"
msgid "Manussā bhattagge sundarīnandaṃ bhikkhuniṃ passitvā avassutā sundarīnandāya bhikkhuniyā aggamaggāni bhojanāni denti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.4"
msgid "Sundarīnandā bhikkhunī kukkuccāyantī na paṭiggaṇhāti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.5"
msgid "Anantarikā bhikkhunī sundarīnandaṃ bhikkhuniṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.6"
msgid "“kissa tvaṃ, ayye, na paṭiggaṇhāsī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.7"
msgid "“Avassutā, ayye”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.8"
msgid "“Tvaṃ pana, ayye, avassutā”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.9"
msgid "“Nāhaṃ, ayye, avassutā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.10"
msgid "“Kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:5.11"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā, bhuñja vā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-ss6:6.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.3"
msgid "“kathañhi nāma bhikkhunī evaṃ vakkhati—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.4"
msgid "‘kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.5"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā’”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.6"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.7"
msgid "‘kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.8"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā’”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.9"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.10"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.11"
msgid "kathañhi nāma, bhikkhave, bhikkhunī evaṃ vakkhati—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.12"
msgid "‘kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā yato tvaṃ anavassutā;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.13"
msgid "iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.14"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:6.15"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-ss6:7.1"
msgid "“Yā pana bhikkhunī evaṃ vadeyya—"
msgstr ""

#. VAR: te → kinte (s1-3, pts1)
#. VAR: yaṃ te → yante (s1-3, pts1)
#. VAR: Iṅgha, ayye → iṅghayye (bj, s1-3)
msgctxt "pli-tv-bi-vb-ss6:7.2"
msgid "‘kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā. Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā’ti, ayampi bhikkhunī paṭhamāpattikaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-ss6:8.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss6:8.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-ss6:8.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss6:8.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:8.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5, pts-vp-pli4.235
msgctxt "pli-tv-bi-vb-ss6:9.1"
msgid "Evaṃ vadeyyāti—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss6:9.2"
msgid "“kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:9.3"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā”ti uyyojeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:9.4"
msgid "Tassā vacanena “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:9.5"
msgid "Ajjhohāre ajjhohāre āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:9.6"
msgid "Bhojanapariyosāne āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-ss6:10.1"
msgid "Ayampīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss6:10.2"
msgid "purimāyo upādāya vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-ss6:11.1"
msgid "Paṭhamāpattikanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss6:11.2"
msgid "saha vatthujjhācārā āpajjati asamanubhāsanāya."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-ss6:12.1"
msgid "Nissāraṇīyanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss6:12.2"
msgid "saṃghamhā nissārīyati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-ss6:13.1"
msgid "Saṃghādisesoti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss6:13.2"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:13.3"
msgid "tenapi vuccati saṃghādisesoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-ss6:14.1"
msgid "Udakadantaponaṃ “paṭiggaṇhā”ti uyyojeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:14.2"
msgid "Tassā vacanena “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-ss6:15.1"
msgid "Ekatoavassute yakkhassa vā petassa vā paṇḍakassa vā tiracchānagatamanussaviggahassa vā hatthato khādanīyaṃ vā bhojanīyaṃ vā “khāda vā bhuñja vā”ti uyyojeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:15.2"
msgid "Tassā vacanena “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:15.3"
msgid "Ajjhohāre ajjhohāre āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:15.4"
msgid "Bhojanapariyosāne āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:15.5"
msgid "Udakadantaponaṃ paṭiggaṇhāti uyyojeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:15.6"
msgid "Tassā vacanena “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-ss6:16.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss6:16.2"
msgid "“anavassuto”ti jānantī uyyojeti, “kupitā na paṭiggaṇhātī”ti uyyojeti, “kulānuddayatāya na paṭiggaṇhātī”ti uyyojeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc13
msgctxt "pli-tv-bi-vb-ss6:17.1"
msgid "Chaṭṭhasaṃghādisesasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
