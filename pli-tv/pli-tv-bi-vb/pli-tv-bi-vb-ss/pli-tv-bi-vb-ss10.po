#. HTML: <section class="sutta" id="pli-tv-bi-vb-ss10"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-ss10:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
#. VAR: Bhikkhunivibhaṅga → ^pi-tv-bu-vb-ss5#23 [Saṅghādisesā 5: Sañcarittasikkhāpada] (5:9)
msgctxt "pli-tv-bi-vb-ss10:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
#. VAR: Saṃghādisesakaṇḍa → ^pi-tv-bu-vb-ss8#18 [Saṅghādisesā 8: Duṭṭhadosasikkhāpada] (8:12)
msgctxt "pli-tv-bi-vb-ss10:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-ss10:4.1"
msgid "10. Sikkhaṃpaccācikkhaṇasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-ss10:5.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:5.2"
msgid "Tena kho pana samayena caṇḍakāḷī bhikkhunī bhikkhunīhi saddhiṃ bhaṇḍitvā kupitā anattamanā evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:5.3"
msgid "“buddhaṃ paccācikkhāmi, dhammaṃ paccācikkhāmi, saṃghaṃ paccācikkhāmi, sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:5.4"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro, santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā tāsāhaṃ santike brahmacariyaṃ carissāmī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.236
msgctxt "pli-tv-bi-vb-ss10:6.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

#. VAR: caṇḍakāḷī → caṇḍakāḷī bhikkhunī (si, mr) | caṇḍakālī bhikkhunī (pts1)
msgctxt "pli-tv-bi-vb-ss10:6.3"
msgid "“kathañhi nāma ayyā caṇḍakāḷī kupitā anattamanā evaṃ vakkhati—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.4"
msgid "‘buddhaṃ paccācikkhāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.5"
msgid "sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.6"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro, santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā tāsāhaṃ santike brahmacariyaṃ carissāmī’”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.7"
msgid "“saccaṃ kira, bhikkhave, caṇḍakāḷī bhikkhunī kupitā anattamanā evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.8"
msgid "‘buddhaṃ paccācikkhāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.9"
msgid "sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.10"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro, santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmī’”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.11"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.12"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.13"
msgid "kathañhi nāma, bhikkhave, caṇḍakāḷī bhikkhunī kupitā anattamanā evaṃ vakkhati—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.14"
msgid "‘buddhaṃ paccācikkhāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.15"
msgid "sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.16"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro, santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.17"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:6.18"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-ss10:7.1"
msgid "“Yā pana bhikkhunī kupitā anattamanā evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.2"
msgid "‘buddhaṃ paccācikkhāmi, dhammaṃ paccācikkhāmi, saṃghaṃ paccācikkhāmi, sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.3"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.4"
msgid "Santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmī’ti, sā bhikkhunī bhikkhunīhi evamassa vacanīyā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.5"
msgid "‘māyye, kupitā anattamanā evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.6"
msgid "buddhaṃ paccācikkhāmi, dhammaṃ paccācikkhāmi, saṃghaṃ paccācikkhāmi, sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.7"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro."
msgstr ""

#. VAR: abhiramāyye → abhiramayye (bj, s1-3, pts1)
msgctxt "pli-tv-bi-vb-ss10:7.8"
msgid "Santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmīti, abhiramāyye, svākkhāto dhammo;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.9"
msgid "cara brahmacariyaṃ sammā dukkhassa antakiriyāyā’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.10"
msgid "Evañca sā bhikkhunī bhikkhunīhi vuccamānā tatheva paggaṇheyya, sā bhikkhunī bhikkhunīhi yāvatatiyaṃ samanubhāsitabbā tassa paṭinissaggāya."
msgstr ""

#. VAR: samanubhāsīyamānā → samanubhāsiyamānā (bj, s1-3, pts1)
msgctxt "pli-tv-bi-vb-ss10:7.11"
msgid "Yāvatatiyañce samanubhāsīyamānā taṃ paṭinissajjeyya iccetaṃ kusalaṃ;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:7.12"
msgid "no ce paṭinissajjeyya, ayampi bhikkhunī yāvatatiyakaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-ss10:8.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:8.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-ss10:8.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:8.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:8.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-ss10:9.1"
msgid "Kupitā anattamanāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:9.2"
msgid "anabhiraddhā āhatacittā khilajātā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-ss10:10.1"
msgid "Evaṃ vadeyyāti—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:10.2"
msgid "“buddhaṃ paccācikkhāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:10.3"
msgid "sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:10.4"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:10.5"
msgid "Santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmī”ti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-ss10:11.1"
msgid "Sā bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:11.2"
msgid "yā sā evaṃvādinī bhikkhunī."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-ss10:11.3"
msgid "Bhikkhunīhīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:11.4"
msgid "aññāhi bhikkhunīhi."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-ss10:12.1"
msgid "Yā passanti yā suṇanti tāhi vattabbā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.2"
msgid "“māyye, kupitā anattamanā evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.3"
msgid "‘buddhaṃ paccācikkhāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.4"
msgid "sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.5"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.6"
msgid "Santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.7"
msgid "Abhiramāyye, svākkhāto dhammo;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.8"
msgid "cara brahmacariyaṃ sammā dukkhassa antakiriyāyā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.9"
msgid "Dutiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.10"
msgid "Tatiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.11"
msgid "Sace paṭinissajjati iccetaṃ kusalaṃ, no ce paṭinissajjati āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.12"
msgid "Sutvā na vadanti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.13"
msgid "Sā bhikkhunī saṃghamajjhampi ākaḍḍhitvā vattabbā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.14"
msgid "“māyye, kupitā anattamanā evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.15"
msgid "‘buddhaṃ paccācikkhāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.16"
msgid "sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.17"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.18"
msgid "Santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.19"
msgid "Abhiramāyye, svākkhāto dhammo;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.20"
msgid "cara brahmacariyaṃ sammā dukkhassa antakiriyāyā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.21"
msgid "Dutiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.22"
msgid "Tatiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.23"
msgid "Sace paṭinissajjati iccetaṃ kusalaṃ, no ce paṭinissajjati āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.24"
msgid "Sā bhikkhunī samanubhāsitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.25"
msgid "Evañca pana, bhikkhave, samanubhāsitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:12.26"
msgid "Byattāya bhikkhuniyā paṭibalāya saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc9, pts-vp-pli4.237
msgctxt "pli-tv-bi-vb-ss10:13.1"
msgid "“Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:13.2"
msgid "Ayaṃ itthannāmā bhikkhunī kupitā anattamanā evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:13.3"
msgid "‘buddhaṃ paccācikkhāmi, dhammaṃ paccācikkhāmi, saṃghaṃ paccācikkhāmi, sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:13.4"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:13.5"
msgid "Santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā tāsāhaṃ santike brahmacariyaṃ carissāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:13.6"
msgid "Sā taṃ vatthuṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:13.7"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuniṃ samanubhāseyya tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:13.8"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-ss10:14.1"
msgid "Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.2"
msgid "Ayaṃ itthannāmā bhikkhunī kupitā anattamanā evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.3"
msgid "‘buddhaṃ paccācikkhāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.4"
msgid "sikkhaṃ paccācikkhāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.5"
msgid "Kinnumāva samaṇiyo yā samaṇiyo sakyadhītaro."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.6"
msgid "Santaññāpi samaṇiyo lajjiniyo kukkuccikā sikkhākāmā, tāsāhaṃ santike brahmacariyaṃ carissāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.7"
msgid "Sā taṃ vatthuṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.8"
msgid "Saṃgho itthannāmaṃ bhikkhuniṃ samanubhāsati tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.9"
msgid "Yassā ayyāya khamati itthannāmāya bhikkhuniyā samanubhāsanā tassa vatthussa paṭinissaggāya, sā tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:14.10"
msgid "yassā nakkhamati sā, bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-ss10:15.1"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:15.2"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:15.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-ss10:16.1"
msgid "Samanubhaṭṭhā saṃghena itthannāmā bhikkhunī tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:16.2"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-ss10:17.1"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:17.2"
msgid "Dvīhi kammavācāhi thullaccayā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:17.3"
msgid "Kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:17.4"
msgid "Saṃghādisesaṃ ajjhāpajjantiyā ñattiyā dukkaṭaṃ, dvīhi kammavācāhi thullaccayā paṭippassambhanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc14
msgctxt "pli-tv-bi-vb-ss10:18.1"
msgid "Ayampīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:18.2"
msgid "purimāyo upādāya vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc15
msgctxt "pli-tv-bi-vb-ss10:19.1"
msgid "Yāvatatiyakanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:19.2"
msgid "yāvatatiyaṃ samanubhāsanāya āpajjati, na saha vatthujjhācārā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc16
msgctxt "pli-tv-bi-vb-ss10:20.1"
msgid "Nissāraṇīyanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:20.2"
msgid "saṃghamhā nissārīyati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc17
msgctxt "pli-tv-bi-vb-ss10:21.1"
msgid "Saṃghādisesoti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss10:21.2"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:21.3"
msgid "tenapi vuccati saṃghādisesoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-bi-vb-ss10:22.1"
msgid "Dhammakamme dhammakammasaññā na paṭinissajjati, āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:22.2"
msgid "Dhammakamme vematikā na paṭinissajjati, āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:22.3"
msgid "Dhammakamme adhammakammasaññā na paṭinissajjati, āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-bi-vb-ss10:23.1"
msgid "Adhammakamme dhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:23.2"
msgid "Adhammakamme vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:23.3"
msgid "Adhammakamme adhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-bi-vb-ss10:24.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss10:24.2"
msgid "asamanubhāsantiyā, paṭinissajjantiyā, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
