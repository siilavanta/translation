#. HTML: <section class="sutta" id="pli-tv-bi-vb-pj5"><article><div class="hgroup"><p class="division">
#. VAR: Theravāda Vinaya → ^pi-tv-bu-vb-pj1#45 [Pārājika 1: Paṭhamapārājikasikkhāpada/ Santhatabhāṇavāra] paṭhamapārājikasikkhāpada (1:1)
msgctxt "pli-tv-bi-vb-pj5:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
#. VAR: Bhikkhunivibhaṅga → ^pi-tv-bu-vb-pj2#20 [Pārājika 2: Dutiyapārājikasikkhāpada] dutiyapārājikasikkhāpada (2:2)
msgctxt "pli-tv-bi-vb-pj5:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
#. VAR: Pārājikakaṇḍa → ^pi-tv-bu-vb-pj3#22 [Pārājika 3: Tatiyapārājikasikkhāpada] tatiyapārājikasikkhāpada (3:3)
msgctxt "pli-tv-bi-vb-pj5:3.1"
msgid "Pārājikakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pj5:4.1"
msgid "5. Ubbhajāṇumaṇḍalikāsikkhāpada"
msgstr ""

#. HTML: </h1></div><p class="namo">
#. REF: sc1
msgctxt "pli-tv-bi-vb-pj5:5.1"
msgid "Namo tassa Bhagavato Arahato Sammāsambuddhassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.211
msgctxt "pli-tv-bi-vb-pj5:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.2"
msgid "Tena kho pana samayena sāḷho migāranattā bhikkhunisaṃghassa vihāraṃ kattukāmo hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.3"
msgid "Atha kho sāḷho migāranattā bhikkhuniyo upasaṅkamitvā etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.4"
msgid "“icchāmahaṃ, ayye, bhikkhunisaṃghassa vihāraṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.5"
msgid "Detha me navakammikaṃ bhikkhunin”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.6"
msgid "Tena kho pana samayena catasso bhaginiyo bhikkhunīsu pabbajitā honti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.7"
msgid "nandā, nandavatī, sundarīnandā, thullanandāti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.8"
msgid "Tāsu sundarīnandā bhikkhunī taruṇapabbajitā abhirūpā hoti dassanīyā pāsādikā paṇḍitā byattā medhāvinī dakkhā analasā, tatrupāyāya vīmaṃsāya samannāgatā, alaṃ kātuṃ alaṃ saṃvidhātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.9"
msgid "Atha kho bhikkhunisaṃgho sundarīnandaṃ bhikkhuniṃ sammannitvā sāḷhassa migāranattuno navakammikaṃ adāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.10"
msgid "Tena kho pana samayena sundarīnandā bhikkhunī sāḷhassa migāranattuno nivesanaṃ abhikkhaṇaṃ gacchati—"
msgstr ""

#. VAR: kuṭhāriṃ → kuddālaṃ (bj) | kudhāriṃ (mr)
#. VAR: parasuṃ → pharasuṃ (bj, s1-3, pts1, mr)
msgctxt "pli-tv-bi-vb-pj5:6.11"
msgid "“vāsiṃ detha, parasuṃ detha, kuṭhāriṃ detha, kudālaṃ detha, nikhādanaṃ dethā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.12"
msgid "Sāḷhopi migāranattā bhikkhunupassayaṃ abhikkhaṇaṃ gacchati katākataṃ jānituṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:6.13"
msgid "Te abhiṇhadassanena paṭibaddhacittā ahesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc3, pts-vp-pli4.212
msgctxt "pli-tv-bi-vb-pj5:7.1"
msgid "Atha kho sāḷho migāranattā sundarīnandaṃ bhikkhuniṃ dūsetuṃ okāsaṃ alabhamāno etadevaatthāya bhikkhunisaṃghassa bhattaṃ akāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.2"
msgid "Atha kho sāḷho migāranattā bhattagge āsanaṃ paññapento “ettakā bhikkhuniyo ayyāya sundarīnandāya vuḍḍhatarā”ti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.3"
msgid "ekamantaṃ āsanaṃ paññapesi “ettakā navakatarā”ti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.4"
msgid "ekamantaṃ āsanaṃ paññapesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.5"
msgid "Paṭicchanne okāse nikūṭe sundarīnandāya bhikkhuniyā āsanaṃ paññapesi, yathā therā bhikkhuniyo jāneyyuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.6"
msgid "“navakānaṃ bhikkhunīnaṃ santike nisinnā”ti;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.7"
msgid "navakāpi bhikkhuniyo jāneyyuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.8"
msgid "“therānaṃ bhikkhunīnaṃ santike nisinnā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.9"
msgid "Atha kho sāḷho migāranattā bhikkhunisaṃghassa kālaṃ ārocāpesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.10"
msgid "“kālo, ayye, niṭṭhitaṃ bhattan”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.11"
msgid "Sundarīnandā bhikkhunī sallakkhetvā—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.12"
msgid "“na bahukato sāḷho migāranattā bhikkhunisaṃghassa bhattaṃ akāsi;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.13"
msgid "maṃ so dūsetukāmo."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.14"
msgid "Sacāhaṃ gamissāmi vissaro me bhavissatī”ti, antevāsiniṃ bhikkhuniṃ āṇāpesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.15"
msgid "“gaccha me piṇḍapātaṃ nīhara."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.16"
msgid "Yo ce maṃ pucchati, ‘gilānā’ti paṭivedehī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:7.17"
msgid "“Evaṃ, ayye”ti kho sā bhikkhunī sundarīnandāya bhikkhuniyā paccassosi."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pj5:8.1"
msgid "Tena kho pana samayena sāḷho migāranattā bahidvārakoṭṭhake ṭhito hoti sundarīnandaṃ bhikkhuniṃ paṭipucchanto—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.2"
msgid "“kahaṃ, ayye, ayyā sundarīnandā?"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.3"
msgid "Kahaṃ, ayye, ayyā sundarīnandā”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.4"
msgid "Evaṃ vutte, sundarīnandāya bhikkhuniyā antevāsinī bhikkhunī sāḷhaṃ migāranattāraṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.5"
msgid "“gilānāvuso;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.6"
msgid "piṇḍapātaṃ nīharissāmī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.7"
msgid "Atha kho sāḷho migāranattā—"
msgstr ""

#. VAR: atthāya → yaṃpāhaṃ (s1-3, pts1)
msgctxt "pli-tv-bi-vb-pj5:8.8"
msgid "“yampāhaṃ atthāya bhikkhunisaṃghassa bhattaṃ akāsiṃ ayyāya sundarīnandāya kāraṇā”ti manusse āṇāpetvā—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.9"
msgid "“bhikkhunisaṃghaṃ bhattena parivisathā”ti vatvā yena bhikkhunupassayo tenupasaṅkami."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.10"
msgid "Tena kho pana samayena sundarīnandā bhikkhunī bahārāmakoṭṭhake ṭhitā hoti sāḷhaṃ migāranattāraṃ patimānentī."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.11"
msgid "Addasā kho sundarīnandā bhikkhunī sāḷhaṃ migāranattāraṃ dūratova āgacchantaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.12"
msgid "Disvāna upassayaṃ pavisitvā sasīsaṃ pārupitvā mañcake nipajji."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.13"
msgid "Atha kho sāḷho migāranattā yena sundarīnandā bhikkhunī tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.14"
msgid "upasaṅkamitvā sundarīnandaṃ bhikkhuniṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.15"
msgid "“kiṃ te, ayye, aphāsu, kissa nipannāsī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.16"
msgid "“Evañhetaṃ, āvuso, hoti yā anicchantaṃ icchatī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.17"
msgid "“Kyāhaṃ taṃ, ayye, na icchissāmi?"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.18"
msgid "Api cāhaṃ okāsaṃ na labhāmi taṃ dūsetun”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:8.19"
msgid "Avassuto avassutāya sundarīnandāya bhikkhuniyā kāyasaṃsaggaṃ samāpajji."
msgstr ""

#. HTML: </p><p>
#. REF: sc5, pts-vp-pli4.213
msgctxt "pli-tv-bi-vb-pj5:9.1"
msgid "Tena kho pana samayena aññatarā bhikkhunī jarādubbalā caraṇagilānā sundarīnandāya bhikkhuniyā avidūre nipannā hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.2"
msgid "Addasā kho sā bhikkhunī sāḷhaṃ migāranattāraṃ avassutaṃ avassutāya sundarīnandāya bhikkhuniyā kāyasaṃsaggaṃ samāpajjantaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.3"
msgid "Disvāna ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.4"
msgid "“kathañhi nāma ayyā sundarīnandā avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyissatī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.5"
msgid "Atha kho sā bhikkhunī bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.6"
msgid "Yā tā bhikkhuniyo appicchā santuṭṭhā lajjiniyo kukkuccikā sikkhākāmā tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.7"
msgid "“kathañhi nāma ayyā sundarīnandā avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyissatī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.8"
msgid "Atha kho tā bhikkhuniyo bhikkhūnaṃ etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.9"
msgid "Ye te bhikkhū appicchā santuṭṭhā lajjino kukkuccakā sikkhākāmā te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:9.10"
msgid "“kathañhi nāma sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyissatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pj5:10.1"
msgid "Atha kho te bhikkhū sundarīnandaṃ bhikkhuniṃ anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.2"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā bhikkhū paṭipucchi—"
msgstr ""

#. VAR: sādiyatī”ti → sādiyīti (bj, pts1, mr)
msgctxt "pli-tv-bi-vb-pj5:10.3"
msgid "“saccaṃ kira, bhikkhave, sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.5"
msgid "Vigarahi buddho bhagavā—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.6"
msgid "“ananucchavikaṃ, bhikkhave, sundarīnandāya bhikkhuniyā ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.7"
msgid "Kathañhi nāma, bhikkhave, sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya pasannānaṃ vā bhiyyobhāvāya."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.9"
msgid "Atha khvetaṃ, bhikkhave, appasannānañceva appasādāya pasannānañca ekaccānaṃ aññathattāyā”ti."
msgstr ""

#. VAR: santuṭṭhassa → santuṭṭhiyā (pts1, mr)
#. VAR: asantuṭṭhitāya → asantuṭṭhiyā (bj, pts1, mr) | asantuṭṭhatāya (s1-3)
msgctxt "pli-tv-bi-vb-pj5:10.10"
msgid "Atha kho bhagavā sundarīnandaṃ bhikkhuniṃ anekapariyāyena vigarahitvā dubbharatāya dupposatāya mahicchatāya asantuṭṭhitāya saṅgaṇikāya kosajjassa avaṇṇaṃ bhāsitvā, anekapariyāyena subharatāya suposatāya appicchassa santuṭṭhassa sallekhassa dhutassa pāsādikassa apacayassa vīriyārambhassa vaṇṇaṃ bhāsitvā, bhikkhūnaṃ tadanucchavikaṃ tadanulomikaṃ dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.11"
msgid "“tena hi, bhikkhave, bhikkhunīnaṃ sikkhāpadaṃ paññapessāmi dasa atthavase paṭicca—"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.12"
msgid "saṃghasuṭṭhutāya, saṃghaphāsutāya, dummaṅkūnaṃ bhikkhunīnaṃ niggahāya, pesalānaṃ bhikkhunīnaṃ phāsuvihārāya, diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya, samparāyikānaṃ āsavānaṃ paṭighātāya, appasannānaṃ pasādāya, pasannānaṃ bhiyyobhāvāya, saddhammaṭṭhitiyā vinayānuggahāya."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:10.13"
msgid "Evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pj5:11.1"
msgid "“Yā pana bhikkhunī avassutā avassutassa purisapuggalassa adhakkhakaṃ ubbhajāṇumaṇḍalaṃ āmasanaṃ vā parāmasanaṃ vā gahaṇaṃ vā chupanaṃ vā paṭipīḷanaṃ vā sādiyeyya, ayampi pārājikā hoti asaṃvāsā ubbhajāṇumaṇḍalikā”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc8, pts-vp-pli4.214
msgctxt "pli-tv-bi-vb-pj5:12.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pj5:12.1"
msgid "yā yādisā yathāyuttā yathājaccā yathānāmā yathāgottā yathāsīlā yathāvihārinī yathāgocarā therā vā navā vā majjhimā vā, esā vuccati yā panāti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pj5:13.1"
msgid "Bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pj5:13.1"
msgid "bhikkhikāti bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.2"
msgid "bhikkhācariyaṃ ajjhupagatāti bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.3"
msgid "bhinnapaṭadharāti bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.4"
msgid "samaññāya bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.5"
msgid "paṭiññāya bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.6"
msgid "ehi bhikkhunīti bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.7"
msgid "tīhi saraṇagamanehi upasampannāti bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.8"
msgid "bhadrā bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.9"
msgid "sārā bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.10"
msgid "sekhā bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.11"
msgid "asekhā bhikkhunī;"
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.12"
msgid "samaggena ubhatosaṃghena ñatticatutthena kammena akuppena ṭhānārahena upasampannāti bhikkhunī."
msgstr ""

msgctxt "pli-tv-bi-vb-pj5:13.13"
msgid "Tatra yāyaṃ bhikkhunī samaggena ubhatosaṃghena ñatticatutthena kammena akuppena ṭhānārahena upasampannā, ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pj5:14.1"
msgid "Avassutā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pj5:14.1"
msgid "sārattā apekkhavatī paṭibaddhacittā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pj5:15.1"
msgid "Avassuto nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pj5:15.1"
msgid "sāratto apekkhavā paṭibaddhacitto."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc12
msgctxt "pli-tv-bi-vb-pj5:16.1"
msgid "Purisapuggalo nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pj5:16.1"
msgid "manussapuriso na yakkho na peto na tiracchānagato viññū paṭibalo kāyasaṃsaggaṃ samāpajjituṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc13
msgctxt "pli-tv-bi-vb-pj5:17.1"
msgid "Adhakkhakanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pj5:17.1"
msgid "heṭṭhakkhakaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc14
msgctxt "pli-tv-bi-vb-pj5:18.1"
msgid "Ubbhajāṇumaṇḍalanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pj5:18.1"
