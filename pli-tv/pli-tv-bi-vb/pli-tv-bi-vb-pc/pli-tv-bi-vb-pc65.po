#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc65"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc65:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc65:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc65:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc65:4.1"
msgid "Gabbhinivagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc65:5.1"
msgid "65. Gihigatasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc65:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:6.2"
msgid "Tena kho pana samayena bhikkhuniyo ūnadvādasavassaṃ gihigataṃ vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:6.3"
msgid "Tā akkhamā honti sītassa uṇhassa jighacchāya pipāsāya ḍaṃsamakasavātātapasarīsapasamphassānaṃ duruttānaṃ durāgatānaṃ vacanapathānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:6.4"
msgid "Uppannānaṃ sārīrikānaṃ vedanānaṃ dukkhānaṃ tibbānaṃ kharānaṃ kaṭukānaṃ asātānaṃ amanāpānaṃ pāṇaharānaṃ anadhivāsakajātikā honti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.322
msgctxt "pli-tv-bi-vb-pc65:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.3"
msgid "“kathañhi nāma bhikkhuniyo ūnadvādasavassaṃ gihigataṃ vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo ūnadvādasavassaṃ gihigataṃ vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo ūnadvādasavassaṃ gihigataṃ vuṭṭhāpessanti."
msgstr ""

#. VAR: Ūnadvādasavassā → ūnadvādasavassā hi (bj, s1-3) | ūnadvādasavassāhi (mr)
msgctxt "pli-tv-bi-vb-pc65:7.8"
msgid "Ūnadvādasavassā, bhikkhave, gihigatā akkhamā hoti sītassa uṇhassa jighacchāya pipāsāya ḍaṃsamakasavātātapasarīsapasamphassānaṃ duruttānaṃ durāgatānaṃ vacanapathānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.9"
msgid "Uppannānaṃ sārīrikānaṃ vedanānaṃ dukkhānaṃ tibbānaṃ kharānaṃ kaṭukānaṃ asātānaṃ amanāpānaṃ pāṇaharānaṃ anadhivāsakajātikā hoti."
msgstr ""

#. VAR: Dvādasavassāva → dvādasavassā ca (s1-3, pts1, mr)
msgctxt "pli-tv-bi-vb-pc65:7.10"
msgid "Dvādasavassāva kho, bhikkhave, gihigatā khamā hoti sītassa uṇhassa jighacchāya pipāsāya ḍaṃsamakasavātātapasarīsapasamphassānaṃ duruttānaṃ durāgatānaṃ vacanapathānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.11"
msgid "Uppannānaṃ sārīrikānaṃ vedanānaṃ dukkhānaṃ tibbānaṃ kharānaṃ kaṭukānaṃ asātānaṃ amanāpānaṃ pāṇaharānaṃ adhivāsakajātikā hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:7.13"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc65:8.1"
msgid "“Yā pana bhikkhunī ūnadvādasavassaṃ gihigataṃ vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc65:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc65:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc65:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc65:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc65:10.1"
msgid "Ūnadvādasavassā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc65:10.2"
msgid "appattadvādasavassā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc65:10.3"
msgid "Gihigatā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc65:10.4"
msgid "purisantaragatā vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc65:10.5"
msgid "Vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc65:10.6"
msgid "upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc65:11.1"
msgid "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:11.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:11.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:11.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:11.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc65:12.1"
msgid "Ūnadvādasavassāya ūnadvādasavassasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:12.2"
msgid "Ūnadvādasavassāya vematikā vuṭṭhāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:12.3"
msgid "Ūnadvādasavassāya paripuṇṇasaññā vuṭṭhāpeti, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc65:13.1"
msgid "Paripuṇṇadvādasavassāya ūnadvādasavassasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:13.2"
msgid "Paripuṇṇadvādasavassāya vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:13.3"
msgid "Paripuṇṇadvādasavassāya paripuṇṇasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc65:14.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc65:14.2"
msgid "ūnadvādasavassaṃ paripuṇṇasaññā vuṭṭhāpeti, paripuṇṇadvādasavassaṃ paripuṇṇasaññā vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc65:15.1"
msgid "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
