#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc27"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc27:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc27:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc27:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc27:4.1"
msgid "Naggavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc27:5.1"
msgid "27. Paṭibāhanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc27:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:6.2"
msgid "Tena kho pana samayena bhikkhunisaṃghassa akālacīvaraṃ uppannaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:6.3"
msgid "Atha kho bhikkhunisaṃgho taṃ cīvaraṃ bhājetukāmo sannipati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:6.4"
msgid "Tena kho pana samayena thullanandāya bhikkhuniyā antevāsiniyo bhikkhuniyo pakkantā honti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:6.5"
msgid "Thullanandā bhikkhunī tā bhikkhuniyo etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:6.6"
msgid "“ayye, bhikkhuniyo pakkantā, na tāva cīvaraṃ bhājīyissatī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:6.7"
msgid "Cīvaravibhaṅgaṃ paṭibāhi."
msgstr ""

#. VAR: pakkamiṃsu → vippakkamiṃsu (s1-3, pts1)
msgctxt "pli-tv-bi-vb-pc27:6.8"
msgid "Bhikkhuniyo na tāva cīvaraṃ bhājīyissatīti pakkamiṃsu."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:6.9"
msgid "Thullanandā bhikkhunī antevāsinīsu bhikkhunīsu āgatāsu taṃ cīvaraṃ bhājāpesi."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc27:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:7.4"
msgid "“kathañhi nāma ayyā thullanandā dhammikaṃ cīvaravibhaṅgaṃ paṭibāhissatī”ti … pe …"
msgstr ""

#. VAR: paṭibāhatī”ti → paṭibāhīti (bj, pts1, mr)
msgctxt "pli-tv-bi-vb-pc27:7.6"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī dhammikaṃ cīvaravibhaṅgaṃ paṭibāhatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:7.7"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:7.8"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:7.10"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī dhammikaṃ cīvaravibhaṅgaṃ paṭibāhissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:7.11"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:7.13"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc27:8.1"
msgid "“Yā pana bhikkhunī dhammikaṃ cīvaravibhaṅgaṃ paṭibāheyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc27:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc27:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc27:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc27:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc27:10.1"
msgid "Dhammiko nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc27:10.1"
msgid "cīvaravibhaṅgo samaggo bhikkhunisaṃgho sannipatitvā bhājeti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6, pts-vp-pli4.285
#. VAR: cīvaraṃ na bhājeyyāti → cīvaraṃ bhājeyyāti (bj, s1-3, pts1, mr)+. HTML: <b>
msgctxt "pli-tv-bi-vb-pc27:11.1"
msgid "Paṭibāheyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc27:11.1"
msgid "kathaṃ imaṃ cīvaraṃ na bhājeyyāti paṭibāhati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc27:12.1"
msgid "Dhammike dhammikasaññā paṭibāhati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:12.2"
msgid "Dhammike vematikā paṭibāhati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:12.3"
msgid "Dhammike adhammikasaññā paṭibāhati, anāpatti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:12.4"
msgid "Adhammike dhammikasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:12.5"
msgid "Adhammike vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:12.6"
msgid "Adhammike adhammikasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc27:13.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc27:13.2"
msgid "ānisaṃsaṃ dassetvā paṭibāhati, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc27:14.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

