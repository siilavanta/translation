#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc95"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc95:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc95:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc95:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc95:4.1"
msgid "Chattupāhanavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc95:5.1"
msgid "95. Pañhāpucchanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc95:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.2"
msgid "Tena kho pana samayena bhikkhuniyo anokāsakataṃ bhikkhuṃ pañhaṃ pucchanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.3"
msgid "Bhikkhū ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.4"
msgid "“kathañhi nāma bhikkhuniyo anokāsakataṃ bhikkhuṃ pañhaṃ pucchissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.6"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo anokāsakataṃ bhikkhuṃ pañhaṃ pucchantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.7"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.8"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.10"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo anokāsakataṃ bhikkhuṃ pañhaṃ pucchissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.11"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:6.13"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc95:7.1"
msgid "“Yā pana bhikkhunī anokāsakataṃ bhikkhuṃ pañhaṃ puccheyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc95:8.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc95:8.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc95:8.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc95:8.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:8.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc95:9.1"
msgid "Anokāsakatanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc95:9.1"
msgid "anāpucchā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc95:9.2"
msgid "Bhikkhunti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc95:9.2"
msgid "upasampannaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc95:9.3"
msgid "Pañhaṃ puccheyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc95:9.3"
msgid "suttante okāsaṃ kārāpetvā vinayaṃ vā abhidhammaṃ vā pucchati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:9.4"
msgid "Vinaye okāsaṃ kārāpetvā suttantaṃ vā abhidhammaṃ vā pucchati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:9.5"
msgid "Abhidhamme okāsaṃ kārāpetvā suttantaṃ vā vinayaṃ vā pucchati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc95:10.1"
msgid "Anāpucchite anāpucchitasaññā pañhaṃ pucchati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:10.2"
msgid "Anāpucchite vematikā pañhaṃ pucchati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:10.3"
msgid "Anāpucchite āpucchitasaññā pañhaṃ pucchati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc95:11.1"
msgid "Āpucchite anāpucchitasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:11.2"
msgid "Āpucchite vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:11.3"
msgid "Āpucchite āpucchitasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc95:12.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc95:12.2"
msgid "okāsaṃ kārāpetvā pucchati, anodissa okāsaṃ kārāpetvā yattha katthaci pucchati, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc95:13.1"
msgid "Dvādasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

