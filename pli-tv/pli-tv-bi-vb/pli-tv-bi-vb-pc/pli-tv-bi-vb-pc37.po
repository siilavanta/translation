#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc37"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc37:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc37:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc37:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc37:4.1"
msgid "Tuvaṭṭavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc37:5.1"
msgid "37. Antoraṭṭhasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc37:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:6.2"
msgid "Tena kho pana samayena bhikkhuniyo antoraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ caranti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:6.3"
msgid "Dhuttā dūsenti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc37:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.3"
msgid "“kathañhi nāma bhikkhuniyo antoraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ carissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo antoraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ carantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo antoraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ carissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc37:8.1"
msgid "“Yā pana bhikkhunī antoraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ careyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc37:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc37:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc37:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc37:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc37:10.1"
msgid "Antoraṭṭheti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc37:10.2"
msgid "yassa vijite viharati, tassa raṭṭhe."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc37:11.1"
msgid "Sāsaṅkaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc37:11.2"
msgid "tasmiṃ magge corānaṃ niviṭṭhokāso dissati, bhuttokāso dissati, ṭhitokāso dissati, nisinnokāso dissati, nipannokāso dissati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc37:12.1"
msgid "Sappaṭibhayaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc37:12.2"
msgid "tasmiṃ magge corehi manussā hatā dissanti, viluttā dissanti, ākoṭitā dissanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc37:13.1"
msgid "Asatthikā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc37:13.2"
msgid "vinā satthena."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc37:14.1"
msgid "Cārikaṃ careyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc37:14.2"
msgid "kukkuṭasampāte gāme gāmantare gāmantare āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:14.3"
msgid "Agāmake araññe addhayojane addhayojane āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc37:15.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc37:15.2"
msgid "satthena saha gacchati, kheme appaṭibhaye gacchati, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc37:16.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
