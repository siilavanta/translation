#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc83"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc83:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc83:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc83:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc83:4.1"
msgid "Kumāribhūtavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc83:5.1"
msgid "83. Ekavassasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.337
msgctxt "pli-tv-bi-vb-pc83:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:6.2"
msgid "Tena kho pana samayena bhikkhuniyo ekaṃ vassaṃ dve vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:6.3"
msgid "Upassayo tatheva na sammati."
msgstr ""

#. VAR: Manussā → manussā vihāracārikaṃ āhiṇḍantā passitvā (bj)
msgctxt "pli-tv-bi-vb-pc83:6.4"
msgid "Manussā tatheva ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:6.5"
msgid "“kathañhi nāma bhikkhuniyo ekaṃ vassaṃ dve vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:6.6"
msgid "Upassayo tatheva na sammatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc83:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.5"
msgid "“kathañhi nāma bhikkhuniyo ekaṃ vassaṃ dve vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.7"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo ekaṃ vassaṃ dve vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.11"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo ekaṃ vassaṃ dve vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:7.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc83:8.1"
msgid "“Yā pana bhikkhunī ekaṃ vassaṃ dve vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc83:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc83:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc83:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc83:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc83:10.1"
msgid "Ekaṃ vassanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc83:10.1"
msgid "ekaṃ saṃvaccharaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc83:10.2"
msgid "Dve vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc83:10.2"
msgid "dve upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc83:11.1"
msgid "“Dve vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:11.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:11.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:11.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:11.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc83:12.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc83:12.2"
msgid "ekantarikaṃ ekaṃ vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc83:13.1"
msgid "Terasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-pc83:13.2"
msgid "Kumāribhūtavaggo aṭṭhamo."
msgstr ""

