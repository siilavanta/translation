#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc38"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc38:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc38:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc38:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc38:4.1"
msgid "Tuvaṭṭavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc38:5.1"
msgid "38. Tiroraṭṭhasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.296
msgctxt "pli-tv-bi-vb-pc38:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:6.2"
msgid "Tena kho pana samayena bhikkhuniyo tiroraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ caranti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:6.3"
msgid "Dhuttā dūsenti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc38:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.4"
msgid "“kathañhi nāma bhikkhuniyo tiroraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ carissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.6"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo tiroraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ carantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.7"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.8"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.10"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo tiroraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ carissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.11"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:7.13"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc38:8.1"
msgid "“Yā pana bhikkhunī tiroraṭṭhe sāsaṅkasammate sappaṭibhaye asatthikā cārikaṃ careyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc38:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc38:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc38:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc38:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc38:10.1"
msgid "Tiroraṭṭheti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc38:10.1"
msgid "yassa vijite viharati, taṃ ṭhapetvā aññassa raṭṭhe."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc38:11.1"
msgid "Sāsaṅkaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc38:11.1"
msgid "tasmiṃ magge corānaṃ niviṭṭhokāso dissati, bhuttokāso dissati, ṭhitokāso dissati, nisinnokāso dissati, nipannokāso dissati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc38:12.1"
msgid "Sappaṭibhayaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc38:12.1"
msgid "tasmiṃ magge corehi manussā hatā dissanti, viluttā dissanti, ākoṭitā dissanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc38:13.1"
msgid "Asatthikā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc38:13.1"
msgid "vinā satthena."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc38:14.1"
msgid "Cārikaṃ careyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc38:14.1"
msgid "kukkuṭasampāte gāme gāmantare gāmantare, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:14.2"
msgid "Agāmake araññe addhayojane addhayojane āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc38:15.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc38:15.2"
msgid "satthena saha gacchati, kheme appaṭibhaye gacchati, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc38:16.1"
msgid "Aṭṭhamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

