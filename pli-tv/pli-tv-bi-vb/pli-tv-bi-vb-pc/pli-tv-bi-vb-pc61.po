#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc61"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc61:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc61:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc61:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc61:4.1"
msgid "Gabbhinivagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc61:5.1"
msgid "61. Gabbhinisikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc61:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:6.2"
msgid "Tena kho pana samayena bhikkhuniyo gabbhiniṃ vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:6.3"
msgid "Sā piṇḍāya carati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:6.4"
msgid "Manussā evamāhaṃsu—"
msgstr ""

#. VAR: garubhārā → garugabbhā (s1-3)
msgctxt "pli-tv-bi-vb-pc61:6.5"
msgid "“dethāyyāya bhikkhaṃ, garubhārā ayyā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc61:7.1"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:7.2"
msgid "“kathañhi nāma bhikkhuniyo gabbhiniṃ vuṭṭhāpessantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc61:8.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.5"
msgid "“kathañhi nāma bhikkhuniyo gabbhiniṃ vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.7"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo gabbhiniṃ vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.11"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo gabbhiniṃ vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:8.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc61:9.1"
msgid "“Yā pana bhikkhunī gabbhiniṃ vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc61:10.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc61:10.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc61:10.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc61:10.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:10.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc61:11.1"
msgid "Gabbhinī nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc61:11.1"
msgid "āpannasattā vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc61:11.2"
msgid "Vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc61:11.2"
msgid "upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc61:12.1"
msgid "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:12.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:12.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:12.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:12.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli4.318
msgctxt "pli-tv-bi-vb-pc61:13.1"
msgid "Gabbhiniyā gabbhinisaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:13.2"
msgid "Gabbhiniyā vematikā vuṭṭhāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:13.3"
msgid "Gabbhiniyā agabbhinisaññā vuṭṭhāpeti, anāpatti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:13.4"
msgid "Agabbhiniyā gabbhinisaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:13.5"
msgid "Agabbhiniyā vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:13.6"
msgid "Agabbhiniyā agabbhinisaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc61:14.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc61:14.2"
msgid "gabbhiniṃ agabbhinisaññā vuṭṭhāpeti, agabbhiniṃ agabbhinisaññā vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc61:15.1"
msgid "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

