#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc89"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc89:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc89:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc89:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc89:4.1"
msgid "Chattupāhanavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc89:5.1"
msgid "89. Vāsitakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc89:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo vāsitakena piññākena nahāyanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:6.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:6.4"
msgid "“kathañhi nāma bhikkhuniyo vāsitakena piññākena nahāyissanti, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc89:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.4"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo vāsitakena piññākena nahāyissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.5"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo vāsitakena piññākena nahāyantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.8"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo vāsitakena piññākena nahāyissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc89:8.1"
msgid "“Yā pana bhikkhunī vāsitakena piññākena nahāyeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc89:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc89:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc89:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc89:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc89:10.1"
msgid "Vāsitakaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc89:10.2"
msgid "yaṃ kiñci gandhavāsitakaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc89:10.3"
msgid "Piññākaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc89:10.4"
msgid "tilapiṭṭhaṃ vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc89:10.5"
msgid "Nahāyeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc89:10.6"
msgid "nahāyati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:10.7"
msgid "Payoge dukkaṭaṃ, nahānapariyosāne āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc89:11.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc89:11.2"
msgid "ābādhappaccayā, pakatipiññākena nahāyati, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc89:12.1"
msgid "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
