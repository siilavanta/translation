#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc22"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc22:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc22:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc22:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc22:4.1"
msgid "Naggavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc22:5.1"
msgid "22. Udakasāṭikasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.279
msgctxt "pli-tv-bi-vb-pc22:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:6.2"
msgid "Tena kho pana samayena bhagavatā bhikkhunīnaṃ udakasāṭikā anuññātā hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:6.3"
msgid "Chabbaggiyā bhikkhuniyo—"
msgstr ""

#. VAR: dhāresuṃ → dhārenti (bj)
msgctxt "pli-tv-bi-vb-pc22:6.4"
msgid "“bhagavatā udakasāṭikā anuññātā”ti appamāṇikāyo udakasāṭikāyo dhāresuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:6.5"
msgid "Puratopi pacchatopi ākaḍḍhantā āhiṇḍanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc22:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.3"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo appamāṇikāyo udakasāṭikāyo dhāressantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.4"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo appamāṇikāyo udakasāṭikāyo dhārentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.7"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo appamāṇikāyo udakasāṭikāyo dhāressanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc22:8.1"
msgid "“Udakasāṭikaṃ pana bhikkhuniyā kārayamānāya pamāṇikā kāretabbā. Tatridaṃ pamāṇaṃ—dīghaso catasso vidatthiyo, sugatavidatthiyā; tiriyaṃ dve vidatthiyo. Taṃ atikkāmentiyā chedanakaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc4
#. VAR: nivatthā → nivatthāya (s1-3)+. HTML: <b>
msgctxt "pli-tv-bi-vb-pc22:9.1"
msgid "Udakasāṭikā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc22:9.2"
msgid "yāya nivatthā nahāyati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc22:10.1"
msgid "Kārayamānāyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc22:10.2"
msgid "karontiyā vā kārāpentiyā vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc22:11.1"
msgid "Pamāṇikā kāretabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:11.2"
msgid "Tatridaṃ pamāṇaṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:11.3"
msgid "dīghaso catasso vidatthiyo, sugatavidatthiyā;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:11.4"
msgid "tiriyaṃ dve vidatthiyo."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:11.5"
msgid "Taṃ atikkāmetvā karoti vā kārāpeti vā, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:11.6"
msgid "Paṭilābhena chinditvā pācittiyaṃ desetabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc22:12.1"
msgid "Attanā vippakataṃ attanā pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:12.2"
msgid "Attanā vippakataṃ parehi pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:12.3"
msgid "Parehi vippakataṃ attanā pariyosāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:12.4"
msgid "Parehi vippakataṃ parehi pariyosāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc22:13.1"
msgid "Aññassatthāya karoti vā kārāpeti vā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:13.2"
msgid "Aññena kataṃ paṭilabhitvā paribhuñjati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc22:14.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc22:14.2"
msgid "pamāṇikaṃ karoti, ūnakaṃ karoti, aññena kataṃ pamāṇātikkantaṃ paṭilabhitvā chinditvā paribhuñjati, vitānaṃ vā bhūmattharaṇaṃ vā sāṇipākāraṃ vā bhisiṃ vā bibbohanaṃ vā karoti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc22:15.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
