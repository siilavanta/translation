#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc91"><article><div class="hgroup"><h1>
msgctxt "pli-tv-bi-vb-pc91:1.1"
msgid "Pācittiyā 91: Theravāda Vinaya"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-bi-vb-pc91:2.1"
msgid "2. Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </h2><h3>
msgctxt "pli-tv-bi-vb-pc91:3.1"
msgid "2.4. Pācittiyakaṇḍa"
msgstr ""

#. HTML: </h3><h4>
msgctxt "pli-tv-bi-vb-pc91:4.1"
msgid "2.4.9 Chattupāhanavagga"
msgstr ""

#. HTML: </h4><h2>
msgctxt "pli-tv-bi-vb-pc91:5.1"
msgid "2.4.9.8 Sikkhamānaummaddāpanasikkhāpada"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli4.343
msgctxt "pli-tv-bi-vb-pc91:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:6.2"
msgid "Tena kho pana samayena bhikkhuniyo sikkhamānāya ummaddāpentipi parimaddāpentipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:6.3"
msgid "Manussā vihāracārikaṃ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:6.4"
msgid "“kathañhi nāma bhikkhuniyo sikkhamānāya ummaddāpessantipi parimaddāpessantipi, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc91:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.5"
msgid "“kathañhi nāma bhikkhuniyo sikkhamānāya ummaddāpessantipi parimaddāpessantipī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.7"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo sikkhamānāya ummaddāpentipi parimaddāpentipī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.11"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo sikkhamānāya ummaddāpessantipi parimaddāpessantipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:7.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc91:8.1"
msgid "“Yā pana bhikkhunī sikkhamānāya ummaddāpeyya vā parimaddāpeyya vā, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc91:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc91:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc91:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc91:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc91:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc91:10.1"
msgid "Sikkhamānā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc91:10.1"
msgid "dve vassāni chasu dhammesu sikkhitasikkhā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc91:11.1"
msgid "Ummaddāpeyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc91:11.1"
msgid "ummaddāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc91:12.1"
msgid "Parimaddāpeyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc91:12.1"
msgid "sambāhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc91:13.1"
msgid "Anāpatti—"
msgstr ""

#. VAR: gilānāya → ābādhappaccayā (s1-3, mr) | ābādhapaccayā (pts1)
msgctxt "pli-tv-bi-vb-pc91:13.2"
msgid "gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc91:14.1"
msgid "Aṭṭhama-navama-dasamasikkhāpadāni niṭṭhitāni."
msgstr ""

