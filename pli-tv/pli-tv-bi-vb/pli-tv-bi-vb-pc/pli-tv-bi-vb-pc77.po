#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc77"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc77:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc77:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc77:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc77:4.1"
msgid "Kumāribhūtavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc77:5.1"
msgid "77. Sikkhamāna-navuṭṭhāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.332
msgctxt "pli-tv-bi-vb-pc77:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:6.2"
msgid "Tena kho pana samayena aññatarā sikkhamānā thullanandaṃ bhikkhuniṃ upasaṅkamitvā upasampadaṃ yāci."
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:6.3"
msgid "Thullanandā bhikkhunī taṃ sikkhamānaṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:6.4"
msgid "“sace me tvaṃ, ayye, cīvaraṃ dassasi evāhaṃ taṃ vuṭṭhāpessāmī”ti vatvā, neva vuṭṭhāpeti na vuṭṭhāpanāya ussukkaṃ karoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc77:7.1"
msgid "Atha kho sā sikkhamānā bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.5"
msgid "“kathañhi nāma ayyā thullanandā sikkhamānaṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.6"
msgid "‘sace me tvaṃ, ayye, cīvaraṃ dassasi evāhaṃ taṃ vuṭṭhāpessāmī’ti vatvā, neva vuṭṭhāpessati na vuṭṭhāpanāya ussukkaṃ karissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.8"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī sikkhamānaṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.9"
msgid "‘sace me tvaṃ, ayye, cīvaraṃ dassasi evāhaṃ taṃ vuṭṭhāpessāmī’ti vatvā, neva vuṭṭhāpeti na vuṭṭhāpanāya ussukkaṃ karotī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.10"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.11"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.13"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī sikkhamānaṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.14"
msgid "‘sace me tvaṃ, ayye, cīvaraṃ dassasi evāhaṃ taṃ vuṭṭhāpessāmī’ti vatvā, neva vuṭṭhāpessati na vuṭṭhāpanāya ussukkaṃ karissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.15"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:7.17"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
#. VAR: evāhaṃ taṃ → evāhantaṃ (bj, s1-3)
msgctxt "pli-tv-bi-vb-pc77:8.1"
msgid "“Yā pana bhikkhunī sikkhamānaṃ—‘sace me tvaṃ, ayye, cīvaraṃ dassasi, evāhaṃ taṃ vuṭṭhāpessāmī’ti vatvā, sā pacchā anantarāyikinī neva vuṭṭhāpeyya na vuṭṭhāpanāya ussukkaṃ kareyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc77:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc77:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc77:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc77:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc77:10.1"
msgid "Sikkhamānā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc77:10.1"
msgid "dve vassāni chasu dhammesu sikkhitasikkhā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc77:11.1"
msgid "Sace me tvaṃ, ayye, cīvaraṃ dassasi evāhaṃ taṃ vuṭṭhāpessāmīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc77:11.1"
msgid "evāhaṃ taṃ upasampādessāmi."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc77:12.1"
msgid "Sā pacchā anantarāyikinīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc77:12.1"
msgid "asati antarāye."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc77:13.1"
msgid "Neva vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc77:13.1"
msgid "na sayaṃ vuṭṭhāpeyya."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc77:14.1"
msgid "Na vuṭṭhāpanāya ussukkaṃ kareyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc77:14.1"
msgid "na aññaṃ āṇāpeyya."
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:14.2"
msgid "“Neva vuṭṭhāpessāmi na vuṭṭhāpanāya ussukkaṃ karissāmī”ti dhuraṃ nikkhittamatte āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc77:15.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc77:15.2"
msgid "sati antarāye, pariyesitvā na labhati, gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc77:16.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

