#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc90"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc90:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc90:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc90:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc90:4.1"
msgid "Chattupāhanavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc90:5.1"
msgid "90. Bhikkhuniummaddāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.342
msgctxt "pli-tv-bi-vb-pc90:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:6.2"
msgid "Tena kho pana samayena bhikkhuniyo bhikkhuniyā ummaddāpentipi parimaddāpentipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:6.3"
msgid "Manussā vihāracārikaṃ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:6.4"
msgid "“kathañhi nāma bhikkhuniyo bhikkhuniyā ummaddāpessantipi parimaddāpessantipi, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc90:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.4"
msgid "“kathañhi nāma bhikkhuniyo bhikkhuniyā ummaddāpessantipi parimaddāpessantipī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.5"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo bhikkhuniyā ummaddāpentipi parimaddāpentipī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.8"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo bhikkhuniyā ummaddāpessantipi parimaddāpessantipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc90:8.1"
msgid "“Yā pana bhikkhunī bhikkhuniyā ummaddāpeyya vā parimaddāpeyya vā, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc90:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc90:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc90:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc90:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc90:10.1"
msgid "Bhikkhuniyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc90:10.2"
msgid "aññāya bhikkhuniyā."
msgstr ""

#. VAR: ummaddāpeti → ubbaṭṭāpeti (bj)+. HTML: <b>
msgctxt "pli-tv-bi-vb-pc90:10.3"
msgid "Ummaddāpeyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc90:10.4"
msgid "ummaddāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc90:10.5"
msgid "Parimaddāpeyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc90:10.6"
msgid "sambāhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc90:11.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc90:11.2"
msgid "gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc90:12.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
