#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc75"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc75:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc75:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc75:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc75:4.1"
msgid "Kumāribhūtavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc75:5.1"
msgid "75. Paripuṇṇadvādasavassasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.330
msgctxt "pli-tv-bi-vb-pc75:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:6.2"
msgid "Tena kho pana samayena bhikkhuniyo paripuṇṇadvādasavassā saṃghena asammatā vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:6.3"
msgid "Tā bālā honti abyattā;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:6.4"
msgid "na jānanti kappiyaṃ vā akappiyaṃ vā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:6.5"
msgid "Saddhivihāriniyopi bālā honti abyattā;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:6.6"
msgid "na jānanti kappiyaṃ vā akappiyaṃ vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc75:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.4"
msgid "“kathañhi nāma bhikkhuniyo paripuṇṇadvādasavassā saṃghena asammatā vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.6"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo paripuṇṇadvādasavassā saṃghena asammatā vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.7"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.8"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.10"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo paripuṇṇadvādasavassā saṃghena asammatā vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.11"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.13"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.15"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.16"
msgid "“anujānāmi, bhikkhave, paripuṇṇadvādasavassāya bhikkhuniyā vuṭṭhāpanasammutiṃ dātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.17"
msgid "Evañca pana, bhikkhave, dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.18"
msgid "Tāya paripuṇṇadvādasavassāya bhikkhuniyā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā vuḍḍhānaṃ bhikkhunīnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.19"
msgid "‘ahaṃ, ayye, itthannāmā paripuṇṇadvādasavassā bhikkhunī saṃghaṃ vuṭṭhāpanasammutiṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.20"
msgid "Dutiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.21"
msgid "Tatiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.22"
msgid "Sā bhikkhunī saṃghena paricchinditabbā—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.23"
msgid "‘byattāyaṃ bhikkhunī lajjinī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.24"
msgid "Sace bālā ca hoti alajjinī ca, na dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.25"
msgid "Sace bālā hoti lajjinī, na dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.26"
msgid "Sace byattā hoti alajjinī, na dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.27"
msgid "Sace byattā ca hoti lajjinī ca, dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.28"
msgid "Evañca pana, bhikkhave, dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:7.29"
msgid "Byattāya bhikkhuniyā paṭibalāya saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc75:8.1"
msgid "‘Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:8.2"
msgid "Ayaṃ itthannāmā paripuṇṇadvādasavassā bhikkhunī saṃghaṃ vuṭṭhāpanasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:8.3"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmāya paripuṇṇadvādasavassāya bhikkhuniyā vuṭṭhāpanasammutiṃ dadeyya."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:8.4"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc75:9.1"
msgid "Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:9.2"
msgid "Ayaṃ itthannāmā paripuṇṇadvādasavassā bhikkhunī saṃghaṃ vuṭṭhāpanasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:9.3"
msgid "Saṃgho itthannāmāya paripuṇṇadvādasavassāya bhikkhuniyā vuṭṭhāpanasammutiṃ deti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:9.4"
msgid "Yassā ayyāya khamati itthannāmāya paripuṇṇadvādasavassāya bhikkhuniyā vuṭṭhāpanasammutiyā dānaṃ, sā tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:9.5"
msgid "yassā nakkhamati, sā bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc75:10.1"
msgid "Dinnā saṃghena itthannāmāya paripuṇṇadvādasavassāya bhikkhuniyā vuṭṭhāpanasammuti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:10.2"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc75:11.1"
msgid "Atha kho bhagavā tā bhikkhuniyo anekapariyāyena vigarahitvā dubbharatāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:11.3"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc75:12.1"
msgid "“Yā pana bhikkhunī paripuṇṇadvādasavassā saṃghena asammatā vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc75:13.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc75:13.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc75:13.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc75:13.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:13.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc75:14.1"
msgid "Paripuṇṇadvādasavassā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc75:14.1"
msgid "pattadvādasavassā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10, pts-vp-pli4.331
msgctxt "pli-tv-bi-vb-pc75:15.1"
msgid "Asammatā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc75:15.1"
msgid "ñattidutiyena kammena vuṭṭhāpanasammuti na dinnā hoti."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc75:15.2"
msgid "Vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc75:15.2"
msgid "upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc75:16.1"
msgid "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:16.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:16.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:16.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:16.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc75:17.1"
msgid "Dhammakamme dhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:17.2"
msgid "Dhammakamme vematikā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:17.3"
msgid "Dhammakamme adhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-pc75:18.1"
msgid "Adhammakamme dhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:18.2"
msgid "Adhammakamme vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:18.3"
msgid "Adhammakamme adhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bi-vb-pc75:19.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc75:19.2"
msgid "paripuṇṇadvādasavassā saṃghena sammatā vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc15
msgctxt "pli-tv-bi-vb-pc75:20.1"
msgid "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

