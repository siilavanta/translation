#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc93"><article><div class="hgroup"><h1>
msgctxt "pli-tv-bi-vb-pc93:1.1"
msgid "Pācittiyā 93: Theravāda Vinaya"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-bi-vb-pc93:2.1"
msgid "2. Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </h2><h3>
msgctxt "pli-tv-bi-vb-pc93:3.1"
msgid "2.4. Pācittiyakaṇḍa"
msgstr ""

#. HTML: </h3><h4>
msgctxt "pli-tv-bi-vb-pc93:4.1"
msgid "2.4.9 Chattupāhanavagga"
msgstr ""

#. HTML: </h4><h2>
msgctxt "pli-tv-bi-vb-pc93:5.1"
msgid "2.4.9.‍10 Gihiniummaddāpanasikkhāpada"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli4.343
msgctxt "pli-tv-bi-vb-pc93:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:6.2"
msgid "Tena kho pana samayena bhikkhuniyo gihiniyā ummaddāpentipi parimaddāpentipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:6.3"
msgid "Manussā vihāracārikaṃ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:6.4"
msgid "“kathañhi nāma bhikkhuniyo gihiniyā ummaddāpessantipi parimaddāpessantipi, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc93:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.4"
msgid "“kathañhi nāma bhikkhuniyo gihiniyā ummaddāpessantipi parimaddāpessantipī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.5"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo gihiniyā ummaddāpentipi parimaddāpentipī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.8"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo gihiniyā ummaddāpessantipi parimaddāpessantipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc93:8.1"
msgid "“Yā pana bhikkhunī gihiniyā ummaddāpeyya vā parimaddāpeyya vā, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc93:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc93:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc93:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc93:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc93:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc93:10.1"
msgid "Gihinī nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc93:10.2"
msgid "agārinī vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc93:11.1"
msgid "Ummaddāpeyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc93:11.2"
msgid "ummaddāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc93:12.1"
msgid "Parimaddāpeyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc93:12.2"
msgid "sambāhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc93:13.1"
msgid "Anāpatti—"
msgstr ""

#. VAR: gilānāya → ābādhappaccayā (s1-3, mr) | ābādhapaccayā (pts1)
msgctxt "pli-tv-bi-vb-pc93:13.2"
msgid "gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc93:14.1"
msgid "Aṭṭhama-navama-dasamasikkhāpadāni niṭṭhitāni."
msgstr ""
