#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc26"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc26:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc26:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc26:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc26:4.1"
msgid "Naggavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc26:5.1"
msgid "26. Gaṇacīvarasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc26:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:6.2"
msgid "Tena kho pana samayena thullanandāya bhikkhuniyā upaṭṭhākakulaṃ thullanandaṃ bhikkhuniṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:6.3"
msgid "“bhikkhunisaṃghassa, ayye, cīvaraṃ dassāmā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:6.4"
msgid "Thullanandā bhikkhunī—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:6.5"
msgid "“tumhe bahukiccā bahukaraṇīyā”ti antarāyaṃ akāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:6.6"
msgid "Tena kho pana samayena tassa kulassa gharaṃ ḍayhati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:6.7"
msgid "Te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:6.8"
msgid "“kathañhi nāma ayyā thullanandā amhākaṃ deyyadhammaṃ antarāyaṃ karissati."
msgstr ""

#. VAR: paribāhirā → parihīnā (bj)
msgctxt "pli-tv-bi-vb-pc26:6.9"
msgid "Ubhayenāmha paribāhirā bhogehi ca puññena cā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc26:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.5"
msgid "“kathañhi nāma ayyā thullanandā gaṇassa cīvaralābhaṃ antarāyaṃ karissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.7"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī gaṇassa cīvaralābhaṃ antarāyaṃ akāsī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.11"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī gaṇassa cīvaralābhaṃ antarāyaṃ karissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:7.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc26:8.1"
msgid "“Yā pana bhikkhunī gaṇassa cīvaralābhaṃ antarāyaṃ kareyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc26:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc26:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc26:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc26:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc26:10.1"
msgid "Gaṇo nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc26:10.1"
msgid "bhikkhunisaṃgho vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc26:11.1"
msgid "Cīvaraṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc26:11.1"
msgid "channaṃ cīvarānaṃ aññataraṃ cīvaraṃ vikappanupagaṃ pacchimaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc7, pts-vp-pli4.284
#. VAR: kathaṃ ime cīvaraṃ na → ime cīvaraṃ (bj) | imaṃ cīvaraṃ (s1-3, pts1, mr)+. HTML: <b>
msgctxt "pli-tv-bi-vb-pc26:12.1"
msgid "Antarāyaṃ kareyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc26:12.1"
msgid "“kathaṃ ime cīvaraṃ na dadeyyun”ti antarāyaṃ karoti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:12.2"
msgid "Aññaṃ parikkhāraṃ antarāyaṃ karoti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:12.3"
msgid "Sambahulānaṃ bhikkhunīnaṃ vā ekabhikkhuniyā vā anupasampannāya vā cīvaraṃ vā aññaṃ vā parikkhāraṃ antarāyaṃ karoti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc26:13.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc26:13.2"
msgid "ānisaṃsaṃ dassetvā nivāreti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc26:14.1"
msgid "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

