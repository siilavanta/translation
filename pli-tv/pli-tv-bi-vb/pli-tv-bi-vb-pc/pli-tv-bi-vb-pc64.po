#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc64"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc64:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc64:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc64:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc64:4.1"
msgid "Gabbhinivagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc64:5.1"
msgid "64. Dutiyasikkhamānasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc64:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:6.2"
msgid "Tena kho pana samayena bhikkhuniyo dve vassāni chasu dhammesu sikkhitasikkhaṃ sikkhamānaṃ saṃghena asammataṃ vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:6.3"
msgid "Bhikkhuniyo evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:6.4"
msgid "“etha, sikkhamānā, imaṃ jānātha, imaṃ detha, imaṃ āharatha, iminā attho, imaṃ kappiyaṃ karothā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:6.5"
msgid "Tā evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:6.6"
msgid "“na mayaṃ, ayye, sikkhamānā, bhikkhuniyo mayan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.321
msgctxt "pli-tv-bi-vb-pc64:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.4"
msgid "“kathañhi nāma bhikkhuniyo dve vassāni chasu dhammesu sikkhitasikkhaṃ sikkhamānaṃ saṃghena asammataṃ vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.6"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo dve vassāni chasu dhammesu sikkhitasikkhaṃ sikkhamānaṃ saṃghena asammataṃ vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.7"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.8"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.10"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo dve vassāni chasu dhammesu sikkhitasikkhaṃ sikkhamānaṃ saṃghena asammataṃ vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.11"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.13"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.15"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.16"
msgid "“anujānāmi, bhikkhave, dve vassāni chasu dhammesu sikkhitasikkhāya sikkhamānāya vuṭṭhānasammutiṃ dātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.17"
msgid "Evañca pana, bhikkhave, dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.18"
msgid "Tāya dve vassāni chasu dhammesu sikkhitasikkhāya sikkhamānāya saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā bhikkhunīnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.19"
msgid "‘ahaṃ, ayye, itthannāmā itthannāmāya ayyāya dve vassāni chasu dhammesu sikkhitasikkhā sikkhamānā saṃghaṃ vuṭṭhānasammutiṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.20"
msgid "Dutiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.21"
msgid "Tatiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:7.22"
msgid "Byattāya bhikkhuniyā paṭibalāya saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc64:8.1"
msgid "‘Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:8.2"
msgid "Ayaṃ itthannāmā itthannāmāya ayyāya dve vassāni chasu dhammesu sikkhitasikkhā sikkhamānā saṃghaṃ vuṭṭhānasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:8.3"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmāya dve vassāni chasu dhammesu sikkhitasikkhāya sikkhamānāya vuṭṭhānasammutiṃ dadeyya."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:8.4"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc64:9.1"
msgid "Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:9.2"
msgid "Ayaṃ itthannāmā itthannāmāya ayyāya dve vassāni chasu dhammesu sikkhitasikkhā sikkhamānā saṃghaṃ vuṭṭhānasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:9.3"
msgid "Saṃgho itthannāmāya dve vassāni chasu dhammesu sikkhitasikkhāya sikkhamānāya vuṭṭhānasammutiṃ deti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:9.4"
msgid "Yassā ayyāya khamati itthannāmāya dve vassāni chasu dhammesu sikkhitasikkhāya sikkhamānāya vuṭṭhānasammutiyā dānaṃ, sā tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:9.5"
msgid "yassā nakkhamati, sā bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc64:10.1"
msgid "Dinnā saṃghena itthannāmāya dve vassāni chasu dhammesu sikkhitasikkhāya sikkhamānāya vuṭṭhānasammuti;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:10.2"
msgid "khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc64:11.1"
msgid "Atha kho bhagavā tā bhikkhuniyo anekapariyāyena vigarahitvā dubbharatāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:11.3"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc64:12.1"
msgid "“Yā pana bhikkhunī dve vassāni chasu dhammesu sikkhitasikkhaṃ sikkhamānaṃ saṃghena asammataṃ vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc64:13.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc64:13.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc64:13.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc64:13.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:13.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc64:14.1"
msgid "Dve vassānīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc64:14.1"
msgid "dve saṃvaccharāni."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc64:14.2"
msgid "Sikkhitasikkhā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc64:14.2"
msgid "chasu dhammesu sikkhitasikkhā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc64:14.3"
msgid "Asammatā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc64:14.3"
msgid "ñattidutiyena kammena vuṭṭhānasammuti na dinnā hoti."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc64:14.4"
msgid "Vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc64:14.4"
msgid "upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc64:15.1"
msgid "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:15.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:15.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:15.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:15.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc64:16.1"
msgid "Dhammakamme dhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:16.2"
msgid "Dhammakamme vematikā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:16.3"
msgid "Dhammakamme adhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc64:17.1"
msgid "Adhammakamme dhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:17.2"
msgid "Adhammakamme vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:17.3"
msgid "Adhammakamme adhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-pc64:18.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc64:18.2"
msgid "dve vassāni chasu dhammesu sikkhitasikkhaṃ sikkhamānaṃ saṃghena sammataṃ vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc14
msgctxt "pli-tv-bi-vb-pc64:19.1"
msgid "Catutthasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

