#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc85"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc85:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc85:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc85:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc85:4.1"
msgid "Chattupāhanavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc85:5.1"
msgid "85. Yānasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc85:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo yānena yāyanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:6.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:6.4"
msgid "“kathañhi nāma bhikkhuniyo yānena yāyissanti, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc85:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.5"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo yānena yāyissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.7"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo yānena yāyantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.11"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo yānena yāyissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:7.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3, pts-vp-pli4.339
msgctxt "pli-tv-bi-vb-pc85:8.1"
msgid "“Yā pana bhikkhunī yānena yāyeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc85:9.1"
msgid "Evañcidaṃ bhagavatā bhikkhunīnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc85:10.1"
msgid "Tena kho pana samayena aññatarā bhikkhunī gilānā hoti, na sakkoti padasā gantuṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:10.3"
msgid "bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:10.5"
msgid "anujānāmi, bhikkhave, gilānāya bhikkhuniyā yānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:10.6"
msgid "Evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc85:11.1"
msgid "“Yā pana bhikkhunī agilānā yānena yāyeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc85:12.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc85:12.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc85:12.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc85:12.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:12.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc85:13.1"
msgid "Agilānā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc85:13.1"
msgid "sakkoti padasā gantuṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc85:14.1"
msgid "Gilānā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc85:14.1"
msgid "na sakkoti padasā gantuṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc85:15.1"
msgid "Yānaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc85:15.1"
msgid "vayhaṃ ratho sakaṭaṃ sandamānikā sivikā pāṭaṅkī."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc85:16.1"
msgid "Yāyeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc85:16.1"
msgid "sakimpi yānena yāyati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc85:17.1"
msgid "Agilānā agilānasaññā yānena yāyati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:17.2"
msgid "Agilānā vematikā yānena yāyati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:17.3"
msgid "Agilānā gilānasaññā yānena yāyati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-pc85:18.1"
msgid "Gilānā agilānasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:18.2"
msgid "Gilānā vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:18.3"
msgid "Gilānā gilānasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bi-vb-pc85:19.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc85:19.2"
msgid "gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc15
msgctxt "pli-tv-bi-vb-pc85:20.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

