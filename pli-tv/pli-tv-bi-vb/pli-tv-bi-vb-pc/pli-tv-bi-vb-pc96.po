#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc96"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc96:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc96:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc96:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc96:4.1"
msgid "96. Chattupāhanavagga"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-bi-vb-pc96:5.1"
msgid "13. Asaṅkaccikasikkhāpada"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli4.345
msgctxt "pli-tv-bi-vb-pc96:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

#. VAR: asaṅkaccikā → asaṅkacchikā (s1-3, pts1)
msgctxt "pli-tv-bi-vb-pc96:6.2"
msgid "Tena kho pana samayena aññatarā bhikkhunī asaṅkaccikā gāmaṃ piṇḍāya pāvisi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:6.3"
msgid "Tassā rathikāya vātamaṇḍalikā saṅghāṭiyo ukkhipiṃsu."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:6.4"
msgid "Manussā ukkuṭṭhiṃ akaṃsu—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:6.5"
msgid "“sundarā ayyāya thanudarā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:6.6"
msgid "Sā bhikkhunī tehi manussehi uppaṇḍiyamānā maṅku ahosi."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc96:7.1"
msgid "Atha kho sā bhikkhunī upassayaṃ gantvā bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.4"
msgid "“kathañhi nāma bhikkhunī asaṅkaccikā gāmaṃ pavisissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.5"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī asaṅkaccikā gāmaṃ pāvisī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.8"
msgid "kathañhi nāma, bhikkhave, bhikkhunī asaṅkaccikā gāmaṃ pavisissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
#. VAR: asaṅkaccikā → asaṅkacchikā (s1-3, pts1)
msgctxt "pli-tv-bi-vb-pc96:8.1"
msgid "“Yā pana bhikkhunī asaṅkaccikā gāmaṃ paviseyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc96:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc96:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc96:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc96:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc96:10.1"
msgid "Asaṅkaccikāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc96:10.2"
msgid "vinā saṅkaccikaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc96:11.1"
msgid "Saṅkaccikaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc96:11.2"
msgid "adhakkhakaṃ ubbhanābhi, tassa paṭicchādanatthāya."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc96:12.1"
msgid "Gāmaṃ paviseyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc96:12.2"
msgid "parikkhittassa gāmassa parikkhepaṃ atikkāmentiyā āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:12.3"
msgid "Aparikkhittassa gāmassa upacāraṃ okkamantiyā āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc96:13.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:13.2"
msgid "acchinnacīvarikāya, naṭṭhacīvarikāya, gilānāya, assatiyā, ajānantiyā, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc96:14.1"
msgid "Terasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-pc96:14.2"
msgid "Chattupāhanavaggo navamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-bi-vb-pc96:15.1"
msgid "Dhammikavagga"
msgstr ""

#. HTML: </h2><h2>
#. VAR: 10 Nandasikkhāpada → ^pi-tv-bu-vb-pc1#6 [Pācittiyā 1: Musāvādasikkhāpada] (1:50)
msgctxt "pli-tv-bi-vb-pc96:16.1"
msgid "10 Nandasikkhāpada"
msgstr ""

#. HTML: </h2><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc96:17.1"
msgid "Uddiṭṭhā kho, ayyāyo, chasaṭṭhisatā pācittiyā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:17.2"
msgid "Tatthāyyāyo pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:17.3"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:17.4"
msgid "Dutiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:17.5"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:17.6"
msgid "Tatiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:17.7"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc96:17.8"
msgid "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṃ dhārayāmīti."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc96:18.1"
msgid "Khuddakaṃ samattaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-pc96:18.2"
msgid "Bhikkhunivibhaṅge pācittiyakaṇḍaṃ niṭṭhitaṃ."
msgstr ""

#. VAR: () → bj potthake tassuddānañca vagguddānañca
msgctxt "pli-tv-bi-vb-pc96:18.3"
msgid "()"
msgstr ""
