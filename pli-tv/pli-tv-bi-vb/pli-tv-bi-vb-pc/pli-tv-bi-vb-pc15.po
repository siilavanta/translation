#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc15"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc15:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc15:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc15:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc15:4.1"
msgid "Rattandhakāravagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc15:5.1"
msgid "15. Anāpucchāpakkamanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.272
msgctxt "pli-tv-bi-vb-pc15:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.2"
msgid "Tena kho pana samayena aññatarā bhikkhunī aññatarassa kulassa kulūpikā hoti niccabhattikā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.3"
msgid "Atha kho sā bhikkhunī pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya yena taṃ kulaṃ tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.4"
msgid "upasaṅkamitvā āsane nisīditvā sāmike anāpucchā pakkāmi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.5"
msgid "Tassa kulassa dāsī gharaṃ sammajjantī taṃ āsanaṃ bhājanantarikāya pakkhipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.6"
msgid "Manussā taṃ āsanaṃ apassantā taṃ bhikkhuniṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.7"
msgid "“kahaṃ taṃ, ayye, āsanan”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.8"
msgid "“Nāhaṃ taṃ, āvuso, āsanaṃ passāmī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.9"
msgid "“Dethāyye, taṃ āsanan”ti paribhāsitvā niccabhattaṃ pacchindiṃsu."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:6.10"
msgid "Atha kho te manussā gharaṃ sodhentā taṃ āsanaṃ bhājanantarikāya passitvā taṃ bhikkhuniṃ khamāpetvā niccabhattaṃ paṭṭhapesuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc15:7.1"
msgid "Atha kho sā bhikkhunī bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.4"
msgid "“kathañhi nāma bhikkhunī purebhattaṃ kulāni upasaṅkamitvā āsane nisīditvā sāmike anāpucchā pakkamissatī”ti … pe …"
msgstr ""

#. VAR: pakkamatī”ti → pakkamīti (bj, mr) | pakkāmīti (pts1)
msgctxt "pli-tv-bi-vb-pc15:7.5"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī purebhattaṃ kulāni upasaṅkamitvā āsane nisīditvā sāmike anāpucchā pakkamatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.8"
msgid "kathañhi nāma, bhikkhave bhikkhunī purebhattaṃ kulāni upasaṅkamitvā āsane nisīditvā sāmike anāpucchā pakkamissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc15:8.1"
msgid "“Yā pana bhikkhunī purebhattaṃ kulāni upasaṅkamitvā āsane nisīditvā sāmike anāpucchā pakkameyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc15:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc15:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc15:10.1"
msgid "Purebhattaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:10.2"
msgid "aruṇuggamanaṃ upādāya yāva majjhanhikā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc15:11.1"
msgid "Kulaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:11.2"
msgid "cattāri kulāni—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:11.3"
msgid "khattiyakulaṃ, brāhmaṇakulaṃ, vessakulaṃ, suddakulaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc15:11.4"
msgid "Upasaṅkamitvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:11.5"
msgid "tattha gantvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc15:12.1"
msgid "Āsanaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:12.2"
msgid "pallaṅkassa okāso vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc15:12.3"
msgid "Nisīditvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:12.4"
msgid "tasmiṃ nisīditvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc15:13.1"
msgid "Sāmike anāpucchā pakkameyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc15:13.2"
msgid "yo tasmiṃ kule manusso viññū taṃ anāpucchā anovassakaṃ atikkāmentiyā āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:13.3"
msgid "Ajjhokāse upacāraṃ atikkāmentiyā āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc15:14.1"
msgid "Anāpucchite anāpucchitasaññā pakkamati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:14.2"
msgid "Anāpucchite vematikā pakkamati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:14.3"
msgid "Anāpucchite āpucchitasaññā pakkamati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc15:15.1"
msgid "Pallaṅkassa anokāse āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:15.2"
msgid "Āpucchite anāpucchitasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:15.3"
msgid "Āpucchite vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:15.4"
msgid "Āpucchite āpucchitasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc15:16.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc15:16.2"
msgid "āpucchā gacchati, asaṃhārime, gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc15:17.1"
msgid "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
