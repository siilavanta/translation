#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc39"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc39:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc39:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc39:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc39:4.1"
msgid "Tuvaṭṭavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc39:5.1"
msgid "39. Antovassasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc39:6.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:6.2"
msgid "Tena kho pana samayena bhikkhuniyo antovassaṃ cārikaṃ caranti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:6.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:6.4"
msgid "“kathañhi nāma bhikkhuniyo antovassaṃ cārikaṃ carissanti haritāni tiṇāni ca sammaddantā, ekindriyaṃ jīvaṃ viheṭhentā, bahū khuddake pāṇe saṅghātaṃ āpādentā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc39:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.5"
msgid "“kathañhi nāma bhikkhuniyo antovassaṃ cārikaṃ carissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.7"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo antovassaṃ cārikaṃ carantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.11"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo antovassaṃ cārikaṃ carissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:7.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc39:8.1"
msgid "“Yā pana bhikkhunī antovassaṃ cārikaṃ careyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4, pts-vp-pli4.297
msgctxt "pli-tv-bi-vb-pc39:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc39:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc39:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc39:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc39:10.1"
msgid "Antovassanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc39:10.1"
msgid "purimaṃ vā temāsaṃ pacchimaṃ vā temāsaṃ avasitvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc39:11.1"
msgid "Cārikaṃ careyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc39:11.1"
msgid "kukkuṭasampāte gāme gāmantare gāmantare āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:11.2"
msgid "Agāmake araññe addhayojane addhayojane āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc39:12.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc39:12.2"
msgid "sattāhakaraṇīyena gacchati, kenaci ubbāḷhā gacchati, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc39:13.1"
msgid "Navamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

