#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc11"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc11:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc11:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc11:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc11:4.1"
msgid "Rattandhakāravagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc11:5.1"
msgid "11. Rattandhakārasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc11:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:6.2"
msgid "Tena kho pana samayena bhaddāya kāpilāniyā antevāsiniyā bhikkhuniyā ñātako puriso gāmakā sāvatthiṃ agamāsi kenacideva karaṇīyena."
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:6.3"
msgid "Atha kho sā bhikkhunī tena purisena saddhiṃ rattandhakāre appadīpe ekenekā santiṭṭhatipi sallapatipi."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc11:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.3"
msgid "“kathañhi nāma bhikkhunī rattandhakāre appadīpe purisena saddhiṃ ekenekā santiṭṭhissatipi sallapissatipī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī rattandhakāre appadīpe purisena saddhiṃ ekenekā santiṭṭhatipi sallapatipī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhunī rattandhakāre appadīpe purisena saddhiṃ ekenekā santiṭṭhissatipi sallapissatipi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc11:8.1"
msgid "“Yā pana bhikkhunī rattandhakāre appadīpe purisena saddhiṃ ekenekā santiṭṭheyya vā sallapeyya vā, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc11:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc11:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc11:10.1"
msgid "Rattandhakāreti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:10.2"
msgid "oggate sūriye."
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:10.3"
msgid "Appadīpeti anāloke."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6, pts-vp-pli4.269
msgctxt "pli-tv-bi-vb-pc11:11.1"
msgid "Puriso nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:11.2"
msgid "manussapuriso na yakkho na peto na tiracchānagato viññū paṭibalo santiṭṭhituṃ sallapituṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc11:12.1"
msgid "Saddhinti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:12.2"
msgid "ekato."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc11:12.3"
msgid "Ekenekāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:12.4"
msgid "puriso ceva hoti bhikkhunī ca."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc11:13.1"
msgid "Santiṭṭheyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:13.2"
msgid "purisassa hatthapāse tiṭṭhati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc11:14.1"
msgid "Sallapeyya vāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc11:14.2"
msgid "purisassa hatthapāse ṭhitā sallapati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc11:15.1"
msgid "Hatthapāsaṃ vijahitvā santiṭṭhati vā sallapati vā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc11:15.2"
msgid "Yakkhena vā petena vā paṇḍakena vā tiracchānagatamanussaviggahena vā saddhiṃ santiṭṭhati vā sallapati vā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc11:16.1"
msgid "Anāpatti—"
msgstr ""

#. VAR: dutiyo → yā kāci viññū dutiyā (s1-3)
msgctxt "pli-tv-bi-vb-pc11:16.2"
msgid "yo koci viññū dutiyo hoti, arahopekkhā, aññavihitā santiṭṭhati vā sallapati vā, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc11:17.1"
msgid "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
