#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc34"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc34:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc34:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc34:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc34:4.1"
msgid "Tuvaṭṭavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc34:5.1"
msgid "34. Naupaṭṭhāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc34:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī dukkhitaṃ sahajīviniṃ neva upaṭṭheti na upaṭṭhāpanāya ussukkaṃ karoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc34:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.3"
msgid "“kathañhi nāma ayyā thullanandā dukkhitaṃ sahajīviniṃ neva upaṭṭhessati na upaṭṭhāpanāya ussukkaṃ karissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.4"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī dukkhitaṃ sahajīviniṃ neva upaṭṭheti na upaṭṭhāpanāya ussukkaṃ karotī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.7"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī dukkhitaṃ sahajīviniṃ neva upaṭṭhessati na upaṭṭhāpanāya ussukkaṃ karissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc34:8.1"
msgid "“Yā pana bhikkhunī dukkhitaṃ sahajīviniṃ neva upaṭṭheyya na upaṭṭhāpanāya ussukkaṃ kareyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc34:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc34:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc34:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc34:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc34:10.1"
msgid "Dukkhitā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc34:10.2"
msgid "gilānā vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc34:11.1"
msgid "Sahajīvinī nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc34:11.2"
msgid "saddhivihārinī vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc34:12.1"
msgid "Neva upaṭṭheyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc34:12.2"
msgid "na sayaṃ upaṭṭheyya."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc34:13.1"
msgid "Na upaṭṭhāpanāya ussukkaṃ kareyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc34:13.2"
msgid "na aññaṃ āṇāpeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc34:14.1"
msgid "“Neva upaṭṭhessāmi, na upaṭṭhāpanāya ussukkaṃ karissāmī”ti dhuraṃ nikkhittamatte āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:14.2"
msgid "Antevāsiniṃ vā anupasampannaṃ vā neva upaṭṭheti, na upaṭṭhāpanāya ussukkaṃ karoti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10, pts-vp-pli4.292
msgctxt "pli-tv-bi-vb-pc34:15.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc34:15.2"
msgid "sati antarāye, pariyesitvā na labhati, gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc34:16.1"
msgid "Catutthasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
