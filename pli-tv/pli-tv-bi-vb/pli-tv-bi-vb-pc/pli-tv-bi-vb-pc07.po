#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc7"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc7:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc7:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc7:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc7:4.1"
msgid "Lasuṇavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc7:5.1"
msgid "7. Āmakadhaññasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc7:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:6.2"
msgid "Tena kho pana samayena bhikkhuniyo sassakāle āmakadhaññaṃ viññāpetvā nagaraṃ atiharanti dvāraṭṭhāne—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:6.3"
msgid "“dethāyye, bhāgan”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:6.4"
msgid "Palibundhetvā muñciṃsu."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc7:7.1"
msgid "Atha kho tā bhikkhuniyo upassayaṃ gantvā bhikkhunīnaṃ etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.4"
msgid "“kathañhi nāma bhikkhuniyo āmakadhaññaṃ viññāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.5"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo āmakadhaññaṃ viññāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.8"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo āmakadhaññaṃ viññāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
#. VAR: viññāpetvā → viññāpāpetvā (bj, s1-3)
#. VAR: koṭṭetvā → koṭṭitvā (pts1)
#. VAR: viññatvā → viññāpetvā (bj, s1-3) | viññitvā (pts1)
#. HTML: <b class="rule">
msgctxt "pli-tv-bi-vb-pc7:8.1"
msgid "“Yā pana bhikkhunī āmakadhaññaṃ viññatvā vā viññāpetvā vā bhajjitvā vā bhajjāpetvā vā koṭṭetvā vā koṭṭāpetvā vā pacitvā vā pacāpetvā vā bhuñjeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc7:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc7:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc7:10.1"
msgid "Āmakadhaññaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:10.2"
msgid "sāli vīhi yavo godhumo kaṅgu varako kudrusako."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc7:11.1"
msgid "Viññatvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:11.2"
msgid "sayaṃ viññatvā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc7:11.3"
msgid "Viññāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:11.4"
msgid "aññaṃ viññāpetvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc7:12.1"
msgid "Bhajjitvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:12.2"
msgid "sayaṃ bhajjitvā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc7:12.3"
msgid "Bhajjāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:12.4"
msgid "aññaṃ bhajjāpetvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc7:13.1"
msgid "Koṭṭetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:13.2"
msgid "sayaṃ koṭṭetvā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc7:13.3"
msgid "Koṭṭāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:13.4"
msgid "aññaṃ koṭṭāpetvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc7:14.1"
msgid "Pacitvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:14.2"
msgid "sayaṃ pacitvā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc7:14.3"
msgid "Pacāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc7:14.4"
msgid "aññaṃ pacāpetvā."
msgstr ""

#. HTML: </p><p>
#. REF: sc10, pts-vp-pli4.265
msgctxt "pli-tv-bi-vb-pc7:15.1"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:15.2"
msgid "Ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc7:16.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc7:16.2"
msgid "ābādhapaccayā, aparaṇṇaṃ viññāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc7:17.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
