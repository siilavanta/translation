#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc46"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc46:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc46:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc46:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc46:4.1"
msgid "Cittāgāravagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc46:5.1"
msgid "46. Bhojanadānasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc46:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī naṭānampi naṭakānampi laṅghakānampi sokajjhāyikānampi kumbhathūṇikānampi sahatthā khādanīyaṃ bhojanīyaṃ deti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:6.3"
msgid "“mayhaṃ parisati vaṇṇaṃ bhāsathā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:6.4"
msgid "Naṭāpi naṭakāpi laṅghakāpi sokajjhāyikāpi kumbhathūṇikāpi thullanandāya bhikkhuniyā parisati vaṇṇaṃ bhāsanti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:6.5"
msgid "“ayyā thullanandā bahussutā bhāṇikā visāradā paṭṭā dhammiṃ kathaṃ kātuṃ;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:6.6"
msgid "detha ayyāya, karotha ayyāyā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc46:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.3"
msgid "“kathañhi nāma ayyā thullanandā agārikassa sahatthā khādanīyaṃ bhojanīyaṃ dassatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.4"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī agārikassa sahatthā khādanīyaṃ bhojanīyaṃ detī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.7"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī agārikassa sahatthā khādanīyaṃ bhojanīyaṃ dassati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc46:8.1"
msgid "“Yā pana bhikkhunī agārikassa vā paribbājakassa vā paribbājikāya vā sahatthā khādanīyaṃ vā bhojanīyaṃ vā dadeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc46:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc46:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc46:10.1"
msgid "Agāriko nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:10.2"
msgid "yo koci agāraṃ ajjhāvasati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc46:11.1"
msgid "Paribbājako nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:11.2"
msgid "bhikkhuñca sāmaṇerañca ṭhapetvā yo koci paribbājakasamāpanno."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc46:12.1"
msgid "Paribbājikā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:12.2"
msgid "bhikkhuniñca sikkhamānañca sāmaṇeriñca ṭhapetvā yā kāci paribbājikasamāpannā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc46:13.1"
msgid "Khādanīyaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:13.2"
msgid "pañca bhojanāni—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:13.3"
msgid "udakadantaponaṃ ṭhapetvā avasesaṃ khādanīyaṃ nāma."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc46:14.1"
msgid "Bhojanīyaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:14.2"
msgid "pañca bhojanāni—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:14.3"
msgid "odano, kummāso, sattu, maccho, maṃsaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10, pts-vp-pli4.303
msgctxt "pli-tv-bi-vb-pc46:15.1"
msgid "Dadeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc46:15.2"
msgid "kāyena vā kāyapaṭibaddhena vā nissaggiyena vā deti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:15.3"
msgid "Udakadantaponaṃ deti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc46:16.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc46:16.2"
msgid "dāpeti na deti, upanikkhipitvā deti, bāhirālepaṃ deti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc46:17.1"
msgid "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
