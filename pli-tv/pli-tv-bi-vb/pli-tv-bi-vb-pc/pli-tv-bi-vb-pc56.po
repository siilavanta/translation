#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc56"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc56:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc56:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc56:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc56:4.1"
msgid "Ārāmavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc56:5.1"
msgid "56. Abhikkhukāvāsasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.313
msgctxt "pli-tv-bi-vb-pc56:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:6.2"
msgid "Tena kho pana samayena sambahulā bhikkhuniyo gāmakāvāse vassaṃvuṭṭhā sāvatthiṃ agamaṃsu."
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:6.3"
msgid "Bhikkhuniyo tā bhikkhuniyo etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:6.4"
msgid "“katthāyyāyo vassaṃvuṭṭhā?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:6.5"
msgid "Kacci ovādo iddho ahosī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:6.6"
msgid "“Natthayye, tattha bhikkhū;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:6.7"
msgid "kuto ovādo iddho bhavissatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc56:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.3"
msgid "“kathañhi nāma bhikkhuniyo abhikkhuke āvāse vassaṃ vasissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo abhikkhuke āvāse vassaṃ vasantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo abhikkhuke āvāse vassaṃ vasissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc56:8.1"
msgid "“Yā pana bhikkhunī abhikkhuke āvāse vassaṃ vaseyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc56:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc56:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc56:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc56:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc56:10.1"
msgid "Abhikkhuko nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc56:10.2"
msgid "āvāso na sakkā hoti ovādāya vā saṃvāsāya vā gantuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:10.3"
msgid "“Vassaṃ vasissāmī”ti senāsanaṃ paññapeti pānīyaṃ paribhojanīyaṃ upaṭṭhapeti pariveṇaṃ sammajjati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:10.4"
msgid "Saha aruṇuggamanā āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc56:11.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc56:11.2"
msgid "vassupagatā bhikkhū pakkantā vā honti vibbhantā vā kālaṅkatā vā pakkhasaṅkantā vā, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc56:12.1"
msgid "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
