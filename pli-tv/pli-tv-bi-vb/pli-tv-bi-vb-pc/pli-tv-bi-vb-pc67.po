#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc67"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc67:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc67:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc67:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc67:4.1"
msgid "Gabbhinivagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc67:5.1"
msgid "67. Tatiyagihigatasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc67:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:6.2"
msgid "Tena kho pana samayena bhikkhuniyo paripuṇṇadvādasavassaṃ gihigataṃ dve vassāni chasu dhammesu sikkhitasikkhaṃ saṃghena asammataṃ vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:6.3"
msgid "Bhikkhuniyo evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:6.4"
msgid "“etha, sikkhamānā, imaṃ jānātha, imaṃ detha, imaṃ āharatha, iminā attho, imaṃ kappiyaṃ karothā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:6.5"
msgid "Tā evamāhaṃsu—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:6.6"
msgid "“na mayaṃ, ayye, sikkhamānā, bhikkhuniyo mayan”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.324
msgctxt "pli-tv-bi-vb-pc67:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.3"
msgid "“kathañhi nāma bhikkhuniyo paripuṇṇadvādasavassaṃ gihigataṃ dve vassāni chasu dhammesu sikkhitasikkhaṃ saṃghena asammataṃ vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo paripuṇṇadvādasavassaṃ gihigataṃ dve vassāni chasu dhammesu sikkhitasikkhaṃ saṃghena asammataṃ vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo paripuṇṇadvādasavassaṃ gihigataṃ dve vassāni chasu dhammesu sikkhitasikkhaṃ saṃghena asammataṃ vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.9"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.10"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.11"
msgid "“anujānāmi, bhikkhave, paripuṇṇadvādasavassāya gihigatāya dve vassāni chasu dhammesu sikkhitasikkhāya vuṭṭhānasammutiṃ dātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.12"
msgid "Evañca pana, bhikkhave, dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.13"
msgid "Tāya paripuṇṇadvādasavassāya gihigatāya dve vassāni chasu dhammesu sikkhitasikkhāya saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā bhikkhunīnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.14"
msgid "‘ahaṃ, ayye, itthannāmā itthannāmāya ayyāya paripuṇṇadvādasavassā gihigatā dve vassāni chasu dhammesu sikkhitasikkhā, saṃghaṃ vuṭṭhānasammutiṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.15"
msgid "Dutiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.16"
msgid "Tatiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:7.17"
msgid "Byattāya bhikkhuniyā paṭibalāya saṃgho ñāpetabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc67:8.1"
msgid "‘Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:8.2"
msgid "Ayaṃ itthannāmā itthannāmāya ayyāya paripuṇṇadvādasavassā gihigatā dve vassāni chasu dhammesu sikkhitasikkhā saṃghaṃ vuṭṭhānasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:8.3"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmāya paripuṇṇadvādasavassāya gihigatāya dve vassāni chasu dhammesu sikkhitasikkhāya vuṭṭhānasammutiṃ dadeyya."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:8.4"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc67:9.1"
msgid "Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:9.2"
msgid "Ayaṃ itthannāmā itthannāmāya ayyāya paripuṇṇadvādasavassā gihigatā dve vassāni chasu dhammesu sikkhitasikkhā saṃghaṃ vuṭṭhānasammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:9.3"
msgid "Saṃgho itthannāmāya paripuṇṇadvādasavassāya gihigatāya dve vassāni chasu dhammesu sikkhitasikkhāya vuṭṭhānasammutiṃ deti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:9.4"
msgid "Yassā ayyāya khamati itthannāmāya paripuṇṇadvādasavassāya gihigatāya dve vassāni chasu dhammesu sikkhitasikkhāya vuṭṭhānasammutiyā dānaṃ, sā tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:9.5"
msgid "yassā nakkhamati, sā bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc67:10.1"
msgid "Dinnā saṃghena itthannāmāya paripuṇṇadvādasavassāya gihigatāya dve vassāni chasu dhammesu sikkhitasikkhāya vuṭṭhānasammuti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:10.2"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc67:11.1"
msgid "Atha kho bhagavā tā bhikkhuniyo anekapariyāyena vigarahitvā dubbharatāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:11.2"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc67:12.1"
msgid "“Yā pana bhikkhunī paripuṇṇadvādasavassaṃ gihigataṃ dve vassāni chasu dhammesu sikkhitasikkhaṃ saṃghena asammataṃ vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc67:13.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:13.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc67:13.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:13.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:13.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc67:14.1"
msgid "Paripuṇṇadvādasavassā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:14.2"
msgid "pattadvādasavassā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc67:14.3"
msgid "Gihigatā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:14.4"
msgid "purisantaragatā vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc67:14.5"
msgid "Dve vassānīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:14.6"
msgid "dve saṃvaccharāni."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc67:14.7"
msgid "Sikkhitasikkhā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:14.8"
msgid "chasu dhammesu sikkhitasikkhā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc67:14.9"
msgid "Asammatā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:14.10"
msgid "ñattidutiyena kammena vuṭṭhānasammuti na dinnā hoti."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc67:14.11"
msgid "Vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc67:14.12"
msgid "upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc67:15.1"
msgid "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:15.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:15.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:15.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:15.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc67:16.1"
msgid "Dhammakamme dhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:16.2"
msgid "Dhammakamme vematikā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:16.3"
msgid "Dhammakamme adhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc67:17.1"
msgid "Adhammakamme dhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:17.2"
msgid "Adhammakamme vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:17.3"
msgid "Adhammakamme adhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-pc67:18.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc67:18.2"
msgid "paripuṇṇadvādasavassaṃ gihigataṃ dve vassāni chasu dhammesu sikkhitasikkhaṃ saṃghena sammataṃ vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc14
msgctxt "pli-tv-bi-vb-pc67:19.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
