#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc9"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc9:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc9:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc9:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc9:4.1"
msgid "Lasuṇavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc9:5.1"
msgid "9. Dutiyauccārachaḍḍanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc9:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:6.2"
msgid "Tena kho pana samayena aññatarassa brāhmaṇassa bhikkhunupassayaṃ nissāya yavakhettaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:6.3"
msgid "Bhikkhuniyo uccārampi passāvampi saṅkārampi vighāsampi khette chaḍḍenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:6.4"
msgid "Atha kho so brāhmaṇo ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:6.5"
msgid "“kathañhi nāma bhikkhuniyo amhākaṃ yavakhettaṃ dūsessantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc9:7.1"
msgid "Assosuṃ kho bhikkhuniyo tassa brāhmaṇassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.4"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.5"
msgid "“kathañhi nāma bhikkhuniyo uccārampi passāvampi saṅkārampi vighāsampi harite chaḍḍessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.7"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo uccārampi passāvampi saṅkārampi vighāsampi harite chaḍḍentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.8"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.9"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.11"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo uccārampi passāvampi saṅkārampi vighāsampi harite chaḍḍessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:7.14"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc9:8.1"
msgid "“Yā pana bhikkhunī uccāraṃ vā passāvaṃ vā saṅkāraṃ vā vighāsaṃ vā harite chaḍḍeyya vā chaḍḍāpeyya vā, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc9:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:9.1"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc9:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:9.3"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc9:10.1"
msgid "Uccāro nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:10.1"
msgid "gūtho vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc9:10.2"
msgid "Passāvo nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:10.2"
msgid "muttaṃ vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc9:11.1"
msgid "Saṅkāraṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:11.1"
msgid "kacavaraṃ vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc9:12.1"
msgid "Vighāsaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:12.1"
msgid "calakāni vā aṭṭhikāni vā ucchiṭṭhodakaṃ vā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8, pts-vp-pli4.267
msgctxt "pli-tv-bi-vb-pc9:13.1"
msgid "Haritaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:13.1"
msgid "pubbaṇṇaṃ aparaṇṇaṃ yaṃ manussānaṃ upabhogaparibhogaṃ ropimaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc9:14.1"
msgid "Chaḍḍeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:14.1"
msgid "sayaṃ chaḍḍeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc9:15.1"
msgid "Chaḍḍāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc9:15.1"
msgid "aññaṃ āṇāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:15.2"
msgid "Sakiṃ āṇattā bahukampi chaḍḍeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc9:16.1"
msgid "Harite haritasaññā chaḍḍeti vā chaḍḍāpeti vā, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:16.2"
msgid "Harite vematikā chaḍḍeti vā chaḍḍāpeti vā, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:16.3"
msgid "Harite aharitasaññā chaḍḍeti vā chaḍḍāpeti vā, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc9:17.1"
msgid "Aharite haritasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:17.2"
msgid "Aharite vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc9:17.3"
msgid "Aharite aharitasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-pc9:18.1"
msgid "Anāpatti—"
msgstr ""

#. VAR: chaḍḍeti, khettamariyāde → chaḍḍitakhette (bj)
msgctxt "pli-tv-bi-vb-pc9:18.2"
msgid "oloketvā chaḍḍeti, khettamariyāde chaḍḍeti sāmike āpucchitvā apaloketvā chaḍḍeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc14
msgctxt "pli-tv-bi-vb-pc9:19.1"
msgid "Navamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

