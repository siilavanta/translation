#. HTML: <section class="sutta" id="pli-tv-bi-vb-sk1-75"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-sk1-75:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-sk1-75:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-sk1-75:3.1"
msgid "Sekhiyā:"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-sk1-75:3.2"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-bi-vb-sk1-75:4.1"
msgid "1. Parimaṇḍalavagga"
msgstr ""

#. HTML: </h2><p class="endsutta">
#. REF: sc1, pts-vp-pli4.349
msgctxt "pli-tv-bi-vb-sk1-75:5.1"
msgid "Ime kho panāyyāyo sekhiyā dhammā uddesaṃ āgacchanti."
msgstr ""

#. HTML: </p><h3>
#. VAR: 1.10 Dutiyaukkhittakasikkhāpada → ^pi-tv-bu-vb-sk2#1 [Sekhiyā 2: Dutiyaparimaṇḍalasikkhāpada] (2:147)
msgctxt "pli-tv-bi-vb-sk1-75:6.1"
msgid "1.10 Dutiyaukkhittakasikkhāpada"
msgstr ""

#. HTML: </h3><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-sk1-75:7.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:7.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo puratopi pacchatopi olambentī nivāsenti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:7.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:7.4"
msgid "“kathañhi nāma bhikkhuniyo puratopi pacchatopi olambentī nivāsessanti, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-sk1-75:8.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.4"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo puratopi pacchatopi olambentī nivāsessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.5"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo puratopi pacchatopi olambentī nivāsentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.8"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo puratopi pacchatopi olambentī nivāsessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:8.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc4
msgctxt "pli-tv-bi-vb-sk1-75:9.1"
msgid "“Parimaṇḍalaṃ nivāsessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-sk1-75:10.1"
msgid "Parimaṇḍalaṃ nivāsetabbaṃ nābhimaṇḍalaṃ jāṇumaṇḍalaṃ paṭicchādentiyā."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:10.2"
msgid "Yā anādariyaṃ paṭicca purato vā pacchato vā olambentī nivāseti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-sk1-75:11.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:11.2"
msgid "asañcicca, assatiyā, ajānantiyā, gilānāya, āpadāsu, ummattikāya, ādikammikāyāti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:11.3"
msgid "(Saṃkhittaṃ.)"
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-bi-vb-sk1-75:12.1"
msgid "7. 7 Pādukavagga"
msgstr ""

#. HTML: </h2><h3>
#. VAR: 7.15 Udakeuccārasikkhāpada → ^pi-tv-bu-vb-sk50#1 [Sekhiyā 50: Capucapukārakasikkhāpada] (50:195)
msgctxt "pli-tv-bi-vb-sk1-75:13.1"
msgid "7.15 Udakeuccārasikkhāpada"
msgstr ""

#. HTML: </h3><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-sk1-75:14.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:14.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo udake uccārampi passāvampi kheḷampi karonti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:14.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:14.4"
msgid "“kathañhi nāma bhikkhuniyo udake uccārampi passāvampi kheḷampi karissanti, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli4.350
msgctxt "pli-tv-bi-vb-sk1-75:15.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.2"
msgid "Yā tā bhikkhuniyo appicchā, tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.3"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo udake uccārampi passāvampi kheḷampi karissantī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.4"
msgid "Atha kho bhikkhuniyo bhikkhūnaṃ etamatthaṃ ārocesuṃ."
msgstr ""

#. VAR: Bhikkhū → ye te bhikkhū ...pe... (?)
msgctxt "pli-tv-bi-vb-sk1-75:15.5"
msgid "Bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.6"
msgid "Atha kho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.7"
msgid "bhikkhū paṭipucchi—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.8"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo udake uccārampi passāvampi kheḷampi karontī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.9"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.10"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.11"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo udake uccārampi passāvampi kheḷampi karissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.12"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:15.13"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc9
msgctxt "pli-tv-bi-vb-sk1-75:16.1"
msgid "“Na udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-sk1-75:17.1"
msgid "Evañcidaṃ bhagavatā bhikkhunīnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-sk1-75:18.1"
msgid "Tena kho pana samayena gilānā bhikkhuniyo udake uccārampi passāvampi kheḷampi kātuṃ kukkuccāyanti."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:18.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:18.3"
msgid "anujānāmi, bhikkhave, gilānāya bhikkhuniyā udake uccārampi passāvampi kheḷampi kātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:18.4"
msgid "Evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc12
msgctxt "pli-tv-bi-vb-sk1-75:19.1"
msgid "“Na udake agilānā uccāraṃ vā passāvaṃ vā kheḷaṃ vā karissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-sk1-75:20.1"
msgid "Na udake agilānāya uccāro vā passāvo vā kheḷo vā kātabbo."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:20.2"
msgid "Yā anādariyaṃ paṭicca udake agilānā uccāraṃ vā passāvaṃ vā kheḷaṃ vā karoti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bi-vb-sk1-75:21.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:21.2"
msgid "asañcicca, assatiyā, ajānantiyā, gilānāya, thale kato udakaṃ ottharati, āpadāsu, ummattikāya, khittacittāya, vedanāṭṭāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc15
msgctxt "pli-tv-bi-vb-sk1-75:22.1"
msgid "Pannarasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-sk1-75:22.2"
msgid "Pādukavaggo sattamo."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-bi-vb-sk1-75:23.1"
msgid "Uddiṭṭhā kho, ayyāyo, sekhiyā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:23.2"
msgid "Tatthāyyāyo pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:23.3"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:23.4"
msgid "Dutiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:23.5"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:23.6"
msgid "Tatiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:23.7"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-sk1-75:23.8"
msgid "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṃ dhārayāmīti."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc17
msgctxt "pli-tv-bi-vb-sk1-75:24.1"
msgid "Sekhiyakaṇḍaṃ niṭṭhitaṃ."
msgstr ""
