#. HTML: <section class="sutta" id="pli-tv-pvr12"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr12:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr12:2.1"
msgid "Aparagāthāsaṅgaṇika"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr12:3.1"
msgid "1. Codanādipucchāvissajjanā"
msgstr ""

#. HTML: </h1></div><blockquote class="gatha"><p>
#. REF: sc1, pts-vp-pli5.158
msgctxt "pli-tv-pvr12:4.1"
msgid "Codanā kimatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:4.2"
msgid "Sāraṇā kissa kāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:4.3"
msgid "Saṃgho kimatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:4.4"
msgid "Matikammaṃ pana kissa kāraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr12:5.1"
msgid "Codanā sāraṇatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:5.2"
msgid "Niggahatthāya sāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:5.3"
msgid "Saṃgho pariggahatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:5.4"
msgid "Matikammaṃ pana pāṭiyekkaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr12:6.1"
msgid "Mā kho turito abhaṇi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:6.2"
msgid "mā kho caṇḍikato bhaṇi;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:6.3"
msgid "Mā kho paṭighaṃ janayi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:6.4"
msgid "sace anuvijjako tuvaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr12:7.1"
msgid "Mā kho sahasā abhaṇi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:7.2"
msgid "Kathaṃ viggāhikaṃ anatthasaṃhitaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:7.3"
msgid "Sutte vinaye anulome,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:7.4"
msgid "Paññatte anulomike."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr12:8.1"
msgid "Anuyogavattaṃ nisāmaya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:8.2"
msgid "Kusalena buddhimatā kataṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:8.3"
msgid "Suvuttaṃ sikkhāpadānulomikaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:8.4"
msgid "Gatiṃ na nāsento samparāyikaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:8.5"
msgid "Hitesī anuyuñjassu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:8.6"
msgid "Kālenatthūpasaṃhitaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr12:9.1"
msgid "Cuditassa ca codakassa ca,"
msgstr ""

#. HTML: <br>
#. VAR: padhāresi → padhārehi (bj)
msgctxt "pli-tv-pvr12:9.2"
msgid "Sahasā vohāraṃ mā padhāresi;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:9.3"
msgid "Codako āha āpannoti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:9.4"
msgid "Cuditako āha anāpannoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr12:10.1"
msgid "Ubho anukkhipanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:10.2"
msgid "Paṭiññānusandhitena kāraye;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:10.3"
msgid "Paṭiññā lajjīsu katā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:10.4"
msgid "Alajjīsu evaṃ na vijjati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:10.5"
msgid "Bahumpi alajjī bhāseyya,"
msgstr ""

#. HTML: <br>
#. VAR: Vattānusandhitena → vuttānusandhitena (si, s1-3, pts1, mr)
msgctxt "pli-tv-pvr12:10.6"
msgid "Vattānusandhitena kāraye."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr12:11.1"
msgid "Alajjī kīdiso hoti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:11.2"
msgid "Paṭiññā yassa na rūhati;"
msgstr ""

#. HTML: <br>
#. VAR: Etañca → evañca (pts1, mr)
msgctxt "pli-tv-pvr12:11.3"
msgid "Etañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:11.4"
msgid "Kīdiso vuccati alajjī puggalo."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr12:12.1"
msgid "Sañcicca āpattiṃ āpajjati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:12.2"
msgid "Āpattiṃ parigūhati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:12.3"
msgid "Agatigamanañca gacchati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:12.4"
msgid "Ediso vuccati alajjīpuggalo."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr12:13.1"
msgid "Saccaṃ ahampi jānāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:13.2"
msgid "Ediso vuccati alajjīpuggalo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:13.3"
msgid "Aññañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:13.4"
msgid "Kīdiso vuccati lajjīpuggalo."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr12:14.1"
msgid "Sañcicca āpattiṃ nāpajjati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:14.2"
msgid "Āpattiṃ na parigūhati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:14.3"
msgid "Agatigamanaṃ na gacchati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:14.4"
msgid "Ediso vuccati lajjīpuggalo."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr12:15.1"
msgid "Saccaṃ ahampi jānāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:15.2"
msgid "Ediso vuccati lajjīpuggalo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:15.3"
msgid "Aññañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:15.4"
msgid "Kīdiso vuccati adhammacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc13, pts-vp-pli5.159
msgctxt "pli-tv-pvr12:16.1"
msgid "Akālena codeti abhūtena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:16.2"
msgid "Pharusena anatthasaṃhitena;"
msgstr ""

#. HTML: <br>
#. VAR: mettācitto → mettacitto (bj, s1-3, pts1)
msgctxt "pli-tv-pvr12:16.3"
msgid "Dosantaro codeti no mettācitto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:16.4"
msgid "Ediso vuccati adhammacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr12:17.1"
msgid "Saccaṃ ahampi jānāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:17.2"
msgid "Ediso vuccati adhammacodako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:17.3"
msgid "Aññañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:17.4"
msgid "Kīdiso vuccati dhammacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr12:18.1"
msgid "Kālena codeti bhūtena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:18.2"
msgid "Saṇhena atthasaṃhitena;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:18.3"
msgid "Mettācitto codeti no dosantaro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:18.4"
msgid "Ediso vuccati dhammacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr12:19.1"
msgid "Saccaṃ ahampi jānāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:19.2"
msgid "Ediso vuccati dhammacodako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:19.3"
msgid "Aññañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:19.4"
msgid "Kīdiso vuccati bālacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr12:20.1"
msgid "Pubbāparaṃ na jānāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:20.2"
msgid "Pubbāparassa akovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:20.3"
msgid "Anusandhivacanapathaṃ na jānāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:20.4"
msgid "Anusandhivacanapathassa akovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:20.5"
msgid "Ediso vuccati bālacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr12:21.1"
msgid "Saccaṃ ahampi jānāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:21.2"
msgid "Ediso vuccati bālacodako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:21.3"
msgid "Aññañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:21.4"
msgid "Kīdiso vuccati paṇḍitacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
#. VAR: Pubbāparampi jānāti → pubbāparaṃ vijānāti (bj)
msgctxt "pli-tv-pvr12:22.1"
msgid "Pubbāparampi jānāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:22.2"
msgid "Pubbāparassa kovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:22.3"
msgid "Anusandhivacanapathaṃ jānāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:22.4"
msgid "Anusandhivacanapathassa kovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:22.5"
msgid "Ediso vuccati paṇḍitacodako."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-pvr12:23.1"
msgid "Saccaṃ ahampi jānāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:23.2"
msgid "Ediso vuccati paṇḍitacodako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:23.3"
msgid "Aññañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:23.4"
msgid "Codanā kinti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr12:24.1"
msgid "Sīlavipattiyā codeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:24.2"
msgid "atho ācāradiṭṭhiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:24.3"
msgid "Ājīvenapi codeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr12:24.4"
msgid "codanā tena vuccatīti."
msgstr ""

#. HTML: </p></blockquote><p class="endsection">
#. REF: sc22
msgctxt "pli-tv-pvr12:25.1"
msgid "Aparaṃ gāthāsaṅgaṇikaṃ niṭṭhitaṃ."
msgstr ""

