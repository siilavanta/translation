#. HTML: <section class="sutta" id="pli-tv-pvr11"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr11:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr11:2.1"
msgid "Adhikaraṇabheda"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr11:3.1"
msgid "1. Ukkoṭanabhedādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli5.150
msgctxt "pli-tv-pvr11:4.1"
msgid "Cattāri adhikaraṇāni."
msgstr ""

msgctxt "pli-tv-pvr11:4.2"
msgid "Vivādādhikaraṇaṃ, anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ, kiccādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:4.3"
msgid "imāni cattāri adhikaraṇāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr11:5.1"
msgid "Imesaṃ catunnaṃ adhikaraṇānaṃ kati ukkoṭā?"
msgstr ""

msgctxt "pli-tv-pvr11:5.2"
msgid "Imesaṃ catunnaṃ adhikaraṇānaṃ dasa ukkoṭā."
msgstr ""

msgctxt "pli-tv-pvr11:5.3"
msgid "Vivādādhikaraṇassa dve ukkoṭā, anuvādādhikaraṇassa cattāro ukkoṭā, āpattādhikaraṇassa tayo ukkoṭā, kiccādhikaraṇassa eko ukkoṭo—"
msgstr ""

msgctxt "pli-tv-pvr11:5.4"
msgid "imesaṃ catunnaṃ adhikaraṇānaṃ ime dasa ukkoṭā."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr11:6.1"
msgid "Vivādādhikaraṇaṃ ukkoṭento kati samathe ukkoṭeti?"
msgstr ""

msgctxt "pli-tv-pvr11:6.2"
msgid "Anuvādādhikaraṇaṃ ukkoṭento kati samathe ukkoṭeti?"
msgstr ""

msgctxt "pli-tv-pvr11:6.3"
msgid "Āpattādhikaraṇaṃ ukkoṭento kati samathe ukkoṭeti?"
msgstr ""

msgctxt "pli-tv-pvr11:6.4"
msgid "Kiccādhikaraṇaṃ ukkoṭento kati samathe ukkoṭeti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr11:7.1"
msgid "Vivādādhikaraṇaṃ ukkoṭento dve samathe ukkoṭeti."
msgstr ""

msgctxt "pli-tv-pvr11:7.2"
msgid "Anuvādādhikaraṇaṃ ukkoṭento cattāro samathe ukkoṭeti."
msgstr ""

msgctxt "pli-tv-pvr11:7.3"
msgid "Āpattādhikaraṇaṃ ukkoṭento tayo samathe ukkoṭeti."
msgstr ""

msgctxt "pli-tv-pvr11:7.4"
msgid "Kiccādhikaraṇaṃ ukkoṭento ekaṃ samathaṃ ukkoṭeti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr11:8.1"
msgid "Kati ukkoṭā?"
msgstr ""

msgctxt "pli-tv-pvr11:8.2"
msgid "Katihākārehi ukkoṭanaṃ pasavati?"
msgstr ""

msgctxt "pli-tv-pvr11:8.3"
msgid "Katihaṅgehi samannāgato puggalo adhikaraṇaṃ ukkoṭeti?"
msgstr ""

msgctxt "pli-tv-pvr11:8.4"
msgid "Kati puggalā adhikaraṇaṃ ukkoṭentā āpattiṃ āpajjanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr11:9.1"
msgid "Dvādasa ukkoṭā."
msgstr ""

msgctxt "pli-tv-pvr11:9.2"
msgid "Dasahākārehi ukkoṭanaṃ pasavati."
msgstr ""

msgctxt "pli-tv-pvr11:9.3"
msgid "Catūhaṅgehi samannāgato puggalo adhikaraṇaṃ ukkoṭeti."
msgstr ""

msgctxt "pli-tv-pvr11:9.4"
msgid "Cattāro puggalā adhikaraṇaṃ ukkoṭentā āpattiṃ āpajjanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr11:10.1"
msgid "Katame dvādasa ukkoṭā?"
msgstr ""

msgctxt "pli-tv-pvr11:10.2"
msgid "Akataṃ kammaṃ, dukkaṭaṃ kammaṃ, puna kātabbaṃ kammaṃ, anihataṃ, dunnihataṃ, puna nihanitabbaṃ, avinicchitaṃ, duvinicchitaṃ, puna vinicchitabbaṃ, avūpasantaṃ, duvūpasantaṃ, puna vūpasametabbanti—"
msgstr ""

msgctxt "pli-tv-pvr11:10.3"
msgid "ime dvādasa ukkoṭā."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli5.151
msgctxt "pli-tv-pvr11:11.1"
msgid "Katamehi dasahākārehi ukkoṭanaṃ pasavati?"
msgstr ""

msgctxt "pli-tv-pvr11:11.2"
msgid "Tattha jātakaṃ adhikaraṇaṃ ukkoṭeti, tattha jātakaṃ vūpasantaṃ adhikaraṇaṃ ukkoṭeti, antarāmagge adhikaraṇaṃ ukkoṭeti, antarāmagge vūpasantaṃ adhikaraṇaṃ ukkoṭeti, tattha gataṃ adhikaraṇaṃ ukkoṭeti, tattha gataṃ vūpasantaṃ adhikaraṇaṃ ukkoṭeti, sativinayaṃ ukkoṭeti, amūḷhavinayaṃ ukkoṭeti, tassapāpiyasikaṃ ukkoṭeti, tiṇavatthārakaṃ ukkoṭeti—"
msgstr ""

msgctxt "pli-tv-pvr11:11.3"
msgid "imehi dasahākārehi ukkoṭanaṃ pasavati."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr11:12.1"
msgid "Katamehi catūhaṅgehi samannāgato puggalo adhikaraṇaṃ ukkoṭeti?"
msgstr ""

msgctxt "pli-tv-pvr11:12.2"
msgid "Chandāgatiṃ gacchanto adhikaraṇaṃ ukkoṭeti, dosāgatiṃ gacchanto adhikaraṇaṃ ukkoṭeti, mohāgatiṃ gacchanto adhikaraṇaṃ ukkoṭeti, bhayāgatiṃ gacchanto adhikaraṇaṃ ukkoṭeti—"
msgstr ""

msgctxt "pli-tv-pvr11:12.3"
msgid "imehi catūhaṅgehi samannāgato puggalo adhikaraṇaṃ ukkoṭeti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr11:13.1"
msgid "Katame cattāro puggalā adhikaraṇaṃ ukkoṭentā āpattiṃ āpajjanti?"
msgstr ""

msgctxt "pli-tv-pvr11:13.2"
msgid "Tadahupasampanno ukkoṭeti ukkoṭanakaṃ pācittiyaṃ, āgantuko ukkoṭeti ukkoṭanakaṃ pācittiyaṃ, kārako ukkoṭeti ukkoṭanakaṃ pācittiyaṃ, chandadāyako ukkoṭeti ukkoṭanakaṃ pācittiyaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:13.3"
msgid "ime cattāro puggalā adhikaraṇaṃ ukkoṭentā āpattiṃ āpajjanti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr11:14.1"
msgid "2. Adhikaraṇanidānādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc11
msgctxt "pli-tv-pvr11:15.1"
msgid "Vivādādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:15.2"
msgid "Anuvādādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:15.3"
msgid "Āpattādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:15.4"
msgid "Kiccādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr11:16.1"
msgid "Vivādādhikaraṇaṃ vivādanidānaṃ vivādasamudayaṃ vivādajātikaṃ vivādapabhavaṃ vivādasambhāraṃ vivādasamuṭṭhānaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:16.2"
msgid "Anuvādādhikaraṇaṃ anuvādanidānaṃ anuvādasamudayaṃ anuvādajātikaṃ anuvādapabhavaṃ anuvādasambhāraṃ anuvādasamuṭṭhānaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:16.3"
msgid "Āpattādhikaraṇaṃ āpattinidānaṃ āpattisamudayaṃ āpattijātikaṃ āpattipabhavaṃ āpattisambhāraṃ āpattisamuṭṭhānaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:16.4"
msgid "Kiccādhikaraṇaṃ kiccayanidānaṃ kiccayasamudayaṃ kiccayajātikaṃ kiccayapabhavaṃ kiccayasambhāraṃ kiccayasamuṭṭhānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr11:17.1"
msgid "Vivādādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:17.2"
msgid "Anuvādādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:17.3"
msgid "āpattādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:17.4"
msgid "kiccādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr11:18.1"
msgid "Vivādādhikaraṇaṃ hetunidānaṃ, hetusamudayaṃ, hetujātikaṃ, hetupabhavaṃ, hetusambhāraṃ, hetusamuṭṭhānaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:18.2"
msgid "Anuvādādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:18.3"
msgid "āpattādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:18.4"
msgid "kiccādhikaraṇaṃ hetunidānaṃ, hetusamudayaṃ, hetujātikaṃ, hetupabhavaṃ, hetusambhāraṃ, hetusamuṭṭhānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr11:19.1"
msgid "Vivādādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:19.2"
msgid "Anuvādādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:19.3"
msgid "āpattādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:19.4"
msgid "kiccādhikaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr11:20.1"
msgid "Vivādādhikaraṇaṃ paccayanidānaṃ, paccayasamudayaṃ, paccayajātikaṃ, paccayapabhavaṃ, paccayasambhāraṃ, paccayasamuṭṭhānaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:20.2"
msgid "Anuvādādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:20.3"
msgid "āpattādhikaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:20.4"
msgid "kiccādhikaraṇaṃ paccayanidānaṃ, paccayasamudayaṃ, paccayajātikaṃ, paccayapabhavaṃ, paccayasambhāraṃ, paccayasamuṭṭhānaṃ."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr11:21.1"
msgid "3. Adhikaraṇamūlādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc17
msgctxt "pli-tv-pvr11:22.1"
msgid "Catunnaṃ adhikaraṇānaṃ kati mūlāni, kati samuṭṭhānā?"
msgstr ""

msgctxt "pli-tv-pvr11:22.2"
msgid "Catunnaṃ adhikaraṇānaṃ tettiṃsa mūlāni, tettiṃsa samuṭṭhānā."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr11:23.1"
msgid "Catunnaṃ adhikaraṇānaṃ katamāni tettiṃsa mūlāni?"
msgstr ""

msgctxt "pli-tv-pvr11:23.2"
msgid "Vivādādhikaraṇassa dvādasa mūlāni, anuvādādhikaraṇassa cuddasa mūlāni, āpattādhikaraṇassa cha mūlāni, kiccādhikaraṇassa ekaṃ mūlaṃ, saṃgho—"
msgstr ""

msgctxt "pli-tv-pvr11:23.3"
msgid "catunnaṃ adhikaraṇānaṃ imāni tettiṃsa mūlāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc19, pts-vp-pli5.152
msgctxt "pli-tv-pvr11:24.1"
msgid "Catunnaṃ adhikaraṇānaṃ katame tettiṃsa samuṭṭhānā?"
msgstr ""

#. VAR: sattāpattikkhandhā → satta āpattikkhandhā (bj, s1-3)
msgctxt "pli-tv-pvr11:24.2"
msgid "Vivādādhikaraṇassa aṭṭhārasabhedakaravatthūni samuṭṭhānā, anuvādādhikaraṇassa catasso vipattiyo samuṭṭhānā, āpattādhikaraṇassa sattāpattikkhandhā samuṭṭhānā, kiccādhikaraṇassa cattāri kammāni samuṭṭhānā—"
msgstr ""

msgctxt "pli-tv-pvr11:24.3"
msgid "catunnaṃ adhikaraṇānaṃ ime tettiṃsa samuṭṭhānā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr11:25.1"
msgid "4. Adhikaraṇapaccayāpatti"
msgstr ""

#. HTML: </h2><p>
#. REF: sc20
msgctxt "pli-tv-pvr11:26.1"
msgid "Vivādādhikaraṇaṃ āpattānāpattīti?"
msgstr ""

msgctxt "pli-tv-pvr11:26.2"
msgid "Vivādādhikaraṇaṃ na āpatti."
msgstr ""

msgctxt "pli-tv-pvr11:26.3"
msgid "Kiṃ pana vivādādhikaraṇapaccayā āpattiṃ āpajjeyyāti?"
msgstr ""

msgctxt "pli-tv-pvr11:26.4"
msgid "Āma, vivādādhikaraṇapaccayā āpattiṃ āpajjeyya."
msgstr ""

msgctxt "pli-tv-pvr11:26.5"
msgid "Vivādādhikaraṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr11:26.6"
msgid "Vivādādhikaraṇapaccayā dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr11:26.7"
msgid "Upasampannaṃ omasati, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr11:26.8"
msgid "anupasampannaṃ omasati, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr11:26.9"
msgid "vivādādhikaraṇapaccayā imā dve āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr11:27.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr11:27.2"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:27.3"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr11:27.4"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr11:27.5"
msgid "Katihi adhikaraṇehi, katisu ṭhānesu, katihi samathehi sammanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc22
msgctxt "pli-tv-pvr11:28.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr11:28.2"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr11:28.3"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:28.4"
msgid "Sattannaṃ āpattikkhandhānaṃ dvīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr11:28.5"
msgid "siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr11:28.6"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr11:28.7"
msgid "Ekena adhikaraṇena—"
msgstr ""

msgctxt "pli-tv-pvr11:28.8"
msgid "kiccādhikaraṇena;"
msgstr ""

msgctxt "pli-tv-pvr11:28.9"
msgid "tīsu ṭhānesu—"
msgstr ""

msgctxt "pli-tv-pvr11:28.10"
msgid "saṃghamajjhe, gaṇamajjhe, puggalassa santike;"
msgstr ""

msgctxt "pli-tv-pvr11:28.11"
msgid "tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr11:28.12"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-pvr11:29.1"
msgid "Anuvādādhikaraṇaṃ āpattānāpattīti?"
msgstr ""

msgctxt "pli-tv-pvr11:29.2"
msgid "Anuvādādhikaraṇaṃ na āpatti."
msgstr ""

msgctxt "pli-tv-pvr11:29.3"
msgid "Kiṃ pana anuvādādhikaraṇapaccayā āpattiṃ āpajjeyyāti?"
msgstr ""

msgctxt "pli-tv-pvr11:29.4"
msgid "Āma, anuvādādhikaraṇapaccayā āpattiṃ āpajjeyya."
msgstr ""

msgctxt "pli-tv-pvr11:29.5"
msgid "Anuvādādhikaraṇapaccayā, kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr11:29.6"
msgid "Anuvādādhikaraṇapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr11:29.7"
msgid "Bhikkhuṃ amūlakena pārājikena dhammena anuddhaṃseti, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr11:29.8"
msgid "amūlakena saṃghādisesena anuddhaṃseti, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr11:29.9"
msgid "amūlikāya ācāravipattiyā anuddhaṃseti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr11:29.10"
msgid "anuvādādhikaraṇapaccayā imā tisso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr11:30.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr11:30.2"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:30.3"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr11:30.4"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr11:30.5"
msgid "Katihi adhikaraṇehi, katisu ṭhānesu, katihi samathehi sammanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc25, pts-vp-pli5.153
msgctxt "pli-tv-pvr11:31.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr11:31.2"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr11:31.3"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:31.4"
msgid "Sattannaṃ āpattikkhandhānaṃ tīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr11:31.5"
msgid "siyā saṃghādisesāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr11:31.6"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr11:31.7"
msgid "Yā tā āpattiyo garukā tā āpattiyo ekena adhikaraṇena—"
msgstr ""

msgctxt "pli-tv-pvr11:31.8"
msgid "kiccādhikaraṇena;"
msgstr ""

msgctxt "pli-tv-pvr11:31.9"
msgid "ekamhi ṭhāne—"
msgstr ""

msgctxt "pli-tv-pvr11:31.10"
msgid "saṃghamajjhe;"
msgstr ""

msgctxt "pli-tv-pvr11:31.11"
msgid "dvīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr11:31.12"
msgid "sammukhāvinayena ca paṭiññātakaraṇena ca."
msgstr ""

msgctxt "pli-tv-pvr11:31.13"
msgid "Yā tā āpattiyo lahukā tā āpattiyo ekena adhikaraṇena—"
msgstr ""

msgctxt "pli-tv-pvr11:31.14"
msgid "kiccādhikaraṇena;"
msgstr ""

msgctxt "pli-tv-pvr11:31.15"
msgid "tīsu ṭhānesu—"
msgstr ""

msgctxt "pli-tv-pvr11:31.16"
msgid "saṃghamajjhe gaṇamajjhe puggalassa santike;"
msgstr ""

msgctxt "pli-tv-pvr11:31.17"
msgid "tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr11:31.18"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr11:32.1"
msgid "Āpattādhikaraṇaṃ āpattānāpattīti?"
msgstr ""

msgctxt "pli-tv-pvr11:32.2"
msgid "Āpattādhikaraṇaṃ āpatti."
msgstr ""

msgctxt "pli-tv-pvr11:32.3"
msgid "Kiṃ pana āpattādhikaraṇapaccayā āpattiṃ āpajjeyyāti?"
msgstr ""

msgctxt "pli-tv-pvr11:32.4"
msgid "Āma, āpattādhikaraṇapaccayā āpattiṃ āpajjeyya."
msgstr ""

msgctxt "pli-tv-pvr11:32.5"
msgid "Āpattādhikaraṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr11:32.6"
msgid "Āpattādhikaraṇapaccayā catasso āpattiyo āpajjati."
msgstr ""

#. VAR: pārājikaṃ dhammaṃ → pārājikaṃ dhammaṃ ajjhāpannaṃ (s1-3)
msgctxt "pli-tv-pvr11:32.7"
msgid "Bhikkhunī jānaṃ pārājikaṃ dhammaṃ paṭicchādeti, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr11:32.8"
msgid "vematikā paṭicchādeti, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr11:32.9"
msgid "bhikkhu saṃghādisesaṃ paṭicchādeti, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr11:32.10"
msgid "ācāravipattiṃ paṭicchādeti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr11:32.11"
msgid "āpattādhikaraṇapaccayā imā catasso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc27
