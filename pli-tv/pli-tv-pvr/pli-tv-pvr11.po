msgstr ""

msgctxt "pli-tv-pvr11:35.2"
msgid "Kiccādhikaraṇaṃ na āpatti."
msgstr ""

msgctxt "pli-tv-pvr11:35.3"
msgid "Kiṃ pana kiccādhikaraṇapaccayā āpattiṃ āpajjeyyāti?"
msgstr ""

msgctxt "pli-tv-pvr11:35.4"
msgid "Āma, kiccādhikaraṇapaccayā āpattiṃ āpajjeyya."
msgstr ""

msgctxt "pli-tv-pvr11:35.5"
msgid "Kiccādhikaraṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr11:35.6"
msgid "Kiccādhikaraṇapaccayā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr11:35.7"
msgid "Ukkhittānuvattikā bhikkhunī yāvatatiyaṃ samanubhāsanāya na paṭinissajjati, ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr11:35.8"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr11:35.9"
msgid "kammavācāpariyosāne āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr11:35.10"
msgid "bhedakānuvattakā bhikkhū yāvatatiyaṃ samanubhāsanāya na paṭinissajjanti, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr11:35.11"
msgid "pāpikāya diṭṭhiyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjanti, āpatti pācittiyassa—"
msgstr ""

msgctxt "pli-tv-pvr11:35.12"
msgid "kiccādhikaraṇapaccayā imā pañca āpattiyo āpajjati."
msgstr ""

