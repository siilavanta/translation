#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-pvr11:56.1"
msgid "Sattannaṃ samathānaṃ kati mūlāni, kati samuṭṭhānā?"
msgstr ""

msgctxt "pli-tv-pvr11:56.2"
msgid "Sattannaṃ samathānaṃ chabbīsa mūlāni, chattiṃsa samuṭṭhānā."
msgstr ""

msgctxt "pli-tv-pvr11:56.3"
msgid "Sattannaṃ samathānaṃ katamāni chabbīsa mūlāni?"
msgstr ""

msgctxt "pli-tv-pvr11:56.4"
msgid "Sammukhāvinayassa cattāri mūlāni—"
msgstr ""

msgctxt "pli-tv-pvr11:56.5"
msgid "saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā;"
msgstr ""

msgctxt "pli-tv-pvr11:56.6"
msgid "sativinayassa cattāri mūlāni;"
msgstr ""

msgctxt "pli-tv-pvr11:56.7"
msgid "amūḷhavinayassa cattāri mūlāni;"
msgstr ""

msgctxt "pli-tv-pvr11:56.8"
msgid "paṭiññātakaraṇassa dve mūlāni—"
msgstr ""

msgctxt "pli-tv-pvr11:56.9"
msgid "yo ca deseti yassa ca deseti;"
msgstr ""

msgctxt "pli-tv-pvr11:56.10"
msgid "yebhuyyasikāya cattāri mūlāni;"
msgstr ""

msgctxt "pli-tv-pvr11:56.11"
msgid "tassapāpiyasikāya cattāri mūlāni;"
msgstr ""

msgctxt "pli-tv-pvr11:56.12"
msgid "tiṇavatthārakassa cattāri mūlāni—"
msgstr ""

msgctxt "pli-tv-pvr11:56.13"
msgid "saṃghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā—"
msgstr ""

msgctxt "pli-tv-pvr11:56.14"
msgid "sattannaṃ samathānaṃ imāni chabbīsa mūlāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc46
msgctxt "pli-tv-pvr11:57.1"
msgid "Sattannaṃ samathānaṃ katame chattiṃsa samuṭṭhānā?"
msgstr ""

msgctxt "pli-tv-pvr11:57.2"
msgid "Sativinayassa kammassa kiriyā, karaṇaṃ, upagamanaṃ, ajjhupagamanaṃ, adhivāsanā, appaṭikkosanā."
msgstr ""

msgctxt "pli-tv-pvr11:57.3"
msgid "Amūḷhavinayassa kammassa … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:57.4"
msgid "paṭiññātakaraṇassa kammassa …"
msgstr ""

msgctxt "pli-tv-pvr11:57.5"
msgid "yebhuyyasikāya kammassa …"
msgstr ""

msgctxt "pli-tv-pvr11:57.6"
msgid "tassapāpiyasikāya kammassa …"
msgstr ""

msgctxt "pli-tv-pvr11:57.7"
msgid "tiṇavatthārakassa kammassa kiriyā, karaṇaṃ, upagamanaṃ, ajjhupagamanaṃ, adhivāsanā, appaṭikkosanā—"
msgstr ""

msgctxt "pli-tv-pvr11:57.8"
msgid "sattannaṃ samathānaṃ ime chattiṃsa samuṭṭhānā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr11:58.1"
