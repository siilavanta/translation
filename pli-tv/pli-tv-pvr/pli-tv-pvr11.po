#. HTML: </p><h2>
msgctxt "pli-tv-pvr11:38.1"
msgid "5. Adhikaraṇādhippāya"
msgstr ""

#. HTML: </h2><p>
#. REF: sc32
msgctxt "pli-tv-pvr11:39.1"
msgid "Vivādādhikaraṇaṃ hoti anuvādādhikaraṇaṃ, hoti āpattādhikaraṇaṃ, hoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.2"
msgid "Vivādādhikaraṇaṃ na hoti anuvādādhikaraṇaṃ, na hoti āpattādhikaraṇaṃ, na hoti kiccādhikaraṇaṃ;"
msgstr ""

msgctxt "pli-tv-pvr11:39.3"
msgid "api ca, vivādādhikaraṇapaccayā hoti anuvādādhikaraṇaṃ, hoti āpattādhikaraṇaṃ, hoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.4"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr11:39.5"
msgid "Idha bhikkhū vivadanti—"
msgstr ""

msgctxt "pli-tv-pvr11:39.6"
msgid "dhammoti vā adhammoti vā … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:39.7"
msgid "duṭṭhullāpattīti vā aduṭṭhullāpattīti vā."
msgstr ""

msgctxt "pli-tv-pvr11:39.8"
msgid "Yaṃ tattha bhaṇḍanaṃ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhakaṃ, idaṃ vuccati vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.9"
msgid "Vivādādhikaraṇe saṃgho vivadati vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.10"
msgid "Vivadamāno anuvadati anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.11"
msgid "Anuvadamāno āpattiṃ āpajjati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.12"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.13"
msgid "Evaṃ vivādādhikaraṇapaccayā hoti anuvādādhikaraṇaṃ, hoti āpattādhikaraṇaṃ, hoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:39.14"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc33
msgctxt "pli-tv-pvr11:40.1"
msgid "Anuvādādhikaraṇaṃ hoti āpattādhikaraṇaṃ, hoti kiccādhikaraṇaṃ, hoti vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.2"
msgid "Anuvādādhikaraṇaṃ na hoti āpattādhikaraṇaṃ, na hoti kiccādhikaraṇaṃ, na hoti vivādādhikaraṇaṃ;"
msgstr ""

msgctxt "pli-tv-pvr11:40.3"
msgid "api ca anuvādādhikaraṇapaccayā hoti āpattādhikaraṇaṃ, hoti kiccādhikaraṇaṃ, hoti vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.4"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr11:40.5"
msgid "Idha bhikkhū bhikkhuṃ anuvadanti sīlavipattiyā vā ācāravipattiyā vā diṭṭhivipattiyā vā ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-pvr11:40.6"
msgid "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ, idaṃ vuccati anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.7"
msgid "Anuvādādhikaraṇe saṃgho vivadati, vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.8"
msgid "Vivadamāno anuvadati, anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.9"
msgid "Anuvadamāno āpattiṃ āpajjati, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.10"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti, kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.11"
msgid "Evaṃ anuvādādhikaraṇapaccayā hoti āpattādhikaraṇaṃ, hoti kiccādhikaraṇaṃ, hoti vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:40.12"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-pvr11:41.1"
msgid "Āpattādhikaraṇaṃ hoti kiccādhikaraṇaṃ, hoti vivādādhikaraṇaṃ, hoti anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.2"
msgid "Āpattādhikaraṇaṃ na hoti kiccādhikaraṇaṃ, na hoti vivādādhikaraṇaṃ, na hoti anuvādādhikaraṇaṃ;"
msgstr ""

msgctxt "pli-tv-pvr11:41.3"
msgid "api ca āpattādhikaraṇapaccayā hoti kiccādhikaraṇaṃ, hoti vivādādhikaraṇaṃ, hoti anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.4"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr11:41.5"
msgid "Pañcapi āpattikkhandhā āpattādhikaraṇaṃ, sattapi āpattikkhandhā āpattādhikaraṇaṃ, idaṃ vuccati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.6"
msgid "Āpattādhikaraṇe saṃgho vivadati vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.7"
msgid "Vivadamāno anuvadati anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.8"
msgid "Anuvadamāno āpattiṃ āpajjati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.9"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.10"
msgid "Evaṃ āpattādhikaraṇapaccayā hoti kiccādhikaraṇaṃ, hoti vivādādhikaraṇaṃ, hoti anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:41.11"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-pvr11:42.1"
msgid "Kiccādhikaraṇaṃ hoti vivādādhikaraṇaṃ, hoti anuvādādhikaraṇaṃ, hoti āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.2"
msgid "Kiccādhikaraṇaṃ na hoti vivādādhikaraṇaṃ, na hoti anuvādādhikaraṇaṃ, na hoti āpattādhikaraṇaṃ;"
msgstr ""

msgctxt "pli-tv-pvr11:42.3"
msgid "api ca kiccādhikaraṇapaccayā hoti vivādādhikaraṇaṃ, hoti anuvādādhikaraṇaṃ, hoti āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.4"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr11:42.5"
msgid "Yā saṃghassa kiccayatā karaṇīyatā apalokanakammaṃ ñattikammaṃ ñattidutiyakammaṃ ñatticatutthakammaṃ, idaṃ vuccati kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.6"
msgid "Kiccādhikaraṇe saṃgho vivadati vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.7"
msgid "Vivadamāno anuvadati anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.8"
msgid "Anuvadamāno āpattiṃ āpajjati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.9"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.10"
msgid "Evaṃ kiccādhikaraṇapaccayā hoti vivādādhikaraṇaṃ, hoti anuvādādhikaraṇaṃ, hoti āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:42.11"
msgid "(4)"
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr11:43.1"
msgid "6. Pucchāvāra"
msgstr ""

