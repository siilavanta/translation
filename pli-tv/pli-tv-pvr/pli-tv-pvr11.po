msgctxt "pli-tv-pvr11:59.7"
msgid "sammukhāvinayoti vā tiṇavatthārakoti vā—"
msgstr ""

msgctxt "pli-tv-pvr11:59.8"
msgid "ime dhammā nānatthā nānābyañjanā, udāhu ekatthā byañjanameva nānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:59.9"
msgid "Sammukhāvinayoti vā sativinayoti vā—"
msgstr ""

msgctxt "pli-tv-pvr11:59.10"
msgid "ime dhammā nānatthā ceva nānābyañjanā ca."
msgstr ""

msgctxt "pli-tv-pvr11:59.11"
msgid "Sammukhāvinayoti vā amūḷhavinayoti vā … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:59.12"
msgid "sammukhāvinayoti vā paṭiññātakaraṇanti vā …"
msgstr ""

msgctxt "pli-tv-pvr11:59.13"
msgid "sammukhāvinayoti vā yebhuyyasikāti vā …"
msgstr ""

msgctxt "pli-tv-pvr11:59.14"
msgid "sammukhāvinayoti vā tassapāpiyasikāti vā …"
msgstr ""

msgctxt "pli-tv-pvr11:59.15"
msgid "sammukhāvinayoti vā tiṇavatthārakoti vā—"
msgstr ""

msgctxt "pli-tv-pvr11:59.16"
msgid "ime dhammā nānatthā ceva nānābyañjanā ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc48, pts-vp-pli5.157
msgctxt "pli-tv-pvr11:60.1"
msgid "Vivādo vivādādhikaraṇaṃ, vivādo no adhikaraṇaṃ, adhikaraṇaṃ no vivādo, adhikaraṇañceva vivādo ca?"
msgstr ""

msgctxt "pli-tv-pvr11:60.2"
msgid "Siyā vivādo vivādādhikaraṇaṃ, siyā vivādo no adhikaraṇaṃ, siyā adhikaraṇaṃ no vivādo, siyā adhikaraṇañceva vivādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc49
msgctxt "pli-tv-pvr11:61.1"
msgid "Tattha katamo vivādo vivādādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:61.2"
msgid "Idha bhikkhū vivadanti dhammoti vā adhammoti vā … pe …"
msgstr ""

msgctxt "pli-tv-pvr11:61.3"
msgid "duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā."
msgstr ""

msgctxt "pli-tv-pvr11:61.4"
msgid "Yaṃ tattha bhaṇḍanaṃ, kalaho, viggaho, vivādo, nānāvādo, aññathāvādo, vipaccatāya vohāro, medhakaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:61.5"
msgid "ayaṃ vivādo vivādādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc50
msgctxt "pli-tv-pvr11:62.1"
msgid "Tattha katamo vivādo no adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:62.2"
msgid "Mātāpi puttena vivadati, puttopi mātarā vivadati, pitāpi puttena vivadati, puttopi pitarā vivadati, bhātāpi bhātarā vivadati, bhātāpi bhaginiyā vivadati, bhaginīpi bhātarā vivadati, sahāyopi sahāyena vivadati—"
msgstr ""

msgctxt "pli-tv-pvr11:62.3"
msgid "ayaṃ vivādo no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc51
msgctxt "pli-tv-pvr11:63.1"
msgid "Tattha katamaṃ adhikaraṇaṃ no vivādo?"
msgstr ""

msgctxt "pli-tv-pvr11:63.2"
msgid "Anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ, kiccādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:63.3"
msgid "idaṃ adhikaraṇaṃ no vivādo."
msgstr ""

#. HTML: </p><p>
#. REF: sc52
msgctxt "pli-tv-pvr11:64.1"
msgid "Tattha katamaṃ adhikaraṇañceva vivādo ca?"
msgstr ""

msgctxt "pli-tv-pvr11:64.2"
msgid "Vivādādhikaraṇaṃ adhikaraṇañceva vivādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc53
msgctxt "pli-tv-pvr11:65.1"
msgid "Anuvādo anuvādādhikaraṇaṃ, anuvādo no adhikaraṇaṃ, adhikaraṇaṃ no anuvādo, adhikaraṇañceva anuvādo ca?"
msgstr ""

msgctxt "pli-tv-pvr11:65.2"
msgid "Siyā anuvādo anuvādādhikaraṇaṃ, siyā anuvādo no adhikaraṇaṃ, siyā adhikaraṇaṃ no anuvādo, siyā adhikaraṇañceva anuvādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc54
msgctxt "pli-tv-pvr11:66.1"
msgid "Tattha katamo anuvādo anuvādādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:66.2"
msgid "Idha bhikkhū bhikkhuṃ anuvadanti sīlavipattiyā vā ācāravipattiyā vā diṭṭhivipattiyā vā ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-pvr11:66.3"
msgid "Yo tattha anuvādo, anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:66.4"
msgid "ayaṃ anuvādo anuvādādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-pvr11:67.1"
msgid "Tattha katamo anuvādo no adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:67.2"
msgid "Mātāpi puttaṃ anuvadati, puttopi mātaraṃ anuvadati, pitāpi puttaṃ anuvadati, puttopi pitaraṃ anuvadati, bhātāpi bhātaraṃ anuvadati, bhātāpi bhaginiṃ anuvadati, bhaginīpi bhātaraṃ anuvadati, sahāyopi sahāyaṃ anuvadati—"
msgstr ""

msgctxt "pli-tv-pvr11:67.3"
msgid "ayaṃ anuvādo no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-pvr11:68.1"
msgid "Tattha katamaṃ adhikaraṇaṃ no anuvādo?"
msgstr ""

msgctxt "pli-tv-pvr11:68.2"
msgid "Āpattādhikaraṇaṃ kiccādhikaraṇaṃ vivādādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:68.3"
msgid "idaṃ adhikaraṇaṃ no anuvādo."
msgstr ""

#. HTML: </p><p>
#. REF: sc57
msgctxt "pli-tv-pvr11:69.1"
msgid "Tattha katamaṃ adhikaraṇañceva anuvādo ca?"
msgstr ""

msgctxt "pli-tv-pvr11:69.2"
msgid "Anuvādādhikaraṇaṃ adhikaraṇañceva anuvādo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-pvr11:70.1"
msgid "Āpatti āpattādhikaraṇaṃ, āpatti no adhikaraṇaṃ, adhikaraṇaṃ no āpatti, adhikaraṇañceva āpatti ca?"
msgstr ""

msgctxt "pli-tv-pvr11:70.2"
msgid "Siyā āpatti āpattādhikaraṇaṃ, siyā āpatti no adhikaraṇaṃ, siyā adhikaraṇaṃ no āpatti, siyā adhikaraṇañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr11:71.1"
msgid "Tattha katamā āpatti āpattādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:71.2"
msgid "Pañcapi āpattikkhandhā āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:71.3"
msgid "Sattapi āpattikkhandhā āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr11:71.4"
msgid "Ayaṃ āpatti āpattādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-pvr11:72.1"
msgid "Tattha katamā āpatti no adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:72.2"
msgid "Sotāpatti samāpatti—"
msgstr ""

msgctxt "pli-tv-pvr11:72.3"
msgid "ayaṃ āpatti no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc61
msgctxt "pli-tv-pvr11:73.1"
msgid "Tattha katamaṃ adhikaraṇaṃ no āpatti?"
msgstr ""

msgctxt "pli-tv-pvr11:73.2"
msgid "Kiccādhikaraṇaṃ vivādādhikaraṇaṃ anuvādādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:73.3"
msgid "idaṃ adhikaraṇaṃ no āpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-pvr11:74.1"
msgid "Tattha katamaṃ adhikaraṇañceva āpatti ca?"
msgstr ""

msgctxt "pli-tv-pvr11:74.2"
msgid "Āpattādhikaraṇaṃ adhikaraṇañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc63
msgctxt "pli-tv-pvr11:75.1"
msgid "Kiccaṃ kiccādhikaraṇaṃ, kiccaṃ no adhikaraṇaṃ, adhikaraṇaṃ no kiccaṃ, adhikaraṇañceva kiccañca?"
msgstr ""

msgctxt "pli-tv-pvr11:75.2"
msgid "Siyā kiccaṃ kiccādhikaraṇaṃ, siyā kiccaṃ no adhikaraṇaṃ, siyā adhikaraṇaṃ no kiccaṃ, siyā adhikaraṇañceva kiccañca."
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-pvr11:76.1"
msgid "Tattha katamaṃ kiccaṃ kiccādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:76.2"
msgid "Yā saṃghassa kiccayatā karaṇīyatā apalokanakammaṃ ñattikammaṃ ñattidutiyakammaṃ ñatticatutthakammaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:76.3"
msgid "idaṃ kiccaṃ kiccādhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-pvr11:77.1"
msgid "Tattha katamaṃ kiccaṃ no adhikaraṇaṃ?"
msgstr ""

#. VAR: upajjhāyakiccaṃ → upajjhāyakiccaṃ sakiccaṃ (mr)
msgctxt "pli-tv-pvr11:77.2"
msgid "Ācariyakiccaṃ upajjhāyakiccaṃ samānupajjhāyakiccaṃ samānācariyakiccaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:77.3"
msgid "idaṃ kiccaṃ no adhikaraṇaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr11:78.1"
msgid "Tattha katamaṃ adhikaraṇaṃ no kiccaṃ?"
msgstr ""

msgctxt "pli-tv-pvr11:78.2"
msgid "Vivādādhikaraṇaṃ anuvādādhikaraṇaṃ āpattādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-pvr11:78.3"
msgid "idaṃ adhikaraṇaṃ no kiccaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr11:79.1"
msgid "Tattha katamaṃ adhikaraṇañceva kiccañca?"
msgstr ""

msgctxt "pli-tv-pvr11:79.2"
msgid "Kiccādhikaraṇaṃ adhikaraṇañceva kiccaṃ cāti."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc68
msgctxt "pli-tv-pvr11:80.1"
msgid "Adhikaraṇabhedaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc69
msgctxt "pli-tv-pvr11:81.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc70
msgctxt "pli-tv-pvr11:82.1"
msgid "Adhikaraṇaṃ ukkoṭā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr11:82.2"
msgid "ākārā puggalena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr11:82.3"
msgid "Nidānahetupaccayā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr11:82.4"
msgid "mūlaṃ samuṭṭhānena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-pvr11:83.1"
msgid "Āpatti hoti yattha ca,"
msgstr ""

#. HTML: <br>
#. VAR: saṃsaṭṭhā nidānena ca → saṃsaṭṭhā nidānappabhavā (bj)
msgctxt "pli-tv-pvr11:83.2"
msgid "saṃsaṭṭhā nidānena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr11:83.3"
msgid "Hetupaccayamūlāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr11:83.4"
msgid "samuṭṭhānena byañjanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr11:83.5"
msgid "Vivādo adhikaraṇanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr11:83.6"
msgid "bhedādhikaraṇe idanti."
msgstr ""
