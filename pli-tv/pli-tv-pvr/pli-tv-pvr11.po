msgctxt "pli-tv-pvr11:59.4"
msgid "sammukhāvinayoti vā paṭiññātakaraṇanti vā …"
msgstr ""

msgctxt "pli-tv-pvr11:59.5"
msgid "sammukhāvinayoti vā yebhuyyasikāti vā …"
msgstr ""

msgctxt "pli-tv-pvr11:59.6"
