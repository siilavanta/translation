#. HTML: <section class="sutta" id="pli-tv-pvr21"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr21:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr21:2.1"
msgid "Pañcavagga"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr21:3.1"
msgid "1. Kammavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli5.220
msgctxt "pli-tv-pvr21:4.1"
msgid "Cattāri kammāni."
msgstr ""

msgctxt "pli-tv-pvr21:4.2"
msgid "Apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ—"
msgstr ""

msgctxt "pli-tv-pvr21:4.3"
msgid "imāni cattāri kammāni."
msgstr ""

msgctxt "pli-tv-pvr21:4.4"
msgid "Katihākārehi vipajjanti?"
msgstr ""

msgctxt "pli-tv-pvr21:4.5"
msgid "Imāni cattāri kammāni pañcahākārehi vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:4.6"
msgid "vatthuto vā ñattito vā anussāvanato vā sīmato vā parisato vā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc2
msgctxt "pli-tv-pvr21:5.1"
msgid "Kathaṃ vatthuto kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:5.2"
msgid "Sammukhākaraṇīyaṃ kammaṃ asammukhā karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.3"
msgid "paṭipucchākaraṇīyaṃ kammaṃ appaṭipucchā karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.4"
msgid "paṭiññāya karaṇīyaṃ kammaṃ appaṭiññāya karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.5"
msgid "sativinayārahassa amūḷhavinayaṃ deti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.6"
msgid "amūḷhavinayārahassa tassapāpiyasikākammaṃ karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.7"
msgid "tassapāpiyasikākammārahassa tajjanīyakammaṃ karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.8"
msgid "tajjanīyakammārahassa niyassakammaṃ karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.9"
msgid "niyassakammārahassa pabbājanīyakammaṃ karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.10"
msgid "pabbājanīyakammārahassa paṭisāraṇīyakammaṃ karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.11"
msgid "paṭisāraṇīyakammārahassa ukkhepanīyakammaṃ karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.12"
msgid "ukkhepanīyakammārahassa parivāsaṃ deti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.13"
msgid "parivāsārahaṃ mūlāya paṭikassati, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.14"
msgid "mūlāyapaṭikassanārahassa mānattaṃ deti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.15"
msgid "mānattārahaṃ abbheti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.16"
msgid "abbhānārahaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.17"
msgid "anuposathe uposathaṃ karoti, vatthuvipannaṃ adhammakammaṃ;"
msgstr ""

msgctxt "pli-tv-pvr21:5.18"
msgid "apavāraṇāya pavāreti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:5.19"
msgid "Evaṃ vatthuto kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc3
msgctxt "pli-tv-pvr21:6.1"
msgid "Kathaṃ ñattito kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:6.2"
msgid "Pañcahākārehi ñattito kammāni vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:6.3"
msgid "vatthuṃ na parāmasati, saṃghaṃ na parāmasati, puggalaṃ na parāmasati, ñattiṃ na parāmasati, pacchā vā ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-pvr21:6.4"
msgid "imehi pañcahākārehi ñattito kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc4, pts-vp-pli5.221
msgctxt "pli-tv-pvr21:7.1"
msgid "Kathaṃ anussāvanato kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:7.2"
msgid "Pañcahākārehi anussāvanato kammāni vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:7.3"
msgid "vatthuṃ na parāmasati, saṃghaṃ na parāmasati, puggalaṃ na parāmasati, sāvanaṃ hāpeti, akāle vā sāveti—"
msgstr ""

msgctxt "pli-tv-pvr21:7.4"
msgid "imehi pañcahākārehi anussāvanato kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-pvr21:8.1"
msgid "Kathaṃ sīmato kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:8.2"
msgid "Ekādasahi ākārehi sīmato kammāni vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:8.3"
msgid "atikhuddakaṃ sīmaṃ sammannati, atimahatiṃ sīmaṃ sammannati, khaṇḍanimittaṃ sīmaṃ sammannati, chāyānimittaṃ sīmaṃ sammannati, animittaṃ sīmaṃ sammannati, bahisīme ṭhito sīmaṃ sammannati, nadiyā sīmaṃ sammannati, samudde sīmaṃ sammannati, jātassare sīmaṃ sammannati, sīmāya sīmaṃ sambhindati, sīmāya sīmaṃ ajjhottharati—"
msgstr ""

msgctxt "pli-tv-pvr21:8.4"
msgid "imehi ekādasahi ākārehi sīmato kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-pvr21:9.1"
msgid "Kathaṃ parisato kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:9.2"
msgid "Dvādasahi ākārehi parisato kammāni vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:9.3"
msgid "catuvaggakaraṇe kamme yāvatikā bhikkhū kammapattā te anāgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti, catuvaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti, catuvaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā paṭikkosanti."
msgstr ""

msgctxt "pli-tv-pvr21:9.4"
msgid "Pañcavaggakaraṇe kamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr21:9.6"
msgid "dasavaggakaraṇe kamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr21:9.8"
msgid "vīsativaggakaraṇe kamme yāvatikā bhikkhū kammapattā te anāgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti, vīsativaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti, vīsativaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-pvr21:9.9"
msgid "imehi dvādasahi ākārehi parisato kammāni vipajjanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr21:10.1"
msgid "Catuvaggakaraṇe kamme cattāro bhikkhū pakatattā kammapattā, avasesā pakatattā chandārahā."
msgstr ""

msgctxt "pli-tv-pvr21:10.2"
msgid "Yassa saṃgho kammaṃ karoti so neva kammapatto nāpi chandāraho, api ca kammāraho."
msgstr ""

msgctxt "pli-tv-pvr21:10.3"
msgid "Pañcavaggakaraṇe kamme pañca bhikkhū pakatattā kammapattā, avasesā pakatattā chandārahā."
msgstr ""

msgctxt "pli-tv-pvr21:10.4"
msgid "Yassa saṃgho kammaṃ karoti so neva kammapatto nāpi chandāraho, api ca kammāraho."
msgstr ""

msgctxt "pli-tv-pvr21:10.5"
msgid "Dasavaggakaraṇe kamme dasa bhikkhū pakatattā kammapattā, avasesā pakatattā chandārahā."
msgstr ""

msgctxt "pli-tv-pvr21:10.6"
msgid "Yassa saṃgho kammaṃ karoti so neva kammapatto nāpi chandāraho, api ca kammāraho."
msgstr ""

msgctxt "pli-tv-pvr21:10.7"
msgid "Vīsativaggakaraṇe kamme vīsati bhikkhū pakatattā kammapattā, avasesā pakatattā chandārahā."
msgstr ""

msgctxt "pli-tv-pvr21:10.8"
msgid "Yassa saṃgho kammaṃ karoti so neva kammapatto nāpi chandāraho, api ca kammāraho."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli5.222
msgctxt "pli-tv-pvr21:11.1"
msgid "Cattāri kammāni—"
msgstr ""

msgctxt "pli-tv-pvr21:11.2"
msgid "apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:11.3"
msgid "Imāni cattāri kammāni katihākārehi vipajjanti?"
msgstr ""

msgctxt "pli-tv-pvr21:11.4"
msgid "Imāni cattāri kammāni pañcahākārehi vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:11.5"
msgid "vatthuto vā ñattito vā anussāvanato vā sīmato vā parisato vā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc9
msgctxt "pli-tv-pvr21:12.1"
msgid "Kathaṃ vatthuto kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:12.2"
msgid "Paṇḍakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.3"
msgid "Theyyasaṃvāsakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.4"
msgid "Titthiyapakkantakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.5"
msgid "Tiracchānagataṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.6"
msgid "Mātughātakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.7"
msgid "Pitughātakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.8"
msgid "Arahantaghātakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.9"
msgid "Bhikkhunidūsakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.10"
msgid "Saṃghabhedakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.11"
msgid "Lohituppādakaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.12"
msgid "Ubhatobyañjanaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.13"
msgid "Ūnavīsativassaṃ puggalaṃ upasampādeti, vatthuvipannaṃ adhammakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:12.14"
msgid "Evaṃ vatthuto kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10
msgctxt "pli-tv-pvr21:13.1"
msgid "Kathaṃ ñattito kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:13.2"
msgid "Pañcahākārehi ñattito kammāni vipajjanti."
msgstr ""

msgctxt "pli-tv-pvr21:13.3"
msgid "Vatthuṃ na parāmasati, saṃghaṃ na parāmasati, puggalaṃ na parāmasati, ñattiṃ na parāmasati, pacchā vā ñattiṃ ṭhapeti—"
msgstr ""

msgctxt "pli-tv-pvr21:13.4"
msgid "imehi pañcahākārehi ñattito kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc11
msgctxt "pli-tv-pvr21:14.1"
msgid "Kathaṃ anussāvanato kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:14.2"
msgid "Pañcahākārehi anussāvanato kammāni vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:14.3"
msgid "vatthuṃ na parāmasati, saṃghaṃ na parāmasati, puggalaṃ na parāmasati, sāvanaṃ hāpeti, akāle vā sāveti—"
msgstr ""

msgctxt "pli-tv-pvr21:14.4"
msgid "imehi pañcahākārehi anussāvanato kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc12
msgctxt "pli-tv-pvr21:15.1"
msgid "Kathaṃ sīmato kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:15.2"
msgid "Ekādasahi ākārehi sīmato kammāni vipajjanti."
msgstr ""

msgctxt "pli-tv-pvr21:15.3"
msgid "Atikhuddakaṃ sīmaṃ sammannati, atimahatiṃ sīmaṃ sammannati, khaṇḍanimittaṃ sīmaṃ sammannati, chāyānimittaṃ sīmaṃ sammannati, animittaṃ sīmaṃ sammannati, bahisīme ṭhito sīmaṃ sammannati, nadiyā sīmaṃ sammannati, samudde sīmaṃ sammannati, jātassare sīmaṃ sammannati, sīmāya sīmaṃ sambhindati, sīmāya sīmaṃ ajjhottharati—"
msgstr ""

msgctxt "pli-tv-pvr21:15.4"
msgid "imehi ekādasahi ākārehi sīmato kammāni vipajjanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc13
msgctxt "pli-tv-pvr21:16.1"
msgid "Kathaṃ parisato kammāni vipajjanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr21:16.2"
msgid "Dvādasahi ākārehi parisato kammāni vipajjanti—"
msgstr ""

msgctxt "pli-tv-pvr21:16.3"
msgid "catuvaggakaraṇe kamme yāvatikā bhikkhū kammapattā te anāgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti, catuvaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti."
msgstr ""

msgctxt "pli-tv-pvr21:16.4"
msgid "Catuvaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā paṭikkosanti."
msgstr ""

msgctxt "pli-tv-pvr21:16.5"
msgid "Pañcavaggakaraṇe kamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr21:16.7"
msgid "dasavaggakaraṇe kamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr21:16.9"
msgid "vīsativaggakaraṇe kamme yāvatikā bhikkhū kammapattā te anāgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti."
msgstr ""

msgctxt "pli-tv-pvr21:16.10"
msgid "Vīsativaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando anāhaṭo hoti, sammukhībhūtā paṭikkosanti."
msgstr ""

msgctxt "pli-tv-pvr21:16.11"
msgid "Vīsativaggakaraṇe kamme yāvatikā bhikkhū kammapattā te āgatā honti, chandārahānaṃ chando āhaṭo hoti, sammukhībhūtā paṭikkosanti—"
msgstr ""

msgctxt "pli-tv-pvr21:16.12"
msgid "imehi dvādasahi ākārehi parisato kammāni vipajjanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr21:17.1"
msgid "Apalokanakammaṃ kati ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:17.2"
msgid "Ñattikammaṃ kati ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:17.3"
msgid "Ñattidutiyakammaṃ kati ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:17.4"
msgid "Ñatticatutthakammaṃ kati ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:17.5"
msgid "Apalokanakammaṃ pañca ṭhānāni gacchati."
msgstr ""

msgctxt "pli-tv-pvr21:17.6"
msgid "Ñattikammaṃ nava ṭhānāni gacchati."
msgstr ""

msgctxt "pli-tv-pvr21:17.7"
msgid "Ñattidutiyakammaṃ satta ṭhānāni gacchati."
msgstr ""

msgctxt "pli-tv-pvr21:17.8"
msgid "Ñatticatutthakammaṃ satta ṭhānāni gacchati."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr21:18.1"
msgid "Apalokanakammaṃ katamāni pañca ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:18.2"
msgid "Osāraṇaṃ, nissāraṇaṃ, bhaṇḍukammaṃ, brahmadaṇḍaṃ, kammalakkhaṇaññeva pañcamaṃ—"
msgstr ""

msgctxt "pli-tv-pvr21:18.3"
msgid "apalokanakammaṃ imāni pañca ṭhānāni gacchati."
msgstr ""

msgctxt "pli-tv-pvr21:18.4"
msgid "Ñattikammaṃ katamāni nava ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:18.5"
msgid "Osāraṇaṃ, nissāraṇaṃ, uposathaṃ, pavāraṇaṃ, sammutiṃ, dānaṃ, paṭiggahaṃ, paccukkaḍḍhanaṃ, kammalakkhaṇaññeva navamaṃ—"
msgstr ""

msgctxt "pli-tv-pvr21:18.6"
msgid "ñattikammaṃ imāni nava ṭhānāni gacchati."
msgstr ""

msgctxt "pli-tv-pvr21:18.7"
msgid "Ñattidutiyakammaṃ katamāni satta ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:18.8"
msgid "Osāraṇaṃ, nissāraṇaṃ, sammutiṃ, dānaṃ, uddharaṇaṃ, desanaṃ, kammalakkhaṇaññeva sattamaṃ—"
msgstr ""

msgctxt "pli-tv-pvr21:18.9"
msgid "ñattidutiyakammaṃ imāni satta ṭhānāni gacchati."
msgstr ""

msgctxt "pli-tv-pvr21:18.10"
msgid "Ñatticatutthakammaṃ katamāni satta ṭhānāni gacchati?"
msgstr ""

msgctxt "pli-tv-pvr21:18.11"
msgid "Osāraṇaṃ, nissāraṇaṃ, sammutiṃ, dānaṃ, niggahaṃ, samanubhāsanaṃ, kammalakkhaṇaññeva sattamaṃ—"
msgstr ""

msgctxt "pli-tv-pvr21:18.12"
msgid "ñatticatutthakammaṃ imāni satta ṭhānāni gacchati."
msgstr ""

