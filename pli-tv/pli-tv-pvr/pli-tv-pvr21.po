msgctxt "pli-tv-pvr21:50.5"
msgid "Sace āhaṃsu—"
msgstr ""

msgctxt "pli-tv-pvr21:50.6"
msgid "“ubhopi tuṭṭhā bhavissāmā”ti, saṃghena taṃ adhikaraṇaṃ sampaṭicchitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:50.7"
msgid "Sace alajjussannā hoti, parisā ubbāhikāya vūpasametabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr21:50.8"
msgid "Sace bālussannā hoti, parisā vinayadharo pariyesitabbo yena dhammena yena vinayena yena satthusāsanena taṃ adhikaraṇaṃ vūpasammati."
msgstr ""

msgctxt "pli-tv-pvr21:50.9"
msgid "Tathā taṃ adhikaraṇaṃ vūpasametabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc44
