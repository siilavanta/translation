msgid "Sukkavissaṭṭhīti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:56.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc50
msgctxt "pli-tv-pvr21:57.1"
msgid "Kāyasaṃsaggoti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:57.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc51
msgctxt "pli-tv-pvr21:58.1"
msgid "Duṭṭhullavācāti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:58.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc52
msgctxt "pli-tv-pvr21:59.1"
msgid "Attakāmanti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:59.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc53
msgctxt "pli-tv-pvr21:60.1"
msgid "Sañcarittanti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:60.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc54
msgctxt "pli-tv-pvr21:61.1"
msgid "Saññācikāya kuṭiṃ kārāpananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:61.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-pvr21:62.1"
msgid "Mahallakaṃ vihāraṃ kārāpananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:62.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-pvr21:63.1"
msgid "Bhikkhuṃ amūlakena pārājikena dhammena anuddhaṃsananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:63.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc57, pts-vp-pli5.225
msgctxt "pli-tv-pvr21:64.1"
msgid "Bhikkhuṃ aññabhāgiyassa adhikaraṇassa kiñci desaṃ lesamattaṃ upādāya pārājikena dhammena anuddhaṃsananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:64.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-pvr21:65.1"
msgid "Saṃghabhedakassa bhikkhuno yāvatatiyaṃ samanubhāsanāya na paṭinissajjananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:65.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr21:66.1"
msgid "Bhedakānuvattakānaṃ bhikkhūnaṃ yāvatatiyaṃ samanubhāsanāya na paṭinissajjananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:66.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-pvr21:67.1"
msgid "Dubbacassa bhikkhuno yāvatatiyaṃ samanubhāsanāya na paṭinissajjananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:67.2"
msgid "saṃghādisesoti nāmañceva āpatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc61
msgctxt "pli-tv-pvr21:68.1"
msgid "Kuladūsakassa bhikkhuno yāvatatiyaṃ samanubhāsanāya na paṭinissajjananti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:68.2"
msgid "saṃghādisesoti nāmañceva āpatti ca …"
msgstr ""

msgctxt "pli-tv-pvr21:68.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-pvr21:69.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karaṇanti vatthu ceva gottañca—"
msgstr ""

msgctxt "pli-tv-pvr21:69.2"
msgid "dukkaṭanti nāmañceva āpatti cāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc63
msgctxt "pli-tv-pvr21:70.1"
msgid "Navasaṅgahavaggo niṭṭhito pañcamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc64
msgctxt "pli-tv-pvr21:71.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc65
msgctxt "pli-tv-pvr21:72.1"
msgid "Apalokanaṃ ñatti ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:72.2"
msgid "dutiyaṃ catutthena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:72.3"
msgid "Vatthu ñatti anussāvanaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:72.4"
msgid "sīmā parisameva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr21:73.1"
msgid "Sammukhā paṭipucchā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:73.2"
msgid "paṭiññā vinayāraho;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:73.3"
msgid "Vatthu saṃghapuggalañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:73.4"
msgid "ñattiṃ na pacchā ñatti ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr21:74.1"
msgid "Vatthuṃ saṃghapuggalañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:74.2"
msgid "sāvanaṃ akālena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:74.3"
msgid "Atikhuddakā mahantā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:74.4"
msgid "khaṇḍacchāyā nimittakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-pvr21:75.1"
msgid "Bahinadī samudde ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:75.2"
msgid "jātassare ca bhindati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:75.3"
msgid "Ajjhottharati sīmāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:75.4"
msgid "catu pañca ca vaggikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc69
msgctxt "pli-tv-pvr21:76.1"
msgid "Dasa vīsativaggā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:76.2"
msgid "anāhaṭā ca āhaṭā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:76.3"
msgid "Kammapattā chandārahā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:76.4"
msgid "kammārahā ca puggalā."
msgstr ""

#. HTML: </p><p>
#. REF: sc70
msgctxt "pli-tv-pvr21:77.1"
msgid "Apalokanaṃ pañcaṭṭhānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:77.2"
msgid "ñatti ca navaṭhānikā;"
msgstr ""

#. HTML: <br>
#. VAR: sattaṭṭhānaṃ → satta ṭhānaṃ (bj)
msgctxt "pli-tv-pvr21:77.3"
msgid "Ñatti dutiyaṃ sattaṭṭhānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:77.4"
msgid "catutthā sattaṭhānikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-pvr21:78.1"
msgid "Suṭṭhu phāsu ca dummaṅku,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:78.2"
msgid "pesalā cāpi āsavā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:78.3"
msgid "Veravajjabhayañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:78.4"
msgid "akusalaṃ gihīnañca."
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-pvr21:79.1"
msgid "Pāpicchā appasannānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:79.2"
msgid "pasannā dhammaṭṭhapanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:79.3"
msgid "Vinayānuggahā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:79.4"
msgid "pātimokkhuddesena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-pvr21:80.1"
msgid "Pātimokkhañca ṭhapanā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:80.2"
msgid "Pavāraṇañca ṭhapanaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:80.3"
msgid "Tajjanīyā niyassañca,"
msgstr ""

#. HTML: <br>
#. VAR: Pabbājanīya paṭisāraṇī → pabbājapaṭisāraṇī (bj, s1-3)
msgctxt "pli-tv-pvr21:80.4"
msgid "Pabbājanīya paṭisāraṇī."
msgstr ""

#. HTML: </p><p>
#. REF: sc74
msgctxt "pli-tv-pvr21:81.1"
msgid "Ukkhepana parivāsaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:81.2"
msgid "mūlamānattaabbhānaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:81.3"
msgid "Osāraṇaṃ nissāraṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:81.4"
msgid "tatheva upasampadā."
msgstr ""

#. HTML: </p><p>
#. REF: sc75
msgctxt "pli-tv-pvr21:82.1"
msgid "Apalokanañatti ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:82.2"
msgid "dutiyañca catutthakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:82.3"
msgid "Apaññattenupaññattaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:82.4"
msgid "sammukhāvinayo sati."
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-pvr21:83.1"
msgid "Amūḷhapaṭiyebhuyya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:83.2"
msgid "pāpiya tiṇavatthārakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:83.3"
msgid "Vatthu vipatti āpatti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:83.4"
msgid "nidānaṃ puggalena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc77, pts-vp-pli5.226
msgctxt "pli-tv-pvr21:84.1"
msgid "Khandhā ceva samuṭṭhānā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:84.2"
msgid "adhikaraṇameva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:84.3"
msgid "Samathā saṅgahā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:84.4"
msgid "nāmaāpattikā tathāti."
msgstr ""

#. HTML: </p></blockquote><p class="endsection">
#. REF: sc78
msgctxt "pli-tv-pvr21:85.1"
msgid "Parivāro niṭṭhito."
msgstr ""

#. HTML: </p><p class="endbook">
#. REF: sc79
msgctxt "pli-tv-pvr21:86.1"
msgid "Parivārapāḷi niṭṭhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc80
msgctxt "pli-tv-pvr21:87.1"
msgid "Pariyosānagāthā"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><div class="var" title="etthantare gāthā sī potthakesu (syā1-3,pā1)"><p>
#. REF: sc81
#. VAR: ca → va (s1-3, pts1)
msgctxt "pli-tv-pvr21:88.1"
msgid "Bubbācariyamaggañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:88.2"
msgid "pucchitvā ca tahiṃ tahiṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:88.3"
msgid "Dīpanāmo mahāpañño,"
msgstr ""

#. HTML: <br>
#. VAR: sutadhāriṃ → sutadharo (s1-3, pts1)
msgctxt "pli-tv-pvr21:88.4"
msgid "sutadhāriṃ vicakkhaṇo."
msgstr ""

#. HTML: </p><p>
#. REF: sc82
msgctxt "pli-tv-pvr21:89.1"
msgid "Imaṃ vitthārasaṅkhepaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:89.2"
msgid "sajjhāmaggena majjhime;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:89.3"
msgid "Cintayitvā likhāpesi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:89.4"
msgid "sissakānaṃ sukhāvahaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-pvr21:90.1"
msgid "Parivāranti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:90.2"
msgid "sabbaṃ vatthuṃ salakkhaṇaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:90.3"
msgid "Atthaṃ atthena saddhamme,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:90.4"
msgid "dhammaṃ dhammena paññatte."
msgstr ""

#. HTML: </p><p>
#. REF: sc84
msgctxt "pli-tv-pvr21:91.1"
msgid "Sāsanaṃ parivāresi,"
msgstr ""

#. HTML: <br>
#. VAR: jambudīpaṃva → jambūdīpaṃva (s1-3)
msgctxt "pli-tv-pvr21:91.2"
msgid "jambudīpaṃva sāgaro;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:91.3"
msgid "Parivāraṃ ajānanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:91.4"
msgid "kuto dhammavinicchayaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc85
msgctxt "pli-tv-pvr21:92.1"
msgid "Vipatti vatthu paññatti,"
msgstr ""

#. HTML: <br>
#. VAR: anupaññatti → anuppaññatti (s1-3)
msgctxt "pli-tv-pvr21:92.2"
msgid "anupaññatti puggalo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:92.3"
msgid "Ekato ubhato ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:92.4"
msgid "lokapaṇṇatti vajjato."
msgstr ""

#. HTML: </p><p>
#. REF: sc86
msgctxt "pli-tv-pvr21:93.1"
msgid "Yassa jāyati vimati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:93.2"
msgid "parivārena chijjati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:93.3"
msgid "Cakkavatti mahāsene,"
msgstr ""

#. HTML: <br>
#. VAR: kesari → kesarī (s1-3, pts1)
msgctxt "pli-tv-pvr21:93.4"
msgid "migamajjheva kesari."
msgstr ""

#. HTML: </p><p>
#. REF: sc87
msgctxt "pli-tv-pvr21:94.1"
msgid "Ravi raṃsiparikkiṇṇo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:94.2"
msgid "cando tāragaṇe yathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:94.3"
msgid "Brahmā brahmaparisāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:94.4"
msgid "gaṇamajjheva nāyako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:94.5"
msgid "Evaṃ saddhammavinayo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr21:94.6"
msgid "parivārena sobhatīti."
msgstr ""

