#. HTML: </p><h1>
msgctxt "pli-tv-pvr1.13:4.1"
msgid "5. Samuṭṭhānavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr1.13:5.1"
msgid "Methunaṃ dhammaṃ paṭisevanapaccayā āpattiyo channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr1.13:5.2"
msgid "Methunaṃ dhammaṃ paṭisevanapaccayā āpattiyo channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr1.13:5.3"
msgid "kāyato ca cittato ca samuṭṭhanti, na vācato …"
msgstr ""

msgctxt "pli-tv-pvr1.13:5.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.13:5.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr1.13:6.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karaṇapaccayā āpatti channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.13:6.2"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karaṇapaccayā āpatti channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.13:6.3"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato …"
msgstr ""

msgctxt "pli-tv-pvr1.13:6.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.13:6.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr1.13:7.1"
msgid "Samuṭṭhānavāro niṭṭhito pañcamo."
msgstr ""
