#. HTML: <section class="sutta" id="pli-tv-pvr9"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr9:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr9:2.1"
msgid "9. Atthavasapakaraṇa"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr9:3.1"
msgid "Dasa atthavase paṭicca tathāgatena sāvakānaṃ sikkhāpadaṃ paññattaṃ—"
msgstr ""

msgctxt "pli-tv-pvr9:3.2"
msgid "saṃghasuṭṭhutāya, saṃghaphāsutāya, dummaṅkūnaṃ puggalānaṃ niggahāya, pesalānaṃ bhikkhūnaṃ phāsuvihārāya, diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya, samparāyikānaṃ āsavānaṃ paṭighātāya, appasannānaṃ pasādāya, pasannānaṃ bhiyyobhāvāya, saddhammaṭṭhitiyā vinayānuggahāya."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr9:4.1"
msgid "Yaṃ saṃghasuṭṭhu taṃ saṃghaphāsu."
msgstr ""

msgctxt "pli-tv-pvr9:4.2"
msgid "Yaṃ saṃghaphāsu taṃ dummaṅkūnaṃ puggalānaṃ niggahāya."
msgstr ""

msgctxt "pli-tv-pvr9:4.3"
msgid "Yaṃ dummaṅkūnaṃ puggalānaṃ niggahāya taṃ pesalānaṃ bhikkhūnaṃ phāsuvihārāya."
msgstr ""

msgctxt "pli-tv-pvr9:4.4"
msgid "Yaṃ pesalānaṃ bhikkhūnaṃ phāsuvihārāya taṃ diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya."
msgstr ""

msgctxt "pli-tv-pvr9:4.5"
msgid "Yaṃ diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya taṃ samparāyikānaṃ āsavānaṃ paṭighātāya."
msgstr ""

msgctxt "pli-tv-pvr9:4.6"
msgid "Yaṃ samparāyikānaṃ āsavānaṃ paṭighātāya taṃ appasannānaṃ pasādāya."
msgstr ""

msgctxt "pli-tv-pvr9:4.7"
msgid "Yaṃ appasannānaṃ pasādāya taṃ pasannānaṃ bhiyyobhāvāya."
msgstr ""

msgctxt "pli-tv-pvr9:4.8"
msgid "Yaṃ pasannānaṃ bhiyyobhāvāya taṃ saddhammaṭṭhitiyā."
msgstr ""

msgctxt "pli-tv-pvr9:4.9"
msgid "Yaṃ saddhammaṭṭhitiyā taṃ vinayānuggahāya."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr9:5.1"
msgid "Yaṃ saṃghasuṭṭhu taṃ saṃghaphāsu."
msgstr ""

msgctxt "pli-tv-pvr9:5.2"
msgid "Yaṃ saṃghasuṭṭhu taṃ dummaṅkūnaṃ puggalānaṃ niggahāya."
msgstr ""

msgctxt "pli-tv-pvr9:5.3"
msgid "Yaṃ saṃghasuṭṭhu taṃ pesalānaṃ bhikkhūnaṃ phāsuvihārāya."
msgstr ""

msgctxt "pli-tv-pvr9:5.4"
msgid "Yaṃ saṃghasuṭṭhu taṃ diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya."
msgstr ""

msgctxt "pli-tv-pvr9:5.5"
msgid "Yaṃ saṃghasuṭṭhu taṃ samparāyikānaṃ āsavānaṃ paṭighātāya."
msgstr ""

msgctxt "pli-tv-pvr9:5.6"
msgid "Yaṃ saṃghasuṭṭhu taṃ appasannānaṃ pasādāya."
msgstr ""

msgctxt "pli-tv-pvr9:5.7"
msgid "Yaṃ saṃghasuṭṭhu taṃ pasannānaṃ bhiyyobhāvāya."
msgstr ""

msgctxt "pli-tv-pvr9:5.8"
msgid "Yaṃ saṃghasuṭṭhu taṃ saddhammaṭṭhitiyā."
msgstr ""

msgctxt "pli-tv-pvr9:5.9"
msgid "Yaṃ saṃghasuṭṭhu taṃ vinayānuggahāya."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr9:6.1"
msgid "Yaṃ saṃghaphāsu taṃ dummaṅkūnaṃ puggalānaṃ niggahāya."
msgstr ""

msgctxt "pli-tv-pvr9:6.2"
msgid "Yaṃ saṃghaphāsu taṃ pesalānaṃ bhikkhūnaṃ phāsuvihārāya."
msgstr ""

msgctxt "pli-tv-pvr9:6.3"
msgid "Yaṃ saṃghaphāsu taṃ diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya."
msgstr ""

msgctxt "pli-tv-pvr9:6.4"
msgid "Yaṃ saṃghaphāsu taṃ samparāyikānaṃ āsavānaṃ paṭighātāya."
msgstr ""

msgctxt "pli-tv-pvr9:6.5"
msgid "Yaṃ saṃghaphāsu taṃ appasannānaṃ pasādāya."
msgstr ""

msgctxt "pli-tv-pvr9:6.6"
msgid "Yaṃ saṃghaphāsu taṃ pasannānaṃ bhiyyobhāvāya."
msgstr ""

msgctxt "pli-tv-pvr9:6.7"
msgid "Yaṃ saṃghaphāsu taṃ saddhammaṭṭhitiyā."
msgstr ""

msgctxt "pli-tv-pvr9:6.8"
msgid "Yaṃ saṃghaphāsu taṃ vinayānuggahāya."
msgstr ""

msgctxt "pli-tv-pvr9:6.9"
msgid "Yaṃ saṃghaphāsu taṃ saṃghasuṭṭhu."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr9:7.1"
msgid "Yaṃ dummaṅkūnaṃ puggalānaṃ niggahāya … pe …"
msgstr ""

msgctxt "pli-tv-pvr9:7.3"
msgid "yaṃ pesalānaṃ bhikkhūnaṃ phāsuvihārāya …"
msgstr ""

msgctxt "pli-tv-pvr9:7.4"
msgid "yaṃ diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya …"
msgstr ""

msgctxt "pli-tv-pvr9:7.5"
msgid "yaṃ samparāyikānaṃ āsavānaṃ paṭighātāya …"
msgstr ""

msgctxt "pli-tv-pvr9:7.6"
msgid "yaṃ appasannānaṃ pasādāya …"
msgstr ""

msgctxt "pli-tv-pvr9:7.7"
msgid "yaṃ pasannānaṃ bhiyyobhāvāya …"
msgstr ""

msgctxt "pli-tv-pvr9:7.8"
msgid "yaṃ saddhammaṭṭhitiyā …"
msgstr ""

msgctxt "pli-tv-pvr9:7.9"
msgid "yaṃ vinayānuggahāya taṃ saṃghasuṭṭhu."
msgstr ""

msgctxt "pli-tv-pvr9:7.10"
msgid "Yaṃ vinayānuggahāya taṃ saṃghaphāsu."
msgstr ""

msgctxt "pli-tv-pvr9:7.11"
msgid "Yaṃ vinayānuggahāya taṃ dummaṅkūnaṃ puggalānaṃ niggahāya."
msgstr ""

msgctxt "pli-tv-pvr9:7.12"
msgid "Yaṃ vinayānuggahāya taṃ pesalānaṃ bhikkhūnaṃ phāsuvihārāya."
msgstr ""

msgctxt "pli-tv-pvr9:7.13"
msgid "Yaṃ vinayānuggahāya taṃ diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya."
msgstr ""

msgctxt "pli-tv-pvr9:7.14"
msgid "Yaṃ vinayānuggahāya taṃ samparāyikānaṃ āsavānaṃ paṭighātāya."
msgstr ""

msgctxt "pli-tv-pvr9:7.15"
msgid "Yaṃ vinayānuggahāya taṃ appasannānaṃ pasādāya."
msgstr ""

msgctxt "pli-tv-pvr9:7.16"
msgid "Yaṃ vinayānuggahāya taṃ pasannānaṃ bhiyyobhāvāya."
msgstr ""

msgctxt "pli-tv-pvr9:7.17"
msgid "Yaṃ vinayānuggahāya taṃ saddhammaṭṭhitiyāti."
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc6
msgctxt "pli-tv-pvr9:8.1"
msgid "Atthasataṃ dhammasataṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:8.2"
msgid "dve ca niruttisatāni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:8.3"
msgid "Cattāri ñāṇasatāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:8.4"
msgid "atthavase pakaraṇeti."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc7
msgctxt "pli-tv-pvr9:9.1"
msgid "Atthavasapakaraṇaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:9.2"
msgid "Mahāvaggo niṭṭhito."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc8
msgctxt "pli-tv-pvr9:10.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc9
msgctxt "pli-tv-pvr9:11.1"
msgid "Paṭhamaṃ aṭṭhapucchāyaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:11.2"
msgid "paccayesu punaṭṭha ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:11.3"
msgid "Bhikkhūnaṃ soḷasa ete,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:11.4"
msgid "bhikkhunīnañca soḷasa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr9:12.1"
msgid "Peyyālaantarā bhedā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:12.2"
msgid "ekuttarikameva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:12.3"
msgid "Pavāraṇatthavasikā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr9:12.4"
msgid "mahāvaggassa saṅgahoti."
msgstr ""

#. HTML: </p></blockquote><p class="endsection">
#. REF: sc11
msgctxt "pli-tv-pvr9:13.1"
msgid "Atthavasapakaraṇaṃ niṭṭhitaṃ."
msgstr ""

