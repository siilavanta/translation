#. HTML: <section class="sutta" id="pli-tv-pvr1.8"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr1.8:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.8:2.1"
msgid "Bhikkhuvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.8:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr1.8:4.1"
msgid "8. Samuccayavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli5.48
msgctxt "pli-tv-pvr1.8:5.1"
msgid "Methunaṃ dhammaṃ paṭisevanto kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.8:5.2"
msgid "Methunaṃ dhammaṃ paṭisevanto tisso āpattiyo āpajjati."
msgstr ""

#. VAR: khāyite → khayite (bj, pts1)
#. VAR: Akkhāyite → akkhayite (bj, pts1)
msgctxt "pli-tv-pvr1.8:5.3"
msgid "Akkhāyite sarīre methunaṃ dhammaṃ paṭisevati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr1.8:5.4"
msgid "yebhuyyena khāyite sarīre methunaṃ dhammaṃ paṭisevati, āpatti thullaccayassa;"
msgstr ""

#. VAR: vaṭṭakate → vattakate (bj, pts1) | vivaṭakate (s1-3)
msgctxt "pli-tv-pvr1.8:5.5"
msgid "vaṭṭakate mukhe acchupantaṃ aṅgajātaṃ paveseti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr1.8:5.6"
msgid "methunaṃ dhammaṃ paṭisevanto imā tisso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr1.8:6.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti, sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā, channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti, catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ, sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr1.8:6.2"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr1.8:6.3"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.8:6.4"
msgid "Sattannaṃ āpattikkhandhānaṃ tīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr1.8:6.5"
msgid "siyā pārājikāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr1.8:6.6"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr1.8:6.7"
msgid "kāyato ca cittato ca samuṭṭhanti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr1.8:6.8"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.8:6.9"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr1.8:6.10"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca …"
msgstr ""

msgctxt "pli-tv-pvr1.8:6.11"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.8:6.12"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr1.8:7.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karonto kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.8:7.2"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karonto ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.8:7.3"
msgid "Dukkaṭaṃ—"
msgstr ""

msgctxt "pli-tv-pvr1.8:7.4"
msgid "anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karonto imaṃ ekaṃ āpattiṃ āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr1.8:8.1"
msgid "Sā āpatti catunnaṃ vipattīnaṃ kati vipattiyo bhajati, sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā, channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhāti, catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ, sattannaṃ, samathānaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr1.8:8.2"
msgid "Sā āpatti catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajati—"
msgstr ""

msgctxt "pli-tv-pvr1.8:8.3"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.8:8.4"
msgid "Sattannaṃ āpattikkhandhānaṃ ekena āpattikkhandhena saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr1.8:8.5"
msgid "dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr1.8:8.6"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.8:8.7"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr1.8:8.8"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.8:8.9"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr1.8:8.10"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr1.8:8.11"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-pvr1.8:9.1"
msgid "Samuccayavāro niṭṭhito aṭṭhamo."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.8:9.2"
msgid "Ime aṭṭha vārā sajjhāyamaggena likhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc6
msgctxt "pli-tv-pvr1.8:10.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc7
msgctxt "pli-tv-pvr1.8:11.1"
msgid "Katthapaññatti kati ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.8:11.2"
msgid "Vipattisaṅgahena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.8:11.3"
msgid "Samuṭṭhānādhikaraṇā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.8:11.4"
msgid "Samatho samuccayena cāti."
msgstr ""
