#. HTML: <section class="sutta" id="pli-tv-pvr2.16"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr2.16:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.16:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.16:3.1"
msgid "Dutiyabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.16:4.1"
msgid "8. Samuccayavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.16:5.1"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.16:5.2"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.16:5.3"
msgid "Avassutā bhikkhunī avassutassa purisapuggalassa adhakkhakaṃ ubbhajāṇumaṇḍalaṃ gahaṇaṃ sādiyati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr2.16:5.4"
msgid "bhikkhu kāyena kāyaṃ āmasati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr2.16:5.5"
msgid "kāyena kāyapaṭibaddhaṃ āmasati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.16:5.6"
msgid "kāyapaṭibaddhena kāyapaṭibaddhaṃ āmasati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.16:5.7"
msgid "aṅgulipatodake pācittiyaṃ—"
msgstr ""

msgctxt "pli-tv-pvr2.16:5.8"
msgid "kāyasaṃsaggaṃ sādiyanapaccayā imā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.16:5.9"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.16:6.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.2"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.3"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.4"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.5"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.6"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.7"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.16:6.8"
msgid "Sattannaṃ āpattikkhandhānaṃ pañcahi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.9"
msgid "siyā pārājikāpattikkhandhena, siyā saṃghādisesāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr2.16:6.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.11"
msgid "kāyato ca cittato ca samuṭṭhanti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr2.16:6.12"
msgid "Catunnaṃ adhikaraṇānaṃ āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.16:6.13"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.14"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca …"
msgstr ""

msgctxt "pli-tv-pvr2.16:6.15"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.16:6.16"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr2.16:7.1"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.16:7.2"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.16:7.3"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.16:7.4"
msgid "ajjhohāre ajjhohāre āpatti pāṭidesanīyassa—"
msgstr ""

msgctxt "pli-tv-pvr2.16:7.5"
msgid "dadhiṃ viññāpetvā bhuñjanapaccayā imā dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.16:7.6"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr2.16:8.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.2"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.3"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.4"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.5"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.6"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.7"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.16:8.8"
msgid "Sattannaṃ āpattikkhandhānaṃ dvīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.9"
msgid "siyā pāṭidesanīyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr2.16:8.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ catūhi samuṭṭhānehi samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.11"
msgid "siyā kāyato samuṭṭhanti, na vācato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.12"
msgid "siyā kāyato ca vācato ca samuṭṭhanti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.13"
msgid "siyā kāyato ca cittato ca samuṭṭhanti na vācato;"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.14"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr2.16:8.15"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.16:8.16"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr2.16:8.17"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena cāti."
msgstr ""

msgctxt "pli-tv-pvr2.16:8.18"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-pvr2.16:9.1"
msgid "Samuccayavāro niṭṭhito aṭṭhamo."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.16:9.2"
msgid "Aṭṭha paccayavārā niṭṭhitā."
msgstr ""

#. VAR: () → imassānantaraṃ bj potthake evampi
msgctxt "pli-tv-pvr2.16:9.3"
msgid "()"
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc6
#. VAR: Bhikkhunivibhaṅge → bhikkhunīvibhaṅge (bj, s1-3, pts1)
msgctxt "pli-tv-pvr2.16:10.1"
msgid "Bhikkhunivibhaṅge soḷasa mahāvārā niṭṭhitā."
msgstr ""
