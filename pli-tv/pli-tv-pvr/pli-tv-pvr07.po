msgstr ""

msgctxt "pli-tv-pvr7:4.62"
msgid "Asamānasaṃvāsako puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.63"
msgid "Ṭhapanaṃ jānitabbanti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc2
msgctxt "pli-tv-pvr7:5.1"
msgid "Ekakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc3
msgctxt "pli-tv-pvr7:6.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc4
msgctxt "pli-tv-pvr7:7.1"
msgid "Karā āpatti lahukā,"
msgstr ""

#. HTML: <br>
#. VAR: ca → ceva (bj)
msgctxt "pli-tv-pvr7:7.2"
msgid "sāvasesā ca duṭṭhullā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:7.3"
msgid "Paṭikammadesanā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:7.4"
msgid "antarā vajjakiriyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr7:8.1"
msgid "Kiriyākiriyaṃ pubbā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:8.2"
msgid "Antarā gaṇanūpagā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:8.3"
msgid "Paññatti anānuppanna,"
msgstr ""

#. HTML: <br>
#. VAR: Sabbasādhāraṇā ca ekato → padesasādhāraṇā ca ekato (bj) | sādhāraṇā ca ekato (s1-3) | sabbā sādhāraṇā ca ekato (pts1)
msgctxt "pli-tv-pvr7:8.4"
msgid "Sabbasādhāraṇā ca ekato."
msgstr ""

#. HTML: </p><p>
#. REF: sc6, pts-vp-pli5.116
msgctxt "pli-tv-pvr7:9.1"
msgid "Thullagihiniyatā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:9.2"
msgid "ādi adhiccacodako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:9.3"
msgid "Adhammadhammaniyato,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:9.4"
msgid "abhabbokkhittanāsitā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:9.5"
msgid "Samānaṃ ṭhapanañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:9.6"
msgid "uddānaṃ ekake idanti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:10.1"
msgid "2. Dukavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc7
msgctxt "pli-tv-pvr7:11.1"
msgid "Atthāpatti saññā vimokkhā, atthāpatti no saññāvimokkhā."
msgstr ""

msgctxt "pli-tv-pvr7:11.2"
msgid "Atthāpatti laddhasamāpattikassa, atthāpatti na laddhasamāpattikassa."
msgstr ""

msgctxt "pli-tv-pvr7:11.3"
msgid "Atthāpatti saddhammapaṭisaññuttā, atthāpatti asaddhammapaṭisaññuttā."
msgstr ""

msgctxt "pli-tv-pvr7:11.4"
msgid "Atthāpatti saparikkhārapaṭisaññuttā, atthāpatti paraparikkhārapaṭisaññuttā."
msgstr ""

msgctxt "pli-tv-pvr7:11.5"
msgid "Atthāpatti sapuggalapaṭisaññuttā, atthāpatti parapuggalapaṭisaññuttā."
msgstr ""

msgctxt "pli-tv-pvr7:11.6"
msgid "Atthi saccaṃ bhaṇanto garukaṃ āpattiṃ āpajjati, musā bhaṇanto lahukaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:11.7"
msgid "Atthi musā bhaṇanto garukaṃ āpattiṃ āpajjati, saccaṃ bhaṇanto lahukaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:11.8"
msgid "Atthāpatti bhūmigato āpajjati, no vehāsagato."
msgstr ""

msgctxt "pli-tv-pvr7:11.9"
msgid "Atthāpatti vehāsagato āpajjati, no bhūmigato."
msgstr ""

msgctxt "pli-tv-pvr7:11.10"
msgid "Atthāpatti nikkhamanto āpajjati, no pavisanto."
msgstr ""

msgctxt "pli-tv-pvr7:11.11"
msgid "Atthāpatti pavisanto āpajjati, no nikkhamanto."
msgstr ""

msgctxt "pli-tv-pvr7:11.12"
msgid "Atthāpatti ādiyanto āpajjati, atthāpatti anādiyanto āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.13"
msgid "Atthāpatti samādiyanto āpajjati, atthāpatti na samādiyanto āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.14"
msgid "Atthāpatti karonto āpajjati, atthāpatti na karonto āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.15"
msgid "Atthāpatti dento āpajjati, atthāpatti na dento āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.16"
msgid "Atthāpatti desento āpajjati, atthāpatti na desento āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.17"
msgid "Atthāpatti paṭiggaṇhanto āpajjati, atthāpatti na paṭiggaṇhanto āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.18"
msgid "Atthāpatti paribhogena āpajjati, atthāpatti na paribhogena āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.19"
msgid "Atthāpatti rattiṃ āpajjati, no divā."
msgstr ""

msgctxt "pli-tv-pvr7:11.20"
msgid "Atthāpatti divā āpajjati, no rattiṃ."
msgstr ""

msgctxt "pli-tv-pvr7:11.21"
msgid "Atthāpatti aruṇugge āpajjati, atthāpatti na aruṇugge āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.22"
msgid "Atthāpatti chindanto āpajjati, atthāpatti na chindanto āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.23"
msgid "Atthāpatti chādento āpajjati, atthāpatti na chādento āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:11.24"
msgid "Atthāpatti dhārento āpajjati, atthāpatti na dhārento āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli5.117
msgctxt "pli-tv-pvr7:12.1"
msgid "Dve uposathā—"
msgstr ""

msgctxt "pli-tv-pvr7:12.2"
msgid "cātuddasiko ca pannarasiko ca."
msgstr ""

msgctxt "pli-tv-pvr7:12.3"
msgid "Dve pavāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr7:12.4"
msgid "cātuddasikā ca pannarasikā ca."
msgstr ""

msgctxt "pli-tv-pvr7:12.5"
msgid "Dve kammāni—"
msgstr ""

msgctxt "pli-tv-pvr7:12.6"
msgid "apalokanakammaṃ, ñattikammaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:12.7"
msgid "Aparānipi dve kammāni—"
msgstr ""

msgctxt "pli-tv-pvr7:12.8"
msgid "ñattidutiyakammaṃ, ñatticatutthakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:12.9"
msgid "Dve kammavatthūni—"
msgstr ""

msgctxt "pli-tv-pvr7:12.10"
msgid "apalokanakammassa vatthu, ñattikammassa vatthu."
msgstr ""

msgctxt "pli-tv-pvr7:12.11"
msgid "Aparānipi dve kammavatthūni—"
msgstr ""

msgctxt "pli-tv-pvr7:12.12"
msgid "ñattidutiyakammassa vatthu, ñatticatutthakammassa vatthu."
msgstr ""

msgctxt "pli-tv-pvr7:12.13"
msgid "Dve kammadosā—"
msgstr ""

msgctxt "pli-tv-pvr7:12.14"
msgid "apalokanakammassa doso, ñattikammassa doso."
msgstr ""

msgctxt "pli-tv-pvr7:12.15"
msgid "Aparepi dve kammadosā—"
msgstr ""

msgctxt "pli-tv-pvr7:12.16"
msgid "ñattidutiyakammassa doso, ñatticatutthakammassa doso."
msgstr ""

msgctxt "pli-tv-pvr7:12.17"
msgid "Dve kammasampattiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:12.18"
msgid "apalokanakammassa sampatti, ñattikammassa sampatti."
msgstr ""

msgctxt "pli-tv-pvr7:12.19"
msgid "Aparāpi dve kammasampattiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:12.20"
msgid "ñattidutiyakammassa sampatti, ñatticatutthakammassa sampatti."
msgstr ""

msgctxt "pli-tv-pvr7:12.21"
msgid "Dve nānāsaṃvāsakabhūmiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:12.22"
msgid "attanā vā attānaṃ nānāsaṃvāsakaṃ karoti, samaggo vā naṃ saṃgho ukkhipati adassane vā appaṭikamme vā appaṭinissagge vā."
msgstr ""

msgctxt "pli-tv-pvr7:12.23"
msgid "Dve samānasaṃvāsakabhūmiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:12.24"
msgid "attanā vā attānaṃ samānasaṃvāsakaṃ karoti, samaggo vā naṃ saṃgho ukkhittaṃ osāreti dassane vā paṭikamme vā paṭinissagge vā."
msgstr ""

msgctxt "pli-tv-pvr7:12.25"
msgid "Dve pārājikā—"
msgstr ""

msgctxt "pli-tv-pvr7:12.26"
msgid "bhikkhūnañca bhikkhunīnañca."
msgstr ""

msgctxt "pli-tv-pvr7:12.27"
msgid "Dve saṃghādisesā, dve thullaccayā, dve pācittiyā, dve pāṭidesanīyā, dve dukkaṭā, dve dubbhāsitā—"
msgstr ""

msgctxt "pli-tv-pvr7:12.28"
msgid "bhikkhūnañca bhikkhunīnañca."
msgstr ""

msgctxt "pli-tv-pvr7:12.29"
msgid "Satta āpattiyo, satta āpattikkhandhā."
msgstr ""

msgctxt "pli-tv-pvr7:12.30"
msgid "Dvīhākārehi saṃgho bhijjati—"
msgstr ""

msgctxt "pli-tv-pvr7:12.31"
msgid "kammena vā salākaggāhena vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr7:13.1"
msgid "Dve puggalā na upasampādetabbā—"
msgstr ""

msgctxt "pli-tv-pvr7:13.2"
msgid "addhānahīno, aṅgahīno."
msgstr ""

msgctxt "pli-tv-pvr7:13.3"
msgid "Aparepi dve puggalā na upasampādetabbā—"
msgstr ""

msgctxt "pli-tv-pvr7:13.4"
msgid "vatthuvipanno, karaṇadukkaṭako."
msgstr ""

msgctxt "pli-tv-pvr7:13.5"
msgid "Aparepi dve puggalā na upasampādetabbā—"
msgstr ""

msgctxt "pli-tv-pvr7:13.6"
msgid "aparipūro paripūro no ca yācati."
msgstr ""

msgctxt "pli-tv-pvr7:13.7"
msgid "Dvinnaṃ puggalānaṃ nissāya na vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:13.8"
msgid "alajjissa ca bālassa ca."
msgstr ""

msgctxt "pli-tv-pvr7:13.9"
msgid "Dvinnaṃ puggalānaṃ nissayo na dātabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:13.10"
msgid "alajjissa ca lajjino ca na yācati."
msgstr ""

msgctxt "pli-tv-pvr7:13.11"
msgid "Dvinnaṃ puggalānaṃ nissayo dātabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:13.12"
msgid "bālassa ca lajjissa ca yācati."
msgstr ""

msgctxt "pli-tv-pvr7:13.13"
msgid "Dve puggalā abhabbā āpattiṃ āpajjituṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:13.14"
msgid "buddhā ca paccekabuddhā ca."
msgstr ""

msgctxt "pli-tv-pvr7:13.15"
msgid "Dve puggalā bhabbā, āpattiṃ āpajjituṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:13.16"
msgid "bhikkhū ca bhikkhuniyo ca."
msgstr ""

msgctxt "pli-tv-pvr7:13.17"
msgid "Dve puggalā abhabbā sañcicca āpattiṃ āpajjituṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:13.18"
msgid "bhikkhū ca bhikkhuniyo ca ariyapuggalā."
msgstr ""

msgctxt "pli-tv-pvr7:13.19"
msgid "Dve puggalā bhabbā sañcicca āpattiṃ āpajjituṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:13.20"
msgid "bhikkhū ca bhikkhuniyo ca puthujjanā."
msgstr ""

msgctxt "pli-tv-pvr7:13.21"
msgid "Dve puggalā abhabbā sañcicca sātisāraṃ vatthuṃ ajjhācarituṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:13.22"
msgid "bhikkhū ca bhikkhuniyo ca ariyapuggalā."
msgstr ""

msgctxt "pli-tv-pvr7:13.23"
msgid "Dve puggalā bhabbā sañcicca sātisāraṃ vatthuṃ ajjhācarituṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:13.24"
msgid "bhikkhū ca bhikkhuniyo ca puthujjanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc10, pts-vp-pli5.118
msgctxt "pli-tv-pvr7:14.1"
msgid "Dve paṭikkosā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.2"
msgid "kāyena vā paṭikkosati vācāya vā paṭikkosati."
msgstr ""

msgctxt "pli-tv-pvr7:14.3"
msgid "Dve nissāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.4"
msgid "atthi puggalo appatto nissāraṇaṃ tañce saṃgho nissāreti ekacco sunissārito, ekacco dunnissārito."
msgstr ""

msgctxt "pli-tv-pvr7:14.5"
msgid "Dve osāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.6"
msgid "atthi puggalo appatto osāraṇaṃ tañce saṃgho osāreti ekacco sosārito, ekacco dosārito."
msgstr ""

msgctxt "pli-tv-pvr7:14.7"
msgid "Dve paṭiññā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.8"
msgid "kāyena vā paṭijānāti vācāya vā paṭijānāti."
msgstr ""

msgctxt "pli-tv-pvr7:14.9"
msgid "Dve paṭiggahā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.10"
msgid "kāyena vā paṭiggaṇhāti kāyapaṭibaddhena vā paṭiggaṇhāti."
msgstr ""

msgctxt "pli-tv-pvr7:14.11"
msgid "Dve paṭikkhepā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.12"
msgid "kāyena vā paṭikkhipati vācāya vā paṭikkhipati."
msgstr ""

msgctxt "pli-tv-pvr7:14.13"
msgid "Dve upaghātikā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.14"
msgid "sikkhūpaghātikā ca bhogūpaghātikā ca."
msgstr ""

msgctxt "pli-tv-pvr7:14.15"
msgid "Dve codanā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.16"
msgid "kāyena vā codeti vācāya vā codeti."
msgstr ""

msgctxt "pli-tv-pvr7:14.17"
msgid "Dve kathinassa palibodhā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.18"
msgid "āvāsapalibodho ca cīvarapalibodho ca."
msgstr ""

msgctxt "pli-tv-pvr7:14.19"
msgid "Dve kathinassa apalibodhā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.20"
msgid "āvāsaapalibodho ca cīvaraapalibodho ca."
msgstr ""

msgctxt "pli-tv-pvr7:14.21"
msgid "Dve cīvarāni—"
msgstr ""

msgctxt "pli-tv-pvr7:14.22"
msgid "gahapatikañca paṃsukūlañca."
msgstr ""

msgctxt "pli-tv-pvr7:14.23"
msgid "Dve pattā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.24"
msgid "ayopatto mattikāpatto."
msgstr ""

msgctxt "pli-tv-pvr7:14.25"
msgid "Dve maṇḍalāni—"
msgstr ""

msgctxt "pli-tv-pvr7:14.26"
msgid "tipumayaṃ, sīsamayaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:14.27"
msgid "Dve pattassa adhiṭṭhānā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.28"
msgid "kāyena vā adhiṭṭheti vācāya vā adhiṭṭheti."
msgstr ""

msgctxt "pli-tv-pvr7:14.29"
msgid "Dve cīvarassa adhiṭṭhānā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.30"
msgid "kāyena vā adhiṭṭheti vācāya vā adhiṭṭheti."
msgstr ""

msgctxt "pli-tv-pvr7:14.31"
msgid "Dve vikappanā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.32"
msgid "sammukhāvikappanā ca parammukhāvikappanā ca."
msgstr ""

msgctxt "pli-tv-pvr7:14.33"
msgid "Dve vinayā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.34"
msgid "bhikkhūnañca bhikkhunīnañca."
msgstr ""

msgctxt "pli-tv-pvr7:14.35"
msgid "Dve venayikā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.36"
msgid "paññattañca paññattānulomañca."
msgstr ""

msgctxt "pli-tv-pvr7:14.37"
msgid "Dve vinayassa sallekhā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.38"
msgid "akappiye setughāto, kappiye mattakāritā."
msgstr ""

msgctxt "pli-tv-pvr7:14.39"
msgid "Dvīhākārehi āpattiṃ āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr7:14.40"
msgid "kāyena vā āpajjati vācāya vā āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:14.41"
msgid "Dvīhākārehi āpattiyā vuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr7:14.42"
msgid "kāyena vā vuṭṭhāti vācāya vā vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:14.43"
msgid "Dve parivāsā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.44"
msgid "paṭicchannaparivāso, appaṭicchannaparivāso."
msgstr ""

msgctxt "pli-tv-pvr7:14.45"
msgid "Aparepi dve parivāsā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.46"
msgid "suddhantaparivāso samodhānaparivāso."
msgstr ""

msgctxt "pli-tv-pvr7:14.47"
msgid "Dve mānattā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.48"
msgid "paṭicchannamānattaṃ, appaṭicchannamānattaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:14.49"
msgid "Aparepi dve mānattā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.50"
msgid "pakkhamānattaṃ, samodhānamānattaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:14.51"
msgid "Dvinnaṃ puggalānaṃ ratticchedo—"
msgstr ""

msgctxt "pli-tv-pvr7:14.52"
msgid "pārivāsikassa ca mānattacārikassa ca."
msgstr ""

msgctxt "pli-tv-pvr7:14.53"
msgid "Dve anādariyāni—"
msgstr ""

msgctxt "pli-tv-pvr7:14.54"
msgid "puggalānādariyañca dhammānādariyañca."
msgstr ""

msgctxt "pli-tv-pvr7:14.55"
msgid "Dve loṇāni—"
msgstr ""

#. VAR: jātimañca kārimañca → jātimayañca khārimayañca (s1-3)
msgctxt "pli-tv-pvr7:14.56"
msgid "jātimañca kārimañca."
msgstr ""

msgctxt "pli-tv-pvr7:14.57"
msgid "Aparānipi dve loṇāni—"
msgstr ""

msgctxt "pli-tv-pvr7:14.58"
msgid "sāmuddaṃ kāḷaloṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:14.59"
msgid "Aparānipi dve loṇāni—"
msgstr ""

#. VAR: sindhavaṃ, ubbhidaṃ → sindhavañca ubbhidañca (s1-3) | sindhavaṃ ubbhiraṃ (itipi)
msgctxt "pli-tv-pvr7:14.60"
msgid "sindhavaṃ, ubbhidaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:14.61"
msgid "Aparānipi dve loṇāni—"
msgstr ""

msgctxt "pli-tv-pvr7:14.62"
msgid "romakaṃ, pakkālakaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:14.63"
msgid "Dve paribhogā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.64"
msgid "abbhantaraparibhogo ca bāhiraparibhogo ca."
msgstr ""

msgctxt "pli-tv-pvr7:14.65"
msgid "Dve akkosā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.66"
msgid "hīno ca akkoso ukkaṭṭho ca akkoso."
msgstr ""

msgctxt "pli-tv-pvr7:14.67"
msgid "Dvīhākārehi pesuññaṃ hoti—"
msgstr ""

msgctxt "pli-tv-pvr7:14.68"
msgid "piyakamyassa vā bhedādhippāyassa vā."
msgstr ""

msgctxt "pli-tv-pvr7:14.69"
msgid "Dvīhākārehi gaṇabhojanaṃ pasavati—"
msgstr ""

msgctxt "pli-tv-pvr7:14.70"
msgid "nimantanato vā viññattito vā."
msgstr ""

msgctxt "pli-tv-pvr7:14.71"
msgid "Dve vassūpanāyikā—"
msgstr ""

msgctxt "pli-tv-pvr7:14.72"
msgid "purimikā, pacchimikā."
msgstr ""

msgctxt "pli-tv-pvr7:14.73"
msgid "Dve adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:14.74"
msgid "Dve dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr7:15.1"
msgid "Dve puggalā bālā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.2"
msgid "yo ca anāgataṃ bhāraṃ vahati, yo ca āgataṃ bhāraṃ na vahati."
msgstr ""

msgctxt "pli-tv-pvr7:15.3"
msgid "Dve puggalā paṇḍitā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.4"
msgid "yo ca anāgataṃ bhāraṃ na vahati, yo ca āgataṃ bhāraṃ vahati."
msgstr ""

msgctxt "pli-tv-pvr7:15.5"
msgid "Aparepi dve puggalā bālā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.6"
msgid "yo ca akappiye kappiyasaññī, yo ca kappiye akappiyasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:15.7"
msgid "Dve puggalā paṇḍitā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.8"
msgid "yo ca akappiye akappiyasaññī, yo ca kappiye kappiyasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:15.9"
msgid "Aparepi dve puggalā bālā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.10"
msgid "yo ca anāpattiyā āpattisaññī, yo ca āpattiyā anāpattisaññī."
msgstr ""

msgctxt "pli-tv-pvr7:15.11"
msgid "Dve puggalā paṇḍitā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.12"
msgid "yo ca āpattiyā āpattisaññī, yo ca anāpattiyā anāpattisaññī."
msgstr ""

msgctxt "pli-tv-pvr7:15.13"
msgid "Aparepi dve puggalā bālā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.14"
msgid "yo ca adhamme dhammasaññī, yo ca dhamme adhammasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:15.15"
msgid "Dve puggalā paṇḍitā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.16"
msgid "yo ca adhamme adhammasaññī, yo ca dhamme dhammasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:15.17"
msgid "Aparepi dve puggalā bālā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.18"
msgid "yo ca avinaye vinayasaññī, yo ca vinaye avinayasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:15.19"
msgid "Dve puggalā paṇḍitā—"
msgstr ""

msgctxt "pli-tv-pvr7:15.20"
msgid "yo ca avinaye avinayasaññī, yo ca vinaye vinayasaññī."
msgstr ""

#. HTML: </p><p>
#. REF: sc12, pts-vp-pli5.119
msgctxt "pli-tv-pvr7:16.1"
msgid "Dvinnaṃ puggalānaṃ āsavā vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.2"
msgid "yo ca na kukkuccāyitabbaṃ kukkuccāyati, yo ca kukkuccāyitabbaṃ na kukkuccāyati."
msgstr ""

msgctxt "pli-tv-pvr7:16.3"
msgid "Dvinnaṃ puggalānaṃ āsavā na vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.4"
msgid "yo ca na kukkuccāyitabbaṃ na kukkuccāyati, yo ca kukkuccāyitabbaṃ kukkuccāyati."
msgstr ""

msgctxt "pli-tv-pvr7:16.5"
msgid "Aparesampi dvinnaṃ puggalānaṃ āsavā vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.6"
msgid "yo ca akappiye kappiyasaññī, yo ca kappiye akappiyasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:16.7"
msgid "Dvinnaṃ puggalānaṃ āsavā na vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.8"
msgid "yo ca akappiye akappiyasaññī, yo ca kappiye kappiyasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:16.9"
msgid "Aparesampi dvinnaṃ puggalānaṃ āsavā vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.10"
msgid "yo ca anāpattiyā āpattisaññī, yo ca āpattiyā anāpattisaññī."
msgstr ""

msgctxt "pli-tv-pvr7:16.11"
msgid "Dvinnaṃ puggalānaṃ āsavā na vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.12"
msgid "yo ca anāpattiyā anāpattisaññī, yo ca āpattiyā āpattisaññī."
msgstr ""

msgctxt "pli-tv-pvr7:16.13"
msgid "Aparesampi dvinnaṃ puggalānaṃ āsavā vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.14"
msgid "yo ca adhamme dhammasaññī, yo ca dhamme adhammasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:16.15"
msgid "Dvinnaṃ puggalānaṃ āsavā na vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.16"
msgid "yo ca adhamme adhammasaññī, yo ca dhamme dhammasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:16.17"
msgid "Aparesampi dvinnaṃ puggalānaṃ āsavā vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.18"
msgid "yo ca avinaye vinayasaññī, yo ca vinaye avinayasaññī."
msgstr ""

msgctxt "pli-tv-pvr7:16.19"
msgid "Dvinnaṃ puggalānaṃ āsavā na vaḍḍhanti—"
msgstr ""

msgctxt "pli-tv-pvr7:16.20"
msgid "yo ca avinaye avinayasaññī, yo ca vinaye vinayasaññī."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc13
msgctxt "pli-tv-pvr7:17.1"
msgid "Dukā niṭṭhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc14
msgctxt "pli-tv-pvr7:18.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc15
msgctxt "pli-tv-pvr7:19.1"
msgid "Saññā laddhā ca saddhammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:19.2"
msgid "parikkhārā ca puggalā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:19.3"
msgid "Saccaṃ bhūmi nikkhamanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:19.4"
msgid "ādiyanto samādiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr7:20.1"
msgid "Karonto dento gaṇhanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:20.2"
msgid "paribhogena ratti ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:20.3"
msgid "Aruṇāchindaṃ chādento,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:20.4"
msgid "dhārento ca uposathā."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr7:21.1"
msgid "Pavāraṇā kammāparā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:21.2"
msgid "vatthu aparā dosā ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:21.3"
msgid "Aparā dve ca sampatti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:21.4"
msgid "nānā samānameva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr7:22.1"
msgid "Pārājisaṃghathullaccaya,"
msgstr ""

#. HTML: <br>
#. VAR: pācitti → pācittiyā (bj) | pācitti (s1-3) | pācitti (pts1)
msgctxt "pli-tv-pvr7:22.2"
msgid "pācitti pāṭidesanā;"
msgstr ""

#. HTML: <br>
#. VAR: dubbhāsitā ceva → bhāsitañceva (s1-3)
msgctxt "pli-tv-pvr7:22.3"
msgid "Dukkaṭā dubbhāsitā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:22.4"
msgid "satta āpattikkhandhā ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-pvr7:23.1"
msgid "Bhijjati upasampadā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:23.2"
msgid "tatheva apare duve;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:23.3"
msgid "Na vatthabbaṃ na dātabbaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:23.4"
msgid "abhabbābhabbameva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-pvr7:24.1"
msgid "Sañcicca sātisārā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:24.2"
msgid "paṭikkosā nissāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:24.3"
msgid "Osāraṇā paṭiññā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:24.4"
msgid "paṭiggahā paṭikkhipā."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr7:25.1"
msgid "Upaghāti codanā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:25.2"
msgid "kathinā ca duve tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:25.3"
msgid "Cīvarā pattamaṇḍalā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:25.4"
msgid "adhiṭṭhānā tatheva dve."
msgstr ""

#. HTML: </p><p>
#. REF: sc22
msgctxt "pli-tv-pvr7:26.1"
msgid "Vikappanā ca vinayā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:26.2"
msgid "venayikā ca sallekhā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:26.3"
msgid "Āpajjati ca vuṭṭhāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:26.4"
msgid "parivāsāpare duve."
msgstr ""

#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-pvr7:27.1"
msgid "Dve mānattā apare ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:27.2"
msgid "ratticchedo anādari;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:27.3"
msgid "Dve loṇā tayo apare,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:27.4"
msgid "paribhogakkosena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr7:28.1"
msgid "Pesuñño ca gaṇāvassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:28.2"
msgid "ṭhapanā bhārakappiyaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:28.3"
msgid "Anāpatti adhammadhammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:28.4"
msgid "vinaye āsave tathāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:29.1"
msgid "3. Tikavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc25, pts-vp-pli5.120
msgctxt "pli-tv-pvr7:30.1"
msgid "Atthāpatti tiṭṭhante bhagavati āpajjati, no parinibbute;"
msgstr ""

msgctxt "pli-tv-pvr7:30.2"
msgid "atthāpatti parinibbute bhagavati āpajjati, no tiṭṭhante;"
msgstr ""

msgctxt "pli-tv-pvr7:30.3"
msgid "atthāpatti tiṭṭhantepi bhagavati āpajjati parinibbutepi."
msgstr ""

msgctxt "pli-tv-pvr7:30.4"
msgid "Atthāpatti kāle āpajjati, no vikāle;"
msgstr ""

msgctxt "pli-tv-pvr7:30.5"
msgid "atthāpatti vikāle āpajjati, no kāle;"
msgstr ""

msgctxt "pli-tv-pvr7:30.6"
msgid "atthāpatti kāle ceva āpajjati vikāle ca."
msgstr ""

msgctxt "pli-tv-pvr7:30.7"
msgid "Atthāpatti rattiṃ āpajjati, no divā;"
msgstr ""

msgctxt "pli-tv-pvr7:30.8"
msgid "atthāpatti divā āpajjati, no rattiṃ;"
msgstr ""

msgctxt "pli-tv-pvr7:30.9"
msgid "atthāpatti rattiñceva āpajjati divā ca."
msgstr ""

msgctxt "pli-tv-pvr7:30.10"
msgid "Atthāpatti dasavasso āpajjati, no ūnadasavasso;"
msgstr ""

msgctxt "pli-tv-pvr7:30.11"
msgid "atthāpatti ūnadasavasso āpajjati, no dasavasso;"
msgstr ""

msgctxt "pli-tv-pvr7:30.12"
msgid "atthāpatti dasavasso ceva āpajjati ūnadasavasso ca."
msgstr ""

msgctxt "pli-tv-pvr7:30.13"
msgid "Atthāpatti pañcavasso āpajjati, no ūnapañcavasso;"
msgstr ""

msgctxt "pli-tv-pvr7:30.14"
msgid "atthāpatti ūnapañcavasso āpajjati, no pañcavasso;"
msgstr ""

msgctxt "pli-tv-pvr7:30.15"
msgid "atthāpatti pañcavasso ceva āpajjati ūnapañcavasso ca."
msgstr ""

msgctxt "pli-tv-pvr7:30.16"
msgid "Atthāpatti kusalacitto āpajjati;"
msgstr ""

msgctxt "pli-tv-pvr7:30.17"
msgid "atthāpatti akusalacitto āpajjati;"
msgstr ""

msgctxt "pli-tv-pvr7:30.18"
msgid "atthāpatti abyākatacitto āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:30.19"
msgid "Atthāpatti sukhavedanāsamaṅgī āpajjati;"
msgstr ""

msgctxt "pli-tv-pvr7:30.20"
msgid "atthāpatti dukkhavedanāsamaṅgī āpajjati;"
msgstr ""

msgctxt "pli-tv-pvr7:30.21"
msgid "atthāpatti adukkhamasukhavedanāsamaṅgī āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:30.22"
msgid "Tīṇi codanāvatthūni—"
msgstr ""

msgctxt "pli-tv-pvr7:30.23"
msgid "diṭṭhena, sutena, parisaṅkāya."
msgstr ""

msgctxt "pli-tv-pvr7:30.24"
msgid "Tayo salākaggāhā—"
msgstr ""

#. VAR: vivaṭako → vivaṭṭako (mr)
msgctxt "pli-tv-pvr7:30.25"
msgid "guḷhako, vivaṭako, sakaṇṇajappako."
msgstr ""

msgctxt "pli-tv-pvr7:30.26"
msgid "Tayo paṭikkhepā—"
msgstr ""

#. VAR: asantuṭṭhitā → asantuṭṭhatā (s1-3)
msgctxt "pli-tv-pvr7:30.27"
msgid "mahicchatā, asantuṭṭhitā, asallekhatā."
msgstr ""

msgctxt "pli-tv-pvr7:30.28"
msgid "Tayo anuññātā—"
msgstr ""

msgctxt "pli-tv-pvr7:30.29"
msgid "appicchatā, santuṭṭhitā, sallekhatā."
msgstr ""

msgctxt "pli-tv-pvr7:30.30"
msgid "Aparepi tayo paṭikkhepā—"
msgstr ""

msgctxt "pli-tv-pvr7:30.31"
msgid "mahicchatā, asantuṭṭhitā, amattaññutā."
msgstr ""

msgctxt "pli-tv-pvr7:30.32"
msgid "Tayo anuññātā—"
msgstr ""

msgctxt "pli-tv-pvr7:30.33"
msgid "appicchatā, santuṭṭhitā, mattaññutā."
msgstr ""

msgctxt "pli-tv-pvr7:30.34"
msgid "Tisso paññattiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:30.35"
msgid "paññatti, anupaññatti, anuppannapaññatti."
msgstr ""

msgctxt "pli-tv-pvr7:30.36"
msgid "Aparāpi tisso paññattiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:30.37"
msgid "sabbatthapaññatti, padesapaññatti, sādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr7:30.38"
msgid "Aparāpi tisso paññattiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:30.39"
msgid "asādhāraṇapaññatti, ekatopaññatti, ubhatopaññatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc26, pts-vp-pli5.121
msgctxt "pli-tv-pvr7:31.1"
msgid "Atthāpatti bālo āpajjati, no paṇḍito;"
msgstr ""

msgctxt "pli-tv-pvr7:31.2"
msgid "atthāpatti paṇḍito āpajjati, no bālo;"
msgstr ""

msgctxt "pli-tv-pvr7:31.3"
msgid "atthāpatti bālo ceva āpajjati paṇḍito ca."
msgstr ""

msgctxt "pli-tv-pvr7:31.4"
msgid "Atthāpatti kāḷe āpajjati, no juṇhe;"
msgstr ""

msgctxt "pli-tv-pvr7:31.5"
msgid "atthāpatti juṇhe āpajjati, no kāḷe;"
msgstr ""

msgctxt "pli-tv-pvr7:31.6"
msgid "atthāpatti kāḷe ceva āpajjati juṇhe ca."
msgstr ""

msgctxt "pli-tv-pvr7:31.7"
msgid "Atthi kāḷe kappati, no juṇhe;"
msgstr ""

msgctxt "pli-tv-pvr7:31.8"
msgid "atthi juṇhe kappati, no kāḷe;"
msgstr ""

msgctxt "pli-tv-pvr7:31.9"
msgid "atthi kāḷe ceva kappati juṇhe ca."
msgstr ""

msgctxt "pli-tv-pvr7:31.10"
msgid "Atthāpatti hemante āpajjati, no gimhe no vasse;"
msgstr ""

msgctxt "pli-tv-pvr7:31.11"
msgid "atthāpatti gimhe āpajjati, no hemante no vasse;"
msgstr ""

msgctxt "pli-tv-pvr7:31.12"
msgid "atthāpatti vasse āpajjati, no hemante no gimhe."
msgstr ""

msgctxt "pli-tv-pvr7:31.13"
msgid "Atthāpatti saṃgho āpajjati, na gaṇo na puggalo;"
msgstr ""

msgctxt "pli-tv-pvr7:31.14"
msgid "atthāpatti gaṇo āpajjati, na saṃgho na puggalo;"
msgstr ""

msgctxt "pli-tv-pvr7:31.15"
msgid "atthāpatti puggalo āpajjati, na saṃgho na gaṇo."
msgstr ""

msgctxt "pli-tv-pvr7:31.16"
msgid "Atthi saṃghassa kappati, na gaṇassa na puggalassa;"
msgstr ""

msgctxt "pli-tv-pvr7:31.17"
msgid "atthi gaṇassa kappati, na saṃghassa na puggalassa;"
msgstr ""

msgctxt "pli-tv-pvr7:31.18"
msgid "atthi puggalassa kappati, na saṃghassa na gaṇassa."
msgstr ""

msgctxt "pli-tv-pvr7:31.19"
msgid "Tisso chādanā vatthuṃ chādeti, no āpattiṃ;"
msgstr ""

msgctxt "pli-tv-pvr7:31.20"
msgid "āpattiṃ chādeti, no vatthuṃ;"
msgstr ""

msgctxt "pli-tv-pvr7:31.21"
msgid "vatthuñceva chādeti āpattiñca."
msgstr ""

msgctxt "pli-tv-pvr7:31.22"
msgid "Tisso paṭicchādiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:31.23"
msgid "jantāgharapaṭicchādi, udakapaṭicchādi, vatthapaṭicchādi."
msgstr ""

msgctxt "pli-tv-pvr7:31.24"
msgid "Tīṇi paṭicchannāni vahanti, no vivaṭāni—"
msgstr ""

msgctxt "pli-tv-pvr7:31.25"
msgid "mātugāmo paṭicchanno vahati, no vivaṭo;"
msgstr ""

msgctxt "pli-tv-pvr7:31.26"
msgid "brāhmaṇānaṃ mantā paṭicchannā vahanti, no vivaṭā;"
msgstr ""

msgctxt "pli-tv-pvr7:31.27"
msgid "micchādiṭṭhi paṭicchannā vahati, no vivaṭā."
msgstr ""

msgctxt "pli-tv-pvr7:31.28"
msgid "Tīṇi vivaṭāni virocanti, no paṭicchannāni—"
msgstr ""

msgctxt "pli-tv-pvr7:31.29"
msgid "candamaṇḍalaṃ vivaṭaṃ virocati, no paṭicchannaṃ;"
msgstr ""

msgctxt "pli-tv-pvr7:31.30"
msgid "sūriyamaṇḍalaṃ vivaṭaṃ virocati, no paṭicchannaṃ;"
msgstr ""

msgctxt "pli-tv-pvr7:31.31"
msgid "tathāgatappavedito dhammavinayo vivaṭo virocati, no paṭicchanno."
msgstr ""

msgctxt "pli-tv-pvr7:31.32"
msgid "Tayo senāsanaggāhā—"
msgstr ""

msgctxt "pli-tv-pvr7:31.33"
msgid "purimako, pacchimako, antarāmuttako."
msgstr ""

msgctxt "pli-tv-pvr7:31.34"
msgid "Atthāpatti gilāno āpajjati, no agilāno;"
msgstr ""

msgctxt "pli-tv-pvr7:31.35"
msgid "atthāpatti agilāno āpajjati, no gilāno;"
msgstr ""

msgctxt "pli-tv-pvr7:31.36"
msgid "atthāpatti gilāno ceva āpajjati agilāno ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-pvr7:32.1"
msgid "Tīṇi adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:32.2"
msgid "Tīṇi dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:32.3"
msgid "Tayo parivāsā—"
msgstr ""

msgctxt "pli-tv-pvr7:32.4"
msgid "paṭicchannaparivāso, appaṭicchannaparivāso, suddhantaparivāso."
msgstr ""

msgctxt "pli-tv-pvr7:32.5"
msgid "Tayo mānattā—"
msgstr ""

msgctxt "pli-tv-pvr7:32.6"
msgid "paṭicchannamānattaṃ, appaṭicchannamānattaṃ, pakkhamānattaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:32.7"
msgid "Tayo pārivāsikassa bhikkhuno ratticchedā—"
msgstr ""

msgctxt "pli-tv-pvr7:32.8"
msgid "sahavāso, vippavāso, anārocanā."
msgstr ""

msgctxt "pli-tv-pvr7:32.9"
msgid "Atthāpatti anto āpajjati, no bahi;"
msgstr ""

msgctxt "pli-tv-pvr7:32.10"
msgid "atthāpatti bahi āpajjati, no anto;"
msgstr ""

msgctxt "pli-tv-pvr7:32.11"
msgid "atthāpatti anto ceva āpajjati bahi ca."
msgstr ""

msgctxt "pli-tv-pvr7:32.12"
msgid "Atthāpatti antosīmāya āpajjati, no bahisīmāya;"
msgstr ""

msgctxt "pli-tv-pvr7:32.13"
msgid "atthāpatti bahisīmāya āpajjati, no antosīmāya;"
msgstr ""

msgctxt "pli-tv-pvr7:32.14"
msgid "atthāpatti antosīmāya ceva āpajjati bahisīmāya ca."
msgstr ""

msgctxt "pli-tv-pvr7:32.15"
msgid "Tīhākārehi āpattiṃ āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr7:32.16"
msgid "kāyena āpajjati, vācāya āpajjati, kāyena vācāya āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:32.17"
msgid "Aparehipi tīhākārehi āpattiṃ āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr7:32.18"
msgid "saṃghamajjhe, gaṇamajjhe, puggalassa santike."
msgstr ""

msgctxt "pli-tv-pvr7:32.19"
msgid "Tīhākārehi āpattiyā vuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr7:32.20"
msgid "kāyena vuṭṭhāti, vācāya vuṭṭhāti, kāyena vācāya vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:32.21"
msgid "Aparehipi tīhākārehi āpattiyā vuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr7:32.22"
msgid "saṃghamajjhe, gaṇamajjhe, puggalassa santike."
msgstr ""

msgctxt "pli-tv-pvr7:32.23"
msgid "Tīṇi adhammikāni amūḷhavinayassa dānāni."
msgstr ""

msgctxt "pli-tv-pvr7:32.24"
msgid "Tīṇi dhammikāni amūḷhavinayassa dānāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc28, pts-vp-pli5.122
msgctxt "pli-tv-pvr7:33.1"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho tajjanīyakammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-pvr7:33.2"
msgid "bhaṇḍanakārako hoti kalahakārako vivādakārako bhassakārako saṃghe adhikaraṇakārako, bālo hoti abyatto, āpattibahulo anapadāno gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi."
msgstr ""

msgctxt "pli-tv-pvr7:33.3"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho niyassakammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-pvr7:33.4"
msgid "bhaṇḍanakārako hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:33.6"
msgid "saṃghe adhikaraṇakārako, bālo hoti abyatto, āpattibahulo anapadāno gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi."
msgstr ""

msgctxt "pli-tv-pvr7:33.7"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho pabbājanīyakammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-pvr7:33.8"
msgid "bhaṇḍanakārako hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:33.10"
msgid "saṃghe adhikaraṇakārako, bālo hoti abyatto, āpattibahulo anapadāno kuladūsako hoti pāpasamācāro pāpasamācārā dissanti ceva suyyanti ca."
msgstr ""

msgctxt "pli-tv-pvr7:33.11"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho paṭisāraṇīyakammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-pvr7:33.12"
msgid "bhaṇḍanakārako hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:33.14"
msgid "saṃghe adhikaraṇakārako, bālo hoti abyatto, āpattibahulo anapadāno gihī akkosati paribhāsati."
msgstr ""

msgctxt "pli-tv-pvr7:33.15"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho āpattiyā adassane ukkhepanīyakammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-pvr7:33.16"
msgid "bhaṇḍanakārako hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:33.18"
msgid "saṃghe adhikaraṇakārako, bālo hoti abyatto, āpattibahulo anapadāno āpattiṃ āpajjitvā na icchati āpattiṃ passituṃ."
msgstr ""

msgctxt "pli-tv-pvr7:33.19"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho āpattiyā appaṭikamme ukkhepanīyakammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-pvr7:33.20"
msgid "bhaṇḍanakārako hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:33.22"
msgid "saṃghe adhikaraṇakārako, bālo hoti abyatto, āpattibahulo anapadāno āpattiṃ āpajjitvā na icchati āpattiṃ paṭikātuṃ."
msgstr ""

msgctxt "pli-tv-pvr7:33.23"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho pāpikāya diṭṭhiyā appaṭinissagge ukkhepanīyakammaṃ kareyya—"
msgstr ""

msgctxt "pli-tv-pvr7:33.24"
msgid "bhaṇḍanakārako hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:33.26"
msgid "saṃghe adhikaraṇakārako, bālo hoti abyatto, āpattibahulo anapadāno na icchati pāpikaṃ diṭṭhiṃ paṭinissajjituṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-pvr7:34.1"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno ākaṅkhamāno saṃgho āgāḷhāya ceteyya—"
msgstr ""

msgctxt "pli-tv-pvr7:34.2"
msgid "bhaṇḍanakārako hoti kalahakārako vivādakārako bhassakārako saṃghe adhikaraṇakārako, bālo hoti abyatto āpattibahulo anapadāno, gihisaṃsaṭṭho viharati ananulomikehi gihisaṃsaggehi."
msgstr ""

msgctxt "pli-tv-pvr7:34.3"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.4"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:34.5"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.6"
msgid "adhisīle sīlavipanno hoti, ajjhācāre ācāravipanno hoti, atidiṭṭhiyā diṭṭhivipanno hoti."
msgstr ""

msgctxt "pli-tv-pvr7:34.7"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.8"
msgid "kāyikena davena samannāgato hoti, vācasikena davena samannāgato hoti, kāyikavācasikena davena samannāgato hoti."
msgstr ""

msgctxt "pli-tv-pvr7:34.9"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.10"
msgid "kāyikena anācārena samannāgato hoti, vācasikena anācārena samannāgato hoti, kāyikavācasikena anācārena samannāgato hoti."
msgstr ""

msgctxt "pli-tv-pvr7:34.11"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.12"
msgid "kāyikena upaghātikena samannāgato hoti, vācasikena upaghātikena samannāgato hoti, kāyikavācasikena upaghātikena samannāgato hoti."
msgstr ""

msgctxt "pli-tv-pvr7:34.13"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.14"
msgid "kāyikena micchājīvena samannāgato hoti, vācasikena micchājīvena samannāgato hoti, kāyikavācasikena micchājīvena samannāgato hoti."
msgstr ""

msgctxt "pli-tv-pvr7:34.15"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.16"
msgid "āpattiṃ āpanno kammakato upasampādeti, nissayaṃ deti, sāmaṇeraṃ upaṭṭhāpeti."
msgstr ""

msgctxt "pli-tv-pvr7:34.17"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.18"
msgid "yāya āpattiyā saṃghena kammaṃ kataṃ hoti taṃ āpattiṃ āpajjati, aññaṃ vā tādisikaṃ, tato vā pāpiṭṭhataraṃ."
msgstr ""

msgctxt "pli-tv-pvr7:34.19"
msgid "Aparehipi tīhaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:34.20"
msgid "buddhassa avaṇṇaṃ bhāsati, dhammassa avaṇṇaṃ bhāsati, saṃghassa avaṇṇaṃ bhāsati."
msgstr ""

#. HTML: </p><p>
#. REF: sc30, pts-vp-pli5.123
msgctxt "pli-tv-pvr7:35.1"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno saṃghamajjhe uposathaṃ ṭhapentassa—"
msgstr ""

msgctxt "pli-tv-pvr7:35.2"
msgid "“alaṃ, bhikkhu, mā bhaṇḍanaṃ mā kalahaṃ mā viggahaṃ mā vivādan”ti omadditvā saṃghena uposatho kātabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.3"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.4"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno saṃghamajjhe pavāraṇaṃ ṭhapentassa—"
msgstr ""

msgctxt "pli-tv-pvr7:35.5"
msgid "“alaṃ, bhikkhu, mā bhaṇḍanaṃ mā kalahaṃ mā viggahaṃ mā vivādan”ti omadditvā saṃghena pavāretabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:35.6"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.7"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno na kāci saṃghasammuti dātabbā—"
msgstr ""

msgctxt "pli-tv-pvr7:35.8"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

#. VAR: saṃghe na voharitabbaṃ → saṃgho na voharitabbo (s1) | saṅgho na voharitabbo (s2, s3)
msgctxt "pli-tv-pvr7:35.9"
msgid "Tīhaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:35.10"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.11"
msgid "Tīhaṅgehi samannāgato bhikkhu na kismiñci paccekaṭṭhāne ṭhapetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.12"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.13"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno nissāya na vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:35.14"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.15"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno nissayo na dātabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.16"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.17"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa nālaṃ okāsakammaṃ kātuṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:35.18"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.19"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno savacanīyaṃ nādātabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:35.20"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.21"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno vinayo na pucchitabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.22"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.23"
msgid "Tīhaṅgehi samannāgatena bhikkhunā vinayo na pucchitabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.24"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.25"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno vinayo na vissajjetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.26"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.27"
msgid "Tīhaṅgehi samannāgatena bhikkhunā vinayo na vissajjetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.28"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.29"
msgid "Tīhaṅgehi samannāgatassa bhikkhuno anuyogo na dātabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.30"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

#. VAR: sākacchitabbo → sākacchātabbo (pts1, mr)
msgctxt "pli-tv-pvr7:35.31"
msgid "Tīhaṅgehi samannāgatena bhikkhunā saddhiṃ vinayo na sākacchitabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.32"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

msgctxt "pli-tv-pvr7:35.33"
msgid "Tīhaṅgehi samannāgatena bhikkhunā na upasampādetabbaṃ na nissayo dātabbo na sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:35.34"
msgid "alajjī ca hoti, bālo ca, apakatatto ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-pvr7:36.1"
msgid "Tayo uposathā—"
msgstr ""

msgctxt "pli-tv-pvr7:36.2"
msgid "cātuddasiko, pannarasiko, sāmaggiuposatho."
msgstr ""

msgctxt "pli-tv-pvr7:36.3"
msgid "Aparepi tayo uposathā—"
msgstr ""

msgctxt "pli-tv-pvr7:36.4"
msgid "saṃgheuposatho, gaṇeuposatho, puggaleuposatho."
msgstr ""

msgctxt "pli-tv-pvr7:36.5"
msgid "Aparepi tayo uposathā—"
msgstr ""

msgctxt "pli-tv-pvr7:36.6"
msgid "suttuddesouposatho, pārisuddhiuposatho, adhiṭṭhānuposatho."
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-pvr7:37.1"
msgid "Tisso pavāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr7:37.2"
msgid "cātuddasikā, pannarasikā, sāmaggipavāraṇā."
msgstr ""

msgctxt "pli-tv-pvr7:37.3"
msgid "Aparāpi tisso pavāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr7:37.4"
msgid "saṃghepavāraṇā, gaṇepavāraṇā, puggalepavāraṇā."
msgstr ""

msgctxt "pli-tv-pvr7:37.5"
msgid "Aparāpi tisso pavāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr7:37.6"
msgid "tevācikāpavāraṇā, dvevācikāpavāraṇā, samānavassikāpavāraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc33, pts-vp-pli5.124
msgctxt "pli-tv-pvr7:38.1"
msgid "Tayo āpāyikā nerayikā—"
msgstr ""

#. VAR: parisuddhabrahmacariyaṃ → parisuddhaṃ brahmacariyaṃ (bj) | suddhaṃ brahmacariyaṃ (s1-3)
#. VAR: evaṃdiṭṭhi → evaṃdiṭṭhī (bj)
msgctxt "pli-tv-pvr7:38.2"
msgid "idamappahāya, yo ca abrahmacārī brahmacāripaṭiñño, yo ca suddhaṃ brahmacāriṃ parisuddhabrahmacariyaṃ carantaṃ amūlakena abrahmacariyena anuddhaṃseti, yo cāyaṃ evaṃvādī evaṃdiṭṭhi—"
msgstr ""

msgctxt "pli-tv-pvr7:38.3"
msgid "“natthi kāmesu doso”ti so kāmesu pātabyataṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:38.4"
msgid "Tīṇi akusalamūlāni—"
msgstr ""

msgctxt "pli-tv-pvr7:38.5"
msgid "lobho akusalamūlaṃ, doso akusalamūlaṃ, moho akusalamūlaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:38.6"
msgid "Tīṇi kusalamūlāni—"
msgstr ""

msgctxt "pli-tv-pvr7:38.7"
msgid "alobho kusalamūlaṃ, adoso kusalamūlaṃ, amoho kusalamūlaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:38.8"
msgid "Tīṇi duccaritāni—"
msgstr ""

msgctxt "pli-tv-pvr7:38.9"
msgid "kāyaduccaritaṃ, vacīduccaritaṃ, manoduccaritaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:38.10"
msgid "Tīṇi sucaritāni—"
msgstr ""

msgctxt "pli-tv-pvr7:38.11"
msgid "kāyasucaritaṃ, vacīsucaritaṃ, manosucaritaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:38.12"
msgid "Tayo atthavase paṭicca bhagavatā kulesu tikabhojanaṃ paññattaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:38.13"
msgid "dummaṅkūnaṃ puggalānaṃ niggahāya, pesalānaṃ bhikkhūnaṃ phāsuvihārāya, “mā pāpicchā pakkhaṃ nissāya saṃghaṃ bhindeyyun”ti kulānuddayatāya ca."
msgstr ""

msgctxt "pli-tv-pvr7:38.14"
msgid "Tīhi asaddhammehi abhibhūto pariyādinnacitto devadatto āpāyiko nerayiko kappaṭṭho atekiccho pāpicchatā pāpamittatā oramattakena visesādhigamena antarā vosānaṃ āpādi."
msgstr ""

msgctxt "pli-tv-pvr7:38.15"
msgid "Tisso sammutiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:38.16"
msgid "daṇḍasammuti, sikkāsammuti, daṇḍasikkāsammuti."
msgstr ""

msgctxt "pli-tv-pvr7:38.17"
msgid "Tisso pādukā dhuvaṭṭhānikā asaṅkamanīyā—"
msgstr ""

msgctxt "pli-tv-pvr7:38.18"
msgid "vaccapādukā, passāvapādukā, ācamanapādukā."
msgstr ""

msgctxt "pli-tv-pvr7:38.19"
msgid "Tisso pādaghaṃsaniyo—"
msgstr ""

msgctxt "pli-tv-pvr7:38.20"
msgid "sakkharaṃ, kathalā, samuddapheṇakoti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc34
msgctxt "pli-tv-pvr7:39.1"
msgid "Tikaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc35
msgctxt "pli-tv-pvr7:40.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc36
msgctxt "pli-tv-pvr7:41.1"
msgid "Tiṭṭhante kāle rattiñca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:41.2"
msgid "dasa pañca kusalena;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:41.3"
msgid "Vedanā codanā vatthu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:41.4"
msgid "salākā dve paṭikkhipā."
msgstr ""

#. HTML: </p><p>
#. REF: sc37
msgctxt "pli-tv-pvr7:42.1"
msgid "Paññatti apare dve ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:42.2"
msgid "bālo kāḷe ca kappati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:42.3"
msgid "Hemante saṃgho saṃghassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:42.4"
msgid "chādanā ca paṭicchādi."
msgstr ""

#. HTML: </p><p>
#. REF: sc38
msgctxt "pli-tv-pvr7:43.1"
msgid "Paṭicchannā vivaṭā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:43.2"
msgid "senāsanagilāyanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:43.3"
msgid "Pātimokkhaṃ parivāsaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:43.4"
msgid "mānattā pārivāsikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc39
msgctxt "pli-tv-pvr7:44.1"
msgid "Anto anto ca sīmāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:44.2"
msgid "āpajjati punāpare;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:44.3"
msgid "Vuṭṭhāti apare ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:44.4"
msgid "amūḷhavinayā duve."
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-pvr7:45.1"
msgid "Tajjanīyā niyassā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:45.2"
msgid "pabbājapaṭisāraṇī;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:45.3"
msgid "Adassanā paṭikamme,"
msgstr ""

#. HTML: <br>
#. VAR: anissagge → anissaggī (si)
msgctxt "pli-tv-pvr7:45.4"
msgid "anissagge ca diṭṭhiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc41
msgctxt "pli-tv-pvr7:46.1"
msgid "Āgāḷhakammādhisīle,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:46.2"
msgid "davānācāraghātikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:46.3"
msgid "Ājīvāpannā tādisikā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:46.4"
msgid "avaṇṇuposathena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc42
msgctxt "pli-tv-pvr7:47.1"
msgid "Pavāraṇā sammuti ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:47.2"
msgid "vohārapaccekena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:47.3"
msgid "Na vatthabbaṃ na dātabbaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:47.4"
msgid "okāsaṃ na kare tathā."
msgstr ""

#. HTML: </p><p>
#. REF: sc43
msgctxt "pli-tv-pvr7:48.1"
msgid "Na kare savacanīyaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:48.2"
msgid "na pucchitabbakā duve;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:48.3"
msgid "Na vissajje duve ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:48.4"
msgid "anuyogampi no dade."
msgstr ""

#. HTML: </p><p>
#. REF: sc44
msgctxt "pli-tv-pvr7:49.1"
msgid "Sākacchā upasampadā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:49.2"
msgid "nissayasāmaṇerā ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:49.3"
msgid "Uposathatikā tīṇi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:49.4"
msgid "pavāraṇatikā tayo."
msgstr ""

#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-pvr7:50.1"
msgid "Āpāyikā akusalā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:50.2"
msgid "kusalā caritā duve;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:50.3"
msgid "Tikabhojanasaddhamme,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:50.4"
msgid "sammuti pādukena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:50.5"
msgid "Pādaghaṃsanikā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:50.6"
msgid "uddānaṃ tikake idanti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:51.1"
msgid "4. Catukkavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc46, pts-vp-pli5.125
msgctxt "pli-tv-pvr7:52.1"
msgid "Atthāpatti sakavācāya āpajjati, paravācāya vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.2"
msgid "atthāpatti paravācāya āpajjati, sakavācāya vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.3"
msgid "atthāpatti sakavācāya āpajjati, sakavācāya vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.4"
msgid "atthāpatti paravācāya āpajjati, paravācāya vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:52.5"
msgid "Atthāpatti kāyena āpajjati, vācāya vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.6"
msgid "atthāpatti vācāya āpajjati, kāyena vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.7"
msgid "atthāpatti kāyena āpajjati, kāyena vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.8"
msgid "atthāpatti vācāya āpajjati, vācāya vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:52.9"
msgid "Atthāpatti pasutto āpajjati, paṭibuddho vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.10"
msgid "atthāpatti paṭibuddho āpajjati, pasutto vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.11"
msgid "atthāpatti pasutto āpajjati, pasutto vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.12"
msgid "atthāpatti paṭibuddho āpajjati, paṭibuddho vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:52.13"
msgid "Atthāpatti acittako āpajjati, sacittako vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.14"
msgid "atthāpatti sacittako āpajjati, acittako vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.15"
msgid "atthāpatti acittako āpajjati, acittako vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.16"
msgid "atthāpatti sacittako āpajjati, sacittako vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:52.17"
msgid "Atthāpatti āpajjanto deseti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.18"
msgid "desento āpajjati;"
msgstr ""

msgctxt "pli-tv-pvr7:52.19"
msgid "atthāpatti āpajjanto vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.20"
msgid "vuṭṭhahanto āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:52.21"
msgid "Atthāpatti kammena āpajjati, akammena vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.22"
msgid "atthāpatti akammena āpajjati, kammena vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.23"
msgid "atthāpatti kammena āpajjati, kammena vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:52.24"
msgid "atthāpatti akammena āpajjati, akammena vuṭṭhāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc47
msgctxt "pli-tv-pvr7:53.1"
msgid "Cattāro anariyavohārā—"
msgstr ""

msgctxt "pli-tv-pvr7:53.2"
msgid "adiṭṭhe diṭṭhavāditā, assute sutavāditā, amute mutavāditā, aviññāte viññātavāditā."
msgstr ""

msgctxt "pli-tv-pvr7:53.3"
msgid "Cattāro ariyavohārā—"
msgstr ""

msgctxt "pli-tv-pvr7:53.4"
msgid "adiṭṭhe adiṭṭhavāditā, assute assutavāditā, amute amutavāditā, aviññāte aviññātavāditā."
msgstr ""

msgctxt "pli-tv-pvr7:53.5"
msgid "Aparepi cattāro anariyavohārā—"
msgstr ""

msgctxt "pli-tv-pvr7:53.6"
msgid "diṭṭhe adiṭṭhavāditā, sute assutavāditā, mute amutavāditā, viññāte aviññātavāditā."
msgstr ""

msgctxt "pli-tv-pvr7:53.7"
msgid "Cattāro ariyavohārā—"
msgstr ""

msgctxt "pli-tv-pvr7:53.8"
msgid "diṭṭhe diṭṭhavāditā, sute sutavāditā, mute mutavāditā, viññāte viññātavāditā."
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-pvr7:54.1"
msgid "Cattāro pārājikā bhikkhūnaṃ bhikkhunīhi sādhāraṇā;"
msgstr ""

msgctxt "pli-tv-pvr7:54.2"
msgid "cattāro pārājikā bhikkhunīnaṃ bhikkhūhi asādhāraṇā."
msgstr ""

msgctxt "pli-tv-pvr7:54.3"
msgid "Cattāro parikkhārā—"
msgstr ""

msgctxt "pli-tv-pvr7:54.4"
msgid "atthi parikkhāro rakkhitabbo gopetabbo mamāyitabbo paribhuñjitabbo;"
msgstr ""

msgctxt "pli-tv-pvr7:54.5"
msgid "atthi parikkhāro rakkhitabbo gopetabbo mamāyitabbo, na paribhuñjitabbo;"
msgstr ""

msgctxt "pli-tv-pvr7:54.6"
msgid "atthi parikkhāro rakkhitabbo gopetabbo, na mamāyitabbo na paribhuñjitabbo;"
msgstr ""

msgctxt "pli-tv-pvr7:54.7"
msgid "atthi parikkhāro na rakkhitabbo na gopetabbo, na mamāyitabbo na paribhuñjitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:54.8"
msgid "Atthāpatti sammukhā āpajjati, parammukhā vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:54.9"
msgid "atthāpatti parammukhā āpajjati, sammukhā vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:54.10"
msgid "atthāpatti sammukhā āpajjati, sammukhā vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:54.11"
msgid "atthāpatti parammukhā āpajjati, parammukhā vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:54.12"
msgid "Atthāpatti ajānanto āpajjati, jānanto vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:54.13"
msgid "atthāpatti jānanto āpajjati, ajānanto vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:54.14"
msgid "atthāpatti ajānanto āpajjati, ajānanto vuṭṭhāti;"
msgstr ""

msgctxt "pli-tv-pvr7:54.15"
msgid "atthāpatti jānanto āpajjati, jānanto vuṭṭhāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc49, pts-vp-pli5.126
msgctxt "pli-tv-pvr7:55.1"
msgid "Catūhākārehi āpattiṃ āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr7:55.2"
msgid "kāyena āpajjati, vācāya āpajjati, kāyena vācāya āpajjati, kammavācāya āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:55.3"
msgid "Aparehipi catūhākārehi āpattiṃ āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr7:55.4"
msgid "saṃghamajjhe, gaṇamajjhe, puggalassa santike, liṅgapātubhāvena."
msgstr ""

msgctxt "pli-tv-pvr7:55.5"
msgid "Catūhākārehi āpattiyā vuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr7:55.6"
msgid "kāyena vuṭṭhāti, vācāya vuṭṭhāti, kāyena vācāya vuṭṭhāti, kammavācāya vuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr7:55.7"
msgid "Aparehipi catūhākārehi āpattiyā vuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr7:55.8"
msgid "saṃghamajjhe, gaṇamajjhe, puggalassa santike, liṅgapātubhāvena."
msgstr ""

#. VAR: paṇṇattiyo → paññattiyo (bj)
msgctxt "pli-tv-pvr7:55.9"
msgid "Saha paṭilābhena purimaṃ jahati, pacchime patiṭṭhāti, viññattiyo paṭippassambhanti, paṇṇattiyo nirujjhanti."
msgstr ""

msgctxt "pli-tv-pvr7:55.10"
msgid "Saha paṭilābhena pacchimaṃ jahati, purime patiṭṭhāti, viññattiyo paṭippassambhanti, paṇṇattiyo nirujjhanti."
msgstr ""

msgctxt "pli-tv-pvr7:55.11"
msgid "Catasso codanā—"
msgstr ""

msgctxt "pli-tv-pvr7:55.12"
msgid "sīlavipattiyā codeti, ācāravipattiyā codeti, diṭṭhivipattiyā codeti, ājīvavipattiyā codeti."
msgstr ""

msgctxt "pli-tv-pvr7:55.13"
msgid "Cattāro parivāsā—"
msgstr ""

msgctxt "pli-tv-pvr7:55.14"
msgid "paṭicchannaparivāso, appaṭicchannaparivāso, suddhantaparivāso, samodhānaparivāso."
msgstr ""

#. VAR: Cattāro mānattā → cattāri mānattāni (bj)
msgctxt "pli-tv-pvr7:55.15"
msgid "Cattāro mānattā—"
msgstr ""

msgctxt "pli-tv-pvr7:55.16"
msgid "paṭicchannamānattaṃ, appaṭicchannamānattaṃ, pakkhamānattaṃ, samodhānamānattaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:55.17"
msgid "Cattāro mānattacārikassa bhikkhuno ratticchedā—"
msgstr ""

msgctxt "pli-tv-pvr7:55.18"
msgid "sahavāso, vippavāso, anārocanā, ūne gaṇe carati."
msgstr ""

msgctxt "pli-tv-pvr7:55.19"
msgid "Cattāro sāmukkaṃsā."
msgstr ""

msgctxt "pli-tv-pvr7:55.20"
msgid "Cattāro paṭiggahitaparibhogā—"
msgstr ""

msgctxt "pli-tv-pvr7:55.21"
msgid "yāvakālikaṃ, yāmakālikaṃ, sattāhakālikaṃ, yāvajīvikaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:55.22"
msgid "Cattāri mahāvikaṭāni—"
msgstr ""

msgctxt "pli-tv-pvr7:55.23"
msgid "gūtho, muttaṃ, chārikā, mattikā."
msgstr ""

msgctxt "pli-tv-pvr7:55.24"
msgid "Cattāri kammāni—"
msgstr ""

msgctxt "pli-tv-pvr7:55.25"
msgid "apalokanakammaṃ, ñattikammaṃ, ñattidutiyakammaṃ, ñatticatutthakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:55.26"
msgid "Aparānipi cattāri kammāni—"
msgstr ""

msgctxt "pli-tv-pvr7:55.27"
msgid "adhammena vaggakammaṃ, adhammena samaggakammaṃ, dhammena vaggakammaṃ, dhammena samaggakammaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:55.28"
msgid "Catasso vipattiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:55.29"
msgid "sīlavipatti, ācāravipatti, diṭṭhivipatti, ājīvavipatti."
msgstr ""

msgctxt "pli-tv-pvr7:55.30"
msgid "Cattāri adhikaraṇāni—"
msgstr ""

msgctxt "pli-tv-pvr7:55.31"
msgid "vivādādhikaraṇaṃ, anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ, kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:55.32"
msgid "Cattāro parisadūsanā—"
msgstr ""

msgctxt "pli-tv-pvr7:55.33"
msgid "bhikkhu dussīlo pāpadhammo parisadūsano, bhikkhunī dussīlā pāpadhammā parisadūsanā, upāsako dussīlo pāpadhammo parisadūsano, upāsikā dussīlā pāpadhammā parisadūsanā."
msgstr ""

#. VAR: parisasobhanā → parisasobhaṇā (s1-3, pts1, mr)
msgctxt "pli-tv-pvr7:55.34"
msgid "Cattāro parisasobhanā—"
msgstr ""

msgctxt "pli-tv-pvr7:55.35"
msgid "bhikkhu sīlavā kalyāṇadhammo parisasobhano, bhikkhunī sīlavatī kalyāṇadhammā parisasobhanā, upāsako sīlavā kalyāṇadhammo parisasobhano, upāsikā sīlavatī kalyāṇadhammā parisasobhanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc50, pts-vp-pli5.127
msgctxt "pli-tv-pvr7:56.1"
msgid "Atthāpatti āgantuko āpajjati, no āvāsiko;"
msgstr ""

msgctxt "pli-tv-pvr7:56.2"
msgid "atthāpatti āvāsiko āpajjati, no āgantuko;"
msgstr ""

msgctxt "pli-tv-pvr7:56.3"
msgid "atthāpatti āgantuko ceva āpajjati āvāsiko ca atthāpatti neva āgantuko āpajjati, no āvāsiko."
msgstr ""

msgctxt "pli-tv-pvr7:56.4"
msgid "Atthāpatti gamiko āpajjati, no āvāsiko;"
msgstr ""

msgctxt "pli-tv-pvr7:56.5"
msgid "atthāpatti āvāsiko āpajjati, no gamiko;"
msgstr ""

msgctxt "pli-tv-pvr7:56.6"
msgid "atthāpatti gamiko ceva āpajjati āvāsiko ca;"
msgstr ""

msgctxt "pli-tv-pvr7:56.7"
msgid "atthāpatti neva gamiko āpajjati no āvāsiko."
msgstr ""

msgctxt "pli-tv-pvr7:56.8"
msgid "Atthi vatthunānattatā no āpattinānattatā, atthi āpattinānattatā no vatthunānattatā, atthi vatthunānattatā ceva āpattinānattatā ca, atthi neva vatthunānattatā no āpattinānattatā."
msgstr ""

msgctxt "pli-tv-pvr7:56.9"
msgid "Atthi vatthusabhāgatā no āpattisabhāgatā, atthi āpattisabhāgatā no vatthusabhāgatā, atthi vatthusabhāgatā ceva āpattisabhāgatā ca atthi neva vatthusabhāgatā no āpattisabhāgatā."
msgstr ""

msgctxt "pli-tv-pvr7:56.10"
msgid "Atthāpatti upajjhāyo āpajjati no saddhivihāriko, atthāpatti saddhivihāriko āpajjati no upajjhāyo, atthāpatti upajjhāyo ceva āpajjati saddhivihāriko ca, atthāpatti neva upajjhāyo āpajjati no saddhivihāriko."
msgstr ""

msgctxt "pli-tv-pvr7:56.11"
msgid "Atthāpatti ācariyo āpajjati no antevāsiko, atthāpatti antevāsiko āpajjati no ācariyo, atthāpatti ācariyo ceva āpajjati antevāsiko ca, atthāpatti neva ācariyo āpajjati no antevāsiko."
msgstr ""

msgctxt "pli-tv-pvr7:56.12"
msgid "Cattāro paccayā anāpatti vassacchedassa—"
msgstr ""

msgctxt "pli-tv-pvr7:56.13"
msgid "saṃgho vā bhinno hoti, saṃghaṃ vā bhinditukāmā honti, jīvitantarāyo vā hoti, brahmacariyantarāyo vā hoti."
msgstr ""

msgctxt "pli-tv-pvr7:56.14"
msgid "Cattāri vacīduccaritāni—"
msgstr ""

msgctxt "pli-tv-pvr7:56.15"
msgid "musāvādo, pisuṇā vācā, pharusā vācā, samphappalāpo."
msgstr ""

msgctxt "pli-tv-pvr7:56.16"
msgid "Cattāri vacīsucaritāni—"
msgstr ""

msgctxt "pli-tv-pvr7:56.17"
msgid "saccavācā, apisuṇā vācā, saṇhā vācā, mantā bhāsā."
msgstr ""

msgctxt "pli-tv-pvr7:56.18"
msgid "Atthi ādiyanto garukaṃ āpattiṃ āpajjati, payojento lahukaṃ;"
msgstr ""

msgctxt "pli-tv-pvr7:56.19"
msgid "atthi ādiyanto lahukaṃ āpattiṃ āpajjati, payojento garukaṃ, atthi ādiyantopi payojentopi garukaṃ āpattiṃ āpajjati;"
msgstr ""

msgctxt "pli-tv-pvr7:56.20"
msgid "atthi ādiyantopi payojentopi lahukaṃ āpattiṃ āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc51
msgctxt "pli-tv-pvr7:57.1"
msgid "Atthi puggalo abhivādanāraho, no paccuṭṭhānāraho;"
msgstr ""

msgctxt "pli-tv-pvr7:57.2"
msgid "atthi puggalo paccuṭṭhānāraho, no abhivādanāraho;"
msgstr ""

msgctxt "pli-tv-pvr7:57.3"
msgid "atthi puggalo abhivādanāraho ceva paccuṭṭhānāraho ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.4"
msgid "atthi puggalo neva abhivādanāraho no paccuṭṭhānāraho."
msgstr ""

msgctxt "pli-tv-pvr7:57.5"
msgid "Atthi puggalo āsanāraho, no abhivādanāraho;"
msgstr ""

msgctxt "pli-tv-pvr7:57.6"
msgid "atthi puggalo abhivādanāraho, no āsanāraho;"
msgstr ""

msgctxt "pli-tv-pvr7:57.7"
msgid "atthi puggalo āsanāraho ceva abhivādanāraho ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.8"
msgid "atthi puggalo neva āsanāraho no abhivādanāraho."
msgstr ""

msgctxt "pli-tv-pvr7:57.9"
msgid "Atthāpatti kāle āpajjati, no vikāle;"
msgstr ""

msgctxt "pli-tv-pvr7:57.10"
msgid "atthāpatti vikāle āpajjati, no kāle;"
msgstr ""

msgctxt "pli-tv-pvr7:57.11"
msgid "atthāpatti kāle ceva āpajjati vikāle ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.12"
msgid "atthāpatti neva kāle āpajjati no vikāle."
msgstr ""

msgctxt "pli-tv-pvr7:57.13"
msgid "Atthi paṭiggahitaṃ kāle kappati, no vikāle;"
msgstr ""

msgctxt "pli-tv-pvr7:57.14"
msgid "atthi paṭiggahitaṃ vikāle kappati, no kāle;"
msgstr ""

msgctxt "pli-tv-pvr7:57.15"
msgid "atthi paṭiggahitaṃ kāle ceva kappati vikāle ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.16"
msgid "atthi paṭiggahitaṃ neva kāle kappati no vikāle."
msgstr ""

msgctxt "pli-tv-pvr7:57.17"
msgid "Atthāpatti paccantimesu janapadesu āpajjati, no majjhimesu;"
msgstr ""

msgctxt "pli-tv-pvr7:57.18"
msgid "atthāpatti majjhimesu janapadesu āpajjati, no paccantimesu;"
msgstr ""

msgctxt "pli-tv-pvr7:57.19"
msgid "atthāpatti paccantimesu ceva janapadesu āpajjati majjhimesu ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.20"
msgid "atthāpatti neva paccantimesu janapadesu āpajjati no majjhimesu."
msgstr ""

msgctxt "pli-tv-pvr7:57.21"
msgid "Atthi paccantimesu janapadesu kappati, no majjhimesu;"
msgstr ""

msgctxt "pli-tv-pvr7:57.22"
msgid "atthi majjhimesu janapadesu kappati, no paccantimesu;"
msgstr ""

msgctxt "pli-tv-pvr7:57.23"
msgid "atthi paccantimesu ceva janapadesu kappati majjhimesu ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.24"
msgid "atthi neva paccantimesu janapadesu kappati no majjhimesu."
msgstr ""

msgctxt "pli-tv-pvr7:57.25"
msgid "Atthāpatti anto āpajjati, no bahi;"
msgstr ""

msgctxt "pli-tv-pvr7:57.26"
msgid "atthāpatti bahi āpajjati, no anto;"
msgstr ""

msgctxt "pli-tv-pvr7:57.27"
msgid "atthāpatti anto ceva āpajjati bahi ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.28"
msgid "atthāpatti neva anto āpajjati no bahi."
msgstr ""

msgctxt "pli-tv-pvr7:57.29"
msgid "Atthāpatti antosīmāya āpajjati, no bahisīmāya;"
msgstr ""

msgctxt "pli-tv-pvr7:57.30"
msgid "atthāpatti bahisīmāya āpajjati, no antosīmāya;"
msgstr ""

msgctxt "pli-tv-pvr7:57.31"
msgid "atthāpatti antosīmāya ceva āpajjati bahisīmāya ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.32"
msgid "atthāpatti neva antosīmāya āpajjati no bahisīmāya."
msgstr ""

msgctxt "pli-tv-pvr7:57.33"
msgid "Atthāpatti gāme āpajjati, no araññe;"
msgstr ""

msgctxt "pli-tv-pvr7:57.34"
msgid "atthāpatti araññe āpajjati, no gāme;"
msgstr ""

msgctxt "pli-tv-pvr7:57.35"
msgid "atthāpatti gāme ceva āpajjati araññe ca;"
msgstr ""

msgctxt "pli-tv-pvr7:57.36"
msgid "atthāpatti neva gāme āpajjati no araññe."
msgstr ""

#. HTML: </p><p>
#. REF: sc52, pts-vp-pli5.128
msgctxt "pli-tv-pvr7:58.1"
msgid "Catasso codanā—"
msgstr ""

msgctxt "pli-tv-pvr7:58.2"
msgid "vatthusandassanā, āpattisandassanā, saṃvāsapaṭikkhepo, sāmīcipaṭikkhepo."
msgstr ""

msgctxt "pli-tv-pvr7:58.3"
msgid "Cattāro pubbakiccā."
msgstr ""

msgctxt "pli-tv-pvr7:58.4"
msgid "Cattāro pattakallā."
msgstr ""

msgctxt "pli-tv-pvr7:58.5"
msgid "Cattāri anaññapācittiyāni."
msgstr ""

msgctxt "pli-tv-pvr7:58.6"
msgid "Catasso bhikkhusammutiyo."
msgstr ""

msgctxt "pli-tv-pvr7:58.7"
msgid "Cattāri agatigamanāni—"
msgstr ""

msgctxt "pli-tv-pvr7:58.8"
msgid "chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.9"
msgid "Cattāri nāgatigamanāni—"
msgstr ""

msgctxt "pli-tv-pvr7:58.10"
msgid "na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.11"
msgid "Catūhaṅgehi samannāgato alajjī bhikkhu saṃghaṃ bhindati—"
msgstr ""

msgctxt "pli-tv-pvr7:58.12"
msgid "chandāgatiṃ gacchanto, dosāgatiṃ gacchanto, mohāgatiṃ gacchanto, bhayāgatiṃ gacchanto."
msgstr ""

msgctxt "pli-tv-pvr7:58.13"
msgid "Catūhaṅgehi samannāgato pesalo bhikkhu bhinnaṃ saṃghaṃ samaggaṃ karoti—"
msgstr ""

msgctxt "pli-tv-pvr7:58.14"
msgid "na chandāgatiṃ gacchanto, na dosāgatiṃ gacchanto, na mohāgatiṃ gacchanto, na bhayāgatiṃ gacchanto."
msgstr ""

msgctxt "pli-tv-pvr7:58.15"
msgid "Catūhaṅgehi samannāgatassa bhikkhuno vinayo na pucchitabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:58.16"
msgid "chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.17"
msgid "Catūhaṅgehi samannāgatena bhikkhunā vinayo na pucchitabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:58.18"
msgid "chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.19"
msgid "Catūhaṅgehi samannāgatassa bhikkhuno vinayo na vissajjetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:58.20"
msgid "chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.21"
msgid "Catūhaṅgehi samannāgatena bhikkhunā vinayo na vissajjetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:58.22"
msgid "chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.23"
msgid "Catūhaṅgehi samannāgatassa bhikkhuno anuyogo na dātabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:58.24"
msgid "chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.25"
msgid "Catūhaṅgehi samannāgatena bhikkhunā saddhiṃ vinayo na sākacchitabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:58.26"
msgid "chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr7:58.27"
msgid "Atthāpatti gilāno āpajjati, no agilāno;"
msgstr ""

msgctxt "pli-tv-pvr7:58.28"
msgid "atthāpatti agilāno āpajjati, no gilāno;"
msgstr ""

msgctxt "pli-tv-pvr7:58.29"
msgid "atthāpatti gilāno ceva āpajjati agilāno ca;"
msgstr ""

msgctxt "pli-tv-pvr7:58.30"
msgid "atthāpatti neva gilāno āpajjati no agilāno."
msgstr ""

msgctxt "pli-tv-pvr7:58.31"
msgid "Cattāri adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:58.32"
msgid "Cattāri dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc53
msgctxt "pli-tv-pvr7:59.1"
msgid "Catukkaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc54
msgctxt "pli-tv-pvr7:60.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc55
msgctxt "pli-tv-pvr7:61.1"
msgid "Sakavācāya kāyena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:61.2"
msgid "pasutto ca acittako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:61.3"
msgid "Āpajjanto ca kammena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:61.4"
msgid "vohārā caturo tathā."
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-pvr7:62.1"
msgid "Bhikkhūnaṃ bhikkhunīnañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:62.2"
msgid "parikkhāro ca sammukhā;"
msgstr ""

#. HTML: <br>
#. VAR: Ajānakāye → ajānakāle (bj)
msgctxt "pli-tv-pvr7:62.3"
msgid "Ajānakāye majjhe ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:62.4"
msgid "vuṭṭhāti duvidhā tathā."
msgstr ""

#. HTML: </p><p>
#. REF: sc57
#. VAR: Paṭilābhena → paṭilābhā (bj, s1-3)
msgctxt "pli-tv-pvr7:63.1"
msgid "Paṭilābhena codanā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:63.2"
msgid "parivāsā ca vuccati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:63.3"
msgid "Mānattacārikā cāpi,"
msgstr ""

#. HTML: <br>
#. VAR: paṭiggahi → paṭiggahā (bj, s1-3)
msgctxt "pli-tv-pvr7:63.4"
msgid "sāmukkaṃsā paṭiggahi."
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-pvr7:64.1"
msgid "Mahāvikaṭakammāni,"
msgstr ""

#. HTML: <br>
#. VAR: vipattiyo → kamma ... (bj, s1-3, pts1)
msgctxt "pli-tv-pvr7:64.2"
msgid "puna kamme vipattiyo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:64.3"
msgid "Adhikaraṇā dussīlā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:64.4"
msgid "sobhanāgantukena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr7:65.1"
msgid "Gamiko vatthunānattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:65.2"
msgid "sabhāgupajjhāyena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:65.3"
msgid "Ācariyo paccayā vā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:65.4"
msgid "duccaritaṃ sucaritaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-pvr7:66.1"
msgid "Ādiyanto puggalo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:66.2"
msgid "araho āsanena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:66.3"
msgid "Kāle ca kappati ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:66.4"
msgid "paccantimesu kappati."
msgstr ""

#. HTML: </p><p>
#. REF: sc61
msgctxt "pli-tv-pvr7:67.1"
msgid "Anto anto ca sīmāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:67.2"
msgid "gāme ca codanāya ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:67.3"
msgid "Pubbakiccaṃ pattakallaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:67.4"
msgid "anaññā sammutiyo ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-pvr7:68.1"
msgid "Agati nāgati ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:68.2"
msgid "alajjī pesalena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:68.3"
msgid "Pucchitabbā duve ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:68.4"
msgid "vissajjeyyā tathā duve;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:68.5"
msgid "Anuyogo ca sākacchā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:68.6"
msgid "gilāno ṭhapanena cāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:69.1"
msgid "5. Pañcakavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc63, pts-vp-pli5.129
msgctxt "pli-tv-pvr7:70.1"
msgid "Pañca āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:70.2"
msgid "Pañca āpattikkhandhā."
msgstr ""

msgctxt "pli-tv-pvr7:70.3"
msgid "Pañca vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:70.4"
msgid "Pañca kammāni ānantarikāni."
msgstr ""

msgctxt "pli-tv-pvr7:70.5"
msgid "Pañca puggalā niyatā."
msgstr ""

msgctxt "pli-tv-pvr7:70.6"
msgid "Pañca chedanakā āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:70.7"
msgid "Pañcahākārehi āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:70.8"
msgid "Pañca āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:70.9"
msgid "Musāvādapaccayā pañcahākārehi kammaṃ na upeti—"
msgstr ""

msgctxt "pli-tv-pvr7:70.10"
msgid "sayaṃ vā kammaṃ na karoti, paraṃ vā na ajjhesati, chandaṃ vā pārisuddhiṃ vā na deti, kayiramāne kamme paṭikkosati, kate vā pana kamme adhammadiṭṭhi hoti."
msgstr ""

msgctxt "pli-tv-pvr7:70.11"
msgid "Pañcahākārehi kammaṃ upeti—"
msgstr ""

msgctxt "pli-tv-pvr7:70.12"
msgid "sayaṃ vā kammaṃ karoti, paraṃ vā ajjhesati, chandaṃ vā pārisuddhiṃ vā deti, kayiramāne kamme nappaṭikkosati, kate vā pana kamme dhammadiṭṭhi hoti."
msgstr ""

msgctxt "pli-tv-pvr7:70.13"
msgid "Pañca piṇḍapātikassa bhikkhuno kappanti—"
msgstr ""

msgctxt "pli-tv-pvr7:70.14"
msgid "anāmantacāro, gaṇabhojanaṃ, paramparabhojanaṃ, anadhiṭṭhānaṃ, avikappanā."
msgstr ""

msgctxt "pli-tv-pvr7:70.15"
msgid "Pañcahaṅgehi samannāgato bhikkhu ussaṅkitaparisaṅkito hoti—"
msgstr ""

msgctxt "pli-tv-pvr7:70.16"
msgid "pāpabhikkhupi akuppadhammopi vesiyāgocaro vā hoti, vidhavāgocaro vā hoti, thullakumārigocaro vā hoti, paṇḍakagocaro vā hoti, bhikkhunigocaro vā hoti."
msgstr ""

msgctxt "pli-tv-pvr7:70.17"
msgid "Pañca telāni—"
msgstr ""

msgctxt "pli-tv-pvr7:70.18"
msgid "tilatelaṃ, sāsapatelaṃ, madhukatelaṃ, eraṇḍakatelaṃ, vasātelaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.19"
msgid "Pañca vasāni—"
msgstr ""

msgctxt "pli-tv-pvr7:70.20"
msgid "acchavasaṃ, macchavasaṃ, susukāvasaṃ, sūkaravasaṃ, gadrabhavasaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.21"
msgid "Pañca byasanāni—"
msgstr ""

msgctxt "pli-tv-pvr7:70.22"
msgid "ñātibyasanaṃ, bhogabyasanaṃ, rogabyasanaṃ, sīlabyasanaṃ, diṭṭhibyasanaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.23"
msgid "Pañca sampadā—"
msgstr ""

msgctxt "pli-tv-pvr7:70.24"
msgid "ñātisampadā, bhogasampadā, arogasampadā, sīlasampadā, diṭṭhisampadā."
msgstr ""

msgctxt "pli-tv-pvr7:70.25"
msgid "Pañca nissayapaṭippassaddhiyo upajjhāyamhā—"
msgstr ""

msgctxt "pli-tv-pvr7:70.26"
msgid "upajjhāyo pakkanto vā hoti, vibbhanto vā, kālaṅkato vā, pakkhasaṅkanto vā, āṇattiyeva pañcamī."
msgstr ""

msgctxt "pli-tv-pvr7:70.27"
msgid "Pañca puggalā na upasampādetabbā—"
msgstr ""

msgctxt "pli-tv-pvr7:70.28"
msgid "addhānahīno, aṅgahīno, vatthuvipanno, karaṇadukkaṭako, aparipūro."
msgstr ""

msgctxt "pli-tv-pvr7:70.29"
msgid "Pañca paṃsukūlāni—"
msgstr ""

msgctxt "pli-tv-pvr7:70.30"
msgid "sosānikaṃ, pāpaṇikaṃ, undūrakkhāyikaṃ, upacikakkhāyikaṃ, aggidaḍḍhaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.31"
msgid "Aparānipi pañca paṃsukūlāni—"
msgstr ""

#. VAR: gatapaṭiyāgataṃ → bhatapaṭiyābhataṃ (mr)
msgctxt "pli-tv-pvr7:70.32"
msgid "gokhāyikaṃ, ajakkhāyikaṃ, thūpacīvaraṃ, ābhisekikaṃ, gatapaṭiyāgataṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.33"
msgid "Pañca avahārā—"
msgstr ""

msgctxt "pli-tv-pvr7:70.34"
msgid "theyyāvahāro, pasayhāvahāro, parikappāvahāro, paṭicchannāvahāro, kusāvahāro."
msgstr ""

#. VAR: lokasmiṃ → lokassa (bj, s1-3, pts1)
msgctxt "pli-tv-pvr7:70.35"
msgid "Pañca mahācorā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.36"
msgid "Pañca avissajjaniyāni."
msgstr ""

msgctxt "pli-tv-pvr7:70.37"
msgid "Pañca avebhaṅgiyāni."
msgstr ""

msgctxt "pli-tv-pvr7:70.38"
msgid "Pañcāpattiyo kāyato samuṭṭhanti, na vācato na cittato."
msgstr ""

msgctxt "pli-tv-pvr7:70.39"
msgid "Pañcāpattiyo kāyato ca vācato ca samuṭṭhanti, na cittato."
msgstr ""

msgctxt "pli-tv-pvr7:70.40"
msgid "Pañcāpattiyo desanāgāminiyo."
msgstr ""

msgctxt "pli-tv-pvr7:70.41"
msgid "Pañca saṃghā."
msgstr ""

msgctxt "pli-tv-pvr7:70.42"
msgid "Pañca pātimokkhuddesā."
msgstr ""

msgctxt "pli-tv-pvr7:70.43"
msgid "Sabbapaccantimesu janapadesu vinayadharapañcamena gaṇena upasampādetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.44"
msgid "Pañcānisaṃsā kathinatthāre."
msgstr ""

msgctxt "pli-tv-pvr7:70.45"
msgid "Pañca kammāni."
msgstr ""

msgctxt "pli-tv-pvr7:70.46"
msgid "Yāvatatiyake pañca āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:70.47"
msgid "Pañcahākārehi adinnaṃ ādiyantassa āpatti pārājikassa."
msgstr ""

msgctxt "pli-tv-pvr7:70.48"
msgid "Pañcahākārehi adinnaṃ ādiyantassa āpatti thullaccayassa."
msgstr ""

msgctxt "pli-tv-pvr7:70.49"
msgid "Pañcahākārehi adinnaṃ ādiyantassa āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr7:70.50"
msgid "Pañca akappiyāni na paribhuñjitabbāni—"
msgstr ""

msgctxt "pli-tv-pvr7:70.51"
msgid "adinnañca hoti, aviditañca hoti, akappiyañca hoti, appaṭiggahitañca hoti, akatātirittañca hoti."
msgstr ""

msgctxt "pli-tv-pvr7:70.52"
msgid "Pañca kappiyāni paribhuñjitabbāni—"
msgstr ""

msgctxt "pli-tv-pvr7:70.53"
msgid "dinnañca hoti, viditañca hoti, kappiyañca hoti, paṭiggahitañca hoti, katātirittañca hoti."
msgstr ""

msgctxt "pli-tv-pvr7:70.54"
msgid "Pañca dānāni apuññāni puññasammatāni lokasmiṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:70.55"
msgid "majjadānaṃ, samajjadānaṃ, itthidānaṃ, usabhadānaṃ, cittakammadānaṃ."
msgstr ""

#. VAR: duppaṭivinodayā → duppaṭivinodiyā (s1-3, pts1)
msgctxt "pli-tv-pvr7:70.56"
msgid "Pañca uppannā duppaṭivinodayā—"
msgstr ""

#. VAR: gamiyacittaṃ → gamikacittaṃ (^an5.160#1 [160. Duppaṭivinodayasutta])
msgctxt "pli-tv-pvr7:70.57"
msgid "uppanno rāgo duppaṭivinodayo, uppanno doso duppaṭivinodayo, uppanno moho duppaṭivinodayo, uppannaṃ paṭibhānaṃ duppaṭivinodayaṃ, uppannaṃ gamiyacittaṃ duppaṭivinodayaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:70.58"
msgid "Pañcānisaṃsā sammajjaniyā—"
msgstr ""

#. VAR: pāsādikasaṃvattanikakammaṃ → ... saṃvattanikaṃ kammaṃ (bj) | ... saṃvattaniyaṃ kammaṃ (s1, s2)
msgctxt "pli-tv-pvr7:70.59"
msgid "sakacittaṃ pasīdati, paracittaṃ pasīdati, devatā attamanā honti, pāsādikasaṃvattanikakammaṃ upacinati, kāyassa bhedā paraṃ maraṇā sugatiṃ saggaṃ lokaṃ upapajjati."
msgstr ""

msgctxt "pli-tv-pvr7:70.60"
msgid "Aparepi pañcānisaṃsā sammajjaniyā—"
msgstr ""

msgctxt "pli-tv-pvr7:70.61"
msgid "sakacittaṃ pasīdati, paracittaṃ pasīdati, devatā attamanā honti, satthusāsanaṃ kataṃ hoti, pacchimā janatā diṭṭhānugatiṃ āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc64, pts-vp-pli5.131
msgctxt "pli-tv-pvr7:71.1"
msgid "Pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

#. VAR: na uggahetvā → anuggahetvā (bj)
msgctxt "pli-tv-pvr7:71.2"
msgid "attano bhāsapariyantaṃ na uggaṇhāti, parassa bhāsapariyantaṃ na uggaṇhāti, attano bhāsapariyantaṃ na uggahetvā parassa bhāsapariyantaṃ na uggahetvā adhammena kāreti appaṭiññāya."
msgstr ""

msgctxt "pli-tv-pvr7:71.3"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.4"
msgid "attano bhāsapariyantaṃ uggaṇhāti, parassa bhāsapariyantaṃ uggaṇhāti, attano bhāsapariyantaṃ uggahetvā parassa bhāsapariyantaṃ uggahetvā dhammena kāreti paṭiññāya."
msgstr ""

msgctxt "pli-tv-pvr7:71.5"
msgid "Aparehipi pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.6"
msgid "āpattiṃ na jānāti, āpattiyā mūlaṃ na jānāti, āpattisamudayaṃ na jānāti, āpattinirodhaṃ na jānāti, āpattinirodhagāminiṃ paṭipadaṃ na jānāti."
msgstr ""

msgctxt "pli-tv-pvr7:71.7"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.8"
msgid "āpattiṃ jānāti, āpattiyā mūlaṃ jānāti, āpattisamudayaṃ jānāti, āpattinirodhaṃ jānāti, āpattinirodhagāminiṃ paṭipadaṃ jānāti."
msgstr ""

msgctxt "pli-tv-pvr7:71.9"
msgid "Aparehipi pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.10"
msgid "adhikaraṇaṃ na jānāti, adhikaraṇassa mūlaṃ na jānāti, adhikaraṇasamudayaṃ na jānāti, adhikaraṇanirodhaṃ na jānāti, adhikaraṇanirodhagāminiṃ paṭipadaṃ na jānāti."
msgstr ""

msgctxt "pli-tv-pvr7:71.11"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.12"
msgid "adhikaraṇaṃ jānāti, adhikaraṇassa mūlaṃ jānāti, adhikaraṇasamudayaṃ jānāti, adhikaraṇanirodhaṃ jānāti, adhikaraṇanirodhagāminiṃ paṭipadaṃ jānāti."
msgstr ""

msgctxt "pli-tv-pvr7:71.13"
msgid "Aparehipi pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.14"
msgid "vatthuṃ na jānāti, nidānaṃ na jānāti, paññattiṃ na jānāti, anupaññattiṃ na jānāti, anusandhivacanapathaṃ na jānāti."
msgstr ""

msgctxt "pli-tv-pvr7:71.15"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.16"
msgid "vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, anupaññattiṃ jānāti, anusandhivacanapathaṃ jānāti."
msgstr ""

msgctxt "pli-tv-pvr7:71.17"
msgid "Aparehipi pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.18"
msgid "ñattiṃ na jānāti, ñattiyā karaṇaṃ na jānāti, na pubbakusalo hoti, na aparakusalo hoti, akālaññū ca hoti."
msgstr ""

msgctxt "pli-tv-pvr7:71.19"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.20"
msgid "ñattiṃ jānāti, ñattiyā karaṇaṃ jānāti, pubbakusalo hoti, aparakusalo hoti, kālaññū ca hoti."
msgstr ""

msgctxt "pli-tv-pvr7:71.21"
msgid "Aparehipi pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.22"
msgid "āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, ācariyaparamparā kho panassa na suggahitā hoti na sumanasikatā na sūpadhāritā."
msgstr ""

msgctxt "pli-tv-pvr7:71.23"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.24"
msgid "āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānati, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, ācariyaparamparā kho panassa suggahitā hoti sumanasikatā sūpadhāritā."
msgstr ""

msgctxt "pli-tv-pvr7:71.25"
msgid "Aparehipi pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.26"
msgid "āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, ubhayāni kho panassa pātimokkhāni na vitthārena svāgatāni honti na suvibhattāni na suppavattīni na suvinicchitāni suttaso anubyañjanaso."
msgstr ""

msgctxt "pli-tv-pvr7:71.27"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.28"
msgid "āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso."
msgstr ""

msgctxt "pli-tv-pvr7:71.29"
msgid "Aparehipi pañcahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.30"
msgid "āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, adhikaraṇe ca na vinicchayakusalo hoti."
msgstr ""

msgctxt "pli-tv-pvr7:71.31"
msgid "Pañcahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:71.32"
msgid "āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, adhikaraṇe ca vinicchayakusalo hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc65
#. VAR: āraññikā → āraññakā (bj, pts1) | araññakā (s1-3)
msgctxt "pli-tv-pvr7:72.1"
msgid "Pañca āraññikā—"
msgstr ""

#. VAR: idamatthitaññeva → idamaṭṭhitaññeva (si, s1-3, pts1)
msgctxt "pli-tv-pvr7:72.2"
msgid "mandattā momūhattā āraññiko hoti, pāpiccho icchāpakato āraññiko hoti, ummādā cittakkhepā āraññiko hoti, “vaṇṇitaṃ buddhehi buddhasāvakehī”ti āraññiko hoti, api ca appicchaññeva nissāya santuṭṭhiññeva nissāya sallekhaññeva nissāya pavivekaññeva nissāya idamatthitaññeva nissāya āraññiko hoti."
msgstr ""

msgctxt "pli-tv-pvr7:72.3"
msgid "Pañca piṇḍapātikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.5"
msgid "pañca paṃsukūlikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.7"
msgid "pañca rukkhamūlikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.9"
msgid "pañca sosānikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.11"
msgid "pañca abbhokāsikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.13"
msgid "pañca tecīvarikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.15"
msgid "pañca sapadānacārikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.17"
msgid "pañca nesajjikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.19"
msgid "pañca yathāsanthatikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.21"
msgid "pañca ekāsanikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.23"
msgid "pañca khalupacchābhattikā … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:72.25"
msgid "pañca pattapiṇḍikā—"
msgstr ""

msgctxt "pli-tv-pvr7:72.26"
msgid "mandattā momūhattā pattapiṇḍiko hoti, pāpiccho icchāpakato pattapiṇḍiko hoti, ummādā cittakkhepā pattapiṇḍiko hoti, “vaṇṇitaṃ buddhehi buddhasāvakehī”ti pattapiṇḍiko hoti, api ca appicchaññeva nissāya santuṭṭhiññeva nissāya sallekhaññeva nissāya pavivekaññeva nissāya idamatthitaññeva nissāya pattapiṇḍiko hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc66, pts-vp-pli5.132
msgctxt "pli-tv-pvr7:73.1"
msgid "Pañcahaṅgehi samannāgatena bhikkhunā nānissitena vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.2"
msgid "uposathaṃ na jānāti, uposathakammaṃ na jānāti, pātimokkhaṃ na jānāti, pātimokkhuddesaṃ na jānāti, ūnapañcavasso hoti."
msgstr ""

msgctxt "pli-tv-pvr7:73.3"
msgid "Pañcahaṅgehi samannāgatena bhikkhunā anissitena vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.4"
msgid "uposathaṃ jānāti, uposathakammaṃ jānāti, pātimokkhaṃ jānāti, pātimokkhuddesaṃ jānāti, pañcavasso vā hoti atirekapañcavasso vā."
msgstr ""

msgctxt "pli-tv-pvr7:73.5"
msgid "Aparehipi pañcahaṅgehi samannāgatena bhikkhunā nānissitena vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.6"
msgid "pavāraṇaṃ na jānāti, pavāraṇākammaṃ na jānāti, pātimokkhaṃ na jānāti, pātimokkhuddesaṃ na jānāti, ūnapañcavasso hoti."
msgstr ""

msgctxt "pli-tv-pvr7:73.7"
msgid "Pañcahaṅgehi samannāgatena bhikkhunā anissitena vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.8"
msgid "pavāraṇaṃ jānāti, pavāraṇākammaṃ jānāti, pātimokkhaṃ jānāti, pātimokkhuddesaṃ jānāti, pañcavasso vā hoti atirekapañcavasso vā."
msgstr ""

msgctxt "pli-tv-pvr7:73.9"
msgid "Aparehipi pañcahaṅgehi samannāgatena bhikkhunā nānissitena vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.10"
msgid "āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, ūnapañcavasso hoti."
msgstr ""

msgctxt "pli-tv-pvr7:73.11"
msgid "Pañcahaṅgehi samannāgatena bhikkhunā anissitena vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.12"
msgid "āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, pañcavasso vā hoti atirekapañcavasso vā."
msgstr ""

msgctxt "pli-tv-pvr7:73.13"
msgid "Pañcahaṅgehi samannāgatāya bhikkhuniyā nānissitāya vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.14"
msgid "uposathaṃ na jānāti, uposathakammaṃ na jānāti, pātimokkhaṃ na jānāti, pātimokkhuddesaṃ na jānāti, ūnapañcavassā hoti."
msgstr ""

msgctxt "pli-tv-pvr7:73.15"
msgid "Pañcahaṅgehi samannāgatāya bhikkhuniyā anissitāya vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.16"
msgid "uposathaṃ jānāti, uposathakammaṃ jānāti, pātimokkhaṃ jānāti, pātimokkhuddesaṃ jānāti, pañcavassā vā hoti atirekapañcavassā vā."
msgstr ""

msgctxt "pli-tv-pvr7:73.17"
msgid "Aparehipi pañcahaṅgehi samannāgatāya bhikkhuniyā nānissitāya vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.18"
msgid "pavāraṇaṃ na jānāti, pavāraṇākammaṃ na jānāti, pātimokkhaṃ na jānāti, pātimokkhuddesaṃ na jānāti, ūnapañcavassā hoti."
msgstr ""

msgctxt "pli-tv-pvr7:73.19"
msgid "Pañcahaṅgehi samannāgatāya bhikkhuniyā anissitāya vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.20"
msgid "pavāraṇaṃ jānāti, pavāraṇākammaṃ jānāti, pātimokkhaṃ jānāti, pātimokkhuddesaṃ jānāti, pañcavassā vā hoti atirekapañcavassā vā."
msgstr ""

msgctxt "pli-tv-pvr7:73.21"
msgid "Aparehipi pañcahaṅgehi samannāgatāya bhikkhuniyā nānissitāya vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.22"
msgid "āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, ūnapañcavassā hoti."
msgstr ""

msgctxt "pli-tv-pvr7:73.23"
msgid "Pañcahaṅgehi samannāgatāya bhikkhuniyā anissitāya vatthabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:73.24"
msgid "āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, pañcavassā vā hoti atirekapañcavassā vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr7:74.1"
msgid "Pañca ādīnavā apāsādike—"
msgstr ""

#. VAR: anuviccapi → anuvicca (bj) | anuvijja pi (pts1)
msgctxt "pli-tv-pvr7:74.2"
msgid "attāpi attānaṃ upavadati, anuviccapi viññū garahanti, pāpako kittisaddo abbhuggacchati, sammūḷho kālaṃ karoti, kāyassa bhedā paraṃ maraṇā apāyaṃ duggatiṃ vinipātaṃ nirayaṃ upapajjati."
msgstr ""

msgctxt "pli-tv-pvr7:74.3"
msgid "Pañcānisaṃsā pāsādike—"
msgstr ""

msgctxt "pli-tv-pvr7:74.4"
msgid "attāpi attānaṃ na upavadati, anuviccapi viññū pasaṃsanti, kalyāṇo kittisaddo abbhuggacchati, asammūḷho kālaṃ karoti, kāyassa bhedā paraṃ maraṇā sugatiṃ saggaṃ lokaṃ upapajjati."
msgstr ""

msgctxt "pli-tv-pvr7:74.5"
msgid "Aparepi pañca ādīnavā apāsādike—"
msgstr ""

msgctxt "pli-tv-pvr7:74.6"
msgid "appasannā na pasīdanti, pasannānaṃ ekaccānaṃ aññathattaṃ hoti, satthusāsanaṃ akataṃ hoti, pacchimā janatā diṭṭhānugatiṃ nāpajjati, cittamassa na pasīdati."
msgstr ""

msgctxt "pli-tv-pvr7:74.7"
msgid "Pañcānisaṃsā pāsādike—"
msgstr ""

msgctxt "pli-tv-pvr7:74.8"
msgid "appasannā pasīdanti, pasannānaṃ bhiyyobhāvāya hoti, satthusāsanaṃ kataṃ hoti, pacchimā janatā diṭṭhānugatiṃ āpajjati, cittamassa pasīdati."
msgstr ""

#. VAR: kulūpake → kulūpage (bj) | kulupake (s1-3)
msgctxt "pli-tv-pvr7:74.9"
msgid "Pañca ādīnavā kulūpake—"
msgstr ""

#. VAR: anāmantacāre → anāmantacāro (bj) | anāmantācāre (s1-3)
msgctxt "pli-tv-pvr7:74.10"
msgid "anāmantacāre āpajjati, raho nisajjāya āpajjati, paṭicchanne āsane āpajjati, mātugāmassa uttarichappañcavācāhi dhammaṃ desento āpajjati, kāmasaṅkappabahulo ca viharati."
msgstr ""

#. VAR: kulūpakassa → kulūpagassa (bj) | kulupakassa (s2, s3)
msgctxt "pli-tv-pvr7:74.11"
msgid "Pañca ādīnavā kulūpakassa bhikkhuno—"
msgstr ""

msgctxt "pli-tv-pvr7:74.12"
msgid "ativelaṃ kulesu saṃsaṭṭhassa viharato mātugāmassa abhiṇhadassanaṃ, dassane sati saṃsaggo, saṃsagge sati vissāso, vissāse sati otāro, otiṇṇacittassetaṃ bhikkhuno pāṭikaṅkhaṃ anabhirato vā brahmacariyaṃ carissati aññataraṃ vā saṃkiliṭṭhaṃ āpattiṃ āpajjissati sikkhaṃ vā paccakkhāya hīnāyāvattissati."
msgstr ""

#. HTML: </p><p>
#. REF: sc68, pts-vp-pli5.133
#. VAR: bījajātāni → bījāni (bj)
msgctxt "pli-tv-pvr7:75.1"
msgid "Pañca bījajātāni—"
msgstr ""

#. VAR: bījabījaññeva → bījabījameva (mr)
#. VAR: phaḷubījaṃ → phalubījaṃ (bj)
msgctxt "pli-tv-pvr7:75.2"
msgid "mūlabījaṃ, khandhabījaṃ, phaḷubījaṃ, aggabījaṃ, bījabījaññeva pañcamaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:75.3"
msgid "Pañcahi samaṇakappehi phalaṃ paribhuñjitabbaṃ—"
msgstr ""

#. VAR: nibbattabījaññeva → nibbaṭṭabījaññeva (bj) | nibbaṭabījaññeva (s1-3) | nivattabījaññeva (pts1) | nippaṭṭabījaññeva (mr)
#. VAR: aggiparijitaṃ, satthaparijitaṃ, nakhaparijitaṃ → aggiparicitaṃ satthaparicitaṃ nakhaparicitaṃ (^pi-tv-kd15#23 [Khuddakavatthu])
msgctxt "pli-tv-pvr7:75.4"
msgid "aggiparijitaṃ, satthaparijitaṃ, nakhaparijitaṃ, abījaṃ, nibbattabījaññeva pañcamaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:75.5"
msgid "Pañca visuddhiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:75.6"
msgid "nidānaṃ uddisitvā avasesaṃ sutena sāvetabbaṃ, ayaṃ paṭhamā visuddhi;"
msgstr ""

msgctxt "pli-tv-pvr7:75.7"
msgid "nidānaṃ uddisitvā cattāri pārājikāni uddisitvā avasesaṃ sutena sāvetabbaṃ, ayaṃ dutiyā visuddhi;"
msgstr ""

msgctxt "pli-tv-pvr7:75.8"
msgid "nidānaṃ uddisitvā cattāri pārājikāni uddisitvā terasa saṃghādisese uddisitvā avasesaṃ sutena sāvetabbaṃ, ayaṃ tatiyā visuddhi;"
msgstr ""

msgctxt "pli-tv-pvr7:75.9"
msgid "nidānaṃ uddisitvā cattāri pārājikāni uddisitvā terasa saṃghādisese uddisitvā dve aniyate uddisitvā avasesaṃ sutena sāvetabbaṃ, ayaṃ catutthā visuddhi;"
msgstr ""

msgctxt "pli-tv-pvr7:75.10"
msgid "vitthāreneva pañcamī."
msgstr ""

msgctxt "pli-tv-pvr7:75.11"
msgid "Aparāpi pañca visuddhiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:75.12"
msgid "suttuddeso, pārisuddhiuposatho, adhiṭṭhānuposatho, pavāraṇā, sāmaggīuposathoyeva pañcamo."
msgstr ""

msgctxt "pli-tv-pvr7:75.13"
msgid "Pañcānisaṃsā vinayadhare—"
msgstr ""

msgctxt "pli-tv-pvr7:75.14"
msgid "attano sīlakkhandho sugutto hoti surakkhito, kukkuccapakatānaṃ paṭisaraṇaṃ hoti, visārado saṃghamajjhe voharati, paccatthike sahadhammena suniggahitaṃ niggaṇhāti, saddhammaṭṭhitiyā paṭipanno hoti."
msgstr ""

msgctxt "pli-tv-pvr7:75.15"
msgid "Pañca adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:75.16"
msgid "Pañca dhammikāni pātimokkhaṭṭhapanānīti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc69
msgctxt "pli-tv-pvr7:76.1"
msgid "Pañcakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc70
msgctxt "pli-tv-pvr7:77.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc71
msgctxt "pli-tv-pvr7:78.1"
msgid "Āpatti āpattikkhandhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:78.2"
msgid "vinītānantarena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:78.3"
msgid "Puggalā chedanā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:78.4"
msgid "āpajjati ca paccayā."
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-pvr7:79.1"
msgid "Na upeti upeti ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:79.2"
msgid "kappantusaṅkitelañca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:79.3"
msgid "Vasaṃ byasanā sampadā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:79.4"
msgid "passaddhi puggalena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-pvr7:80.1"
msgid "Sosānikaṃ khāyitañca,"
msgstr ""

#. HTML: <br>
#. VAR: ca vuccati → coro pavuccati (bj)
msgctxt "pli-tv-pvr7:80.2"
msgid "theyyaṃ coro ca vuccati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:80.3"
msgid "Avissajji avebhaṅgi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:80.4"
msgid "kāyato kāyavācato."
msgstr ""

#. HTML: </p><p>
#. REF: sc74
msgctxt "pli-tv-pvr7:81.1"
msgid "Desanā saṃghaṃ uddesaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:81.2"
msgid "paccantikathinena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:81.3"
msgid "Kammāni yāvatatiyaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:81.4"
msgid "pārājithulladukkaṭaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc75
msgctxt "pli-tv-pvr7:82.1"
msgid "Akappiyaṃ kappiyañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:82.2"
msgid "apuññaduvinodayā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:82.3"
msgid "Sammajjanī apare ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:82.4"
msgid "bhāsaṃ āpattimeva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-pvr7:83.1"
msgid "Adhikaraṇaṃ vatthu ñatti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:83.2"
msgid "āpattā ubhayāni ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:83.3"
msgid "Lahukaṭṭhamakā ete,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:83.4"
msgid "kaṇhasukkā vijānatha."
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-pvr7:84.1"
msgid "Araññaṃ piṇḍapātañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:84.2"
msgid "paṃsurukkhasusānikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:84.3"
msgid "Abbhokāso cīvarañca,"
msgstr ""

#. HTML: <br>
#. VAR: sapadāno nisajjiko → sapadānanisajjikā (bj) | sapādāno nesajjiko (pts1)
msgctxt "pli-tv-pvr7:84.4"
msgid "sapadāno nisajjiko."
msgstr ""

#. HTML: </p><p>
#. REF: sc78
msgctxt "pli-tv-pvr7:85.1"
msgid "Santhati khalu pacchāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:85.2"
msgid "pattapiṇḍikameva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:85.3"
msgid "Uposathaṃ pavāraṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:85.4"
msgid "āpattānāpattipi ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc79
msgctxt "pli-tv-pvr7:86.1"
msgid "Kaṇhasukkapadā ete,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:86.2"
msgid "bhikkhunīnampi te tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:86.3"
msgid "Apāsādikapāsādi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:86.4"
msgid "tatheva apare duve."
msgstr ""

#. HTML: </p><p>
#. REF: sc80
msgctxt "pli-tv-pvr7:87.1"
msgid "Kulūpake ativelaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:87.2"
msgid "bījaṃ samaṇakappi ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:87.3"
msgid "Visuddhi apare ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:87.4"
msgid "vinayādhammikena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:87.5"
msgid "Dhammikā ca tathā vuttā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:87.6"
msgid "niṭṭhitā suddhipañcakāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:88.1"
msgid "6. Chakkavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc81, pts-vp-pli5.134
msgctxt "pli-tv-pvr7:89.1"
msgid "Cha agāravā."
msgstr ""

msgctxt "pli-tv-pvr7:89.2"
msgid "Cha gāravā."
msgstr ""

msgctxt "pli-tv-pvr7:89.3"
msgid "Cha vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:89.4"
msgid "Cha sāmīciyo."
msgstr ""

msgctxt "pli-tv-pvr7:89.5"
msgid "Cha āpattisamuṭṭhānā."
msgstr ""

msgctxt "pli-tv-pvr7:89.6"
msgid "Chacchedanakā āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:89.7"
msgid "Chahākārehi āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr7:89.8"
msgid "Chānisaṃsā vinayadhare."
msgstr ""

msgctxt "pli-tv-pvr7:89.9"
msgid "Cha paramāni."
msgstr ""

msgctxt "pli-tv-pvr7:89.10"
msgid "Chārattaṃ ticīvarena vippavasitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:89.11"
msgid "Cha cīvarāni."
msgstr ""

msgctxt "pli-tv-pvr7:89.12"
msgid "Cha rajanāni."
msgstr ""

msgctxt "pli-tv-pvr7:89.13"
msgid "Cha āpattiyo kāyato ca cittato ca samuṭṭhanti na vācato."
msgstr ""

msgctxt "pli-tv-pvr7:89.14"
msgid "Cha āpattiyo vācato ca cittato ca samuṭṭhanti na kāyato."
msgstr ""

msgctxt "pli-tv-pvr7:89.15"
msgid "Cha āpattiyo kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr7:89.16"
msgid "Cha kammāni."
msgstr ""

msgctxt "pli-tv-pvr7:89.17"
msgid "Cha vivādamūlāni."
msgstr ""

msgctxt "pli-tv-pvr7:89.18"
msgid "Cha anuvādamūlāni."
msgstr ""

msgctxt "pli-tv-pvr7:89.19"
msgid "Cha sāraṇīyā dhammā dīghaso."
msgstr ""

msgctxt "pli-tv-pvr7:89.20"
msgid "Cha vidatthiyo, sugatavidatthiyā, tiriyaṃ cha vidatthiyo."
msgstr ""

msgctxt "pli-tv-pvr7:89.21"
msgid "Cha nissayapaṭippassaddhiyo ācariyamhā."
msgstr ""

msgctxt "pli-tv-pvr7:89.22"
msgid "Cha nahāne anupaññattiyo—"
msgstr ""

msgctxt "pli-tv-pvr7:89.23"
msgid "vippakatacīvaraṃ ādāya pakkamati, vippakatacīvaraṃ samādāya pakkamati."
msgstr ""

#. HTML: </p><p>
#. REF: sc82
msgctxt "pli-tv-pvr7:90.1"
msgid "Chahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:90.2"
msgid "asekkhena sīlakkhandhena samannāgato hoti, asekkhena samādhikkhandhena samannāgato hoti, asekkhena paññakkhandhena samannāgato hoti, asekkhena vimuttikkhandhena samannāgato hoti, asekkhena vimuttiñāṇadassanakkhandhena samannāgato hoti, dasavasso vā hoti atirekadasavasso vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-pvr7:91.1"
msgid "Aparehipi chahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:91.2"
msgid "attanā asekkhena sīlakkhandhena samannāgato hoti, paraṃ asekkhe sīlakkhandhe samādapetā;"
msgstr ""

msgctxt "pli-tv-pvr7:91.3"
msgid "attanā asekkhena samādhikkhandhena samannāgato hoti, paraṃ asekkhe samādhikkhandhe samādapetā;"
msgstr ""

msgctxt "pli-tv-pvr7:91.4"
msgid "attanā asekkhena paññakkhandhena samannāgato hoti, paraṃ asekkhe paññakkhandhe samādapetā;"
msgstr ""

msgctxt "pli-tv-pvr7:91.5"
msgid "attanā asekkhena vimuttikkhandhena samannāgato hoti, paraṃ asekkhe vimuttikkhandhe samādapetā;"
msgstr ""

msgctxt "pli-tv-pvr7:91.6"
msgid "attanā asekkhena vimuttiñāṇadassanakkhandhena samannāgato hoti, paraṃ asekkhe vimuttiñāṇadassanakkhandhe samādapetā;"
msgstr ""

msgctxt "pli-tv-pvr7:91.7"
msgid "dasavasso vā hoti atirekadasavasso vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc84
msgctxt "pli-tv-pvr7:92.1"
msgid "Aparehipi chahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:92.2"
msgid "saddho hoti, hirimā hoti, ottappī hoti, āraddhavīriyo hoti, upaṭṭhitassati hoti, dasavasso vā hoti atirekadasavasso vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc85
msgctxt "pli-tv-pvr7:93.1"
msgid "Aparehipi chahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:93.2"
msgid "na adhisīle sīlavipanno hoti, na ajjhācāre ācāravipanno hoti, na atidiṭṭhiyā diṭṭhivipanno hoti, bahussuto hoti, paññavā hoti, dasavasso vā hoti atirekadasavasso vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc86
msgctxt "pli-tv-pvr7:94.1"
msgid "Aparehipi chahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

#. VAR: anabhirataṃ → anabhiratiṃ (si, s1-3, mr)
#. VAR: dhammato vinodetuṃ → vinodetuṃ vā vinodāpetuṃ vā (s1-3)
msgctxt "pli-tv-pvr7:94.2"
msgid "paṭibalo hoti antevāsiṃ vā saddhivihāriṃ vā gilānaṃ upaṭṭhātuṃ vā upaṭṭhāpetuṃ vā, anabhirataṃ vūpakāsetuṃ vā vūpakāsāpetuṃ vā, uppannaṃ kukkuccaṃ dhammato vinodetuṃ, āpattiṃ jānāti, āpattivuṭṭhānaṃ jānāti, dasavasso vā hoti atirekadasavasso vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc87
msgctxt "pli-tv-pvr7:95.1"
msgid "Aparehipi chahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:95.2"
msgid "paṭibalo hoti antevāsiṃ vā saddhivihāriṃ vā ābhisamācārikāya sikkhāya sikkhāpetuṃ, ādibrahmacāriyakāya sikkhāya vinetuṃ, abhidhamme vinetuṃ, abhivinaye vinetuṃ, uppannaṃ diṭṭhigataṃ dhammato vivecetuṃ, dasavasso vā hoti atirekadasavasso vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc88
msgctxt "pli-tv-pvr7:96.1"
msgid "Aparehipi chahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo—"
msgstr ""

msgctxt "pli-tv-pvr7:96.2"
msgid "āpattiṃ jānāti, anāpattiṃ jānāti, lahukaṃ āpattiṃ jānāti, garukaṃ āpattiṃ jānāti, ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso, dasavasso vā hoti atirekadasavasso vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc89
msgctxt "pli-tv-pvr7:97.1"
msgid "Cha adhammikāni pātimokkhaṭṭhapanāni, cha dhammikāni pātimokkhaṭṭhapanānīti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc90
msgctxt "pli-tv-pvr7:98.1"
msgid "Chakkaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc91
msgctxt "pli-tv-pvr7:99.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc92
msgctxt "pli-tv-pvr7:100.1"
msgid "Agāravā gāravā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:100.2"
msgid "vinītā sāmīcipi ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:100.3"
msgid "Samuṭṭhānā chedanā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:100.4"
msgid "ākārānisaṃsena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc93
msgctxt "pli-tv-pvr7:101.1"
msgid "Paramāni ca chārattaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:101.2"
msgid "cīvaraṃ rajanāni ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:101.3"
msgid "Kāyato cittato cāpi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:101.4"
msgid "vācato cittatopi ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc94
msgctxt "pli-tv-pvr7:102.1"
msgid "Kāyavācācittato ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:102.2"
msgid "kammavivādameva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:102.3"
msgid "Anuvādā dīghaso ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:102.4"
msgid "tiriyaṃ nissayena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc95
msgctxt "pli-tv-pvr7:103.1"
msgid "Anupaññatti ādāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:103.2"
msgid "samādāya tatheva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:103.3"
msgid "Asekkhe samādapetā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:103.4"
msgid "saddho adhisīlena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:103.5"
msgid "Gilānābhisamācārī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:103.6"
msgid "āpattādhammadhammikāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:104.1"
msgid "7. Sattakavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc96, pts-vp-pli5.135
msgctxt "pli-tv-pvr7:105.1"
msgid "Sattāpattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:105.2"
msgid "Sattāpattikkhandhā."
msgstr ""

msgctxt "pli-tv-pvr7:105.3"
msgid "Satta vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:105.4"
msgid "Satta sāmīciyo."
msgstr ""

msgctxt "pli-tv-pvr7:105.5"
msgid "Satta adhammikā paṭiññātakaraṇā."
msgstr ""

msgctxt "pli-tv-pvr7:105.6"
msgid "Satta dhammikā paṭiññātakaraṇā."
msgstr ""

msgctxt "pli-tv-pvr7:105.7"
msgid "Sattannaṃ anāpatti sattāhakaraṇīyena gantuṃ."
msgstr ""

msgctxt "pli-tv-pvr7:105.8"
msgid "Sattānisaṃsā vinayadhare."
msgstr ""

msgctxt "pli-tv-pvr7:105.9"
msgid "Satta paramāni."
msgstr ""

msgctxt "pli-tv-pvr7:105.10"
msgid "Sattame aruṇuggamane nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-pvr7:105.11"
msgid "Satta samathā."
msgstr ""

msgctxt "pli-tv-pvr7:105.12"
msgid "Satta kammāni."
msgstr ""

msgctxt "pli-tv-pvr7:105.13"
msgid "Satta āmakadhaññāni."
msgstr ""

msgctxt "pli-tv-pvr7:105.14"
msgid "Tiriyaṃ sattantarā."
msgstr ""

msgctxt "pli-tv-pvr7:105.15"
msgid "Gaṇabhojane satta anupaññattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:105.16"
msgid "Bhesajjāni paṭiggahetvā sattāhaparamaṃ sannidhikārakaṃ paribhuñjitabbāni."
msgstr ""

msgctxt "pli-tv-pvr7:105.17"
msgid "Katacīvaraṃ ādāya pakkamati."
msgstr ""

msgctxt "pli-tv-pvr7:105.18"
msgid "Katacīvaraṃ samādāya pakkamati."
msgstr ""

#. VAR: daṭṭhabbā → paṭikātabbā (sabbattha)
msgctxt "pli-tv-pvr7:105.19"
msgid "Bhikkhussa na hoti āpatti daṭṭhabbā."
msgstr ""

msgctxt "pli-tv-pvr7:105.20"
msgid "Bhikkhussa hoti āpatti daṭṭhabbā."
msgstr ""

msgctxt "pli-tv-pvr7:105.21"
msgid "Bhikkhussa hoti āpatti daṭṭhabbā."
msgstr ""

msgctxt "pli-tv-pvr7:105.22"
msgid "Satta adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:105.23"
msgid "Satta dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc97
msgctxt "pli-tv-pvr7:106.1"
msgid "Sattahaṅgehi samannāgato bhikkhu vinayadharo hoti—"
msgstr ""

msgctxt "pli-tv-pvr7:106.2"
msgid "āpattiṃ jānāti, anāpattiṃ jānāti, lahukaṃ āpattiṃ jānāti, garukaṃ āpattiṃ jānāti, sīlavā hoti, pātimokkhasaṃvarasaṃvuto viharati ācāragocarasampanno aṇumattesu vajjesu bhayadassāvī samādāya sikkhati sikkhāpadesu, catunnaṃ jhānānaṃ ābhicetasikānaṃ diṭṭhadhammasukhavihārānaṃ nikāmalābhī hoti akicchalābhī akasiralābhī, āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc98
msgctxt "pli-tv-pvr7:107.1"
msgid "Aparehipi sattahaṅgehi samannāgato bhikkhu vinayadharo hoti—"
msgstr ""

#. VAR: sātthaṃ sabyañjanaṃ → satthā savyañjanā (bj)
#. VAR: dhātā → dhatā (bj, s1-3)
msgctxt "pli-tv-pvr7:107.2"
msgid "āpattiṃ jānāti, anāpattiṃ jānāti, lahukaṃ āpattiṃ jānāti, garukaṃ āpattiṃ jānāti, bahussuto hoti sutadharo sutasannicayo ye te dhammā ādikalyāṇā majjhekalyāṇā pariyosānakalyāṇā sātthaṃ sabyañjanaṃ kevalaparipuṇṇaṃ parisuddhaṃ brahmacariyaṃ abhivadanti tathārūpassa dhammā bahussutā honti dhātā vacasā paricitā manasānupekkhitā diṭṭhiyā suppaṭividdhā, catunnaṃ jhānānaṃ ābhicetasikānaṃ diṭṭhadhammasukhavihārānaṃ nikāmalābhī hoti akicchalābhī akasiralābhī, āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc99
msgctxt "pli-tv-pvr7:108.1"
msgid "Aparehipi sattahaṅgehi samannāgato bhikkhu vinayadharo hoti—"
msgstr ""

msgctxt "pli-tv-pvr7:108.2"
msgid "āpattiṃ jānāti, anāpattiṃ jānāti, lahukaṃ āpattiṃ jānāti, garukaṃ āpattiṃ jānāti, ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso, catunnaṃ jhānānaṃ ābhicetasikānaṃ diṭṭhadhammasukhavihārānaṃ nikāmalābhī hoti akicchalābhī akasiralābhī, āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc100
msgctxt "pli-tv-pvr7:109.1"
msgid "Aparehipi sattahaṅgehi samannāgato bhikkhu vinayadharo hoti—"
msgstr ""

msgctxt "pli-tv-pvr7:109.2"
msgid "āpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:109.3"
msgid "anāpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:109.4"
msgid "lahukaṃ āpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:109.5"
msgid "garukaṃ āpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:109.6"
msgid "anekavihitaṃ pubbenivāsaṃ anussarati, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:109.7"
msgid "ekampi jātiṃ dvepi jātiyo tissopi jātiyo catassopi jātiyo pañcapi jātiyo dasapi jātiyo vīsampi jātiyo tiṃsampi jātiyo cattālīsampi jātiyo paññāsampi jātiyo jātisatampi jātisahassampi jātisatasahassampi anekepi saṃvaṭṭakappe anekepi vivaṭṭakappe anekepi saṃvaṭṭavivaṭṭakappe—"
msgstr ""

msgctxt "pli-tv-pvr7:109.8"
msgid "“amutrāsiṃ evaṃnāmo evaṃgotto evaṃvaṇṇo evamāhāro evaṃsukhadukkhappaṭisaṃvedī evamāyupariyanto, so tato cuto amutra udapādiṃ;"
msgstr ""

msgctxt "pli-tv-pvr7:109.9"
msgid "tatrāpāsiṃ evaṃnāmo evaṃgotto evaṃvaṇṇo evamāhāro evaṃsukhadukkhappaṭisaṃvedī evamāyupariyanto, so tato cuto idhūpapanno”ti iti sākāraṃ sauddesaṃ anekavihitaṃ pubbenivāsaṃ anussarati;"
msgstr ""

#. VAR: atikkantamānusakena → atikkantamānussakena (mr)
msgctxt "pli-tv-pvr7:109.10"
msgid "dibbena cakkhunā visuddhena atikkantamānusakena satte passati cavamāne upapajjamāne hīne paṇīte suvaṇṇe dubbaṇṇe, sugate duggate yathākammūpage satte pajānāti—"
msgstr ""

msgctxt "pli-tv-pvr7:109.11"
msgid "“ime vata bhonto sattā kāyaduccaritena samannāgatā, vacīduccaritena samannāgatā, manoduccaritena samannāgatā, ariyānaṃ upavādakā, micchādiṭṭhikā, micchādiṭṭhikammasamādānā, te kāyassa bhedā paraṃ maraṇā apāyaṃ duggatiṃ vinipātaṃ nirayaṃ upapannā, ime vā pana bhonto sattā kāyasucaritena samannāgatā, vacīsucaritena samannāgatā, manosucaritena samannāgatā, ariyānaṃ anupavādakā, sammādiṭṭhikā, sammādiṭṭhikammasamādānā, te kāyassa bhedā paraṃ maraṇā sugatiṃ saggaṃ lokaṃ upapannā”ti iti dibbena cakkhunā visuddhena atikkantamānusakena satte passati cavamāne upapajjamāne hīne paṇīte suvaṇṇe dubbaṇṇe sugate duggate yathākammūpage satte pajānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:109.12"
msgid "āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc101, pts-vp-pli5.136
msgctxt "pli-tv-pvr7:110.1"
msgid "Sattahaṅgehi samannāgato vinayadharo sobhati—"
msgstr ""

msgctxt "pli-tv-pvr7:110.2"
msgid "āpattiṃ jānāti, anāpattiṃ jānāti, lahukaṃ āpattiṃ jānāti, garukaṃ āpattiṃ jānāti, sīlavā hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:110.4"
msgid "samādāya sikkhati sikkhāpadesu, catunnaṃ jhānānaṃ ābhicetasikānaṃ diṭṭhadhammasukhavihārānaṃ nikāmalābhī hoti akicchalābhī akasiralābhī, āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc102
msgctxt "pli-tv-pvr7:111.1"
msgid "Aparehipi sattahaṅgehi samannāgato vinayadharo."
msgstr ""

msgctxt "pli-tv-pvr7:111.2"
msgid "Sobhati—"
msgstr ""

msgctxt "pli-tv-pvr7:111.3"
msgid "āpattiṃ jānāti, anāpattiṃ jānāti, lahukaṃ āpattiṃ jānāti, garukaṃ āpattiṃ jānāti, bahussuto hoti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:111.5"
msgid "diṭṭhiyā suppaṭividdho catunnaṃ jhānānaṃ ābhicetasikānaṃ diṭṭhadhammasukhavihārānaṃ nikāmalābhī hoti akicchalābhī akasiralābhī, āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc103
msgctxt "pli-tv-pvr7:112.1"
msgid "Aparehipi sattahaṅgehi samannāgato vinayadharo sobhati—"
msgstr ""

msgctxt "pli-tv-pvr7:112.2"
msgid "āpattiṃ jānāti, anāpattiṃ jānāti, lahukaṃ āpattiṃ jānāti, garukaṃ āpattiṃ jānāti, ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso, catunnaṃ jhānānaṃ ābhicetasikānaṃ diṭṭhadhammasukhavihārānaṃ nikāmalābhī hoti akicchalābhī akasiralābhī, āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc104
msgctxt "pli-tv-pvr7:113.1"
msgid "Aparehipi sattahaṅgehi samannāgato vinayadharo sobhati—"
msgstr ""

msgctxt "pli-tv-pvr7:113.2"
msgid "āpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:113.3"
msgid "anāpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:113.4"
msgid "lahukaṃ āpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:113.5"
msgid "garukaṃ āpattiṃ jānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:113.6"
msgid "anekavihitaṃ pubbenivāsaṃ anussarati, seyyathidaṃ—"
msgstr ""

msgctxt "pli-tv-pvr7:113.7"
msgid "ekampi jātiṃ dvepi jātiyo … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:113.9"
msgid "iti sākāraṃ sauddesaṃ anekavihitaṃ pubbenivāsaṃ anussarati, dibbena cakkhunā visuddhena atikkantamānusakena satte passati cavamāne upapajjamāne hīne paṇīte suvaṇṇe dubbaṇṇe sugate duggate yathākammūpage satte pajānāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr7:113.11"
msgid "iti dibbena cakkhunā visuddhena atikkantamānusakena satte passati cavamāne upapajjamāne hīne paṇīte suvaṇṇe dubbaṇṇe sugate duggate yathākammūpage satte pajānāti;"
msgstr ""

msgctxt "pli-tv-pvr7:113.12"
msgid "āsavānañca khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

#. HTML: </p><p>
#. REF: sc105
msgctxt "pli-tv-pvr7:114.1"
msgid "Satta asaddhammā—"
msgstr ""

msgctxt "pli-tv-pvr7:114.2"
msgid "assaddho hoti, ahiriko hoti, anottappī hoti, appassuto hoti, kusīto hoti, muṭṭhassati hoti, duppañño hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc106
msgctxt "pli-tv-pvr7:115.1"
msgid "Satta saddhammā—"
msgstr ""

#. VAR: upaṭṭhitassati → upaṭṭhitasati (bj, pts1)
msgctxt "pli-tv-pvr7:115.2"
msgid "saddho hoti, hirimā hoti, ottappī hoti, bahussuto hoti, āraddhavīriyo hoti, upaṭṭhitassati hoti, paññavā hotīti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc107
msgctxt "pli-tv-pvr7:116.1"
msgid "Sattakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc108
msgctxt "pli-tv-pvr7:117.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc109
msgctxt "pli-tv-pvr7:118.1"
msgid "Āpatti āpattikkhandhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:118.2"
msgid "vinītā sāmīcipi ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:118.3"
msgid "Adhammikā dhammikā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:118.4"
msgid "anāpatti ca sattāhaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc110
msgctxt "pli-tv-pvr7:119.1"
msgid "Ānisaṃsā paramāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:119.2"
msgid "aruṇasamathena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:119.3"
msgid "Kammā āmakadhaññā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:119.4"
msgid "tiriyaṃ gaṇabhojane."
msgstr ""

#. HTML: </p><p>
#. REF: sc111
msgctxt "pli-tv-pvr7:120.1"
msgid "Sattāhaparamaṃ ādāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:120.2"
msgid "samādāya tatheva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:120.3"
msgid "Na hoti hoti hoti ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:120.4"
msgid "adhammā dhammikāni ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc112
#. VAR: Caturo → cattāro (bj, s1-3, pts1)
msgctxt "pli-tv-pvr7:121.1"
msgid "Caturo vinayadharā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:121.2"
msgid "catubhikkhū ca sobhane;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:121.3"
msgid "Satta ceva asaddhammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:121.4"
msgid "satta saddhammā desitāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:122.1"
msgid "8. Aṭṭhakavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc113, pts-vp-pli5.137
msgctxt "pli-tv-pvr7:123.1"
msgid "Aṭṭhānisaṃse sampassamānena na so bhikkhu āpattiyā adassane ukkhipitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:123.2"
msgid "Aṭṭhānisaṃse sampassamānena paresampi saddhāya sā āpatti desetabbā."
msgstr ""

msgctxt "pli-tv-pvr7:123.3"
msgid "Aṭṭha yāvatatiyakā."
msgstr ""

msgctxt "pli-tv-pvr7:123.4"
msgid "Aṭṭhahākārehi kulāni dūseti."
msgstr ""

msgctxt "pli-tv-pvr7:123.5"
msgid "Aṭṭha mātikā cīvarassa uppādāya."
msgstr ""

msgctxt "pli-tv-pvr7:123.6"
msgid "Aṭṭha mātikā kathinassa ubbhārāya."
msgstr ""

msgctxt "pli-tv-pvr7:123.7"
msgid "Aṭṭha pānāni."
msgstr ""

msgctxt "pli-tv-pvr7:123.8"
msgid "Aṭṭhahi asaddhammehi abhibhūto pariyādinnacitto devadatto āpāyiko nerayiko kappaṭṭho atekiccho."
msgstr ""

msgctxt "pli-tv-pvr7:123.9"
msgid "Aṭṭha lokadhammā."
msgstr ""

msgctxt "pli-tv-pvr7:123.10"
msgid "Aṭṭha garudhammā."
msgstr ""

msgctxt "pli-tv-pvr7:123.11"
msgid "Aṭṭha pāṭidesanīyā."
msgstr ""

msgctxt "pli-tv-pvr7:123.12"
msgid "Aṭṭhaṅgiko musāvādo."
msgstr ""

msgctxt "pli-tv-pvr7:123.13"
msgid "Aṭṭha uposathaṅgāni."
msgstr ""

msgctxt "pli-tv-pvr7:123.14"
msgid "Aṭṭha dūteyyaṅgāni."
msgstr ""

msgctxt "pli-tv-pvr7:123.15"
msgid "Aṭṭha titthiyavattāni."
msgstr ""

msgctxt "pli-tv-pvr7:123.16"
msgid "Aṭṭha acchariyā abbhutadhammā mahāsamudde."
msgstr ""

msgctxt "pli-tv-pvr7:123.17"
msgid "Aṭṭha acchariyā abbhutadhammā imasmiṃ dhammavinaye."
msgstr ""

msgctxt "pli-tv-pvr7:123.18"
msgid "Aṭṭha anatirittā."
msgstr ""

msgctxt "pli-tv-pvr7:123.19"
msgid "Aṭṭha atirittā."
msgstr ""

msgctxt "pli-tv-pvr7:123.20"
msgid "Aṭṭhame aruṇuggamane nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-pvr7:123.21"
msgid "Aṭṭha pārājikā."
msgstr ""

msgctxt "pli-tv-pvr7:123.22"
msgid "Aṭṭhamaṃ vatthuṃ paripūrentī nāsetabbā."
msgstr ""

msgctxt "pli-tv-pvr7:123.23"
msgid "Aṭṭhamaṃ vatthuṃ paripūrentiyā desitāpi adesitā hoti."
msgstr ""

msgctxt "pli-tv-pvr7:123.24"
msgid "Aṭṭhavācikā upasampadā."
msgstr ""

msgctxt "pli-tv-pvr7:123.25"
msgid "Aṭṭhannaṃ paccuṭṭhātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:123.26"
msgid "Aṭṭhannaṃ āsanaṃ dātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:123.27"
msgid "Upāsikā aṭṭha varāni yācati."
msgstr ""

#. VAR: sammannitabbo → sammanitabbo (mr)
msgctxt "pli-tv-pvr7:123.28"
msgid "Aṭṭhahaṅgehi samannāgato bhikkhu bhikkhunovādako sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:123.29"
msgid "Aṭṭhānisaṃsā vinayadhare."
msgstr ""

msgctxt "pli-tv-pvr7:123.30"
msgid "Aṭṭha paramāni."
msgstr ""

msgctxt "pli-tv-pvr7:123.31"
msgid "Tassapāpiyasikākammakatena bhikkhunā aṭṭhasu dhammesu sammā vattitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:123.32"
msgid "Aṭṭha adhammikāni pātimokkhaṭṭhapanāni aṭṭha dhammikāni pātimokkhaṭṭhapanānīti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc114
msgctxt "pli-tv-pvr7:124.1"
msgid "Aṭṭhakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc115
msgctxt "pli-tv-pvr7:125.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc116
msgctxt "pli-tv-pvr7:126.1"
msgid "Na so bhikkhu paresampi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:126.2"
msgid "yāvatatiyadūsanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:126.3"
msgid "Mātikā kathinubbhārā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:126.4"
msgid "pānā abhibhūtena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc117
msgctxt "pli-tv-pvr7:127.1"
msgid "Lokadhammā garudhammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:127.2"
msgid "pāṭidesanīyā musā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:127.3"
msgid "Uposathā ca dūtaṅgā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:127.4"
msgid "titthiyā samuddepi ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc118
msgctxt "pli-tv-pvr7:128.1"
msgid "Abbhutā anatirittaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:128.2"
msgid "atirittaṃ nissaggiyaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:128.3"
msgid "Pārājikaṭṭhamaṃ vatthu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:128.4"
msgid "adesitūpasampadā."
msgstr ""

#. HTML: </p><p>
#. REF: sc119
msgctxt "pli-tv-pvr7:129.1"
msgid "Paccuṭṭhānāsanañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:129.2"
msgid "varaṃ ovādakena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:129.3"
msgid "Ānisaṃsā paramāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:129.4"
msgid "aṭṭhadhammesu vattanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:129.5"
msgid "Adhammikā dhammikā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:129.6"
msgid "aṭṭhakā suppakāsitāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:130.1"
msgid "9. Navakavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc120, pts-vp-pli5.138
msgctxt "pli-tv-pvr7:131.1"
msgid "Nava āghātavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:131.2"
msgid "Nava āghātapaṭivinayā."
msgstr ""

msgctxt "pli-tv-pvr7:131.3"
msgid "Nava vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:131.4"
msgid "Nava paṭhamāpattikā."
msgstr ""

msgctxt "pli-tv-pvr7:131.5"
msgid "Navahi saṃgho bhijjati."
msgstr ""

msgctxt "pli-tv-pvr7:131.6"
msgid "Nava paṇītabhojanāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.7"
msgid "Navamaṃsehi dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:131.8"
msgid "Nava pātimokkhuddesā."
msgstr ""

msgctxt "pli-tv-pvr7:131.9"
msgid "Nava paramāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.10"
msgid "Nava taṇhāmūlakā dhammā."
msgstr ""

msgctxt "pli-tv-pvr7:131.11"
msgid "Nava vidhamānā."
msgstr ""

msgctxt "pli-tv-pvr7:131.12"
msgid "Nava cīvarāni adhiṭṭhātabbāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.13"
msgid "Nava cīvarāni na vikappetabbāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.14"
msgid "Dīghaso nava vidatthiyo sugatavidatthiyā."
msgstr ""

msgctxt "pli-tv-pvr7:131.15"
msgid "Nava adhammikāni dānāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.16"
msgid "Nava adhammikā paṭiggahā."
msgstr ""

msgctxt "pli-tv-pvr7:131.17"
msgid "Nava adhammikā paribhogā—"
msgstr ""

msgctxt "pli-tv-pvr7:131.18"
msgid "tīṇi dhammikāni dānāni, tayo dhammikā paṭiggahā, tayo dhammikā paribhogā."
msgstr ""

#. VAR: saññattiyo → paññattiyo (si)
msgctxt "pli-tv-pvr7:131.19"
msgid "Nava adhammikā saññattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:131.20"
msgid "Nava dhammikā saññattiyo."
msgstr ""

msgctxt "pli-tv-pvr7:131.21"
msgid "Adhammakamme dve navakāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.22"
msgid "Dhammakamme dve navakāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.23"
msgid "Nava adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:131.24"
msgid "Nava dhammikāni pātimokkhaṭṭhapanānīti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc121
msgctxt "pli-tv-pvr7:132.1"
msgid "Navakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc122
msgctxt "pli-tv-pvr7:133.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc123
msgctxt "pli-tv-pvr7:134.1"
msgid "Āghātavatthuvinayā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:134.2"
msgid "vinītā paṭhamena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:134.3"
msgid "Bhijjati ca paṇītañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:134.4"
msgid "maṃsuddesaparamāni ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc124
msgctxt "pli-tv-pvr7:135.1"
msgid "Taṇhā mānā adhiṭṭhānā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:135.2"
msgid "vikappe ca vidatthiyo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:135.3"
msgid "Dānā paṭiggahā bhogā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:135.4"
msgid "tividhā puna dhammikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc125
msgctxt "pli-tv-pvr7:136.1"
msgid "Adhammadhammasaññatti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:136.2"
msgid "duve dve navakāni ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:136.3"
msgid "Pātimokkhaṭṭhapanāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:136.4"
msgid "adhammadhammikāni cāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:137.1"
msgid "10. Dasakavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc126
msgctxt "pli-tv-pvr7:138.1"
msgid "Dasa āghātavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:138.2"
msgid "Dasa āghātapaṭivinayā."
msgstr ""

msgctxt "pli-tv-pvr7:138.3"
msgid "Dasa vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:138.4"
msgid "Dasavatthukā micchādiṭṭhi."
msgstr ""

msgctxt "pli-tv-pvr7:138.5"
msgid "Dasavatthukā sammādiṭṭhi."
msgstr ""

msgctxt "pli-tv-pvr7:138.6"
msgid "Dasa antaggāhikā diṭṭhi."
msgstr ""

msgctxt "pli-tv-pvr7:138.7"
msgid "Dasa micchattā."
msgstr ""

msgctxt "pli-tv-pvr7:138.8"
msgid "Dasa sammattā."
msgstr ""

msgctxt "pli-tv-pvr7:138.9"
msgid "Dasa akusalakammapathā."
msgstr ""

msgctxt "pli-tv-pvr7:138.10"
msgid "Dasa kusalakammapathā."
msgstr ""

msgctxt "pli-tv-pvr7:138.11"
msgid "Dasa adhammikā salākaggāhā."
msgstr ""

msgctxt "pli-tv-pvr7:138.12"
msgid "Dasa dhammikā salākaggāhā."
msgstr ""

msgctxt "pli-tv-pvr7:138.13"
msgid "Sāmaṇerānaṃ dasa sikkhāpadāni."
msgstr ""

msgctxt "pli-tv-pvr7:138.14"
msgid "Dasahaṅgehi samannāgato sāmaṇero nāsetabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc127
msgctxt "pli-tv-pvr7:139.1"
msgid "Dasahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:139.2"
msgid "attano bhāsapariyantaṃ na uggaṇhāti, parassa bhāsapariyantaṃ na uggaṇhāti, attano bhāsapariyantaṃ anuggahetvā parassa bhāsapariyantaṃ anuggahetvā adhammena kāreti, appaṭiññāya āpattiṃ na jānāti, āpattiyā mūlaṃ na jānāti, āpattisamudayaṃ na jānāti, āpattinirodhaṃ na jānāti, āpattinirodhagāminiṃ paṭipadaṃ na jānāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc128
msgctxt "pli-tv-pvr7:140.1"
msgid "Dasahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:140.2"
msgid "attano bhāsapariyantaṃ uggaṇhāti, parassa bhāsapariyantaṃ uggaṇhāti, attano bhāsapariyantaṃ uggahetvā parassa bhāsapariyantaṃ uggahetvā dhammena kāreti, paṭiññāya āpattiṃ jānāti, āpattiyā mūlaṃ jānāti, āpattisamudayaṃ jānāti, āpattinirodhaṃ jānāti, āpattinirodhagāminiṃ paṭipadaṃ jānāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc129
msgctxt "pli-tv-pvr7:141.1"
msgid "Aparehipi dasahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

#. VAR: nidānaṃ → uddānaṃ (mr)
msgctxt "pli-tv-pvr7:141.2"
msgid "adhikaraṇaṃ na jānāti, adhikaraṇassa mūlaṃ na jānāti, adhikaraṇasamudayaṃ na jānāti, adhikaraṇanirodhaṃ na jānāti, adhikaraṇanirodhagāminiṃ paṭipadaṃ na jānāti, vatthuṃ na jānāti, nidānaṃ na jānāti, paññattiṃ na jānāti, anupaññattiṃ na jānāti, anusandhivacanapathaṃ na jānāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc130
msgctxt "pli-tv-pvr7:142.1"
msgid "Dasahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:142.2"
msgid "adhikaraṇaṃ jānāti, adhikaraṇassa mūlaṃ jānāti, adhikaraṇasamudayaṃ jānāti, adhikaraṇanirodhaṃ jānāti, adhikaraṇanirodhagāminiṃ paṭipadaṃ jānāti, vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, anupaññattiṃ jānāti, anusandhivacanapathaṃ jānāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc131, pts-vp-pli5.139
msgctxt "pli-tv-pvr7:143.1"
msgid "Aparehipi dasahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:143.2"
msgid "ñattiṃ na jānāti, ñattiyā karaṇaṃ na jānāti, na pubbakusalo hoti, na aparakusalo hoti, akālaññū ca hoti, āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, ācariyaparamparā kho panassa na suggahitā hoti na sumanasikatā na sūpadhāritā."
msgstr ""

#. HTML: </p><p>
#. REF: sc132
msgctxt "pli-tv-pvr7:144.1"
msgid "Dasahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:144.2"
msgid "ñattiṃ jānāti, ñattiyā karaṇaṃ jānāti, pubbakusalo hoti, aparakusalo hoti, kālaññū ca hoti, āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, ācariyaparamparā kho panassa suggahitā hoti sumanasikatā sūpadhāritā."
msgstr ""

#. HTML: </p><p>
#. REF: sc133
msgctxt "pli-tv-pvr7:145.1"
msgid "Aparehipi dasahaṅgehi samannāgato vinayadharo “bālo” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:145.2"
msgid "āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, ubhayāni kho panassa pātimokkhāni vitthārena na svāgatāni honti na suvibhattāni na suppavattīni na suvinicchitāni suttaso anubyañjanaso, āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, adhikaraṇe ca na vinicchayakusalo hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc134
msgctxt "pli-tv-pvr7:146.1"
msgid "Dasahaṅgehi samannāgato vinayadharo “paṇḍito” tveva saṅkhaṃ gacchati—"
msgstr ""

msgctxt "pli-tv-pvr7:146.2"
msgid "āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso, āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, adhikaraṇe ca vinicchayakusalo hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc135
msgctxt "pli-tv-pvr7:147.1"
msgid "Dasahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:147.2"
msgid "Dasa atthavase paṭicca tathāgatena sāvakānaṃ sikkhāpadaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:147.3"
msgid "Dasa ādīnavā rājantepurappavesane."
msgstr ""

msgctxt "pli-tv-pvr7:147.4"
msgid "Dasa dānavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:147.5"
msgid "Dasa ratanāni."
msgstr ""

msgctxt "pli-tv-pvr7:147.6"
msgid "Dasavaggo bhikkhusaṃgho."
msgstr ""

msgctxt "pli-tv-pvr7:147.7"
msgid "Dasavaggena gaṇena upasampādetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:147.8"
msgid "Dasa paṃsukūlāni."
msgstr ""

#. VAR: cīvaradhāraṇā → cīvaradhāraṇāni (s1-3)
msgctxt "pli-tv-pvr7:147.9"
msgid "Dasa cīvaradhāraṇā."
msgstr ""

msgctxt "pli-tv-pvr7:147.10"
msgid "Dasāhaparamaṃ atirekacīvaraṃ dhāretabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr7:147.11"
msgid "Dasa sukkāni."
msgstr ""

msgctxt "pli-tv-pvr7:147.12"
msgid "Dasa itthiyo."
msgstr ""

msgctxt "pli-tv-pvr7:147.13"
msgid "Dasa bhariyāyo."
msgstr ""

msgctxt "pli-tv-pvr7:147.14"
msgid "Vesāliyā dasa vatthūni dīpenti."
msgstr ""

msgctxt "pli-tv-pvr7:147.15"
msgid "Dasa puggalā avandiyā."
msgstr ""

msgctxt "pli-tv-pvr7:147.16"
msgid "Dasa akkosavatthūni."
msgstr ""

msgctxt "pli-tv-pvr7:147.17"
msgid "Dasahākārehi pesuññaṃ upasaṃharati."
msgstr ""

msgctxt "pli-tv-pvr7:147.18"
msgid "Dasa senāsanāni."
msgstr ""

msgctxt "pli-tv-pvr7:147.19"
msgid "Dasa varāni yāciṃsu."
msgstr ""

msgctxt "pli-tv-pvr7:147.20"
msgid "Dasa adhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:147.21"
msgid "Dasa dhammikāni pātimokkhaṭṭhapanāni."
msgstr ""

msgctxt "pli-tv-pvr7:147.22"
msgid "Dasānisaṃsā yāguyā."
msgstr ""

msgctxt "pli-tv-pvr7:147.23"
msgid "Dasa maṃsā akappiyā."
msgstr ""

msgctxt "pli-tv-pvr7:147.24"
msgid "Dasa paramāni."
msgstr ""

msgctxt "pli-tv-pvr7:147.25"
msgid "Dasavassena bhikkhunā byattena paṭibalena pabbājetabbaṃ upasampādetabbaṃ nissayo dātabbo sāmaṇero upaṭṭhāpetabbo."
msgstr ""

msgctxt "pli-tv-pvr7:147.26"
msgid "Dasavassāya bhikkhuniyā byattāya paṭibalāya pabbājetabbaṃ upasampādetabbaṃ nissayo dātabbo sāmaṇerī upaṭṭhāpetabbā."
msgstr ""

msgctxt "pli-tv-pvr7:147.27"
msgid "Dasavassāya bhikkhuniyā byattāya paṭibalāya vuṭṭhāpanasammuti sāditabbā."
msgstr ""

msgctxt "pli-tv-pvr7:147.28"
msgid "Dasavassāya gihigatāya sikkhā dātabbāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc136
msgctxt "pli-tv-pvr7:148.1"
msgid "Dasakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc137
msgctxt "pli-tv-pvr7:149.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc138, pts-vp-pli5.140
#. VAR: Āghātaṃ vinayaṃ → āghātavinayā (bj, s1-3)
msgctxt "pli-tv-pvr7:150.1"
msgid "Āghātaṃ vinayaṃ vatthu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:150.2"
msgid "micchā sammā ca antagā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:150.3"
msgid "Micchattā ceva sammattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:150.4"
msgid "akusalā kusalāpi ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc139
msgctxt "pli-tv-pvr7:151.1"
msgid "Salākā adhammā dhammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:151.2"
msgid "sāmaṇerā ca nāsanā;"
msgstr ""

#. HTML: <br>
#. VAR: Bhāsādhikaraṇañceva → bhāsādhikaraṇā ceva (bj)
msgctxt "pli-tv-pvr7:151.3"
msgid "Bhāsādhikaraṇañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:151.4"
msgid "ñattilahukameva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc140
msgctxt "pli-tv-pvr7:152.1"
msgid "Lahukā garukā ete,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:152.2"
msgid "kaṇhasukkā vijānatha;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:152.3"
msgid "Ubbāhikā ca sikkhā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:152.4"
msgid "antepure ca vatthūni."
msgstr ""

#. HTML: </p><p>
#. REF: sc141
msgctxt "pli-tv-pvr7:153.1"
msgid "Ratanaṃ dasavaggo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:153.2"
msgid "tatheva upasampadā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:153.3"
msgid "Paṃsukūladhāraṇā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:153.4"
msgid "dasāhasukkaitthiyo."
msgstr ""

#. HTML: </p><p>
#. REF: sc142
msgctxt "pli-tv-pvr7:154.1"
msgid "Bhariyā dasa vatthūni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:154.2"
msgid "avandiyakkosena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:154.3"
msgid "Pesuññañceva senāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:154.4"
msgid "varāni ca adhammikā."
msgstr ""

#. HTML: </p><p>
#. REF: sc143
msgctxt "pli-tv-pvr7:155.1"
msgid "Dhammikā yāgumaṃsā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:155.2"
msgid "paramā bhikkhu bhikkhunī;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:155.3"
msgid "Vuṭṭhāpanā gihigatā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:155.4"
msgid "dasakā suppakāsitāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr7:156.1"
msgid "11. Ekādasakavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc144
msgctxt "pli-tv-pvr7:157.1"
msgid "Ekādasa puggalā anupasampannā na upasampādetabbā, upasampannā nāsetabbā."
msgstr ""

msgctxt "pli-tv-pvr7:157.2"
msgid "Ekādasa pādukā akappiyā."
msgstr ""

msgctxt "pli-tv-pvr7:157.3"
msgid "Ekādasa pattā akappiyā."
msgstr ""

msgctxt "pli-tv-pvr7:157.4"
msgid "Ekādasa cīvarāni akappiyāni."
msgstr ""

msgctxt "pli-tv-pvr7:157.5"
msgid "Ekādasa yāvatatiyakā."
msgstr ""

msgctxt "pli-tv-pvr7:157.6"
msgid "Bhikkhunīnaṃ ekādasa antarāyikā dhammā pucchitabbā."
msgstr ""

msgctxt "pli-tv-pvr7:157.7"
msgid "Ekādasa cīvarāni adhiṭṭhātabbāni."
msgstr ""

msgctxt "pli-tv-pvr7:157.8"
msgid "Ekādasa cīvarāni na vikappetabbāni."
msgstr ""

msgctxt "pli-tv-pvr7:157.9"
msgid "Ekādase aruṇuggamane nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-pvr7:157.10"
msgid "Ekādasa gaṇṭhikā kappiyā."
msgstr ""

#. VAR: vidhā → vīṭhā (bj) | vīthā (s1-3)
msgctxt "pli-tv-pvr7:157.11"
msgid "Ekādasa vidhā kappiyā."
msgstr ""

msgctxt "pli-tv-pvr7:157.12"
msgid "Ekādasa pathavī akappiyā."
msgstr ""

msgctxt "pli-tv-pvr7:157.13"
msgid "Ekādasa pathavī kappiyā."
msgstr ""

msgctxt "pli-tv-pvr7:157.14"
msgid "Ekādasa nissayapaṭippassaddhiyo."
msgstr ""

msgctxt "pli-tv-pvr7:157.15"
msgid "Ekādasa puggalā avandiyā."
msgstr ""

msgctxt "pli-tv-pvr7:157.16"
msgid "Ekādasa paramāni."
msgstr ""

msgctxt "pli-tv-pvr7:157.17"
msgid "Ekādasa varāni yāciṃsu."
msgstr ""

msgctxt "pli-tv-pvr7:157.18"
msgid "Ekādasa sīmādosā."
msgstr ""

msgctxt "pli-tv-pvr7:157.19"
msgid "Akkosakaparibhāsake puggale ekādasādīnavā pāṭikaṅkhā."
msgstr ""

msgctxt "pli-tv-pvr7:157.20"
msgid "Mettāya cetovimuttiyā āsevitāya bhāvitāya bahulīkatāya yānīkatāya vatthukatāya anuṭṭhitāya paricitāya susamāraddhāya ekādasānisaṃsā pāṭikaṅkhā."
msgstr ""

msgctxt "pli-tv-pvr7:157.21"
msgid "Sukhaṃ supati, sukhaṃ paṭibujjhati, na pāpakaṃ supinaṃ passati, manussānaṃ piyo hoti, amanussānaṃ piyo hoti, devatā rakkhanti, nāssa aggi vā visaṃ vā satthaṃ vā kamati, tuvaṭṭaṃ cittaṃ samādhiyati, mukhavaṇṇo vippasīdati, asammūḷho kālaṃ karoti, uttari appaṭivijjhanto brahmalokūpago hoti—"
msgstr ""

msgctxt "pli-tv-pvr7:157.22"
msgid "mettāya cetovimuttiyā āsevitāya bhāvitāya bahulīkatāya yānīkatāya vatthukatāya anuṭṭhitāya paricitāya susamāraddhāya ime ekādasānisaṃsā pāṭikaṅkhāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc145
msgctxt "pli-tv-pvr7:158.1"
msgid "Ekādasakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc146
msgctxt "pli-tv-pvr7:159.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc147
#. VAR: Nāsetabbā → nāsetabbo (si)
msgctxt "pli-tv-pvr7:160.1"
msgid "Nāsetabbā pādukā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:160.2"
msgid "pattā ca cīvarāni ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:160.3"
msgid "Tatiyā pucchitabbā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:160.4"
msgid "adhiṭṭhānavikappanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc148, pts-vp-pli5.141
msgctxt "pli-tv-pvr7:161.1"
msgid "Aruṇā gaṇṭhikā vidhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:161.2"
msgid "akappiyā ca kappiyaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:161.3"
msgid "Nissayāvandiyā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:161.4"
msgid "paramāni varāni ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:161.5"
msgid "Sīmādosā ca akkosā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:161.6"
msgid "mettāyekādasā katāti."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc149
msgctxt "pli-tv-pvr7:162.1"
msgid "Ekuttarikaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc150
msgctxt "pli-tv-pvr7:163.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc151
msgctxt "pli-tv-pvr7:164.1"
msgid "Ekakā ca dukā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:164.2"
msgid "tikā ca catupañcakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:164.3"
msgid "Chasattaṭṭhanavakā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:164.4"
msgid "dasa ekādasāni ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc152
msgctxt "pli-tv-pvr7:165.1"
msgid "Hitāya sabbasattānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:165.2"
msgid "ñātadhammena tādinā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:165.3"
msgid "Ekuttarikā vimalā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr7:165.4"
msgid "mahāvīrena desitāti."
msgstr ""

#. HTML: </p></blockquote><p class="endsection">
#. REF: sc153
msgctxt "pli-tv-pvr7:166.1"
msgid "Ekuttarikaṃ niṭṭhitaṃ."
msgstr ""

