msgstr ""

msgctxt "pli-tv-pvr7:4.52"
msgid "Dhammacuditako puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.53"
msgid "Niyato puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.54"
