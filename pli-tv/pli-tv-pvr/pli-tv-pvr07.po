msgid "Abhabbāpattiko puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.56"
msgid "Bhabbāpattiko puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.57"
msgid "Ukkhittako puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.58"
msgid "Anukkhittako puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.59"
msgid "Nāsitako puggalo jānitabbo."
msgstr ""

msgctxt "pli-tv-pvr7:4.60"
