#. HTML: </p><h3>
msgctxt "pli-tv-pvr4:70.1"
msgid "4.4. Vipattipaccayavāra"
msgstr ""

#. HTML: </h3><p>
#. REF: sc64
msgctxt "pli-tv-pvr4:71.1"
msgid "Sīlavipattipaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:71.2"
msgid "Sīlavipattipaccayā catasso āpattiyo āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr4:71.3"
msgid "bhikkhunī jānaṃ pārājikaṃ dhammaṃ paṭicchādeti, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr4:71.4"
msgid "vematikā paṭicchādeti, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr4:71.5"
msgid "bhikkhu saṃghādisesaṃ paṭicchādeti, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr4:71.6"
msgid "attano duṭṭhullaṃ āpattiṃ paṭicchādeti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr4:71.7"
msgid "sīlavipattipaccayā imā catasso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-pvr4:72.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:72.2"
msgid "sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr4:72.3"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr4:72.4"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr4:72.5"
msgid "Sattannaṃ āpattikkhandhānaṃ catūhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr4:72.6"
msgid "siyā pārājikāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr4:72.7"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr4:72.8"
msgid "kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr4:72.9"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:72.10"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr4:72.11"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr4:72.12"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr4:73.1"
msgid "Ācāravipattipaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:73.2"
msgid "Ācāravipattipaccayā ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr4:73.3"
msgid "Ācāravipattiṃ paṭicchādeti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr4:73.4"
msgid "ācāravipattipaccayā imaṃ ekaṃ āpattiṃ āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr4:74.1"
msgid "Sā āpatti catunnaṃ vipattīnaṃ kati vipattiyo bhajati … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:74.2"
msgid "sattannaṃ samathānaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr4:74.3"
msgid "Sā āpatti catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajati—"
msgstr ""

msgctxt "pli-tv-pvr4:74.4"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr4:74.5"
msgid "Sattannaṃ āpattikkhandhānaṃ ekena āpattikkhandhena saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr4:74.6"
msgid "dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr4:74.7"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr4:74.8"
msgid "kāyato ca vācato ca cittato ca samuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr4:74.9"
msgid "Catunnaṃ adhikaraṇānaṃ āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:74.10"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr4:74.11"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr4:74.12"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-pvr4:75.1"
msgid "Diṭṭhivipattipaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:75.2"
msgid "Diṭṭhivipattipaccayā dve āpattiyo āpajjati."
msgstr ""

#. VAR: paṭinissajjati, ñattiyā dukkaṭaṃ → dvīhi kammavācāhi dukkaṭā (s1-3, mr)
msgctxt "pli-tv-pvr4:75.3"
msgid "Pāpikāya diṭṭhiyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjati, ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr4:75.4"
msgid "kammavācāpariyosāne āpatti pācittiyassa—"
msgstr ""

msgctxt "pli-tv-pvr4:75.5"
msgid "diṭṭhivipattipaccayā imā dve āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc69, pts-vp-pli5.99
