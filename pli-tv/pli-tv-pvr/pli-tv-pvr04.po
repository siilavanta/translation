#. HTML: <section class="sutta" id="pli-tv-pvr4"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr4:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr4:2.1"
msgid "4. Antarapeyyāla"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr4:3.1"
msgid "Katipucchāvāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli5.91
msgctxt "pli-tv-pvr4:4.1"
msgid "Kati āpattiyo?"
msgstr ""

msgctxt "pli-tv-pvr4:4.2"
msgid "Kati āpattikkhandhā?"
msgstr ""

msgctxt "pli-tv-pvr4:4.3"
msgid "Kati vinītavatthūni?"
msgstr ""

msgctxt "pli-tv-pvr4:4.4"
msgid "Kati agāravā?"
msgstr ""

msgctxt "pli-tv-pvr4:4.5"
msgid "Kati gāravā?"
msgstr ""

msgctxt "pli-tv-pvr4:4.6"
msgid "Kati vinītavatthūni?"
msgstr ""

msgctxt "pli-tv-pvr4:4.7"
msgid "Kati vipattiyo?"
msgstr ""

msgctxt "pli-tv-pvr4:4.8"
msgid "Kati āpattisamuṭṭhānā?"
msgstr ""

msgctxt "pli-tv-pvr4:4.9"
msgid "Kati vivādamūlāni?"
msgstr ""

msgctxt "pli-tv-pvr4:4.10"
msgid "Kati anuvādamūlāni?"
msgstr ""

msgctxt "pli-tv-pvr4:4.11"
msgid "Kati sāraṇīyā dhammā?"
msgstr ""

msgctxt "pli-tv-pvr4:4.12"
msgid "Kati bhedakaravatthūni?"
msgstr ""

msgctxt "pli-tv-pvr4:4.13"
msgid "Kati adhikaraṇāni?"
msgstr ""

msgctxt "pli-tv-pvr4:4.14"
msgid "Kati samathā?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr4:5.1"
msgid "Pañca āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr4:5.2"
msgid "Pañca āpattikkhandhā."
msgstr ""

msgctxt "pli-tv-pvr4:5.3"
msgid "Pañca vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:5.4"
msgid "Satta āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr4:5.5"
msgid "Satta āpattikkhandhā."
msgstr ""

msgctxt "pli-tv-pvr4:5.6"
msgid "Satta vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:5.7"
msgid "Cha agāravā."
msgstr ""

msgctxt "pli-tv-pvr4:5.8"
msgid "Cha gāravā."
msgstr ""

msgctxt "pli-tv-pvr4:5.9"
msgid "Cha vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:5.10"
msgid "Catasso vipattiyo."
msgstr ""

msgctxt "pli-tv-pvr4:5.11"
msgid "Cha āpattisamuṭṭhānā."
msgstr ""

msgctxt "pli-tv-pvr4:5.12"
msgid "Cha vivādamūlāni."
msgstr ""

msgctxt "pli-tv-pvr4:5.13"
msgid "Cha anuvādamūlāni."
msgstr ""

msgctxt "pli-tv-pvr4:5.14"
msgid "Cha sāraṇīyā dhammā."
msgstr ""

msgctxt "pli-tv-pvr4:5.15"
msgid "Aṭṭhārasa bhedakaravatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:5.16"
msgid "Cattāri adhikaraṇāni."
msgstr ""

msgctxt "pli-tv-pvr4:5.17"
msgid "Satta samathā."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr4:6.1"
msgid "Tattha katamā pañca āpattiyo?"
msgstr ""

msgctxt "pli-tv-pvr4:6.2"
msgid "Pārājikāpatti, saṃghādisesāpatti, pācittiyāpatti, pāṭidesanīyāpatti, dukkaṭāpatti—"
msgstr ""

msgctxt "pli-tv-pvr4:6.3"
msgid "imā pañca āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr4:6.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr4:7.1"
msgid "Tattha katame pañca āpattikkhandhā?"
msgstr ""

msgctxt "pli-tv-pvr4:7.2"
msgid "Pārājikāpattikkhandho, saṃghādisesāpattikkhandho, pācittiyāpattikkhandho, pāṭidesanīyāpattikkhandho, dukkaṭāpattikkhandho—"
msgstr ""

msgctxt "pli-tv-pvr4:7.3"
msgid "ime pañca āpattikkhandhā."
msgstr ""

msgctxt "pli-tv-pvr4:7.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr4:8.1"
msgid "Tattha katamāni pañca vinītavatthūni?"
msgstr ""

msgctxt "pli-tv-pvr4:8.2"
msgid "Pañcahi āpattikkhandhehi ārati virati paṭivirati veramaṇī akiriyā akaraṇaṃ anajjhāpatti velāanatikkamo setughāto—"
msgstr ""

msgctxt "pli-tv-pvr4:8.3"
msgid "imāni pañca vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:8.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr4:9.1"
msgid "Tattha katamā satta āpattiyo?"
msgstr ""

msgctxt "pli-tv-pvr4:9.2"
msgid "Pārājikāpatti, saṃghādisesāpatti, thullaccayāpatti, pācittiyāpatti, pāṭidesanīyāpatti, dukkaṭāpatti, dubbhāsitāpatti—"
msgstr ""

msgctxt "pli-tv-pvr4:9.3"
msgid "imā satta āpattiyo."
msgstr ""

msgctxt "pli-tv-pvr4:9.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr4:10.1"
msgid "Tattha katame satta āpattikkhandhā?"
msgstr ""

msgctxt "pli-tv-pvr4:10.2"
msgid "Pārājikāpattikkhandho, saṃghādisesāpattikkhandho, thullaccayāpattikkhandho, pācittiyāpattikkhandho, pāṭidesanīyāpattikkhandho, dukkaṭāpattikkhandho, dubbhāsitāpattikkhandho—"
msgstr ""

msgctxt "pli-tv-pvr4:10.3"
msgid "ime satta āpattikkhandhā."
msgstr ""

msgctxt "pli-tv-pvr4:10.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr4:11.1"
msgid "Tattha katamāni satta vinītavatthūni?"
msgstr ""

msgctxt "pli-tv-pvr4:11.2"
msgid "Sattahi āpattikkhandhehi ārati virati paṭivirati veramaṇī akiriyā akaraṇaṃ anajjhāpatti velāanatikkamo setughāto—"
msgstr ""

msgctxt "pli-tv-pvr4:11.3"
msgid "imāni satta vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:11.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc9, pts-vp-pli5.92
msgctxt "pli-tv-pvr4:12.1"
msgid "Tattha katame cha agāravā?"
msgstr ""

msgctxt "pli-tv-pvr4:12.2"
msgid "Buddhe agāravo, dhamme agāravo, saṃghe agāravo, sikkhāya agāravo, appamāde agāravo, paṭisandhāre agāravo—"
msgstr ""

msgctxt "pli-tv-pvr4:12.3"
msgid "ime cha agāravā."
msgstr ""

msgctxt "pli-tv-pvr4:12.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr4:13.1"
msgid "Tattha katame cha gāravā?"
msgstr ""

msgctxt "pli-tv-pvr4:13.2"
msgid "Buddhe gāravo, dhamme gāravo, saṃghe gāravo, sikkhāya gāravo, appamāde gāravo, paṭisandhāre gāravo—"
msgstr ""

msgctxt "pli-tv-pvr4:13.3"
msgid "ime cha gāravā."
msgstr ""

msgctxt "pli-tv-pvr4:13.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr4:14.1"
msgid "Tattha katamāni cha vinītavatthūni?"
msgstr ""

msgctxt "pli-tv-pvr4:14.2"
msgid "Chahi agāravehi ārati virati paṭivirati veramaṇī akiriyā akaraṇaṃ anajjhāpatti velāanatikkamo setughāto—"
msgstr ""

msgctxt "pli-tv-pvr4:14.3"
msgid "imāni cha vinītavatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:14.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr4:15.1"
msgid "Tattha katamā catasso vipattiyo?"
msgstr ""

msgctxt "pli-tv-pvr4:15.2"
msgid "Sīlavipatti, ācāravipatti, diṭṭhivipatti, ājīvavipatti—"
msgstr ""

msgctxt "pli-tv-pvr4:15.3"
msgid "imā catasso vipattiyo."
msgstr ""

msgctxt "pli-tv-pvr4:15.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr4:16.1"
msgid "Tattha katame cha āpattisamuṭṭhānā?"
msgstr ""

msgctxt "pli-tv-pvr4:16.2"
msgid "Atthāpatti kāyato samuṭṭhāti, na vācato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr4:16.3"
msgid "atthāpatti vācato samuṭṭhāti, na kāyato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr4:16.4"
msgid "atthāpatti kāyato ca vācato ca samuṭṭhāti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr4:16.5"
msgid "atthāpatti kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr4:16.6"
msgid "atthāpatti vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr4:16.7"
msgid "atthāpatti kāyato ca vācato ca cittato ca samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr4:16.8"
msgid "ime cha āpattisamuṭṭhānā."
msgstr ""

msgctxt "pli-tv-pvr4:16.9"
msgid "(11)"
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr4:17.1"
msgid "Tattha katamāni cha vivādamūlāni?"
msgstr ""

msgctxt "pli-tv-pvr4:17.2"
msgid "Idha bhikkhu kodhano hoti upanāhī."
msgstr ""

msgctxt "pli-tv-pvr4:17.3"
msgid "Yo so bhikkhu kodhano hoti upanāhī so sattharipi agāravo viharati appatisso, dhammepi agāravo viharati appatisso, saṃghepi agāravo viharati appatisso, sikkhāyapi na paripūrakārī hoti."
msgstr ""

msgctxt "pli-tv-pvr4:17.4"
msgid "Yo so bhikkhu satthari agāravo viharati appatisso, dhamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:17.5"
msgid "saṃghe … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:17.6"
msgid "sikkhāya na paripūrakārī, so saṃghe vivādaṃ janeti."
msgstr ""

msgctxt "pli-tv-pvr4:17.7"
msgid "Yo hoti vivādo bahujanāhitāya bahujanāsukhāya bahuno janassa anatthāya ahitāya dukkhāya devamanussānaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:17.8"
msgid "Evarūpañce tumhe vivādamūlaṃ ajjhattaṃ vā bahiddhā vā samanupasseyyātha tatra tumhe tasseva pāpakassa vivādamūlassa pahānāya vāyameyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:17.9"
msgid "Evarūpañce tumhe vivādamūlaṃ ajjhattaṃ vā bahiddhā vā na samanupasseyyātha tatra tumhe tasseva pāpakassa vivādamūlassa āyatiṃ anavassavāya paṭipajjeyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:17.10"
msgid "Evametassa pāpakassa vivādamūlassa pahānaṃ hoti."
msgstr ""

msgctxt "pli-tv-pvr4:17.11"
msgid "Evametassa pāpakassa vivādamūlassa āyatiṃ anavassavo hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr4:18.1"
msgid "Puna caparaṃ bhikkhu makkhī hoti paḷāsī … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:18.2"
msgid "issukī hoti maccharī, saṭho hoti māyāvī, pāpiccho hoti micchādiṭṭhi, sandiṭṭhiparāmāsī hoti ādhānaggāhī duppaṭinissaggī."
msgstr ""

msgctxt "pli-tv-pvr4:18.3"
msgid "Yo so bhikkhu sandiṭṭhiparāmāsī hoti ādhānaggāhī duppaṭinissaggī so sattharipi agāravo viharati appatisso, dhammepi agāravo viharati appatisso, saṃghepi agāravo viharati appatisso, sikkhāyapi na paripūrakārī hoti."
msgstr ""

msgctxt "pli-tv-pvr4:18.4"
msgid "Yo so bhikkhu satthari agāravo viharati appatisso dhamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:18.5"
msgid "saṃghe … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:18.6"
msgid "sikkhāya na paripūrakārī, so saṃghe vivādaṃ janeti."
msgstr ""

msgctxt "pli-tv-pvr4:18.7"
msgid "Yo so hoti vivādo bahujanāhitāya bahujanāsukhāya bahuno janassa anatthāya ahitāya dukkhāya devamanussānaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:18.8"
msgid "Evarūpañce tumhe vivādamūlaṃ ajjhattaṃ vā bahiddhā vā samanupasseyyātha tatra tumhe tasseva pāpakassa vivādamūlassa pahānāya vāyameyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:18.9"
msgid "Evarūpañce tumhe vivādamūlaṃ ajjhattaṃ vā bahiddhā vā na samanupasseyyātha tatra tumhe tasseva pāpakassa vivādamūlassa āyatiṃ anavassavāya paṭipajjeyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:18.10"
msgid "Evametassa pāpakassa vivādamūlassa pahānaṃ hoti."
msgstr ""

msgctxt "pli-tv-pvr4:18.11"
msgid "Evametassa pāpakassa vivādamūlassa āyatiṃ anavassavo hoti."
msgstr ""

msgctxt "pli-tv-pvr4:18.12"
msgid "Imāni cha vivādamūlāni."
msgstr ""

msgctxt "pli-tv-pvr4:18.13"
msgid "(12)"
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr4:19.1"
msgid "Tattha katamāni cha anuvādamūlāni?"
msgstr ""

msgctxt "pli-tv-pvr4:19.2"
msgid "Idha bhikkhu kodhano hoti upanāhī."
msgstr ""

msgctxt "pli-tv-pvr4:19.3"
msgid "Yo so bhikkhu kodhano hoti upanāhī so sattharipi agāravo viharati appatisso, dhammepi agāravo viharati appatisso, saṃghepi agāravo viharati appatisso, sikkhāyapi na paripūrakārī hoti."
msgstr ""

msgctxt "pli-tv-pvr4:19.4"
msgid "Yo so bhikkhu satthari agāravo viharati appatisso dhamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:19.5"
msgid "saṃghe … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:19.6"
msgid "sikkhāya na paripūrakārī so saṃghe anuvādaṃ janeti."
msgstr ""

msgctxt "pli-tv-pvr4:19.7"
msgid "Yo hoti anuvādo bahujanāhitāya bahujanāsukhāya bahuno janassa anatthāya ahitāya dukkhāya devamanussānaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:19.8"
msgid "Evarūpañce tumhe anuvādamūlaṃ ajjhattaṃ vā bahiddhā vā samanupasseyyātha tatra tumhe tasseva pāpakassa anuvādamūlassa pahānāya vāyameyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:19.9"
msgid "Evarūpañce tumhe anuvādamūlaṃ ajjhattaṃ vā bahiddhā vā na samanupasseyyātha tatra tumhe tasseva pāpakassa anuvādamūlassa āyatiṃ anavassavāya paṭipajjeyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:19.10"
msgid "Evametassa pāpakassa anuvādamūlassa pahānaṃ hoti."
msgstr ""

msgctxt "pli-tv-pvr4:19.11"
msgid "Evametassa pāpakassa anuvādamūlassa āyatiṃ anavassavo hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr4:20.1"
msgid "Puna caparaṃ bhikkhu makkhī hoti palāsī … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:20.2"
msgid "issukī hoti maccharī, saṭho hoti māyāvī, pāpiccho hoti micchādiṭṭhi, sandiṭṭhiparāmāsī hoti ādhānaggāhī duppaṭinissaggī."
msgstr ""

msgctxt "pli-tv-pvr4:20.3"
msgid "Yo so bhikkhu sandiṭṭhiparāmāsī hoti ādhānaggāhī duppaṭinissaggī so sattharipi agāravo viharati appatisso, dhammepi agāravo viharati appatisso, saṃghepi agāravo viharati appatisso, sikkhāyapi na paripūrakārī hoti."
msgstr ""

msgctxt "pli-tv-pvr4:20.4"
msgid "Yo so bhikkhu satthari agāravo viharati appatisso dhamme … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:20.5"
msgid "saṃghe … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:20.6"
msgid "sikkhāya na paripūrakārī so saṃghe anuvādaṃ janeti."
msgstr ""

msgctxt "pli-tv-pvr4:20.7"
msgid "Yo hoti anuvādo bahujanāhitāya bahujanāsukhāya bahuno janassa anatthāya ahitāya dukkhāya devamanussānaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:20.8"
msgid "Evarūpañce tumhe anuvādamūlaṃ ajjhattaṃ vā bahiddhā vā samanupasseyyātha tatra tumhe tasseva pāpakassa anuvādamūlassa pahānāya vāyameyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:20.9"
msgid "Evarūpañce tumhe anuvādamūlaṃ ajjhattaṃ vā bahiddhā vā na samanupasseyyātha tatra tumhe tasseva pāpakassa anuvādamūlassa āyatiṃ anavassavāya paṭipajjeyyātha."
msgstr ""

msgctxt "pli-tv-pvr4:20.10"
msgid "Evametassa pāpakassa anuvādamūlassa pahānaṃ hoti."
msgstr ""

msgctxt "pli-tv-pvr4:20.11"
msgid "Evametassa pāpakassa anuvādamūlassa āyatiṃ anavassavo hoti."
msgstr ""

msgctxt "pli-tv-pvr4:20.12"
msgid "Imāni cha anuvādamūlāni."
msgstr ""

msgctxt "pli-tv-pvr4:20.13"
msgid "(13)"
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr4:21.1"
msgid "Tattha katame cha sāraṇīyā dhammā?"
msgstr ""

msgctxt "pli-tv-pvr4:21.2"
msgid "Idha bhikkhuno mettaṃ kāyakammaṃ paccupaṭṭhitaṃ hoti sabrahmacārīsu āvi ceva raho ca."
msgstr ""

msgctxt "pli-tv-pvr4:21.3"
msgid "Ayampi dhammo sāraṇīyo piyakaraṇo garukaraṇo saṅgahāya avivādāya sāmaggiyā ekībhāvāya saṃvattati."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-pvr4:22.1"
msgid "Puna caparaṃ bhikkhuno mettaṃ vacīkammaṃ paccupaṭṭhitaṃ hoti sabrahmacārīsu āvi ceva raho ca."
msgstr ""

msgctxt "pli-tv-pvr4:22.2"
msgid "Ayampi dhammo sāraṇīyo piyakaraṇo garukaraṇo saṅgahāya avivādāya sāmaggiyā ekībhāvāya saṃvattati."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-pvr4:23.1"
msgid "Puna caparaṃ bhikkhuno mettaṃ manokammaṃ paccupaṭṭhitaṃ hoti sabrahmacārīsu āvi ceva raho ca."
msgstr ""

msgctxt "pli-tv-pvr4:23.2"
msgid "Ayampi dhammo sāraṇīyo piyakaraṇo garukaraṇo saṅgahāya avivādāya sāmaggiyā ekībhāvāya saṃvattati."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr4:24.1"
msgid "Puna caparaṃ bhikkhu ye te lābhā dhammikā dhammaladdhā antamaso pattapariyāpannamattampi tathārūpehi lābhehi appaṭivibhattabhogī hoti sīlavantehi sabrahmacārīhi sādhāraṇabhogī."
msgstr ""

msgctxt "pli-tv-pvr4:24.2"
msgid "Ayampi dhammo sāraṇīyo piyakaraṇo garukaraṇo saṅgahāya avivādāya sāmaggiyā ekībhāvāya saṃvattati."
msgstr ""

#. HTML: </p><p>
#. REF: sc22, pts-vp-pli5.93
msgctxt "pli-tv-pvr4:25.1"
msgid "Puna caparaṃ bhikkhu yāni tāni sīlāni akhaṇḍāni acchiddāni asabalāni akammāsāni bhujissāni viññupasatthāni aparāmaṭṭhāni samādhisaṃvattanikāni, tathārūpesu sīlesu sīlasāmaññagato viharati sabrahmacārīhi āvi ceva raho ca."
msgstr ""

msgctxt "pli-tv-pvr4:25.2"
msgid "Ayampi dhammo sāraṇīyo piyakaraṇo garukaraṇo saṅgahāya avivādāya sāmaggiyā ekībhāvāya saṃvattati."
msgstr ""

#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-pvr4:26.1"
msgid "Puna caparaṃ bhikkhu yāyaṃ diṭṭhi ariyā niyyānikā niyyāti takkarassa sammā dukkhakkhayāya tathārūpāya diṭṭhiyā diṭṭhisāmaññagato viharati sabrahmacārīhi āvi ceva raho ca."
msgstr ""

msgctxt "pli-tv-pvr4:26.2"
msgid "Ayampi dhammo sāraṇīyo piyakaraṇo garukaraṇo saṅgahāya avivādāya sāmaggiyā ekībhāvāya saṃvattati."
msgstr ""

msgctxt "pli-tv-pvr4:26.3"
msgid "Ime cha sāraṇīyā dhammā."
msgstr ""

msgctxt "pli-tv-pvr4:26.4"
msgid "(14)"
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr4:27.1"
msgid "Tattha katamāni aṭṭhārasa bhedakaravatthūni?"
msgstr ""

msgctxt "pli-tv-pvr4:27.2"
msgid "Idha bhikkhu adhammaṃ “dhammo”ti dīpeti, dhammaṃ “adhammo”ti dīpeti, avinayaṃ “vinayo”ti dīpeti, vinayaṃ “avinayo”ti dīpeti, abhāsitaṃ alapitaṃ tathāgatena “bhāsitaṃ lapitaṃ tathāgatenā”ti dīpeti, bhāsitaṃ lapitaṃ tathāgatena “abhāsitaṃ alapitaṃ tathāgatenā”ti dīpeti, anāciṇṇaṃ tathāgatena “āciṇṇaṃ tathāgatenā”ti dīpeti, āciṇṇaṃ tathāgatena “anāciṇṇaṃ tathāgatenā”ti dīpeti, apaññattaṃ tathāgatena “paññattaṃ tathāgatenā”ti dīpeti, paññattaṃ tathāgatena “apaññattaṃ tathāgatenā”ti dīpeti, āpattiṃ “anāpattī”ti dīpeti, anāpattiṃ “āpattī”ti dīpeti, lahukaṃ āpattiṃ “garukā āpattī”ti dīpeti, garukaṃ āpattiṃ “lahukā āpattī”ti dīpeti, sāvasesaṃ āpattiṃ “anavasesā āpattī”ti dīpeti, anavasesaṃ āpattiṃ “sāvasesā āpattī”ti dīpeti, duṭṭhullaṃ āpattiṃ “aduṭṭhullā āpattī”ti dīpeti, aduṭṭhullaṃ āpattiṃ “duṭṭhullā āpattī”ti dīpeti."
msgstr ""

msgctxt "pli-tv-pvr4:27.3"
msgid "Imāni aṭṭhārasa bhedakaravatthūni."
msgstr ""

msgctxt "pli-tv-pvr4:27.4"
msgid "(15)"
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-pvr4:28.1"
msgid "Tattha katamāni cattāri adhikaraṇāni?"
msgstr ""

msgctxt "pli-tv-pvr4:28.2"
msgid "Vivādādhikaraṇaṃ, anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ, kiccādhikaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-pvr4:28.3"
msgid "imāni cattāri adhikaraṇāni."
msgstr ""

msgctxt "pli-tv-pvr4:28.4"
msgid "(16)"
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr4:29.1"
msgid "Tattha katame satta samathā?"
msgstr ""

msgctxt "pli-tv-pvr4:29.2"
msgid "Sammukhāvinayo, sativinayo, amūḷhavinayo, paṭiññātakaraṇaṃ, yebhuyyasikā, tassapāpiyasikā, tiṇavatthārako—"
msgstr ""

msgctxt "pli-tv-pvr4:29.3"
msgid "ime satta samathā."
msgstr ""

msgctxt "pli-tv-pvr4:29.4"
msgid "(17)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc27
msgctxt "pli-tv-pvr4:30.1"
msgid "Katipucchāvāro niṭṭhito."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc28
msgctxt "pli-tv-pvr4:31.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc29
msgctxt "pli-tv-pvr4:32.1"
msgid "Āpatti āpattikkhandhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:32.2"
msgid "vinītā sattadhā puna;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:32.3"
msgid "Vinītāgāravā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:32.4"
msgid "gāravā mūlameva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-pvr4:33.1"
msgid "Puna vinītā vipatti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:33.2"
msgid "samuṭṭhānā vivādanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:33.3"
msgid "Anuvādā sāraṇīyaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:33.4"
msgid "bhedādhikaraṇena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:33.5"
msgid "Satteva samathā vuttā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:33.6"
msgid "padā sattarasā imeti."
msgstr ""

#. HTML: </p></blockquote><h3>
