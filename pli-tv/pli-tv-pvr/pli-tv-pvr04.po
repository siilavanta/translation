msgstr ""

msgctxt "pli-tv-pvr4:78.4"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr4:78.5"
msgid "Sattannaṃ āpattikkhandhānaṃ chahi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr4:78.6"
msgid "siyā pārājikāpattikkhandhena, siyā saṃghādisesāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā pāṭidesanīyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr4:78.7"
msgid "Channaṃ āpattisamuṭṭhānānaṃ chahi samuṭṭhānehi samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr4:78.8"
msgid "siyā kāyato samuṭṭhanti, na vācato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr4:78.9"
msgid "siyā vācato samuṭṭhanti, na kāyato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr4:78.10"
msgid "siyā kāyato ca vācato ca samuṭṭhanti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr4:78.11"
msgid "siyā kāyato ca cittato ca samuṭṭhanti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr4:78.12"
msgid "siyā vācato ca cittato ca samuṭṭhanti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr4:78.13"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr4:78.14"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:78.15"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr4:78.16"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr4:78.17"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc72
msgctxt "pli-tv-pvr4:79.1"
msgid "Vipattipaccayavāro niṭṭhito catuttho."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr4:80.1"
msgid "4.5. Adhikaraṇapaccayavāra"
msgstr ""

#. HTML: </h3><p>
#. REF: sc73
msgctxt "pli-tv-pvr4:81.1"
msgid "Vivādādhikaraṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:81.2"
msgid "Vivādādhikaraṇapaccayā dve āpattiyo āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr4:81.3"
msgid "upasampannaṃ omasati, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr4:81.4"
msgid "anupasampannaṃ omasati, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr4:81.5"
msgid "vivādādhikaraṇapaccayā imā dve āpattiyo āpajjati."
