msgstr ""

msgctxt "pli-tv-pvr4:84.6"
msgid "siyā saṃghādisesāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr4:84.7"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr4:84.8"
msgid "siyā kāyato ca cittato ca samuṭṭhanti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr4:84.9"
msgid "siyā vācato ca cittato ca samuṭṭhanti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr4:84.10"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr4:84.11"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:84.12"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr4:84.13"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-pvr4:85.1"
msgid "Āpattādhikaraṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:85.2"
msgid "Āpattādhikaraṇapaccayā catasso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr4:85.3"
msgid "Bhikkhunī jānaṃ pārājikaṃ dhammaṃ paṭicchādeti, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr4:85.4"
msgid "vematikā paṭicchādeti, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr4:85.5"
msgid "bhikkhu saṃghādisesaṃ paṭicchādeti;"
msgstr ""

msgctxt "pli-tv-pvr4:85.6"
msgid "āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr4:85.7"
msgid "ācāravipattiṃ paṭicchādeti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr4:85.8"
msgid "āpattādhikaraṇapaccayā imā catasso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc78
msgctxt "pli-tv-pvr4:86.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:86.2"
msgid "sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr4:86.3"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr4:86.4"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr4:86.5"
msgid "Sattannaṃ āpattikkhandhānaṃ catūhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr4:86.6"
msgid "siyā pārājikāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr4:86.7"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr4:86.8"
msgid "kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr4:86.9"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:86.10"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr4:86.11"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr4:86.12"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc79, pts-vp-pli5.101
msgctxt "pli-tv-pvr4:87.1"
msgid "Kiccādhikaraṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:87.2"
msgid "Kiccādhikaraṇapaccayā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr4:87.3"
msgid "Ukkhittānuvattikā bhikkhunī yāvatatiyaṃ samanubhāsanāya na paṭinissajjati, ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr4:87.4"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr4:87.5"
msgid "kammavācāpariyosāne āpatti pārājikassa."
msgstr ""

msgctxt "pli-tv-pvr4:87.6"
msgid "Bhedakānuvattakā bhikkhū yāvatatiyaṃ samanubhāsanāya na paṭinissajjanti, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr4:87.7"
msgid "pāpikāya diṭṭhiyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjati, āpatti pācittiyassa—"
msgstr ""

msgctxt "pli-tv-pvr4:87.8"
msgid "kiccādhikaraṇapaccayā imā pañca āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc80
msgctxt "pli-tv-pvr4:88.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:88.2"
msgid "sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr4:88.3"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr4:88.4"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr4:88.5"
msgid "Sattannaṃ āpattikkhandhānaṃ pañcahi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr4:88.6"
msgid "siyā pārājikāpattikkhandhena, siyā saṃghādisesāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr4:88.7"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr4:88.8"
msgid "kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr4:88.9"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:88.10"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr4:88.11"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc81
msgctxt "pli-tv-pvr4:89.1"
msgid "Ṭhapetvā satta āpattiyo satta āpattikkhandhe, avasesā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr4:89.2"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr4:89.3"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr4:89.4"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr4:89.5"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr4:89.6"
msgid "Ṭhapetvā satta āpattiyo satta āpattikkhandhe avasesā āpattiyo catunnaṃ vipattīnaṃ na katamaṃ vipattiṃ bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr4:89.7"
msgid "Sattannaṃ āpattikkhandhānaṃ na katamena āpattikkhandhena saṅgahitā."
msgstr ""

msgctxt "pli-tv-pvr4:89.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ na katamena āpattisamuṭṭhānena samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr4:89.9"
msgid "Catunnaṃ adhikaraṇānaṃ na katamaṃ adhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:89.10"
msgid "Sattannaṃ samathānaṃ na katamena samathena sammanti."
msgstr ""

msgctxt "pli-tv-pvr4:89.11"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "pli-tv-pvr4:89.12"
msgid "Ṭhapetvā satta āpattiyo satta āpattikkhandhe, natthaññā āpattiyoti."
msgstr ""

msgctxt "pli-tv-pvr4:89.13"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc82
msgctxt "pli-tv-pvr4:90.1"
msgid "Adhikaraṇapaccayavāro niṭṭhito pañcamo."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc83
#. VAR: Antarapeyyālaṃ → antarapeyyālo (bj) | anantarapeyyālaṃ (si, s1-3, pts1)
msgctxt "pli-tv-pvr4:91.1"
msgid "Antarapeyyālaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc84
msgctxt "pli-tv-pvr4:92.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc85
msgctxt "pli-tv-pvr4:93.1"
msgid "Katipucchā samuṭṭhānā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:93.2"
msgid "katāpatti tatheva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:93.3"
msgid "Samuṭṭhānā vipatti ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr4:93.4"
msgid "tathādhikaraṇena cāti."
msgstr ""
