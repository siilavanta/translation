msgstr ""

msgctxt "pli-tv-pvr4:76.10"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr4:76.11"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr4:76.12"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc70
msgctxt "pli-tv-pvr4:77.1"
msgid "Ājīvavipattipaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:77.2"
msgid "Ājīvavipattipaccayā cha āpattiyo āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr4:77.3"
msgid "ājīvahetu ājīvakāraṇā pāpiccho icchāpakato asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr4:77.4"
msgid "ājīvahetu ājīvakāraṇā sañcarittaṃ samāpajjati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr4:77.5"
msgid "ājīvahetu ājīvakāraṇā “yo te vihāre vasati, so bhikkhu arahā”ti bhaṇati, paṭivijānantassa āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr4:77.6"
msgid "ājīvahetu ājīvakāraṇā bhikkhu paṇītabhojanāni attano atthāya viññāpetvā bhuñjati, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr4:77.7"
msgid "ājīvahetu ājīvakāraṇā bhikkhunī paṇītabhojanāni attano atthāya viññāpetvā bhuñjati, āpatti pāṭidesanīyassa;"
msgstr ""

msgctxt "pli-tv-pvr4:77.8"
msgid "ājīvahetu ājīvakāraṇā sūpaṃ vā odanaṃ vā agilāno attano atthāya viññāpetvā bhuñjati, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr4:77.9"
msgid "ājīvavipattipaccayā imā cha āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-pvr4:78.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti … pe …"
msgstr ""

msgctxt "pli-tv-pvr4:78.2"
msgid "sattannaṃ samathānaṃ katihi samathehi sammanti."
msgstr ""

msgctxt "pli-tv-pvr4:78.3"
