#. HTML: </p><h3>
msgctxt "pli-tv-pvr4:42.1"
msgid "4.2. Katāpattivāra"
msgstr ""

#. HTML: </h3><p>
#. REF: sc38
msgctxt "pli-tv-pvr4:43.1"
msgid "Paṭhamena āpattisamuṭṭhānena kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:43.2"
msgid "Paṭhamena āpattisamuṭṭhānena pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr4:43.3"
msgid "Bhikkhu kappiyasaññī saññācikāya kuṭiṃ karoti adesitavatthukaṃ pamāṇātikkantaṃ sārambhaṃ aparikkamanaṃ, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr4:43.4"
msgid "ekaṃ piṇḍaṃ anāgate āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr4:43.5"
msgid "tasmiṃ piṇḍe āgate āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr4:43.6"
msgid "bhikkhu kappiyasaññī vikāle bhojanaṃ bhuñjati, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr4:43.7"
msgid "bhikkhu kappiyasaññī aññātikāya bhikkhuniyā antaragharaṃ paviṭṭhāya hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā bhuñjati, āpatti pāṭidesanīyassa—"
msgstr ""

msgctxt "pli-tv-pvr4:43.8"
msgid "paṭhamena āpattisamuṭṭhānena imā pañca āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc39
msgctxt "pli-tv-pvr4:44.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr4:44.2"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr4:44.3"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr4:44.4"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr4:44.5"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr4:44.6"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr4:44.7"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr4:44.8"
msgid "Sattannaṃ āpattikkhandhānaṃ pañcahi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr4:44.9"
msgid "siyā saṃghādisesāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā pāṭidesanīyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr4:44.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr4:44.11"
msgid "kāyato samuṭṭhanti, na vācato na cittato."
msgstr ""

msgctxt "pli-tv-pvr4:44.12"
msgid "Catunnaṃ adhikaraṇānaṃ āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:44.13"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr4:44.14"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc40, pts-vp-pli5.95
msgctxt "pli-tv-pvr4:45.1"
msgid "Dutiyena āpattisamuṭṭhānena kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr4:45.2"
msgid "Dutiyena āpattisamuṭṭhānena catasso āpattiyo āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr4:45.3"
msgid "bhikkhu kappiyasaññī samādisati—"
msgstr ""

msgctxt "pli-tv-pvr4:45.4"
msgid "“kuṭiṃ me karothā”ti."
msgstr ""

msgctxt "pli-tv-pvr4:45.5"
msgid "Tassa kuṭiṃ karonti adesitavatthukaṃ pamāṇātikkantaṃ sārambhaṃ aparikkamanaṃ."
msgstr ""

msgctxt "pli-tv-pvr4:45.6"
msgid "Payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr4:45.7"
msgid "ekaṃ piṇḍaṃ anāgate āpatti thullaccayassa;"
