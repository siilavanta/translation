msgid "Anādariyaṃ paṭicca nīce āsane nisīditvā ucce āsane nisinnassa dhammaṃ desento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:262.2"
msgid "Dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:262.3"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc234
msgctxt "pli-tv-pvr1.2:263.1"
msgid "Anādariyaṃ paṭicca ṭhito nisinnassa dhammaṃ desento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:263.2"
msgid "Dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:263.3"
msgid "(10)"
msgstr ""

#. HTML: </p><p>
#. REF: sc235
msgctxt "pli-tv-pvr1.2:264.1"
msgid "Anādariyaṃ paṭicca pacchato gacchanto purato gacchantassa dhammaṃ desento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:264.2"
msgid "Dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:264.3"
msgid "(11)"
msgstr ""

#. HTML: </p><p>
#. REF: sc236
msgctxt "pli-tv-pvr1.2:265.1"
msgid "Anādariyaṃ paṭicca uppathena gacchanto pathena gacchantassa dhammaṃ desento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:265.2"
msgid "Dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:265.3"
msgid "(12)"
msgstr ""

#. HTML: </p><p>
#. REF: sc237
msgctxt "pli-tv-pvr1.2:266.1"
msgid "Anādariyaṃ paṭicca ṭhito uccāraṃ vā passāvaṃ vā karonto ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:266.2"
msgid "Dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:266.3"
msgid "(13)"
msgstr ""

#. HTML: </p><p>
#. REF: sc238
msgctxt "pli-tv-pvr1.2:267.1"
msgid "Anādariyaṃ paṭicca harite uccāraṃ vā passāvaṃ vā kheḷaṃ vā karonto ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:267.2"
msgid "Dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:267.3"
msgid "(14)"
msgstr ""

#. HTML: </p><p>
#. REF: sc239
msgctxt "pli-tv-pvr1.2:268.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karonto kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.2:268.2"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karonto ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:268.3"
msgid "Dukkaṭaṃ—"
msgstr ""

msgctxt "pli-tv-pvr1.2:268.4"
msgid "anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karonto imaṃ ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:268.5"
msgid "(15)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc240
msgctxt "pli-tv-pvr1.2:269.1"
msgid "Pādukavaggo sattamo."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.2:269.2"
msgid "Sekhiyā niṭṭhitā."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.2:269.3"
msgid "Katāpattivāro niṭṭhito dutiyo."
msgstr ""

