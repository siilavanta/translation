#. HTML: <section class="sutta" id="pli-tv-pvr1.2"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr1.2:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.2:2.1"
msgid "Bhikkhuvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.2:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr1.2:4.1"
msgid "Katāpattivāra"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr1.2:5.1"
msgid "1. Pārājikakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1
msgctxt "pli-tv-pvr1.2:6.1"
msgid "Methunaṃ dhammaṃ paṭisevanto kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.2:6.2"
msgid "Methunaṃ dhammaṃ paṭisevanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:6.3"
msgid "Akkhāyite sarīre methunaṃ dhammaṃ paṭisevati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:6.4"
msgid "yebhuyyena khāyite sarīre methunaṃ dhammaṃ paṭisevati, āpatti thullaccayassa;"
msgstr ""

#. VAR: vaṭṭakate → vivaṭakate (s1-3) | vattakate (pts1)
msgctxt "pli-tv-pvr1.2:6.5"
msgid "vaṭṭakate mukhe acchupantaṃ aṅgajātaṃ paveseti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr1.2:6.6"
msgid "methunaṃ dhammaṃ paṭisevanto imā tisso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr1.2:7.1"
msgid "Adinnaṃ ādiyanto kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.2:7.2"
msgid "Adinnaṃ ādiyanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:7.3"
msgid "Pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ adinnaṃ theyyasaṅkhātaṃ ādiyati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:7.4"
msgid "atirekamāsakaṃ vā ūnapañcamāsakaṃ vā agghanakaṃ adinnaṃ theyyasaṅkhātaṃ ādiyati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:7.5"
msgid "māsakaṃ vā ūnamāsakaṃ vā agghanakaṃ adinnaṃ theyyasaṅkhātaṃ ādiyati, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr1.2:7.6"
msgid "adinnaṃ ādiyanto imā tisso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr1.2:8.1"
msgid "Sañcicca manussaviggahaṃ jīvitā voropento kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.2:8.2"
msgid "Sañcicca manussaviggahaṃ jīvitā voropento tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:8.3"
msgid "Manussaṃ odissa opātaṃ khaṇati “papatitvā marissatī”ti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:8.4"
msgid "papatite dukkhā vedanā uppajjati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:8.5"
msgid "marati, āpatti pārājikassa—"
msgstr ""

msgctxt "pli-tv-pvr1.2:8.6"
msgid "sañcicca manussaviggahaṃ jīvitā voropento imā tisso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr1.2:9.1"
msgid "Asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapanto kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.2:9.2"
msgid "Asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:9.3"
msgid "Pāpiccho icchāpakato asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:9.4"
msgid "“yo te vihāre vasati, so bhikkhu arahā”ti bhaṇati, paṭivijānantassa āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:9.5"
msgid "na paṭivijānantassa āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr1.2:9.6"
msgid "asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapanto imā tisso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-pvr1.2:10.1"
msgid "Cattāro pārājikā niṭṭhitā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr1.2:11.1"
msgid "2. Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc6, pts-vp-pli5.34
#. VAR: () → (upakkamitvā asuciṃ mocento kati āpattiyo āpajjati) (bj, s1-3)
msgctxt "pli-tv-pvr1.2:12.1"
msgid "() Upakkamitvā asuciṃ mocento tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:12.2"
msgid "Ceteti upakkamati muccati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:12.3"
msgid "ceteti upakkamati na muccati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:12.4"
msgid "payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:12.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr1.2:13.1"
msgid "Mātugāmena saddhiṃ kāyasaṃsaggaṃ samāpajjanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:13.2"
msgid "Kāyena kāyaṃ āmasati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:13.3"
msgid "kāyena kāyapaṭibaddhaṃ āmasati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:13.4"
msgid "kāyapaṭibaddhena kāyapaṭibaddhaṃ āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:13.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr1.2:14.1"
msgid "Mātugāmaṃ duṭṭhullāhi vācāhi obhāsento tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:14.2"
msgid "Vaccamaggaṃ passāvamaggaṃ ādissa vaṇṇampi bhaṇati, avaṇṇampi bhaṇati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:14.3"
msgid "vaccamaggaṃ passāvamaggaṃ ṭhapetvā adhakkhakaṃ ubbhajāṇumaṇḍalaṃ ādissa vaṇṇampi bhaṇati avaṇṇampi bhaṇati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:14.4"
msgid "kāyapaṭibaddhaṃ ādissa vaṇṇampi bhaṇati avaṇṇampi bhaṇati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:14.5"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr1.2:15.1"
msgid "Attakāmapāricariyāya vaṇṇaṃ bhāsanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:15.2"
msgid "Mātugāmassa santike attakāmapāricariyāya vaṇṇaṃ bhāsati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:15.3"
msgid "paṇḍakassa santike attakāmapāricariyāya vaṇṇaṃ bhāsati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:15.4"
msgid "tiracchānagatassa santike attakāmapāricariyāya vaṇṇaṃ bhāsati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:15.5"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr1.2:16.1"
msgid "Sañcarittaṃ samāpajjanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:16.2"
msgid "Paṭiggaṇhāti vīmaṃsati paccāharati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:16.3"
msgid "paṭiggaṇhāti vīmaṃsati na paccāharati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:16.4"
msgid "paṭiggaṇhāti na vīmaṃsati na paccāharati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:16.5"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr1.2:17.1"
msgid "Saññācikāya kuṭiṃ kārāpento tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:17.2"
msgid "Kārāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:17.3"
msgid "ekaṃ piṇḍaṃ anāgate, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:17.4"
msgid "tasmiṃ piṇḍe āgate, āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:17.5"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr1.2:18.1"
msgid "Mahallakaṃ vihāraṃ kārāpento tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:18.2"
msgid "Kārāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:18.3"
msgid "ekaṃ piṇḍaṃ anāgate, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:18.4"
msgid "tasmiṃ piṇḍe āgate, āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:18.5"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr1.2:19.1"
msgid "Bhikkhuṃ amūlakena pārājikena dhammena anuddhaṃsento tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:19.2"
msgid "Anokāsaṃ kārāpetvā cāvanādhippāyo vadeti, āpatti saṃghādisesena dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:19.3"
msgid "okāsaṃ kārāpetvā akkosādhippāyo vadeti, āpatti omasavādassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:19.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr1.2:20.1"
msgid "Bhikkhuṃ aññabhāgiyassa adhikaraṇassa kiñci desaṃ lesamattaṃ upādāya pārājikena dhammena anuddhaṃsento tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:20.2"
msgid "Anokāsaṃ kārāpetvā cāvanādhippāyo vadeti, āpatti saṃghādisesena dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:20.3"
msgid "okāsaṃ kārāpetvā akkosādhippāyo vadeti, āpatti omasavādassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:20.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc15
#. VAR: samanubhāsanāya → samanubhāsiyamāno (bj, s1-3)
msgctxt "pli-tv-pvr1.2:21.1"
msgid "Saṃghabhedako bhikkhu yāvatatiyaṃ samanubhāsanāya na paṭinissajjanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:21.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:21.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr1.2:21.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:21.5"
msgid "(10)"
msgstr ""

#. HTML: </p><p>
#. REF: sc16, pts-vp-pli5.35
#. VAR: samanubhāsanāya → samanubhāsiyamānā (bj, s1-3)
msgctxt "pli-tv-pvr1.2:22.1"
msgid "Bhedakānuvattakā bhikkhū yāvatatiyaṃ samanubhāsanāya na paṭinissajjantā tisso āpattiyo āpajjanti."
msgstr ""

msgctxt "pli-tv-pvr1.2:22.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:22.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr1.2:22.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:22.5"
msgid "(11)"
msgstr ""

#. HTML: </p><p>
#. REF: sc17
#. VAR: samanubhāsanāya → samanubhāsiyamāno (bj, s1-3)
msgctxt "pli-tv-pvr1.2:23.1"
msgid "Dubbaco bhikkhu yāvatatiyaṃ samanubhāsanāya na paṭinissajjanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:23.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:23.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr1.2:23.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:23.5"
msgid "(12)"
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr1.2:24.1"
msgid "Kuladūsako bhikkhu yāvatatiyaṃ samanubhāsanāya na paṭinissajjanto tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:24.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:24.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr1.2:24.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:24.5"
msgid "(13)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc19
msgctxt "pli-tv-pvr1.2:25.1"
msgid "Terasa saṃghādisesā niṭṭhitā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr1.2:26.1"
msgid "4. 4 Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </h2><h3>
msgctxt "pli-tv-pvr1.2:27.1"
msgid "4.1. Kathinavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc20
msgctxt "pli-tv-pvr1.2:28.1"
msgid "Atirekacīvaraṃ dasāhaṃ atikkāmento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:28.2"
msgid "Nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:28.3"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr1.2:29.1"
msgid "Ekarattaṃ ticīvarena vippavasanto ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:29.2"
msgid "Nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:29.3"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc22
msgctxt "pli-tv-pvr1.2:30.1"
msgid "Akālacīvaraṃ paṭiggahetvā māsaṃ atikkāmento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:30.2"
msgid "Nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:30.3"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-pvr1.2:31.1"
msgid "Aññātikāya bhikkhuniyā purāṇacīvaraṃ dhovāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:31.2"
msgid "Dhovāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:31.3"
msgid "dhovāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:31.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr1.2:32.1"
msgid "Aññātikāya bhikkhuniyā hatthato cīvaraṃ paṭiggaṇhanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:32.2"
msgid "Gaṇhāti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:32.3"
msgid "gahite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:32.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-pvr1.2:33.1"
msgid "Aññātakaṃ gahapatiṃ vā gahapatāniṃ vā cīvaraṃ viññāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:33.2"
msgid "Viññāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:33.3"
msgid "viññāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:33.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr1.2:34.1"
msgid "Aññātakaṃ gahapatiṃ vā gahapatāniṃ vā tatuttari cīvaraṃ viññāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:34.2"
msgid "Viññāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:34.3"
msgid "viññāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:34.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-pvr1.2:35.1"
msgid "Pubbe appavārito aññātakaṃ gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:35.2"
msgid "Vikappaṃ āpajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:35.3"
msgid "vikappaṃ āpanne nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:35.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc28
msgctxt "pli-tv-pvr1.2:36.1"
msgid "Pubbe appavārito aññātake gahapatike upasaṅkamitvā cīvare vikappaṃ āpajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:36.2"
msgid "Vikappaṃ āpajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:36.3"
msgid "vikappaṃ āpanne nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:36.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-pvr1.2:37.1"
msgid "Atirekatikkhattuṃ codanāya atirekachakkhattuṃ ṭhānena cīvaraṃ abhinipphādento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:37.2"
msgid "Abhinipphādeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:37.3"
msgid "abhinipphādite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:37.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc30
msgctxt "pli-tv-pvr1.2:38.1"
msgid "Kathinavaggo paṭhamo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:39.1"
msgid "4.2. Kosiyavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc31
msgctxt "pli-tv-pvr1.2:40.1"
msgid "Kosiyamissakaṃ santhataṃ kārāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:40.2"
msgid "Kārāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:40.3"
msgid "kārāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:40.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-pvr1.2:41.1"
msgid "Suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:41.2"
msgid "Kārāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:41.3"
msgid "kārāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:41.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc33, pts-vp-pli5.36
msgctxt "pli-tv-pvr1.2:42.1"
msgid "Anādiyitvā tulaṃ odātānaṃ tulaṃ gocariyānaṃ navaṃ santhataṃ kārāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:42.2"
msgid "Kārāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:42.3"
msgid "kārāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:42.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-pvr1.2:43.1"
msgid "Anuvassaṃ santhataṃ kārāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:43.2"
msgid "Kārāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:43.3"
msgid "kārāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:43.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-pvr1.2:44.1"
msgid "Anādiyitvā purāṇasanthatassa sāmantā sugatavidatthiṃ navaṃ nisīdanasanthataṃ kārāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:44.2"
msgid "Kārāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:44.3"
msgid "kārāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:44.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc36
msgctxt "pli-tv-pvr1.2:45.1"
msgid "Eḷakalomāni paṭiggahetvā tiyojanaṃ atikkāmento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:45.2"
msgid "Paṭhamaṃ pādaṃ tiyojanaṃ atikkāmeti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:45.3"
msgid "dutiyaṃ pādaṃ atikkāmeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:45.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc37
msgctxt "pli-tv-pvr1.2:46.1"
msgid "Aññātikāya bhikkhuniyā eḷakalomāni dhovāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:46.2"
msgid "Dhovāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:46.3"
msgid "dhovāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:46.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc38
msgctxt "pli-tv-pvr1.2:47.1"
msgid "Rūpiyaṃ paṭiggaṇhanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:47.2"
msgid "Gaṇhāti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:47.3"
msgid "gahite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:47.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc39
msgctxt "pli-tv-pvr1.2:48.1"
msgid "Nānappakārakaṃ rūpiyasaṃvohāraṃ samāpajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:48.2"
msgid "Samāpajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:48.3"
msgid "samāpanne nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:48.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-pvr1.2:49.1"
msgid "Nānappakārakaṃ kayavikkayaṃ samāpajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:49.2"
msgid "Samāpajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:49.3"
msgid "samāpanne nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:49.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc41
msgctxt "pli-tv-pvr1.2:50.1"
msgid "Kosiyavaggo dutiyo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:51.1"
msgid "4.3. Pattavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc42
msgctxt "pli-tv-pvr1.2:52.1"
msgid "Atirekapattaṃ dasāhaṃ atikkāmento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:52.2"
msgid "Nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:52.3"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc43
msgctxt "pli-tv-pvr1.2:53.1"
msgid "Ūnapañcabandhanena pattena aññaṃ navaṃ pattaṃ cetāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:53.2"
msgid "Cetāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:53.3"
msgid "cetāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:53.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc44
msgctxt "pli-tv-pvr1.2:54.1"
msgid "Bhesajjāni paṭiggahetvā sattāhaṃ atikkāmento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:54.2"
msgid "Nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:54.3"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-pvr1.2:55.1"
msgid "Atirekamāse sese gimhāne vassikasāṭikacīvaraṃ pariyesanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:55.2"
msgid "Pariyesati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:55.3"
msgid "pariyiṭṭhe nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:55.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc46
msgctxt "pli-tv-pvr1.2:56.1"
msgid "Bhikkhussa sāmaṃ cīvaraṃ datvā kupito anattamano acchindanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:56.2"
msgid "Acchindati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:56.3"
msgid "acchinne nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:56.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc47
msgctxt "pli-tv-pvr1.2:57.1"
msgid "Sāmaṃ suttaṃ viññāpetvā tantavāyehi cīvaraṃ vāyāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:57.2"
msgid "Vāyāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:57.3"
msgid "vāyāpite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:57.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-pvr1.2:58.1"
msgid "Pubbe appavārito aññātakassa gahapatikassa tantavāye upasaṅkamitvā cīvare vikappaṃ āpajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:58.2"
msgid "Vikappaṃ āpajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:58.3"
msgid "vikappaṃ āpanne nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:58.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc49, pts-vp-pli5.37
msgctxt "pli-tv-pvr1.2:59.1"
msgid "Accekacīvaraṃ paṭiggahetvā cīvarakālasamayaṃ atikkāmento ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:59.2"
msgid "Nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:59.3"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc50
msgctxt "pli-tv-pvr1.2:60.1"
msgid "Tiṇṇaṃ cīvarānaṃ aññataraṃ cīvaraṃ antaraghare nikkhipitvā atirekachārattaṃ vippavasanto ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:60.2"
msgid "Nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:60.3"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc51
msgctxt "pli-tv-pvr1.2:61.1"
msgid "Jānaṃ saṃghikaṃ lābhaṃ pariṇataṃ attano pariṇāmento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:61.2"
msgid "Pariṇāmeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:61.3"
msgid "pariṇāmite nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:61.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc52
msgctxt "pli-tv-pvr1.2:62.1"
msgid "Pattavaggo tatiyo."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.2:62.2"
msgid "Tiṃsa nissaggiyā pācittiyā niṭṭhitā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr1.2:63.1"
msgid "5. 5 Pācittiyakaṇḍa"
msgstr ""

#. HTML: </h2><h3>
msgctxt "pli-tv-pvr1.2:64.1"
msgid "5.1. Musāvādavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc53
msgctxt "pli-tv-pvr1.2:65.1"
msgid "Sampajānamusāvādaṃ bhāsanto kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.2:65.2"
msgid "Sampajānamusāvādaṃ bhāsanto pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:65.3"
msgid "Pāpiccho icchāpakato asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:65.4"
msgid "bhikkhuṃ amūlakena pārājikena dhammena anuddhaṃseti, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:65.5"
msgid "“yo te vihāre vasati, so bhikkhu arahā”ti bhaṇati, paṭivijānantassa āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:65.6"
msgid "na paṭivijānantassa āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:65.7"
msgid "sampajānamusāvāde pācittiyaṃ—"
msgstr ""

msgctxt "pli-tv-pvr1.2:65.8"
msgid "sampajānamusāvādaṃ bhāsanto imā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:65.9"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc54
msgctxt "pli-tv-pvr1.2:66.1"
msgid "Omasanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:66.2"
msgid "Upasampannaṃ omasati, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:66.3"
msgid "anupasampannaṃ omasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:66.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-pvr1.2:67.1"
msgid "Pesuññaṃ upasaṃharanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:67.2"
msgid "Upasampannassa pesuññaṃ upasaṃharati, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:67.3"
msgid "anupasampannassa pesuññaṃ upasaṃharati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:67.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-pvr1.2:68.1"
msgid "Anupasampannaṃ padaso dhammaṃ vācento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:68.2"
msgid "Vāceti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:68.3"
msgid "pade pade āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:68.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc57
msgctxt "pli-tv-pvr1.2:69.1"
msgid "Anupasampannena uttaridirattatirattaṃ sahaseyyaṃ kappento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:69.2"
msgid "Nipajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:69.3"
msgid "nipanne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:69.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-pvr1.2:70.1"
msgid "Mātugāmena sahaseyyaṃ kappento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:70.2"
msgid "Nipajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:70.3"
msgid "nipanne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:70.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr1.2:71.1"
msgid "Mātugāmassa uttarichappañcavācāhi dhammaṃ desento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:71.2"
msgid "Deseti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:71.3"
msgid "pade pade āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:71.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-pvr1.2:72.1"
msgid "Anupasampannassa uttarimanussadhammaṃ bhūtaṃ ārocento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:72.2"
msgid "Āroceti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:72.3"
msgid "ārocite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:72.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc61
msgctxt "pli-tv-pvr1.2:73.1"
msgid "Bhikkhussa duṭṭhullaṃ āpattiṃ anupasampannassa ārocento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:73.2"
msgid "Āroceti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:73.3"
msgid "ārocite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:73.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-pvr1.2:74.1"
msgid "Pathaviṃ khaṇanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:74.2"
msgid "Khaṇati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:74.3"
msgid "pahāre pahāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:74.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc63
msgctxt "pli-tv-pvr1.2:75.1"
msgid "Musāvādavaggo paṭhamo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:76.1"
msgid "5.2. Bhūtagāmavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc64, pts-vp-pli5.38
msgctxt "pli-tv-pvr1.2:77.1"
msgid "Bhūtagāmaṃ pātento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:77.2"
msgid "Pāteti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:77.3"
msgid "pahāre pahāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:77.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-pvr1.2:78.1"
msgid "Aññenaññaṃ paṭicaranto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:78.2"
msgid "Anāropite aññavādake aññenaññaṃ paṭicarati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:78.3"
msgid "āropite aññavādake aññenaññaṃ paṭicarati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:78.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr1.2:79.1"
msgid "Bhikkhuṃ ujjhāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:79.2"
msgid "Ujjhāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:79.3"
msgid "ujjhāpite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:79.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr1.2:80.1"
msgid "Saṃghikaṃ mañcaṃ vā pīṭhaṃ vā bhisiṃ vā kocchaṃ vā ajjhokāse santharitvā anuddharitvā anāpucchā pakkamanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:80.2"
msgid "Paṭhamaṃ pādaṃ leḍḍupātaṃ atikkāmeti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:80.3"
msgid "dutiyaṃ pādaṃ atikkāmeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:80.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-pvr1.2:81.1"
msgid "Saṃghike vihāre seyyaṃ santharitvā anuddharitvā anāpucchā pakkamanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:81.2"
msgid "Paṭhamaṃ pādaṃ parikkhepaṃ atikkāmeti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:81.3"
msgid "dutiyaṃ pādaṃ atikkāmeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:81.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc69
msgctxt "pli-tv-pvr1.2:82.1"
msgid "Saṃghike vihāre jānaṃ pubbupagataṃ bhikkhuṃ anupakhajja seyyaṃ kappento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:82.2"
msgid "Nipajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:82.3"
msgid "nipanne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:82.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc70
msgctxt "pli-tv-pvr1.2:83.1"
msgid "Bhikkhuṃ kupito anattamano saṃghikā vihārā nikkaḍḍhento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:83.2"
msgid "Nikkaḍḍhati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:83.3"
msgid "nikkaḍḍhite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:83.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-pvr1.2:84.1"
msgid "Saṃghike vihāre uparivehāsakuṭiyā āhaccapādakaṃ mañcaṃ vā pīṭhaṃ vā abhinisīdanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:84.2"
msgid "Abhinisīdati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:84.3"
msgid "abhinisinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:84.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc72
#. VAR: tatuttari → tatuttariṃ (bj) | taduttariṃ (s1-3, pts1)
msgctxt "pli-tv-pvr1.2:85.1"
msgid "Dvattipariyāye adhiṭṭhahitvā tatuttari adhiṭṭhahanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:85.2"
msgid "Adhiṭṭheti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:85.3"
msgid "adhiṭṭhite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:85.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-pvr1.2:86.1"
msgid "Jānaṃ sappāṇakaṃ udakaṃ tiṇaṃ vā mattikaṃ vā siñcanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:86.2"
msgid "Siñcati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:86.3"
msgid "siñcite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:86.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc74
msgctxt "pli-tv-pvr1.2:87.1"
msgid "Bhūtagāmavaggo dutiyo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:88.1"
msgid "5.3. Ovādavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc75
msgctxt "pli-tv-pvr1.2:89.1"
msgid "Asammato bhikkhuniyo ovadanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:89.2"
msgid "Ovadati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:89.3"
msgid "ovadite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:89.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-pvr1.2:90.1"
msgid "Atthaṅgate sūriye bhikkhuniyo ovadanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:90.2"
msgid "Ovadati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:90.3"
msgid "ovadite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:90.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-pvr1.2:91.1"
msgid "Bhikkhunupassayaṃ upasaṅkamitvā bhikkhuniyo ovadanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:91.2"
msgid "Ovadati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:91.3"
msgid "ovadite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:91.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc78
msgctxt "pli-tv-pvr1.2:92.1"
msgid "“Āmisahetu bhikkhū bhikkhuniyo ovadantī”ti bhaṇanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:92.2"
msgid "Bhaṇati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:92.3"
msgid "bhaṇite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:92.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc79
msgctxt "pli-tv-pvr1.2:93.1"
msgid "Aññātikāya bhikkhuniyā cīvaraṃ dento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:93.2"
msgid "Deti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:93.3"
msgid "dinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:93.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc80, pts-vp-pli5.39
msgctxt "pli-tv-pvr1.2:94.1"
msgid "Aññātikāya bhikkhuniyā cīvaraṃ sibbento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:94.2"
msgid "Sibbeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:94.3"
msgid "ārāpathe ārāpathe āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:94.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc81
msgctxt "pli-tv-pvr1.2:95.1"
msgid "Bhikkhuniyā saddhiṃ saṃvidhāya ekaddhānamaggaṃ paṭipajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:95.2"
msgid "Paṭipajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:95.3"
msgid "paṭipanne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:95.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc82
msgctxt "pli-tv-pvr1.2:96.1"
msgid "Bhikkhuniyā saddhiṃ saṃvidhāya ekaṃ nāvaṃ abhiruhanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:96.2"
msgid "Abhiruhati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:96.3"
msgid "abhiruḷhe āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:96.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-pvr1.2:97.1"
msgid "Jānaṃ bhikkhuniparipācitaṃ piṇḍapātaṃ bhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:97.2"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:97.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:97.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc84
msgctxt "pli-tv-pvr1.2:98.1"
msgid "Bhikkhuniyā saddhiṃ eko ekāya raho nisajjaṃ kappento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:98.2"
msgid "Nisīdati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:98.3"
msgid "nisinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:98.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc85
msgctxt "pli-tv-pvr1.2:99.1"
msgid "Ovādavaggo tatiyo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:100.1"
msgid "5.4. Bhojanavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc86
msgctxt "pli-tv-pvr1.2:101.1"
msgid "Tatuttari āvasathapiṇḍaṃ bhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:101.2"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:101.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:101.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc87
msgctxt "pli-tv-pvr1.2:102.1"
msgid "Gaṇabhojanaṃ bhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:102.2"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:102.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:102.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc88
msgctxt "pli-tv-pvr1.2:103.1"
msgid "Paramparabhojanaṃ bhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:103.2"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:103.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:103.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc89
msgctxt "pli-tv-pvr1.2:104.1"
msgid "Dvattipattapūre pūve paṭiggahetvā tatuttari paṭiggaṇhanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:104.2"
msgid "Gaṇhāti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:104.3"
msgid "gahite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:104.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc90
msgctxt "pli-tv-pvr1.2:105.1"
msgid "Bhuttāvī pavārito anatirittaṃ khādanīyaṃ vā bhojanīyaṃ vā bhuñjanto dve āpattiyo āpajjati."
msgstr ""

#. VAR: Bhuñjissāmī”ti → khādissāmi bhuñjissāmīti (bj)
msgctxt "pli-tv-pvr1.2:105.2"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:105.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:105.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc91
msgctxt "pli-tv-pvr1.2:106.1"
msgid "Bhikkhuṃ bhuttāviṃ pavāritaṃ anatirittena khādanīyena vā bhojanīyena vā abhihaṭṭhuṃ pavārento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:106.2"
msgid "Tassa vacanena “khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:106.3"
msgid "bhojanapariyosāne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:106.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc92
msgctxt "pli-tv-pvr1.2:107.1"
msgid "Vikāle khādanīyaṃ vā bhojanīyaṃ vā bhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:107.2"
msgid "“Khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:107.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:107.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc93
msgctxt "pli-tv-pvr1.2:108.1"
msgid "Sannidhikārakaṃ khādanīyaṃ vā bhojanīyaṃ vā bhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:108.2"
msgid "“Khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:108.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:108.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc94
msgctxt "pli-tv-pvr1.2:109.1"
msgid "Paṇītabhojanāni attano atthāya viññāpetvā bhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:109.2"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:109.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:109.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc95
msgctxt "pli-tv-pvr1.2:110.1"
msgid "Adinnaṃ mukhadvāraṃ āhāraṃ āharanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:110.2"
msgid "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:110.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:110.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc96
msgctxt "pli-tv-pvr1.2:111.1"
msgid "Bhojanavaggo catuttho."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:112.1"
msgid "5.5. Acelakavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc97, pts-vp-pli5.40
#. VAR: Acelakassa → aceḷakassa (bj)
msgctxt "pli-tv-pvr1.2:113.1"
msgid "Acelakassa vā paribbājakassa vā paribbājikāya vā sahatthā khādanīyaṃ vā bhojanīyaṃ vā dento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:113.2"
msgid "Deti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:113.3"
msgid "dinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:113.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc98
msgctxt "pli-tv-pvr1.2:114.1"
msgid "Bhikkhuṃ—"
msgstr ""

msgctxt "pli-tv-pvr1.2:114.2"
msgid "“ehāvuso, gāmaṃ vā nigamaṃ vā piṇḍāya pavisissāmā”ti tassa dāpetvā vā adāpetvā vā uyyojento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:114.3"
msgid "Uyyojeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:114.4"
msgid "uyyojite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:114.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc99
msgctxt "pli-tv-pvr1.2:115.1"
msgid "Sabhojane kule anupakhajja nisajjaṃ kappento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:115.2"
msgid "Nisīdati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:115.3"
msgid "nisinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:115.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc100
msgctxt "pli-tv-pvr1.2:116.1"
msgid "Mātugāmena saddhiṃ raho paṭicchanne āsane nisajjaṃ kappento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:116.2"
msgid "Nisīdati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:116.3"
msgid "nisinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:116.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc101
msgctxt "pli-tv-pvr1.2:117.1"
msgid "Mātugāmena saddhiṃ eko ekāya raho nisajjaṃ kappento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:117.2"
msgid "Nisīdati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:117.3"
msgid "nisinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:117.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc102
#. VAR: purebhattaṃ pacchābhattaṃ → purebhattaṃ vā pacchābhattaṃ vā santaṃ bhikkhuṃ anāpucchā (bj, s1-3) | purebhattaṃ pacchābhattaṃ santaṃ bhikkhuṃ anāpucchā (pts1)
msgctxt "pli-tv-pvr1.2:118.1"
msgid "Nimantito sabhatto samāno purebhattaṃ pacchābhattaṃ kulesu cārittaṃ āpajjanto dve āpattiyo āpajjati."
msgstr ""

#. VAR: ummāraṃ → upacāraṃ (bj)
msgctxt "pli-tv-pvr1.2:118.2"
msgid "Paṭhamaṃ pādaṃ ummāraṃ atikkāmeti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:118.3"
msgid "dutiyaṃ pādaṃ atikkāmeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:118.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc103
msgctxt "pli-tv-pvr1.2:119.1"
msgid "Tatuttari bhesajjaṃ viññāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:119.2"
msgid "Viññāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:119.3"
msgid "viññāpite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:119.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc104
msgctxt "pli-tv-pvr1.2:120.1"
msgid "Uyyuttaṃ senaṃ dassanāya gacchanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:120.2"
msgid "Gacchati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:120.3"
msgid "yattha ṭhito passati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:120.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc105
msgctxt "pli-tv-pvr1.2:121.1"
msgid "Atirekatirattaṃ senāya vasanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:121.2"
msgid "Vasati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:121.3"
msgid "vasite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:121.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc106
msgctxt "pli-tv-pvr1.2:122.1"
msgid "Uyyodhikaṃ gacchanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:122.2"
msgid "Gacchati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:122.3"
msgid "yattha ṭhito passati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:122.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc107
#. VAR: Acelakavaggo → aceḷakavaggo (bj)
msgctxt "pli-tv-pvr1.2:123.1"
msgid "Acelakavaggo pañcamo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:124.1"
msgid "5.6. Surāmerayavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc108
msgctxt "pli-tv-pvr1.2:125.1"
msgid "Majjaṃ pivanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:125.2"
msgid "“Pivissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:125.3"
msgid "ajjhohāre ajjhohāre āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:125.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc109
msgctxt "pli-tv-pvr1.2:126.1"
msgid "Bhikkhuṃ aṅgulipatodakena hāsento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:126.2"
msgid "Hāseti, payoge dukkaṭaṃ;"
msgstr ""

#. VAR: hasite → hāsite (bj, s1-3, pts1)
msgctxt "pli-tv-pvr1.2:126.3"
msgid "hasite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:126.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc110
msgctxt "pli-tv-pvr1.2:127.1"
msgid "Udake kīḷanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:127.2"
msgid "Heṭṭhāgopphake udake kīḷati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:127.3"
msgid "uparigopphake kīḷati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:127.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc111
msgctxt "pli-tv-pvr1.2:128.1"
msgid "Anādariyaṃ karonto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:128.2"
msgid "Karoti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:128.3"
msgid "kate āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:128.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc112
msgctxt "pli-tv-pvr1.2:129.1"
msgid "Bhikkhuṃ bhiṃsāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:129.2"
msgid "Bhiṃsāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:129.3"
msgid "bhiṃsāpite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:129.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc113
msgctxt "pli-tv-pvr1.2:130.1"
msgid "Jotiṃ samādahitvā visibbento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:130.2"
msgid "Samādahati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:130.3"
msgid "samādahite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:130.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc114
msgctxt "pli-tv-pvr1.2:131.1"
msgid "Orenaddhamāsaṃ nahāyanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:131.2"
msgid "Nahāyati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:131.3"
msgid "nahānapariyosāne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:131.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc115, pts-vp-pli5.41
msgctxt "pli-tv-pvr1.2:132.1"
msgid "Anādiyitvā tiṇṇaṃ dubbaṇṇakaraṇānaṃ aññataraṃ dubbaṇṇakaraṇaṃ navaṃ cīvaraṃ paribhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:132.2"
msgid "Paribhuñjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:132.3"
msgid "paribhutte āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:132.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc116
msgctxt "pli-tv-pvr1.2:133.1"
msgid "Bhikkhussa vā bhikkhuniyā vā sikkhamānāya vā sāmaṇerassa vā sāmaṇeriyā vā sāmaṃ cīvaraṃ vikappetvā appaccuddhāraṇaṃ paribhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:133.2"
msgid "Paribhuñjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:133.3"
msgid "paribhutte āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:133.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc117
msgctxt "pli-tv-pvr1.2:134.1"
msgid "Bhikkhussa pattaṃ vā cīvaraṃ vā nisīdanaṃ vā sūcigharaṃ vā kāyabandhanaṃ vā apanidhento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:134.2"
msgid "Apanidheti, payoge dukkaṭaṃ;"
msgstr ""

#. VAR: apanidhite → apaniddhe (bj)
msgctxt "pli-tv-pvr1.2:134.3"
msgid "apanidhite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:134.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc118
msgctxt "pli-tv-pvr1.2:135.1"
msgid "Surāmerayavaggo chaṭṭho."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:136.1"
msgid "5.7. Sappāṇakavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc119
msgctxt "pli-tv-pvr1.2:137.1"
msgid "Sañcicca pāṇaṃ jīvitā voropento kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.2:137.2"
msgid "Sañcicca pāṇaṃ jīvitā voropento catasso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:137.3"
msgid "Anodissa opātaṃ khaṇati—"
msgstr ""

msgctxt "pli-tv-pvr1.2:137.4"
msgid "“yo koci papatitvā marissatī”ti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:137.5"
msgid "manusso tasmiṃ papatitvā marati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:137.6"
msgid "yakkho vā peto vā tiracchānagatamanussaviggaho vā tasmiṃ papatitvā marati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:137.7"
msgid "tiracchānagato tasmiṃ papatitvā marati, āpatti pācittiyassa—"
msgstr ""

msgctxt "pli-tv-pvr1.2:137.8"
msgid "sañcicca pāṇaṃ jīvitā voropento imā catasso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:137.9"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc120
msgctxt "pli-tv-pvr1.2:138.1"
msgid "Jānaṃ sappāṇakaṃ udakaṃ paribhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:138.2"
msgid "Paribhuñjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:138.3"
msgid "paribhutte āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:138.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc121
msgctxt "pli-tv-pvr1.2:139.1"
msgid "Jānaṃ yathādhammaṃ nihatādhikaraṇaṃ punakammāya ukkoṭento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:139.2"
msgid "Ukkoṭeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:139.3"
msgid "ukkoṭite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:139.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc122
msgctxt "pli-tv-pvr1.2:140.1"
msgid "Bhikkhussa jānaṃ duṭṭhullaṃ āpattiṃ paṭicchādento ekaṃ āpattiṃ āpajjati, pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.2:140.2"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc123
msgctxt "pli-tv-pvr1.2:141.1"
msgid "Jānaṃ ūnavīsativassaṃ puggalaṃ upasampādento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:141.2"
msgid "Upasampādeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:141.3"
msgid "upasampādite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:141.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc124
msgctxt "pli-tv-pvr1.2:142.1"
msgid "Jānaṃ theyyasatthena saddhiṃ saṃvidhāya ekaddhānamaggaṃ paṭipajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:142.2"
msgid "Paṭipajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:142.3"
msgid "paṭipanne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:142.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc125
msgctxt "pli-tv-pvr1.2:143.1"
msgid "Mātugāmena saddhiṃ saṃvidhāya ekaddhānamaggaṃ paṭipajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:143.2"
msgid "Paṭipajjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:143.3"
msgid "paṭipanne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:143.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc126
#. VAR: samanubhāsanāya → samanubhāsisamāno (bj)
msgctxt "pli-tv-pvr1.2:144.1"
msgid "Pāpikāya diṭṭhiyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:144.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:144.3"
msgid "kammavācāpariyosāne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:144.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc127
#. VAR: akaṭānudhammena → akatānudhammena (bj)
msgctxt "pli-tv-pvr1.2:145.1"
msgid "Jānaṃ tathāvādinā bhikkhunā akaṭānudhammena taṃ diṭṭhiṃ appaṭinissaṭṭhena saddhiṃ sambhuñjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:145.2"
msgid "Sambhuñjati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:145.3"
msgid "sambhutte āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:145.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc128, pts-vp-pli5.42
msgctxt "pli-tv-pvr1.2:146.1"
msgid "Jānaṃ tathānāsitaṃ samaṇuddesaṃ upalāpento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:146.2"
msgid "Upalāpeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:146.3"
msgid "upalāpite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:146.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc129
msgctxt "pli-tv-pvr1.2:147.1"
msgid "Sappāṇakavaggo sattamo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.2:148.1"
msgid "5.8. Sahadhammikavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc130
msgctxt "pli-tv-pvr1.2:149.1"
msgid "Bhikkhūhi sahadhammikaṃ vuccamāno—"
msgstr ""

msgctxt "pli-tv-pvr1.2:149.2"
msgid "“na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paripucchissāmī”ti bhaṇanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:149.3"
msgid "Bhaṇati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:149.4"
msgid "bhaṇite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:149.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc131
msgctxt "pli-tv-pvr1.2:150.1"
msgid "Vinayaṃ vivaṇṇento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:150.2"
msgid "Vivaṇṇeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:150.3"
msgid "vivaṇṇite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:150.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc132
msgctxt "pli-tv-pvr1.2:151.1"
msgid "Mohento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:151.2"
msgid "Anāropite mohe moheti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:151.3"
msgid "āropite mohe moheti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:151.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc133
msgctxt "pli-tv-pvr1.2:152.1"
msgid "Bhikkhussa kupito anattamano pahāraṃ dento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:152.2"
msgid "Paharati, payoge dukkaṭaṃ;"
msgstr ""

#. VAR: pahate → paharite (bj) | pahaṭe (s1, s2, pts1)
msgctxt "pli-tv-pvr1.2:152.3"
msgid "pahate āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:152.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc134
msgctxt "pli-tv-pvr1.2:153.1"
msgid "Bhikkhussa kupito anattamano talasattikaṃ uggiranto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:153.2"
msgid "Uggirati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:153.3"
msgid "uggirite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:153.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc135
msgctxt "pli-tv-pvr1.2:154.1"
msgid "Bhikkhuṃ amūlakena saṃghādisesena anuddhaṃsento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:154.2"
msgid "Anuddhaṃseti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:154.3"
msgid "anuddhaṃsite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:154.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc136
msgctxt "pli-tv-pvr1.2:155.1"
msgid "Bhikkhussa sañcicca kukkuccaṃ upadahanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:155.2"
msgid "Upadahati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:155.3"
msgid "upadahite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:155.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc137
msgctxt "pli-tv-pvr1.2:156.1"
msgid "Bhikkhūnaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ upassutiṃ tiṭṭhanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:156.2"
msgid "“Sossāmī”ti gacchati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:156.3"
msgid "yattha ṭhito suṇāti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:156.4"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc138
msgctxt "pli-tv-pvr1.2:157.1"
msgid "Dhammikānaṃ kammānaṃ chandaṃ datvā pacchā khīyanadhammaṃ āpajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:157.2"
msgid "Khiyyati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:157.3"
msgid "khiyyite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:157.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc139
msgctxt "pli-tv-pvr1.2:158.1"
msgid "Saṃghe vinicchayakathāya vattamānāya chandaṃ adatvā uṭṭhāyāsanā pakkamanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:158.2"
msgid "Parisāya hatthapāsaṃ vijahantassa āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.2:158.3"
msgid "vijahite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:158.4"
msgid "(10)"
msgstr ""

#. HTML: </p><p>
#. REF: sc140
msgctxt "pli-tv-pvr1.2:159.1"
msgid "Samaggena saṃghena cīvaraṃ datvā pacchā khīyanadhammaṃ āpajjanto dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:159.2"
msgid "Khiyyati, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:159.3"
msgid "khiyyite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-pvr1.2:159.4"
msgid "(11)"
msgstr ""

#. HTML: </p><p>
#. REF: sc141
msgctxt "pli-tv-pvr1.2:160.1"
msgid "Jānaṃ saṃghikaṃ lābhaṃ pariṇataṃ puggalassa pariṇāmento dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.2:160.2"
msgid "Pariṇāmeti, payoge dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr1.2:160.3"
