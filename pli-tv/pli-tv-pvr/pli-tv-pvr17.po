#. HTML: <section class="sutta" id="pli-tv-pvr17"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr17:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr17:2.1"
msgid "Upālipañcaka"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr17:3.1"
msgid "1. Anissitavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli5.180
msgctxt "pli-tv-pvr17:4.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-pvr17:4.2"
msgid "Atha kho āyasmā upāli yena bhagavā tenupasaṅkami;"
msgstr ""

msgctxt "pli-tv-pvr17:4.3"
msgid "upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pli-tv-pvr17:4.4"
msgid "Ekamantaṃ nisinno kho āyasmā upāli bhagavantaṃ etadavoca—"
msgstr ""

msgctxt "pli-tv-pvr17:4.5"
msgid "“katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā yāvajīvaṃ nānissitena vatthabban”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr17:5.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā yāvajīvaṃ nānissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:5.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:5.3"
msgid "Uposathaṃ na jānāti, uposathakammaṃ na jānāti, pātimokkhaṃ na jānāti, pātimokkhuddesaṃ na jānāti, ūnapañcavasso hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:5.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ nānissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:5.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā yāvajīvaṃ anissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:5.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:5.7"
msgid "Uposathaṃ jānāti, uposathakammaṃ jānāti, pātimokkhaṃ jānāti, pātimokkhuddesaṃ jānāti, pañcavasso vā hoti atirekapañcavasso vā—"
msgstr ""

msgctxt "pli-tv-pvr17:5.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ anissitena vatthabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr17:6.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ nānissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:6.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:6.3"
msgid "Pavāraṇaṃ na jānāti, pavāraṇākammaṃ na jānāti, pātimokkhaṃ na jānāti, pātimokkhuddesaṃ na jānāti, ūnapañcavasso hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:6.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ nānissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:6.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā yāvajīvaṃ anissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:6.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:6.7"
msgid "Pavāraṇaṃ jānāti, pavāraṇākammaṃ jānāti, pātimokkhaṃ jānāti, pātimokkhuddesaṃ jānāti, pañcavasso vā hoti atirekapañcavasso vā—"
msgstr ""

msgctxt "pli-tv-pvr17:6.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ anissitena vatthabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr17:7.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ nānissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:7.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:7.3"
msgid "Āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, ūnapañcavasso hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:7.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ nānissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:7.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā yāvajīvaṃ anissitena vatthabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:7.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:7.7"
msgid "Āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, pañcavasso vā hoti atirekapañcavasso vā—"
msgstr ""

msgctxt "pli-tv-pvr17:7.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā yāvajīvaṃ anissitena vatthabbaṃ”."
msgstr ""

#. HTML: </p><p>
#. REF: sc5, pts-vp-pli5.181
msgctxt "pli-tv-pvr17:8.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā na upasampādetabbaṃ, na nissayo dātabbo, na sāmaṇero upaṭṭhāpetabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr17:9.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā na upasampādetabbaṃ, na nissayo dātabbo, na sāmaṇero upaṭṭhāpetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:9.2"
msgid "Katamehi pañcahi?"
msgstr ""

#. VAR: vinodetuṃ → imasmiṃ ṭhāne sabbattha \
msgctxt "pli-tv-pvr17:9.3"
msgid "Na paṭibalo hoti antevāsiṃ vā saddhivihāriṃ vā gilānaṃ upaṭṭhātuṃ vā upaṭṭhāpetuṃ vā, anabhirataṃ vūpakāsetuṃ vā vūpakāsāpetuṃ vā, uppannaṃ kukkuccaṃ dhammato vinodetuṃ, abhidhamme vinetuṃ, abhivinaye vinetuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:9.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā na upasampādetabbaṃ, na nissayo dātabbo, na sāmaṇero upaṭṭhāpetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:9.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā upasampādetabbaṃ, nissayo dātabbo, sāmaṇero upaṭṭhāpetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:9.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:9.7"
msgid "Paṭibalo hoti antevāsiṃ vā saddhivihāriṃ vā gilānaṃ upaṭṭhātuṃ vā upaṭṭhāpetuṃ vā, anabhirataṃ vūpakāsetuṃ vā vūpakāsāpetuṃ vā, uppannaṃ kukkuccaṃ dhammato vinodetuṃ, abhidhamme vinetuṃ, abhivinaye vinetuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:9.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ, nissayo dātabbo, sāmaṇero upaṭṭhāpetabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr17:10.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā na upasampādetabbaṃ, na nissayo dātabbo, na sāmaṇero upaṭṭhāpetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:10.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:10.3"
msgid "Na paṭibalo hoti antevāsiṃ vā saddhivihāriṃ vā abhisamācārikāya sikkhāya sikkhāpetuṃ, ādibrahmacāriyakāya sikkhāya vinetuṃ, adhisīle vinetuṃ, adhicitte vinetuṃ, adhipaññāya vinetuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:10.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā na upasampādetabbaṃ, na nissayo dātabbo, na sāmaṇero upaṭṭhāpetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:10.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā upasampādetabbaṃ, nissayo dātabbo, sāmaṇero upaṭṭhāpetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:10.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:10.7"
msgid "Paṭibalo hoti antevāsiṃ vā saddhivihāriṃ vā abhisamācārikāya sikkhāya sikkhāpetuṃ, ādibrahmacāriyakāya sikkhāya vinetuṃ, adhisīle vinetuṃ, adhicitte vinetuṃ, adhipaññāya vinetuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:10.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā upasampādetabbaṃ, nissayo dātabbo, sāmaṇero upaṭṭhāpetabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr17:11.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatassa bhikkhuno kammaṃ kātabban”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr17:12.1"
msgid "Pañcahupāli, aṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:12.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:12.3"
msgid "Alajjī ca hoti, bālo ca, apakatatto ca, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:12.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:12.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr17:13.1"
msgid "“Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:13.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:13.3"
msgid "Adhisīle sīlavipanno hoti, ajjhācāre ācāravipanno hoti, atidiṭṭhiyā diṭṭhivipanno hoti, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:13.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:13.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc11, pts-vp-pli5.182
msgctxt "pli-tv-pvr17:14.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:14.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:14.3"
msgid "Kāyikena davena samannāgato hoti, vācasikena davena samannāgato hoti, kāyikavācasikena davena samannāgato hoti, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:14.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:14.5"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr17:15.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:15.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:15.3"
msgid "Kāyikena anācārena samannāgato hoti, vācasikena anācārena samannāgato hoti, kāyikavācasikena anācārena samannāgato hoti, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:15.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:15.5"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr17:16.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:16.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:16.3"
msgid "Kāyikena upaghātikena samannāgato hoti, vācasikena upaghātikena samannāgato hoti, kāyikavācasikena upaghātikena samannāgato hoti, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:16.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:16.5"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr17:17.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:17.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:17.3"
msgid "Kāyikena micchājīvena samannāgato hoti, vācasikena micchājīvena samannāgato hoti, kāyikavācasikena micchājīvena samannāgato hoti, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:17.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:17.5"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr17:18.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:18.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:18.3"
msgid "Āpattiṃ āpanno kammakato upasampādeti, nissayaṃ deti, sāmaṇeraṃ upaṭṭhāpeti, bhikkhunovādakasammutiṃ sādiyati, sammatopi bhikkhuniyo ovadati—"
msgstr ""

msgctxt "pli-tv-pvr17:18.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:18.5"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr17:19.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:19.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:19.3"
msgid "Yāya āpattiyā saṃghena kammaṃ kataṃ hoti taṃ āpattiṃ āpajjati, aññaṃ vā tādisikaṃ, tato vā pāpiṭṭhataraṃ, kammaṃ garahati, kammike garahati—"
msgstr ""

msgctxt "pli-tv-pvr17:19.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:19.5"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr17:20.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:20.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:20.3"
msgid "Buddhassa avaṇṇaṃ bhāsati, dhammassa avaṇṇaṃ bhāsati, saṃghassa avaṇṇaṃ bhāsati, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:20.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ kātabban”ti."
msgstr ""

msgctxt "pli-tv-pvr17:20.5"
msgid "(9)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc18
msgctxt "pli-tv-pvr17:21.1"
msgid "Anissitavaggo niṭṭhito paṭhamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc19
msgctxt "pli-tv-pvr17:22.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc20
msgctxt "pli-tv-pvr17:23.1"
msgid "Uposathaṃ pavāraṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:23.2"
msgid "āpatti ca gilānakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:23.3"
msgid "Abhisamācāralajjī ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:23.4"
msgid "adhisīle davena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr17:24.1"
msgid "Anācāraṃ upaghāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:24.2"
msgid "micchā āpattimeva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:24.3"
msgid "Yāyāpattiyā buddhassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:24.4"
msgid "paṭhamo vaggasaṅgahoti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:25.1"
msgid "2. Nappaṭippassambhanavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc22
msgctxt "pli-tv-pvr17:26.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabban”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc23, pts-vp-pli5.183
msgctxt "pli-tv-pvr17:27.1"
msgid "“Pañcahupāli, aṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:27.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:27.3"
msgid "Āpattiṃ āpanno kammakato upasampādeti, nissayaṃ deti, sāmaṇeraṃ upaṭṭhāpeti, bhikkhunovādakasammutiṃ sādiyati, sammatopi bhikkhuniyo ovadati—"
msgstr ""

msgctxt "pli-tv-pvr17:27.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:27.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr17:28.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:28.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:28.3"
msgid "Yāya āpattiyā saṃghena kammaṃ kataṃ hoti taṃ āpattiṃ āpajjati, aññaṃ vā tādisikaṃ, tato vā pāpiṭṭhataraṃ, kammaṃ garahati, kammike garahati—"
msgstr ""

msgctxt "pli-tv-pvr17:28.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:28.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-pvr17:29.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:29.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:29.3"
msgid "Buddhassa avaṇṇaṃ bhāsati, dhammassa avaṇṇaṃ bhāsati, saṃghassa avaṇṇaṃ bhāsati, micchādiṭṭhiko ca hoti, ājīvavipanno ca—"
msgstr ""

msgctxt "pli-tv-pvr17:29.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:29.5"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr17:30.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:30.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:30.3"
msgid "Alajjī ca hoti, bālo ca, apakatatto ca, omaddakārako ca hoti, vattesu sikkhāya ca na paripūrakārī—"
msgstr ""

msgctxt "pli-tv-pvr17:30.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno kammaṃ nappaṭippassambhetabban”ti."
msgstr ""

msgctxt "pli-tv-pvr17:30.5"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-pvr17:31.1"
msgid "“Saṅgāmāvacarena, bhante, bhikkhunā saṃghaṃ upasaṅkamantena kati dhamme ajjhattaṃ upaṭṭhāpetvā saṃgho upasaṅkamitabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc28
msgctxt "pli-tv-pvr17:32.1"
msgid "“Saṅgāmāvacarena, upāli, bhikkhunā saṃghaṃ upasaṅkamantena pañca dhamme ajjhattaṃ upaṭṭhāpetvā saṃgho upasaṅkamitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:32.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:32.3"
msgid "Saṅgāmāvacarena, upāli, bhikkhunā saṃghaṃ upasaṅkamantena nīcacittena saṃgho upasaṅkamitabbo, rajoharaṇasamena cittena, āsanakusalena bhavitabbaṃ nissajjakusalena, there bhikkhū anupakhajjantena, nave bhikkhū āsanena appaṭibāhantena yathāpatirūpe āsane nisīditabbaṃ, anānākathikena bhavitabbaṃ atiracchānakathikena, sāmaṃ vā dhammo bhāsitabbo, paro vā ajjhesitabbo, ariyo vā tuṇhībhāvo nātimaññitabbo, sace, upāli, saṃgho samaggakaraṇīyāni kammāni karoti;"
msgstr ""

msgctxt "pli-tv-pvr17:32.4"
msgid "tatra ce, upāli, bhikkhuno nakkhamati, api diṭṭhāvikammaṃ katvā ñāpetabbā sāmaggī."
msgstr ""

msgctxt "pli-tv-pvr17:32.5"
msgid "Taṃ kissahetu?"
msgstr ""

msgctxt "pli-tv-pvr17:32.6"
msgid "Māhaṃ saṃghena nānatto assanti."
msgstr ""

msgctxt "pli-tv-pvr17:32.7"
msgid "Saṅgāmāvacarenupāli, bhikkhunā saṃghaṃ upasaṅkamantena ime pañca dhamme ajjhattaṃ upaṭṭhāpetvā saṃgho upasaṅkamitabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-pvr17:33.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgato bhikkhu saṃghe voharanto bahujanaakanto ca hoti bahujanaamanāpo ca bahujanaarucito cā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-pvr17:34.1"
msgid "“Pañcahupāli, aṅgehi samannāgato bhikkhu saṃghe voharanto bahujanaakanto ca hoti bahujanaamanāpo ca bahujanarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:34.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:34.3"
msgid "Ussitamantī ca hoti, nissitajappī ca, na ca bhāsānusandhikusalo hoti, na yathādhamme yathāvinaye yathāpattiyā codetā hoti, na yathādhamme yathāvinaye yathāpattiyā kāretā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:34.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanaakanto ca hoti bahujanaamanāpo ca bahujanaarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:34.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu saṃghe voharanto bahujanakanto ca hoti bahujanamanāpo ca bahujanarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:34.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:34.7"
msgid "Na ussitamantī ca hoti, na nissitajappī ca, bhāsānusandhikusalo ca hoti, yathādhamme yathāvinaye yathāpattiyā codetā hoti, yathādhamme yathāvinaye yathāpattiyā kāretā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:34.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanakanto ca hoti bahujanamanāpo ca bahujanarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:34.9"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc31, pts-vp-pli5.184
msgctxt "pli-tv-pvr17:35.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanaakanto ca hoti bahujanaamanāpo ca bahujanaarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:35.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:35.3"
msgid "Ussādetā ca hoti, apasādetā ca, adhammaṃ gaṇhāti, dhammaṃ paṭibāhati, samphañca bahuṃ bhāsati—"
msgstr ""

msgctxt "pli-tv-pvr17:35.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanaakanto ca hoti bahujanaamanāpo ca bahujanaarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:35.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu saṃghe voharanto bahujanakanto ca hoti bahujanamanāpo ca bahujanarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:35.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:35.7"
msgid "Na ussādetā ca hoti, na apasādetā ca, dhammaṃ gaṇhāti, adhammaṃ paṭibāhati, samphañca na bahuṃ bhāsati—"
msgstr ""

msgctxt "pli-tv-pvr17:35.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanakanto ca hoti bahujanamanāpo ca bahujanarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:35.9"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-pvr17:36.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanaakanto ca hoti bahujanaamanāpo ca bahujanaarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:36.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:36.3"
msgid "Pasayhapavattā hoti, anokāsakammaṃ kāretvā pavattā hoti, na yathādhamme yathāvinaye yathāpattiyā codetā hoti, na yathādhamme yathāvinaye yathāpattiyā kāretā hoti, na yathādiṭṭhiyā byākatā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:36.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanaakanto ca hoti bahujanaamanāpo ca bahujanaarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:36.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu saṃghe voharanto bahujanakanto ca hoti bahujanamanāpo ca bahujanarucito ca."
msgstr ""

msgctxt "pli-tv-pvr17:36.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:36.7"
msgid "Na pasayhapavattā hoti, okāsakammaṃ kāretvā pavattā hoti, yathādhamme yathāvinaye yathāpattiyā codetā hoti, yathādhamme yathāvinaye yathāpattiyā kāretā hoti, yathādiṭṭhiyā byākatā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:36.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu saṃghe voharanto bahujanakanto ca hoti bahujanamanāpo ca bahujanarucito cā”ti."
msgstr ""

msgctxt "pli-tv-pvr17:36.9"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc33
msgctxt "pli-tv-pvr17:37.1"
msgid "“Kati nu kho, bhante, ānisaṃsā vinayapariyattiyā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-pvr17:38.1"
msgid "“Pañcime, upāli, ānisaṃsā vinayapariyattiyā."
msgstr ""

msgctxt "pli-tv-pvr17:38.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:38.3"
msgid "Attano sīlakkhandho sugutto hoti surakkhito, kukkuccapakatānaṃ paṭisaraṇaṃ hoti, visārado saṃghamajjhe voharati, paccatthike sahadhammena suniggahitaṃ niggaṇhāti, saddhammaṭṭhitiyā paṭipanno hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:38.4"
msgid "imehi kho, upāli, pañcānisaṃsā vinayapariyattiyā”."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc35
msgctxt "pli-tv-pvr17:39.1"
msgid "Nappaṭippassambhanavaggo niṭṭhito dutiyo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc36
msgctxt "pli-tv-pvr17:40.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc37
msgctxt "pli-tv-pvr17:41.1"
msgid "Āpanno yāyavaṇṇañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:41.2"
msgid "alajjī saṅgāmena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:41.3"
msgid "Ussitā ussādetā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:41.4"
msgid "pasayha pariyattiyāti."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc38
msgctxt "pli-tv-pvr17:42.1"
msgid "Paṭhamayamakapaññatti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr17:43.1"
msgid "3. Vohāravagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc39, pts-vp-pli5.185
msgctxt "pli-tv-pvr17:44.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā saṃghe na voharitabban”ti"
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-pvr17:45.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:45.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:45.3"
msgid "Āpattiṃ na jānāti, āpattisamuṭṭhānaṃ na jānāti, āpattiyā payogaṃ na jānāti, āpattiyā vūpasamaṃ na jānāti, āpattiyā na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:45.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:45.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:45.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:45.7"
msgid "Āpattiṃ jānāti, āpattisamuṭṭhānaṃ jānāti, āpattiyā payogaṃ jānāti, āpattiyā vūpasamaṃ jānāti, āpattiyā vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:45.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:45.9"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc41
msgctxt "pli-tv-pvr17:46.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:46.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:46.3"
msgid "Adhikaraṇaṃ na jānāti, adhikaraṇasamuṭṭhānaṃ na jānāti, adhikaraṇassa payogaṃ na jānāti, adhikaraṇassa vūpasamaṃ na jānāti, adhikaraṇassa na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:46.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:46.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:46.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:46.7"
msgid "Adhikaraṇaṃ jānāti, adhikaraṇasamuṭṭhānaṃ jānāti, adhikaraṇassa payogaṃ jānāti, adhikaraṇassa vūpasamaṃ jānāti, adhikaraṇassa vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:46.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:46.9"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc42
msgctxt "pli-tv-pvr17:47.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:47.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:47.3"
msgid "Pasayhapavattā hoti, anokāsakammaṃ kāretvā pavattā hoti, na yathādhamme yathāvinaye yathāpattiyā codetā hoti, na yathādhamme yathāvinaye yathāpattiyā kāretā hoti, na yathādiṭṭhiyā byākatā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:47.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:47.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:47.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:47.7"
msgid "Na pasayhapavattā hoti, okāsakammaṃ kāretvā pavattā hoti, yathādhamme yathāvinaye yathāpattiyā codetā hoti, yathādhamme yathāvinaye yathāpattiyā kāretā hoti, yathādiṭṭhiyā byākatā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:47.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:47.9"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc43
msgctxt "pli-tv-pvr17:48.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:48.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:48.3"
msgid "Āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, sappaṭikammaṃ appaṭikammaṃ āpattiṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:48.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:48.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:48.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:48.7"
msgid "Āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, sappaṭikammaṃ appaṭikammaṃ āpattiṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:48.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:48.9"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc44, pts-vp-pli5.186
msgctxt "pli-tv-pvr17:49.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:49.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:49.3"
msgid "Kammaṃ na jānāti, kammassa karaṇaṃ na jānāti, kammassa vatthuṃ na jānāti, kammassa vattaṃ na jānāti, kammassa vūpasamaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:49.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:49.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:49.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:49.7"
msgid "Kammaṃ jānāti, kammassa karaṇaṃ jānāti, kammassa vatthuṃ jānāti, kammassa vattaṃ jānāti, kammassa vūpasamaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:49.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:49.9"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-pvr17:50.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:50.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:50.3"
msgid "Vatthuṃ na jānāti, nidānaṃ na jānāti, paññattiṃ na jānāti, padapaccābhaṭṭhaṃ na jānāti, anusandhivacanapathaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:50.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:50.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:50.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:50.7"
msgid "Vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, padapaccābhaṭṭhaṃ jānāti, anusandhivacanapathaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:50.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:50.9"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc46
msgctxt "pli-tv-pvr17:51.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:51.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:51.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, alajjī ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:51.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:51.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:51.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:51.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, lajjī ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:51.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:51.9"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc47
msgctxt "pli-tv-pvr17:52.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:52.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:52.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, akusalo ca hoti vinaye—"
msgstr ""

msgctxt "pli-tv-pvr17:52.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:52.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:52.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:52.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, kusalo ca hoti vinaye—"
msgstr ""

msgctxt "pli-tv-pvr17:52.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:52.9"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-pvr17:53.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:53.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:53.3"
msgid "Ñattiṃ na jānāti, ñattiyā karaṇaṃ na jānāti, ñattiyā anussāvanaṃ na jānāti, ñattiyā samathaṃ na jānāti, ñattiyā vūpasamaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:53.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:53.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:53.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:53.7"
msgid "Ñattiṃ jānāti, ñattiyā karaṇaṃ jānāti, ñattiyā anussāvanaṃ jānāti, ñattiyā samathaṃ jānāti, ñattiyā vūpasamaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:53.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:53.9"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc49
msgctxt "pli-tv-pvr17:54.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:54.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:54.3"
msgid "Suttaṃ na jānāti, suttānulomaṃ na jānāti, vinayaṃ na jānāti, vinayānulomaṃ na jānāti, na ca ṭhānāṭhānakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:54.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:54.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:54.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:54.7"
msgid "Suttaṃ jānāti, suttānulomaṃ jānāti, vinayaṃ jānāti, vinayānulomaṃ jānāti, ṭhānāṭhānakusalo ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:54.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:54.9"
msgid "(10)"
msgstr ""

#. HTML: </p><p>
#. REF: sc50, pts-vp-pli5.187
msgctxt "pli-tv-pvr17:55.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:55.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:55.3"
msgid "Dhammaṃ na jānāti, dhammānulomaṃ na jānāti, vinayaṃ na jānāti, vinayānulomaṃ na jānāti, na ca pubbāparakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:55.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe na voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:55.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saṃghe voharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:55.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:55.7"
msgid "Dhammaṃ jānāti, dhammānulomaṃ jānāti, vinayaṃ jānāti, vinayānulomaṃ jānāti, pubbāparakusalo ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:55.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saṃghe voharitabban”ti."
msgstr ""

msgctxt "pli-tv-pvr17:55.9"
msgid "(11)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc51
msgctxt "pli-tv-pvr17:56.1"
msgid "Vohāravaggo niṭṭhito tatiyo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc52
msgctxt "pli-tv-pvr17:57.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc53
msgctxt "pli-tv-pvr17:58.1"
msgid "Āpatti adhikaraṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:58.2"
msgid "pasayhāpatti jānanā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:58.3"
msgid "Kammaṃ vatthuṃ alajjī ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:58.4"
msgid "akusalo ca ñattiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:58.5"
msgid "Suttaṃ na jānāti dhammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:58.6"
msgid "tatiyo vaggasaṅgahoti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:59.1"
msgid "4. Diṭṭhāvikammavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc54
msgctxt "pli-tv-pvr17:60.1"
msgid "“Kati nu kho, bhante, adhammikā diṭṭhāvikammā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-pvr17:61.1"
msgid "“Pañcime, upāli, adhammikā diṭṭhāvikammā."
msgstr ""

msgctxt "pli-tv-pvr17:61.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:61.3"
msgid "Anāpattiyā diṭṭhiṃ āvi karoti, adesanāgāminiyā āpattiyā diṭṭhiṃ āvi karoti, desitāya āpattiyā diṭṭhiṃ āvi karoti, catūhi pañcahi diṭṭhiṃ āvi karoti, mano mānasena diṭṭhiṃ āvi karoti—"
msgstr ""

msgctxt "pli-tv-pvr17:61.4"
msgid "ime kho, upāli, pañca adhammikā diṭṭhāvikammā."
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-pvr17:62.1"
msgid "Pañcime, upāli, dhammikā diṭṭhāvikammā."
msgstr ""

msgctxt "pli-tv-pvr17:62.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:62.3"
msgid "Āpattiyā diṭṭhiṃ āvi karoti, desanāgāminiyā āpattiyā diṭṭhiṃ āvi karoti, adesitāya āpattiyā diṭṭhiṃ āvi karoti, na catūhi pañcahi diṭṭhiṃ āvi karoti, na mano mānasena diṭṭhiṃ āvi karoti—"
msgstr ""

msgctxt "pli-tv-pvr17:62.4"
msgid "ime kho, upāli, pañca dhammikā diṭṭhāvikammā."
msgstr ""

#. HTML: </p><p>
#. REF: sc57
msgctxt "pli-tv-pvr17:63.1"
msgid "Aparepi, upāli, pañca adhammikā diṭṭhāvikammā."
msgstr ""

msgctxt "pli-tv-pvr17:63.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:63.3"
msgid "Nānāsaṃvāsakassa santike diṭṭhiṃ āvi karoti, nānāsīmāya ṭhitassa santike diṭṭhiṃ āvi karoti, apakatattassa santike diṭṭhiṃ āvi karoti, catūhi pañcahi diṭṭhiṃ āvi karoti, mano mānasena diṭṭhiṃ āvi karoti—"
msgstr ""

msgctxt "pli-tv-pvr17:63.4"
msgid "ime kho, upāli, pañca adhammikā diṭṭhāvikammā."
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-pvr17:64.1"
msgid "Pañcime, upāli, dhammikā diṭṭhāvikammā."
msgstr ""

msgctxt "pli-tv-pvr17:64.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:64.3"
msgid "Samānasaṃvāsakassa santike diṭṭhiṃ āvi karoti, samānasīmāya ṭhitassa santike diṭṭhiṃ āvi karoti, pakatattassa santike diṭṭhiṃ āvi karoti, na catūhi pañcahi diṭṭhiṃ āvi karoti, na mano mānasena diṭṭhiṃ āvi karoti—"
msgstr ""

msgctxt "pli-tv-pvr17:64.4"
msgid "ime kho, upāli, pañca dhammikā diṭṭhāvikammā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr17:65.1"
msgid "“Kati nu kho, bhante, adhammikā paṭiggahā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-pvr17:66.1"
msgid "“Pañcime, upāli, adhammikā paṭiggahā."
msgstr ""

msgctxt "pli-tv-pvr17:66.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:66.3"
msgid "Kāyena diyyamānaṃ kāyena appaṭiggahitaṃ, kāyena diyyamānaṃ kāyappaṭibaddhena appaṭiggahitaṃ, kāyappaṭibaddhena diyyamānaṃ kāyena appaṭiggahitaṃ, kāyappaṭibaddhena diyyamānaṃ kāyappaṭibaddhena appaṭiggahitaṃ, nissaggiyena diyyamānaṃ kāyena vā kāyappaṭibaddhena vā appaṭiggahitaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:66.4"
msgid "ime kho, upāli, pañca adhammikā paṭiggahā."
msgstr ""

#. HTML: </p><p>
#. REF: sc61
msgctxt "pli-tv-pvr17:67.1"
msgid "Pañcime, upāli, dhammikā paṭiggahā."
msgstr ""

msgctxt "pli-tv-pvr17:67.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:67.3"
msgid "Kāyena diyyamānaṃ kāyena paṭiggahitaṃ, kāyena diyyamānaṃ kāyappaṭibaddhena paṭiggahitaṃ, kāyappaṭibaddhena diyyamānaṃ kāyena paṭiggahitaṃ, kāyappaṭibaddhena diyyamānaṃ kāyappaṭibaddhena paṭiggahitaṃ, nissaggiyena diyyamānaṃ kāyena vā kāyappaṭibaddhena vā paṭiggahitaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:67.4"
msgid "ime kho, upāli, pañca dhammikā paṭiggahā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc62, pts-vp-pli5.188
msgctxt "pli-tv-pvr17:68.1"
msgid "“Kati nu kho, bhante, anatirittā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc63
msgctxt "pli-tv-pvr17:69.1"
msgid "“Pañcime, upāli, anatirittā."
msgstr ""

msgctxt "pli-tv-pvr17:69.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:69.3"
msgid "Akappiyakataṃ hoti, appaṭiggahitakataṃ hoti anuccāritakataṃ hoti, ahatthapāse kataṃ hoti, alametaṃ sabbanti avuttaṃ hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:69.4"
msgid "ime kho, upāli, pañca anatirittā."
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-pvr17:70.1"
msgid "Pañcime, upāli, atirittā."
msgstr ""

msgctxt "pli-tv-pvr17:70.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:70.3"
msgid "Kappiyakataṃ hoti, paṭiggahitakataṃ hoti, uccāritakataṃ hoti, hatthapāse kataṃ hoti, alametaṃ sabbanti vuttaṃ hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:70.4"
msgid "ime kho, upāli, pañca atirittā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-pvr17:71.1"
msgid "“Katihi nu kho, bhante, ākārehi pavāraṇā paññāyatī”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr17:72.1"
msgid "“Pañcahupāli, ākārehi pavāraṇā paññāyati."
msgstr ""

msgctxt "pli-tv-pvr17:72.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:72.3"
msgid "Asanaṃ paññāyati, bhojanaṃ paññāyati, hatthapāse ṭhito abhiharati, paṭikkhepo paññāyati—"
msgstr ""

msgctxt "pli-tv-pvr17:72.4"
msgid "imehi kho, upāli, pañcahākārehi pavāraṇā paññāyatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr17:73.1"
msgid "“Kati nu kho, bhante, adhammikā paṭiññātakaraṇā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-pvr17:74.1"
msgid "“Pañcime, upāli, adhammikā paṭiññātakaraṇā."
msgstr ""

msgctxt "pli-tv-pvr17:74.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:74.3"
msgid "Bhikkhu pārājikaṃ ajjhāpanno hoti, pārājikena codiyamāno saṃghādisesaṃ ajjhāpanno paṭijānāti, taṃ saṃgho saṃghādisesena kāreti, adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:74.4"
msgid "Bhikkhu pārājikaṃ ajjhāpanno hoti, pārājikena codiyamāno pācittiyaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:74.5"
msgid "pāṭidesanīyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr17:74.6"
msgid "dukkaṭaṃ ajjhāpanno paṭijānāti, taṃ saṃgho dukkaṭena kāreti, adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:74.7"
msgid "Bhikkhu saṃghādisesaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:74.8"
msgid "pācittiyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr17:74.9"
msgid "pāṭidesanīyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr17:74.10"
msgid "dukkaṭaṃ ajjhāpanno hoti."
msgstr ""

msgctxt "pli-tv-pvr17:74.11"
msgid "Dukkaṭena codiyamāno pārājikaṃ ajjhāpanno paṭijānāti, taṃ saṃgho pārājikena kāreti, adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:74.12"
msgid "Bhikkhu dukkaṭaṃ ajjhāpanno hoti, dukkaṭena codiyamāno saṃghādisesaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:74.13"
msgid "pācittiyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr17:74.14"
msgid "pāṭidesanīyaṃ ajjhāpanno paṭijānāti, taṃ saṃgho pāṭidesanīyena kāreti—"
msgstr ""

msgctxt "pli-tv-pvr17:74.15"
msgid "adhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:74.16"
msgid "Ime kho, upāli, pañca adhammikā paṭiññātakaraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc69
msgctxt "pli-tv-pvr17:75.1"
msgid "Pañcime, upāli, dhammikā paṭiññātakaraṇā."
msgstr ""

msgctxt "pli-tv-pvr17:75.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:75.3"
msgid "Bhikkhu pārājikaṃ ajjhāpanno hoti, pārājikena codiyamāno pārājikaṃ ajjhāpanno paṭijānāti, taṃ saṃgho pārājikena kāreti, dhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:75.4"
msgid "Bhikkhu saṃghādisesaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:75.5"
msgid "pācittiyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr17:75.6"
msgid "pāṭidesanīyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr17:75.7"
msgid "dukkaṭaṃ ajjhāpanno hoti, dukkaṭena codiyamāno dukkaṭaṃ ajjhāpanno paṭijānāti, taṃ saṃgho dukkaṭena kāreti, dhammikaṃ paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:75.8"
msgid "Ime kho, upāli, pañca dhammikā paṭiññātakaraṇā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc70, pts-vp-pli5.189
msgctxt "pli-tv-pvr17:76.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa nālaṃ okāsakammaṃ kātun”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-pvr17:77.1"
msgid "“Pañcahupāli, aṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa nālaṃ okāsakammaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:77.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:77.3"
msgid "Alajjī ca hoti, bālo ca, apakatatto ca, cāvanādhippāyo vattā hoti, no vuṭṭhānādhippāyo—"
msgstr ""

msgctxt "pli-tv-pvr17:77.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa nālaṃ okāsakammaṃ kātuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-pvr17:78.1"
msgid "Pañcahupāli, aṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa alaṃ okāsakammaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:78.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:78.3"
msgid "Lajjī ca hoti, paṇḍito ca, pakatatto ca, vuṭṭhānādhippāyo vattā hoti, no cāvanādhippāyo—"
msgstr ""

msgctxt "pli-tv-pvr17:78.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa alaṃ okāsakammaṃ kātun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-pvr17:79.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā saddhiṃ vinayo na sākacchitabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc74
msgctxt "pli-tv-pvr17:80.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā saddhiṃ vinayo na sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:80.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:80.3"
msgid "Vatthuṃ na jānāti, nidānaṃ na jānāti, paññattiṃ na jānāti, padapaccābhaṭṭhaṃ na jānāti, anusandhivacanapathaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:80.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saddhiṃ vinayo na sākacchitabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc75
msgctxt "pli-tv-pvr17:81.1"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saddhiṃ vinayo sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:81.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:81.3"
msgid "Vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, padapaccābhaṭṭhaṃ jānāti, anusandhivacanapathaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:81.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saddhiṃ vinayo sākacchitabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-pvr17:82.1"
msgid "“Kati nu kho, bhante, pañhāpucchā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-pvr17:83.1"
msgid "“Pañcimā, upāli, pañhāpucchā."
msgstr ""

msgctxt "pli-tv-pvr17:83.2"
msgid "Katamā pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:83.3"
msgid "Mandattā momūhattā pañhaṃ pucchati, pāpiccho icchāpakato pañhaṃ pucchati, paribhavā pañhaṃ pucchati, aññātukāmo pañhaṃ pucchati, sace me pañhaṃ puṭṭho sammadeva byākarissati iccetaṃ kusalaṃ no ce pañhaṃ puṭṭho sammadeva byākarissati ahamassa sammadeva byākarissāmīti pañhaṃ pucchati—"
msgstr ""

msgctxt "pli-tv-pvr17:83.4"
msgid "imā kho, upāli, pañca pañhāpucchā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc78
msgctxt "pli-tv-pvr17:84.1"
msgid "“Kati nu kho, bhante, aññabyākaraṇā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc79
msgctxt "pli-tv-pvr17:85.1"
msgid "“Pañcime, upāli, aññabyākaraṇā."
msgstr ""

msgctxt "pli-tv-pvr17:85.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:85.3"
msgid "Mandattā momūhattā aññaṃ byākaroti, pāpiccho icchāpakato aññaṃ byākaroti, ummādā cittakkhepā aññaṃ byākaroti, adhimānena aññaṃ byākaroti, bhūtaṃ aññaṃ byākaroti—"
msgstr ""

msgctxt "pli-tv-pvr17:85.4"
msgid "ime kho, upāli, pañca aññabyākaraṇā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc80
msgctxt "pli-tv-pvr17:86.1"
msgid "“Kati nu kho, bhante, visuddhiyo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc81
msgctxt "pli-tv-pvr17:87.1"
msgid "“Pañcimā, upāli, visuddhiyo."
msgstr ""

msgctxt "pli-tv-pvr17:87.2"
msgid "Katamā pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:87.3"
msgid "Nidānaṃ uddisitvā avasesaṃ sutena sāvetabbaṃ ayaṃ paṭhamā visuddhi, nidānaṃ uddisitvā cattāri pārājikāni uddisitvā avasesaṃ sutena sāvetabbaṃ ayaṃ dutiyā visuddhi, nidānaṃ uddisitvā cattāri pārājikāni uddisitvā terasa saṃghādisese uddisitvā avasesaṃ sutena sāvetabbaṃ ayaṃ tatiyā visuddhi, nidānaṃ uddisitvā cattāri pārājikāni uddisitvā terasa saṃghādisese uddisitvā dve aniyate uddisitvā avasesaṃ sutena sāvetabbaṃ ayaṃ catutthā visuddhi, vitthāreneva pañcamī—"
msgstr ""

msgctxt "pli-tv-pvr17:87.4"
msgid "imā kho, upāli, pañca visuddhiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc82, pts-vp-pli5.190
msgctxt "pli-tv-pvr17:88.1"
msgid "“Kati nu kho, bhante, bhojanā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-pvr17:89.1"
msgid "“Pañcime, upāli, bhojanā."
msgstr ""

msgctxt "pli-tv-pvr17:89.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:89.3"
msgid "Odano, kummāso, sattu, maccho, maṃsaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:89.4"
msgid "ime kho, upāli, pañca bhojanā”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc84
msgctxt "pli-tv-pvr17:90.1"
msgid "Diṭṭhāvikammavaggo niṭṭhito catuttho."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc85
msgctxt "pli-tv-pvr17:91.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc86
msgctxt "pli-tv-pvr17:92.1"
msgid "Diṭṭhāvikammā apare,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:92.2"
msgid "paṭiggahānatirittā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:92.3"
msgid "Pavāraṇā paṭiññātaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:92.4"
msgid "okāsaṃ sākacchena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:92.5"
msgid "Pañhaṃ aññabyākaraṇā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:92.6"
msgid "visuddhi cāpi bhojanāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:93.1"
msgid "5. Attādānavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc87
msgctxt "pli-tv-pvr17:94.1"
msgid "“Codakena, bhante, bhikkhunā paraṃ codetukāmena kati dhamme ajjhattaṃ paccavekkhitvā paro codetabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc88
msgctxt "pli-tv-pvr17:95.1"
msgid "“Codakenupāli, bhikkhunā paraṃ codetukāmena pañca dhamme ajjhattaṃ paccavekkhitvā paro codetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:95.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:95.3"
msgid "Codakenupāli, bhikkhunā paraṃ codetukāmena evaṃ paccavekkhitabbaṃ—"
msgstr ""

#. HTML: </p><p>
#. REF: sc89
msgctxt "pli-tv-pvr17:96.1"
msgid "‘Parisuddhakāyasamācāro nu khomhi, parisuddhenamhi kāyasamācārena samannāgato acchiddena appaṭimaṃsena, saṃvijjati nu kho me eso dhammo udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:96.2"
msgid "No ce, upāli, bhikkhu parisuddhakāyasamācāro hoti, parisuddhena kāyasamācārena samannāgato acchiddena appaṭimaṃsena, tassa bhavanti vattāro—"
msgstr ""

msgctxt "pli-tv-pvr17:96.3"
msgid "‘iṅgha tāva āyasmā kāyikaṃ sikkhassū’ti itissa bhavanti vattāro."
msgstr ""

#. HTML: </p><p>
#. REF: sc90
msgctxt "pli-tv-pvr17:97.1"
msgid "Puna caparaṃ, upāli, codakena bhikkhunā paraṃ codetukāmena evaṃ paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:97.2"
msgid "‘parisuddhavacīsamācāro nu khomhi, parisuddhenamhi vacīsamācārena samannāgato acchiddena appaṭimaṃsena, saṃvijjati nu kho me eso dhammo udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:97.3"
msgid "No ce, upāli, bhikkhu parisuddhavacīsamācāro hoti, parisuddhena vacīsamācārena samannāgato acchiddena appaṭimaṃsena, tassa bhavanti vattāro—"
msgstr ""

msgctxt "pli-tv-pvr17:97.4"
msgid "‘iṅgha tāva āyasmā vācasikaṃ sikkhassū’ti itissa bhavanti vattāro."
msgstr ""

#. HTML: </p><p>
#. REF: sc91
msgctxt "pli-tv-pvr17:98.1"
msgid "Puna caparaṃ, upāli, codakena bhikkhunā paraṃ codetukāmena evaṃ paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:98.2"
msgid "‘mettaṃ nu kho me cittaṃ paccupaṭṭhitaṃ sabrahmacārīsu anāghātaṃ, saṃvijjati nu kho me eso dhammo udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:98.3"
msgid "No ce, upāli, bhikkhuno mettaṃ cittaṃ paccupaṭṭhitaṃ hoti sabrahmacārīsu anāghātaṃ, tassa bhavanti vattāro—"
msgstr ""

msgctxt "pli-tv-pvr17:98.4"
msgid "‘iṅgha tāva āyasmā sabrahmacārīsu mettaṃ cittaṃ upaṭṭhāpehī’ti itissa bhavanti vattāro."
msgstr ""

#. HTML: </p><p>
#. REF: sc92
msgctxt "pli-tv-pvr17:99.1"
msgid "Puna caparaṃ, upāli, codakena bhikkhunā paraṃ codetukāmena evaṃ paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:99.2"
msgid "‘bahussuto nu khomhi sutadharo sutasannicayo, ye te dhammā ādikalyāṇā majjhekalyāṇā pariyosānakalyāṇā sātthaṃ sabyañjanaṃ kevalaparipuṇṇaṃ parisuddhaṃ brahmacariyaṃ abhivadanti, tathārūpā me dhammā bahussutā honti dhātā vacasā paricitā manasānupekkhitā diṭṭhiyā suppaṭividdhā, saṃvijjati nu kho me eso dhammo udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:99.3"
msgid "No ce, upāli, bhikkhu bahussuto hoti sutadharo sutasannicayo, ye te dhammā ādikalyāṇā majjhekalyāṇā pariyosānakalyāṇā sātthaṃ sabyañjanaṃ kevalaparipuṇṇaṃ parisuddhaṃ brahmacariyaṃ abhivadanti, tathārūpassa dhammā na bahussutā honti dhātā vacasā paricitā manasānupekkhitā diṭṭhiyā suppaṭividdhā, tassa bhavanti vattāro—"
msgstr ""

msgctxt "pli-tv-pvr17:99.4"
msgid "‘iṅgha tāva āyasmā āgamaṃ pariyāpuṇassū’ti itissa bhavanti vattāro."
msgstr ""

#. HTML: </p><p>
#. REF: sc93
msgctxt "pli-tv-pvr17:100.1"
msgid "Puna caparaṃ, upāli, codakena bhikkhunā paraṃ codetukāmena evaṃ paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:100.2"
msgid "‘ubhayāni kho me pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso, saṃvijjati nu kho me eso dhammo udāhu no’ti."
msgstr ""

#. VAR: () → (na saññāyati) (bj)
#. VAR: na sampāyati → na sampādeti (bj, s1-3) | na sampādayati (mr)
msgctxt "pli-tv-pvr17:100.3"
msgid "No ce, upāli, bhikkhuno ubhayāni pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso, ‘idaṃ panāvuso kattha vuttaṃ bhagavatā’ti iti puṭṭho na sampāyati, () tassa bhavanti vattāro—"
msgstr ""

msgctxt "pli-tv-pvr17:100.4"
msgid "‘iṅgha tāva āyasmā vinayaṃ pariyāpuṇassū’ti itissa bhavanti vattāro."
msgstr ""

msgctxt "pli-tv-pvr17:100.5"
msgid "Codakenupāli, bhikkhunā paraṃ codetukāmena ime pañca dhamme ajjhattaṃ paccavekkhitvā paro codetabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc94
msgctxt "pli-tv-pvr17:101.1"
msgid "“Codakena, bhante, bhikkhunā paraṃ codetukāmena kati dhamme ajjhattaṃ upaṭṭhāpetvā paro codetabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc95
msgctxt "pli-tv-pvr17:102.1"
msgid "“Codakenupāli, bhikkhunā paraṃ codetukāmena pañca dhamme ajjhattaṃ upaṭṭhāpetvā paro codetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:102.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:102.3"
msgid "Kālena vakkhāmi no akālena, bhūtena vakkhāmi no abhūtena, saṇhena vakkhāmi no pharusena, atthasaṃhitena vakkhāmi no anatthasaṃhitena, mettācitto vakkhāmi no dosantaroti—"
msgstr ""

msgctxt "pli-tv-pvr17:102.4"
msgid "codakenupāli, bhikkhunā paraṃ codetukāmena ime pañca dhamme ajjhattaṃ upaṭṭhāpetvā paro codetabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc96
msgctxt "pli-tv-pvr17:103.1"
msgid "“Codakena, bhante, bhikkhunā paraṃ codetukāmena kati dhamme ajjhattaṃ manasi karitvā paro codetabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc97
msgctxt "pli-tv-pvr17:104.1"
msgid "“Codakenupāli, bhikkhunā paraṃ codetukāmena pañca dhamme ajjhattaṃ manasi karitvā paro codetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:104.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:104.3"
msgid "Kāruññatā, hitesitā, anukampatā, āpattivuṭṭhānatā, vinayapurekkhāratā—"
msgstr ""

msgctxt "pli-tv-pvr17:104.4"
msgid "codakenupāli, bhikkhunā paraṃ codetukāmena ime pañca dhamme ajjhattaṃ manasi karitvā paro codetabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc98
msgctxt "pli-tv-pvr17:105.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa nālaṃ okāsakammaṃ kātun”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc99, pts-vp-pli5.191
msgctxt "pli-tv-pvr17:106.1"
msgid "“Pañcahupāli, aṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa nālaṃ okāsakammaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:106.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:106.3"
msgid "Aparisuddhakāyasamācāro hoti, aparisuddhavacīsamācāro hoti, aparisuddhājīvo hoti, bālo hoti abyatto, na paṭibalo anuyuñjiyamāno anuyogaṃ dātuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:106.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa nālaṃ okāsakammaṃ kātuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc100
msgctxt "pli-tv-pvr17:107.1"
msgid "Pañcahupāli, aṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa alaṃ okāsakammaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:107.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:107.3"
msgid "Parisuddhakāyasamācāro hoti, parisuddhavacīsamācāro hoti, parisuddhājīvo hoti, paṇḍito hoti byatto paṭibalo anuyuñjiyamāno anuyogaṃ dātuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:107.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno okāsakammaṃ kārāpentassa alaṃ okāsakammaṃ kātun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc101
msgctxt "pli-tv-pvr17:108.1"
msgid "“Attādānaṃ ādātukāmena, bhante, bhikkhunā katihaṅgehi samannāgataṃ attādānaṃ ādātabban”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc102
#. VAR: pañcaṅgasamannāgataṃ → pañcahaṅgehi samannāgataṃ (bj, s1-3)
msgctxt "pli-tv-pvr17:109.1"
msgid "“Attādānaṃ ādātukāmenupāli, bhikkhunā pañcaṅgasamannāgataṃ attādānaṃ ādātabbaṃ."
msgstr ""

#. VAR: Katame pañca → katamehi pañcahi (s1-3)
msgctxt "pli-tv-pvr17:109.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:109.3"
msgid "Attādānaṃ ādātukāmena, upāli, bhikkhunā evaṃ paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:109.4"
msgid "‘yaṃ kho ahaṃ imaṃ attādānaṃ ādātukāmo, kālo nu kho imaṃ attādānaṃ ādātuṃ udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:109.5"
msgid "Sace, upāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:109.6"
msgid "‘akālo imaṃ attādānaṃ ādātuṃ no kālo’ti, na taṃ, upāli, attādānaṃ ādātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc103
msgctxt "pli-tv-pvr17:110.1"
msgid "Sace panupāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:110.2"
msgid "‘kālo imaṃ attādānaṃ ādātuṃ no akālo’ti, tenupāli, bhikkhunā uttari paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:110.3"
msgid "‘yaṃ kho ahaṃ imaṃ attādānaṃ ādātukāmo bhūtaṃ nu kho idaṃ attādānaṃ udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:110.4"
msgid "Sace, upāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:110.5"
msgid "‘abhūtaṃ idaṃ attādānaṃ no bhūtan’ti, na taṃ, upāli, attādānaṃ ādātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc104
msgctxt "pli-tv-pvr17:111.1"
msgid "Sace panupāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:111.2"
msgid "‘bhūtaṃ idaṃ attādānaṃ no abhūtan’ti, tenupāli, bhikkhunā uttari paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:111.3"
msgid "‘yaṃ kho ahaṃ imaṃ attādānaṃ ādātukāmo, atthasaṃhitaṃ nu kho idaṃ attādānaṃ udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:111.4"
msgid "Sace, upāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:111.5"
msgid "‘anatthasaṃhitaṃ idaṃ attādānaṃ no atthasaṃhitan’ti, na taṃ, upāli, attādānaṃ ādātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc105
msgctxt "pli-tv-pvr17:112.1"
msgid "Sace panupāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:112.2"
msgid "‘atthasaṃhitaṃ idaṃ attādānaṃ no anatthasaṃhitan’ti, tenupāli, bhikkhunā uttari paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:112.3"
msgid "‘imaṃ kho ahaṃ attādānaṃ ādiyamāno labhissāmi sandiṭṭhe sambhatte bhikkhū dhammato vinayato pakkhe udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:112.4"
msgid "Sace, upāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:112.5"
msgid "‘imaṃ kho ahaṃ attādānaṃ ādiyamāno na labhissāmi sandiṭṭhe sambhatte bhikkhū dhammato vinayato pakkhe’ti, na taṃ, upāli, attādānaṃ ādātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc106
msgctxt "pli-tv-pvr17:113.1"
msgid "Sace panupāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:113.2"
msgid "‘idaṃ kho ahaṃ attādānaṃ ādiyamāno labhissāmi sandiṭṭhe sambhatte bhikkhū dhammato vinayato pakkhe’ti, tenupāli, bhikkhunā uttari paccavekkhitabbaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:113.3"
msgid "‘imaṃ kho me attādānaṃ ādiyato bhavissati saṃghassa tatonidānaṃ bhaṇḍanaṃ kalaho viggaho vivādo saṃghabhedo saṃgharāji saṃghavavatthānaṃ saṃghanānākaraṇaṃ udāhu no’ti."
msgstr ""

msgctxt "pli-tv-pvr17:113.4"
msgid "Sace, upāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:113.5"
msgid "‘imaṃ kho me attādānaṃ ādiyato bhavissati saṃghassa tatonidānaṃ bhaṇḍanaṃ kalaho viggaho vivādo saṃghabhedo saṃgharāji saṃghavavatthānaṃ saṃghanānākaraṇan’ti, na taṃ, upāli, attādānaṃ ādātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc107
msgctxt "pli-tv-pvr17:114.1"
msgid "Sace panupāli, bhikkhu paccavekkhamāno evaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:114.2"
msgid "‘imaṃ kho me attādānaṃ ādiyato na bhavissati saṃghassa tatonidānaṃ bhaṇḍanaṃ kalaho viggaho vivādo saṃghabhedo saṃgharāji saṃghavavatthānaṃ saṃghanānākaraṇan’ti, taṃ ādātabbaṃ, upāli, attādānaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:114.3"
msgid "Evaṃ pañcaṅgasamannāgataṃ kho, upāli, attādānaṃ ādinnaṃ pacchāpi avippaṭisārakaraṃ bhavissatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc108
msgctxt "pli-tv-pvr17:115.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgato bhikkhu adhikaraṇajātānaṃ bhikkhūnaṃ bahūpakāro hotī”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc109
msgctxt "pli-tv-pvr17:116.1"
msgid "“Pañcahupāli, aṅgehi samannāgato bhikkhu adhikaraṇajātānaṃ bhikkhūnaṃ bahūpakāro hoti."
msgstr ""

msgctxt "pli-tv-pvr17:116.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:116.3"
msgid "Sīlavā hoti, pātimokkhasaṃvarasaṃvuto viharati ācāragocarasampanno aṇumattesu vajjesu bhayadassāvī, samādāya sikkhati sikkhāpadesu;"
msgstr ""

msgctxt "pli-tv-pvr17:116.4"
msgid "bahussuto hoti sutadharo sutasannicayo;"
msgstr ""

msgctxt "pli-tv-pvr17:116.5"
msgid "ye te dhammā ādikalyāṇā majjhekalyāṇā pariyosānakalyāṇā sātthaṃ sabyañjanaṃ kevalaparipuṇṇaṃ parisuddhaṃ brahmacariyaṃ abhivadanti tathārūpassa dhammā bahussutā honti dhātā vacasā paricitā manasānupekkhitā diṭṭhiyā suppaṭividdhā, ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni suppavattīni suvinicchitāni suttaso anubyañjanaso;"
msgstr ""

msgctxt "pli-tv-pvr17:116.6"
msgid "vinaye kho pana ṭhito hoti asaṃhīro;"
msgstr ""

msgctxt "pli-tv-pvr17:116.7"
msgid "paṭibalo hoti ubho atthapaccatthike assāsetuṃ saññāpetuṃ nijjhāpetuṃ pekkhetuṃ pasādetuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:116.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu adhikaraṇajātānaṃ bhikkhūnaṃ bahūpakāro hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc110
msgctxt "pli-tv-pvr17:117.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu adhikaraṇajātānaṃ bhikkhūnaṃ bahūpakāro hoti."
msgstr ""

msgctxt "pli-tv-pvr17:117.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:117.3"
msgid "Parisuddhakāyasamācāro hoti, parisuddhavacīsamācāro hoti, parisuddhājīvo hoti, paṇḍito hoti byatto, paṭibalo anuyuñjiyamāno anuyogaṃ dātuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:117.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu adhikaraṇajātānaṃ bhikkhūnaṃ bahūpakāro hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc111, pts-vp-pli5.192
msgctxt "pli-tv-pvr17:118.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu adhikaraṇajātānaṃ bhikkhūnaṃ bahūpakāro hoti."
msgstr ""

msgctxt "pli-tv-pvr17:118.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:118.3"
msgid "Vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, padapaccābhaṭṭhaṃ jānāti, anusandhivacanapathaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:118.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu adhikaraṇajātānaṃ bhikkhūnaṃ bahūpakāro hotī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc112
msgctxt "pli-tv-pvr17:119.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā nānuyuñjitabban”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc113
msgctxt "pli-tv-pvr17:120.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:120.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:120.3"
msgid "Suttaṃ na jānāti, suttānulomaṃ na jānāti, vinayaṃ na jānāti, vinayānulomaṃ na jānāti, na ca ṭhānāṭhānakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:120.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc114
msgctxt "pli-tv-pvr17:121.1"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:121.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:121.3"
msgid "Suttaṃ jānāti, suttānulomaṃ jānāti, vinayaṃ jānāti, vinayānulomaṃ jānāti, ṭhānāṭhānakusalo ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:121.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc115
msgctxt "pli-tv-pvr17:122.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:122.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:122.3"
msgid "Dhammaṃ na jānāti, dhammānulomaṃ na jānāti, vinayaṃ na jānāti, vinayānulomaṃ na jānāti, na ca pubbāparakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:122.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc116
msgctxt "pli-tv-pvr17:123.1"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:123.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:123.3"
msgid "Dhammaṃ jānāti, dhammānulomaṃ jānāti, vinayaṃ jānāti, vinayānulomaṃ jānāti, pubbāparakusalo ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:123.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc117
msgctxt "pli-tv-pvr17:124.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:124.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:124.3"
msgid "Vatthuṃ na jānāti, nidānaṃ na jānāti, paññattiṃ na jānāti, padapaccābhaṭṭhaṃ na jānāti, anusandhivacanapathaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:124.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc118
msgctxt "pli-tv-pvr17:125.1"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:125.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:125.3"
msgid "Vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, padapaccābhaṭṭhaṃ jānāti, anusandhivacanapathaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:125.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc119
msgctxt "pli-tv-pvr17:126.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:126.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:126.3"
msgid "Āpattiṃ na jānāti, āpattisamuṭṭhānaṃ na jānāti, āpattiyā payogaṃ na jānāti, āpattiyā vūpasamaṃ na jānāti, āpattiyā na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:126.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc120
msgctxt "pli-tv-pvr17:127.1"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:127.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:127.3"
msgid "Āpattiṃ jānāti, āpattisamuṭṭhānaṃ jānāti, āpattiyā payogaṃ jānāti, āpattiyā vūpasamaṃ jānāti, āpattiyā vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:127.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc121
msgctxt "pli-tv-pvr17:128.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:128.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:128.3"
msgid "Adhikaraṇaṃ na jānāti, adhikaraṇasamuṭṭhānaṃ na jānāti, adhikaraṇassa payogaṃ na jānāti, adhikaraṇassa vūpasamaṃ na jānāti, adhikaraṇassa na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:128.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā nānuyuñjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc122
msgctxt "pli-tv-pvr17:129.1"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā anuyuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:129.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:129.3"
msgid "Adhikaraṇaṃ jānāti, adhikaraṇasamuṭṭhānaṃ jānāti, adhikaraṇassa payogaṃ jānāti, adhikaraṇassa vūpasamaṃ jānāti, adhikaraṇassa vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:129.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā anuyuñjitabban”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc123
msgctxt "pli-tv-pvr17:130.1"
msgid "Attādānavaggo niṭṭhito pañcamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc124
msgctxt "pli-tv-pvr17:131.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc125
msgctxt "pli-tv-pvr17:132.1"
msgid "Parisuddhañca kālena,"
msgstr ""

#. HTML: <br>
#. VAR: Kāruññaṃ → kāruññena (bj) | kāruññe (s1-3, pts1)
msgctxt "pli-tv-pvr17:132.2"
msgid "Kāruññaṃ okāsena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:132.3"
msgid "Attādānaṃ adhikaraṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:132.4"
msgid "Aparehipi vatthuñca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:132.5"
msgid "Suttaṃ dhammaṃ puna vatthuñca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:132.6"
msgid "Āpatti adhikaraṇena cāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:133.1"
msgid "6. Dhutaṅgavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc126, pts-vp-pli5.193
msgctxt "pli-tv-pvr17:134.1"
msgid "“Kati nu kho, bhante, āraññikā”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:134.2"
msgid "“Pañcime, upāli, āraññikā."
msgstr ""

msgctxt "pli-tv-pvr17:134.3"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:134.4"
msgid "Mandattā momūhattā āraññiko hoti, pāpiccho icchāpakato āraññiko hoti, ummādā cittakkhepā āraññiko hoti, vaṇṇitaṃ buddhehi buddhasāvakehīti āraññiko hoti, api ca appicchaññeva nissāya santuṭṭhiññeva nissāya—"
msgstr ""

msgctxt "pli-tv-pvr17:134.5"
msgid "sallekhaññeva nissāya pavivekaññeva nissāya idamatthitaññeva nissāya āraññiko hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:134.6"
msgid "ime kho, upāli, pañca āraññikā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc127
msgctxt "pli-tv-pvr17:135.1"
msgid "“Kati nu kho, bhante, piṇḍapātikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.2"
msgid "kati nu kho, bhante, paṃsukūlikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.3"
msgid "kati nu kho, bhante, rukkhamūlikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.4"
msgid "kati nu kho, bhante, sosānikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.5"
msgid "kati nu kho, bhante, abbhokāsikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.6"
msgid "kati nu kho, bhante, tecīvarikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.7"
msgid "kati nu kho, bhante, sapadānacārikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.8"
msgid "kati nu kho, bhante, nesajjikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.9"
msgid "kati nu kho, bhante, yathāsanthatikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.10"
msgid "kati nu kho, bhante, ekāsanikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.11"
msgid "kati nu kho, bhante, khalupacchābhattikāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:135.12"
msgid "kati nu kho, bhante, pattapiṇḍikā”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:135.13"
msgid "“Pañcime, upāli, pattapiṇḍikā."
msgstr ""

msgctxt "pli-tv-pvr17:135.14"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:135.15"
msgid "Mandattā momūhattā pattapiṇḍiko hoti, pāpiccho icchāpakato pattapiṇḍiko hoti, ummādā cittakkhepā pattapiṇḍiko hoti, vaṇṇitaṃ buddhehi buddhasāvakehīti pattapiṇḍiko hoti, api ca appicchaññeva nissāya santuṭṭhiññeva nissāya sallekhaññeva nissāya pavivekaññeva nissāya idamatthitaññeva nissāya pattapiṇḍiko hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:135.16"
msgid "ime kho, upāli, pañca pattapiṇḍikā”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc128
msgctxt "pli-tv-pvr17:136.1"
msgid "Dhutaṅgavaggo niṭṭhito chaṭṭho."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc129
msgctxt "pli-tv-pvr17:137.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc130
msgctxt "pli-tv-pvr17:138.1"
msgid "Āraññiko piṇḍipaṃsu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:138.2"
msgid "Rukkhasusānapañcamaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:138.3"
msgid "Abbho tecīvarañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:138.4"
msgid "Sapadānanesajjikā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:138.5"
msgid "Santhatekāsanañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:138.6"
msgid "Khalupacchā pattapiṇḍikāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:139.1"
msgid "7. Musāvādavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc131
msgctxt "pli-tv-pvr17:140.1"
msgid "“Kati nu kho, bhante, musāvādā”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:140.2"
msgid "“Pañcime, upāli, musāvādā."
msgstr ""

msgctxt "pli-tv-pvr17:140.3"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:140.4"
msgid "Atthi musāvādo pārājikagāmī, atthi musāvādo saṃghādisesagāmī, atthi musāvādo thullaccayagāmī, atthi musāvādo pācittiyagāmī, atthi musāvādo dukkaṭagāmī—"
msgstr ""

msgctxt "pli-tv-pvr17:140.5"
msgid "ime kho, upāli, pañca musāvādā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc132
msgctxt "pli-tv-pvr17:141.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatassa bhikkhuno saṃghamajjhe uposathaṃ vā pavāraṇaṃ vā ṭhapentassa—"
msgstr ""

msgctxt "pli-tv-pvr17:141.2"
msgid "‘alaṃ, bhikkhu, mā bhaṇḍanaṃ, mā kalahaṃ, mā viggahaṃ, mā vivādan’ti omadditvā saṃghena uposatho vā pavāraṇā vā kātabbā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc133
msgctxt "pli-tv-pvr17:142.1"
msgid "“Pañcahupāli, aṅgehi samannāgatassa bhikkhuno saṃghamajjhe uposathaṃ vā pavāraṇaṃ vā ṭhapentassa—"
msgstr ""

msgctxt "pli-tv-pvr17:142.2"
msgid "‘alaṃ, bhikkhu, mā bhaṇḍanaṃ, mā kalahaṃ, mā viggahaṃ, mā vivādan’ti omadditvā saṃghena uposatho vā pavāraṇā vā kātabbā."
msgstr ""

msgctxt "pli-tv-pvr17:142.3"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:142.4"
msgid "Alajjī ca hoti, bālo ca, apakatatto ca, cāvanādhippāyo vattā hoti, no vuṭṭhānādhippāyo—"
msgstr ""

msgctxt "pli-tv-pvr17:142.5"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno saṃghamajjhe uposathaṃ vā pavāraṇaṃ vā ṭhapentassa—"
msgstr ""

msgctxt "pli-tv-pvr17:142.6"
msgid "‘alaṃ, bhikkhu, mā bhaṇḍanaṃ, mā kalahaṃ, mā viggahaṃ, mā vivādan’ti omadditvā saṃghena uposatho vā pavāraṇā vā kātabbā."
msgstr ""

#. HTML: </p><p>
#. REF: sc134, pts-vp-pli5.194
msgctxt "pli-tv-pvr17:143.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno saṃghamajjhe uposathaṃ vā pavāraṇaṃ vā ṭhapentassa—"
msgstr ""

msgctxt "pli-tv-pvr17:143.2"
msgid "‘alaṃ, bhikkhu, mā bhaṇḍanaṃ, mā kalahaṃ, mā viggahaṃ, mā vivādan’ti omadditvā saṃghena uposatho vā pavāraṇā vā kātabbā."
msgstr ""

msgctxt "pli-tv-pvr17:143.3"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:143.4"
msgid "Aparisuddhakāyasamācāro hoti, aparisuddhavacīsamācāro hoti, aparisuddhājīvo hoti, bālo hoti abyatto, bhaṇḍanakārako hoti kalahakārako—"
msgstr ""

msgctxt "pli-tv-pvr17:143.5"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno saṃghamajjhe uposathaṃ vā pavāraṇaṃ vā ṭhapentassa—"
msgstr ""

msgctxt "pli-tv-pvr17:143.6"
msgid "‘alaṃ, bhikkhu, mā bhaṇḍanaṃ, mā kalahaṃ, mā viggahaṃ, mā vivādan’ti omadditvā saṃghena uposatho vā pavāraṇā vā kātabbā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc135
msgctxt "pli-tv-pvr17:144.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatassa bhikkhuno anuyogo na dātabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc136
msgctxt "pli-tv-pvr17:145.1"
msgid "“Pañcahupāli, aṅgehi samannāgatassa bhikkhuno anuyogo na dātabbo."
msgstr ""

msgctxt "pli-tv-pvr17:145.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:145.3"
msgid "Āpattānāpattiṃ na jānāti, lahukagarukaṃ āpattiṃ na jānāti, sāvasesānavasesaṃ āpattiṃ na jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ na jānāti, sappaṭikammāpaṭikammaṃ āpattiṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:145.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno anuyogo na dātabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc137
msgctxt "pli-tv-pvr17:146.1"
msgid "Pañcahupāli, aṅgehi samannāgatassa bhikkhuno anuyogo dātabbo."
msgstr ""

msgctxt "pli-tv-pvr17:146.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:146.3"
msgid "Āpattānāpattiṃ jānāti, lahukagarukaṃ āpattiṃ jānāti, sāvasesānavasesaṃ āpattiṃ jānāti, duṭṭhullāduṭṭhullaṃ āpattiṃ jānāti, sappaṭikammāpaṭikammaṃ āpattiṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:146.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno anuyogo dātabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc138
msgctxt "pli-tv-pvr17:147.1"
msgid "“Katihi nu kho, bhante, ākārehi bhikkhu āpattiṃ āpajjatī”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:147.2"
msgid "“Pañcahupāli, ākārehi bhikkhu āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr17:147.3"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:147.4"
msgid "Alajjitā, aññāṇatā, kukkuccapakatatā, akappiye kappiyasaññitā, kappiye akappiyasaññitā—"
msgstr ""

msgctxt "pli-tv-pvr17:147.5"
msgid "imehi kho, upāli, pañcahākārehi bhikkhu āpattiṃ āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc139
msgctxt "pli-tv-pvr17:148.1"
msgid "Aparehipi, upāli, pañcahākārehi bhikkhu āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr17:148.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:148.3"
msgid "Adassanena, assavanena, pasuttakatā, tathāsaññī, satisammosā—"
msgstr ""

msgctxt "pli-tv-pvr17:148.4"
msgid "imehi kho, upāli, pañcahākārehi bhikkhu āpattiṃ āpajjatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc140
msgctxt "pli-tv-pvr17:149.1"
msgid "“Kati nu kho, bhante, verā”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:149.2"
msgid "“Pañcime, upāli, verā."
msgstr ""

msgctxt "pli-tv-pvr17:149.3"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:149.4"
msgid "Pāṇātipāto, adinnādānaṃ, kāmesumicchācāro, musāvādo, surāmerayamajjappamādaṭṭhānaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:149.5"
msgid "ime kho, upāli, pañca verā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc141
msgctxt "pli-tv-pvr17:150.1"
msgid "“Kati nu kho, bhante, veramaṇiyo”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:150.2"
msgid "“Pañcimā, upāli, veramaṇiyo."
msgstr ""

msgctxt "pli-tv-pvr17:150.3"
msgid "Katamā pañca?"
msgstr ""

#. VAR: veramaṇī → veraṃani (pts1) | veramaṇi (mr)
msgctxt "pli-tv-pvr17:150.4"
msgid "Pāṇātipātā veramaṇī, adinnādānā veramaṇī, kāmesumicchācārā veramaṇī, musāvādā veramaṇī, surāmerayamajjappamādaṭṭhānā veramaṇī—"
msgstr ""

msgctxt "pli-tv-pvr17:150.5"
msgid "imā kho, upāli, pañca veramaṇiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc142
msgctxt "pli-tv-pvr17:151.1"
msgid "“Kati nu kho, bhante, byasanānī”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:151.2"
msgid "“Pañcimāni, upāli, byasanāni."
msgstr ""

msgctxt "pli-tv-pvr17:151.3"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:151.4"
msgid "Ñātibyasanaṃ, bhogabyasanaṃ, rogabyasanaṃ, sīlabyasanaṃ, diṭṭhibyasanaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:151.5"
msgid "imāni kho, upāli, pañca byasanānī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc143
msgctxt "pli-tv-pvr17:152.1"
msgid "“Kati nu kho, bhante, sampadā”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:152.2"
msgid "“Pañcimā, upāli, sampadā."
msgstr ""

msgctxt "pli-tv-pvr17:152.3"
msgid "Katamā pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:152.4"
msgid "Ñātisampadā, bhogasampadā, ārogyasampadā, sīlasampadā, diṭṭhisampadā—"
msgstr ""

msgctxt "pli-tv-pvr17:152.5"
msgid "imā kho, upāli, pañca sampadā”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc144
msgctxt "pli-tv-pvr17:153.1"
msgid "Musāvādavaggo niṭṭhito sattamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc145
msgctxt "pli-tv-pvr17:154.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc146, pts-vp-pli5.195
msgctxt "pli-tv-pvr17:155.1"
msgid "Musāvādo ca omaddi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:155.2"
msgid "aparehi anuyogo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:155.3"
msgid "Āpattiñca aparehi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:155.4"
msgid "verā veramaṇīpi ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:155.5"
msgid "Byasanaṃ sampadā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:155.6"
msgid "sattamo vaggasaṅgahoti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:156.1"
msgid "8. Bhikkhunovādavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc147
msgctxt "pli-tv-pvr17:157.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatassa bhikkhuno bhikkhunisaṃgheneva kammaṃ kātabban”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc148
msgctxt "pli-tv-pvr17:158.1"
msgid "“Pañcahupāli, aṅgehi samannāgatassa bhikkhuno bhikkhunisaṃgheneva kammaṃ kātabbaṃ, avandiyo so bhikkhu bhikkhunisaṃghena."
msgstr ""

msgctxt "pli-tv-pvr17:158.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:158.3"
msgid "Vivaritvā kāyaṃ bhikkhunīnaṃ dasseti, ūruṃ dasseti, aṅgajātaṃ dasseti, ubho aṃsakūṭe dasseti, obhāsati, gihī sampayojeti—"
msgstr ""

msgctxt "pli-tv-pvr17:158.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno bhikkhunisaṃgheneva kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:158.5"
msgid "Avandiyo so bhikkhu bhikkhunisaṃghena."
msgstr ""

#. HTML: </p><p>
#. REF: sc149
msgctxt "pli-tv-pvr17:159.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno bhikkhunisaṃgheneva kammaṃ kātabbaṃ, avandiyo so bhikkhu bhikkhunisaṃghena."
msgstr ""

msgctxt "pli-tv-pvr17:159.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:159.3"
msgid "Bhikkhunīnaṃ alābhāya parisakkati, bhikkhunīnaṃ anatthāya parisakkati, bhikkhunīnaṃ avāsāya parisakkati, bhikkhuniyo akkosati paribhāsati, bhikkhū bhikkhunīhi bhedeti—"
msgstr ""

msgctxt "pli-tv-pvr17:159.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno bhikkhunisaṃgheneva kammaṃ kātabbaṃ, avandiyo so bhikkhu bhikkhunisaṃghena."
msgstr ""

#. HTML: </p><p>
#. REF: sc150
msgctxt "pli-tv-pvr17:160.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatassa bhikkhuno bhikkhunisaṃgheneva kammaṃ kātabbaṃ, avandiyo so bhikkhu bhikkhunisaṃghena."
msgstr ""

msgctxt "pli-tv-pvr17:160.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:160.3"
msgid "Bhikkhunīnaṃ alābhāya parisakkati, bhikkhunīnaṃ anatthāya parisakkati, bhikkhunīnaṃ avāsāya parisakkati, bhikkhuniyo akkosati paribhāsati, bhikkhū bhikkhunīhi sampayojeti—"
msgstr ""

msgctxt "pli-tv-pvr17:160.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatassa bhikkhuno bhikkhunisaṃgheneva kammaṃ kātabbaṃ, avandiyo so bhikkhu bhikkhunisaṃghenā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc151
msgctxt "pli-tv-pvr17:161.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatāya bhikkhuniyā kammaṃ kātabban”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc152
msgctxt "pli-tv-pvr17:162.1"
msgid "“Pañcahupāli, aṅgehi samannāgatāya bhikkhuniyā kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:162.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:162.3"
msgid "Vivaritvā kāyaṃ bhikkhūnaṃ dasseti, ūruṃ dasseti, aṅgajātaṃ dasseti, ubho aṃsakūṭe dasseti, obhāsati, gihī sampayojeti—"
msgstr ""

msgctxt "pli-tv-pvr17:162.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatāya bhikkhuniyā kammaṃ kātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc153
msgctxt "pli-tv-pvr17:163.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatāya bhikkhuniyā kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:163.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:163.3"
msgid "Bhikkhūnaṃ alābhāya parisakkati, bhikkhūnaṃ anatthāya parisakkati, bhikkhūnaṃ avāsāya parisakkati, bhikkhū akkosati paribhāsati, bhikkhuniyo bhikkhūhi bhedeti—"
msgstr ""

msgctxt "pli-tv-pvr17:163.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatāya bhikkhuniyā kammaṃ kātabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc154, pts-vp-pli5.196
msgctxt "pli-tv-pvr17:164.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatāya bhikkhuniyā kammaṃ kātabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:164.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:164.3"
msgid "Bhikkhūnaṃ alābhāya parisakkati, bhikkhūnaṃ anatthāya parisakkati, bhikkhūnaṃ avāsāya parisakkati, bhikkhū akkosati paribhāsati, bhikkhuniyo bhikkhūhi sampayojeti—"
msgstr ""

msgctxt "pli-tv-pvr17:164.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatāya bhikkhuniyā kammaṃ kātabban”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc155
msgctxt "pli-tv-pvr17:165.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc156
msgctxt "pli-tv-pvr17:166.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:166.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:166.3"
msgid "Alajjī ca hoti, bālo ca, apakatatto ca, cāvanādhippāyo vattā hoti, no vuṭṭhānādhippāyo—"
msgstr ""

msgctxt "pli-tv-pvr17:166.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc157
msgctxt "pli-tv-pvr17:167.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:167.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:167.3"
msgid "Aparisuddhakāyasamācāro hoti, aparisuddhavacīsamācāro hoti, aparisuddhājīvo hoti, bālo hoti, abyatto, na paṭibalo anuyuñjiyamāno anuyogaṃ dātuṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:167.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc158
msgctxt "pli-tv-pvr17:168.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:168.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:168.3"
msgid "Kāyikena anācārena samannāgato hoti, vācasikena anācārena samannāgato hoti, kāyikavācasikena anācārena samannāgato hoti, bhikkhunīnaṃ akkosakaparibhāsako hoti, bhikkhunīhi saddhiṃ saṃsaṭṭho viharati ananulomikena saṃsaggena—"
msgstr ""

msgctxt "pli-tv-pvr17:168.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc159
msgctxt "pli-tv-pvr17:169.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:169.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:169.3"
msgid "Alajjī ca hoti, bālo ca, apakatatto ca, bhaṇḍanakārako ca hoti kalahakārako, sikkhāya ca na paripūrikārī—"
msgstr ""

msgctxt "pli-tv-pvr17:169.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na ṭhapetabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc160
msgctxt "pli-tv-pvr17:170.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na gahetabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc161
msgctxt "pli-tv-pvr17:171.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na gahetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:171.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:171.3"
msgid "Kāyikena anācārena samannāgato hoti, vācasikena anācārena samannāgato hoti, kāyikavācasikena anācārena samannāgato hoti, bhikkhunīnaṃ akkosakaparibhāsako hoti, bhikkhunīhi saddhiṃ saṃsaṭṭho viharati ananulomikena saṃsaggena—"
msgstr ""

msgctxt "pli-tv-pvr17:171.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na gahetabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc162
msgctxt "pli-tv-pvr17:172.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na gahetabbo."
msgstr ""

msgctxt "pli-tv-pvr17:172.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:172.3"
msgid "Alajjī ca hoti, bālo ca, apakatatto ca, gamiko vā hoti, gilāno vā—"
msgstr ""

msgctxt "pli-tv-pvr17:172.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā bhikkhunīnaṃ ovādo na gahetabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc163
msgctxt "pli-tv-pvr17:173.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgatena bhikkhunā saddhiṃ na sākacchitabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc164, pts-vp-pli5.197
msgctxt "pli-tv-pvr17:174.1"
msgid "“Pañcahupāli, aṅgehi samannāgatena bhikkhunā saddhiṃ na sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:174.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:174.3"
msgid "Na asekkhena sīlakkhandhena samannāgato hoti, na asekkhena samādhikkhandhena samannāgato hoti, na asekkhena paññākkhandhena samannāgato hoti, na asekkhena vimuttikkhandhena samannāgato hoti, na asekkhena vimuttiñāṇadassanakkhandhena samannāgato hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:174.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saddhiṃ na sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:174.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saddhiṃ sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:174.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:174.7"
msgid "Asekkhena sīlakkhandhena samannāgato hoti, asekkhena samādhikkhandhena samannāgato hoti, asekkhena paññākkhandhena samannāgato hoti, asekkhena vimuttikkhandhena samannāgato hoti, asekkhena vimuttiñāṇadassanakkhandhena samannāgato hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:174.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saddhiṃ sākacchitabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc165
msgctxt "pli-tv-pvr17:175.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgatena bhikkhunā saddhiṃ na sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:175.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:175.3"
msgid "Na atthapaṭisambhidāpatto hoti, na dhammapaṭisambhidāpatto hoti, na niruttipaṭisambhidāpatto hoti, na paṭibhānapaṭisambhidāpatto hoti, yathāvimuttaṃ cittaṃ na paccavekkhitā—"
msgstr ""

msgctxt "pli-tv-pvr17:175.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saddhiṃ na sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:175.5"
msgid "Pañcahupāli, aṅgehi samannāgatena bhikkhunā saddhiṃ sākacchitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:175.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:175.7"
msgid "Atthapaṭisambhidāpatto hoti, dhammapaṭisambhidāpatto hoti, niruttipaṭisambhidāpatto hoti, paṭibhānapaṭisambhidāpatto hoti, yathāvimuttaṃ cittaṃ paccavekkhitā—"
msgstr ""

msgctxt "pli-tv-pvr17:175.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgatena bhikkhunā saddhiṃ sākacchitabbo”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc166
msgctxt "pli-tv-pvr17:176.1"
msgid "Bhikkhunovādavaggo niṭṭhito aṭṭhamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc167
msgctxt "pli-tv-pvr17:177.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc168
msgctxt "pli-tv-pvr17:178.1"
msgid "Bhikkhunīheva kātabbaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:178.2"
msgid "aparehi tathā duve;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:178.3"
msgid "Bhikkhunīnaṃ tayo kammā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:178.4"
msgid "na ṭhapetabbā dve dukā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:178.5"
msgid "Na gahetabbā dve vuttā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:178.6"
msgid "sākacchāsu ca dve dukāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:179.1"
msgid "9. Ubbāhikavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc169
msgctxt "pli-tv-pvr17:180.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc170
msgctxt "pli-tv-pvr17:181.1"
msgid "“Pañcahupāli, aṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:181.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:181.3"
msgid "Na atthakusalo hoti, na dhammakusalo hoti, na niruttikusalo hoti, na byañjanakusalo hoti, na pubbāparakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:181.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:181.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:181.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:181.7"
msgid "Atthakusalo hoti, dhammakusalo hoti, niruttikusalo hoti, byañjanakusalo hoti, pubbāparakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:181.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc171
msgctxt "pli-tv-pvr17:182.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:182.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:182.3"
msgid "Kodhano hoti kodhābhibhūto, makkhī hoti makkhābhibhūto, paḷāsī hoti paḷāsābhibhūto, issukī hoti issābhibhūto, sandiṭṭhiparāmāsī hoti ādhānaggāhī duppaṭinissaggī—"
msgstr ""

msgctxt "pli-tv-pvr17:182.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:182.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:182.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:182.7"
msgid "Na kodhano hoti na kodhābhibhūto, na makkhī hoti na makkhābhibhūto, na paḷāsī hoti na paḷāsābhibhūto, na issukī hoti na issābhibhūto, asandiṭṭhiparāmāsī hoti anādhānaggāhī suppaṭinissaggī—"
msgstr ""

msgctxt "pli-tv-pvr17:182.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc172, pts-vp-pli5.198
msgctxt "pli-tv-pvr17:183.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:183.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:183.3"
msgid "Kuppati, byāpajjati, patiṭṭhiyati, kopaṃ janeti, akkhamo hoti apadakkhiṇaggāhī anusāsaniṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:183.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:183.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:183.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:183.7"
msgid "Na kuppati, na byāpajjati, na patiṭṭhiyati, na kopaṃ janeti, khamo hoti padakkhiṇaggāhī anusāsaniṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:183.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc173
msgctxt "pli-tv-pvr17:184.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:184.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:184.3"
msgid "Pasāretā hoti no sāretā, anokāsakammaṃ kāretvā pavattā hoti, na yathādhamme yathāvinaye yathāpattiyā codetā hoti, na yathādhamme yathāvinaye yathāpattiyā kāretā hoti, na yathādiṭṭhiyā byākatā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:184.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:184.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:184.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:184.7"
msgid "Sāretā hoti no pasāretā, okāsakammaṃ kāretvā pavattā hoti, yathādhamme yathāvinaye yathāpattiyā codetā hoti, yathādhamme yathāvinaye yathāpattiyā kāretā hoti, yathādiṭṭhiyā byākatā hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:184.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc174
msgctxt "pli-tv-pvr17:185.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:185.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:185.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, alajjī ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:185.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:185.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:185.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:185.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, lajjī ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:185.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

#. HTML: </p><p>
#. REF: sc175
msgctxt "pli-tv-pvr17:186.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:186.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:186.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, akusalo ca hoti vinaye—"
msgstr ""

msgctxt "pli-tv-pvr17:186.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya na sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:186.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo."
msgstr ""

msgctxt "pli-tv-pvr17:186.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:186.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, kusalo ca hoti vinaye—"
msgstr ""

msgctxt "pli-tv-pvr17:186.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu ubbāhikāya sammannitabbo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc176
msgctxt "pli-tv-pvr17:187.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchatī”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc177
msgctxt "pli-tv-pvr17:188.1"
msgid "“Pañcahupāli, aṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:188.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:188.3"
msgid "Suttaṃ na jānāti, suttānulomaṃ na jānāti, vinayaṃ na jānāti, vinayānulomaṃ na jānāti, na ca ṭhānāṭhānakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:188.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:188.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:188.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:188.7"
msgid "Suttaṃ jānāti, suttānulomaṃ jānāti, vinayaṃ jānāti, vinayānulomaṃ jānāti, ṭhānāṭhānakusalo ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:188.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

#. HTML: </p><p>
#. REF: sc178, pts-vp-pli5.199
msgctxt "pli-tv-pvr17:189.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:189.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:189.3"
msgid "Dhammaṃ na jānāti, dhammānulomaṃ jānāti, vinayaṃ na jānāti, vinayānulomaṃ na jānāti, na ca pubbāparakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:189.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:189.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:189.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:189.7"
msgid "Dhammaṃ jānāti, dhammānulomaṃ jānāti, vinayaṃ jānāti, vinayānulomaṃ jānāti, pubbāparakusalo ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:189.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

#. HTML: </p><p>
#. REF: sc179
msgctxt "pli-tv-pvr17:190.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:190.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:190.3"
msgid "Vatthuṃ na jānāti, nidānaṃ na jānāti, paññattiṃ na jānāti, padapaccābhaṭṭhaṃ na jānāti, anusandhivacanapathaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:190.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:190.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:190.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:190.7"
msgid "Vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, padapaccābhaṭṭhaṃ jānāti, anusandhivacanapathaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:190.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

#. HTML: </p><p>
#. REF: sc180
msgctxt "pli-tv-pvr17:191.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:191.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:191.3"
msgid "Āpattiṃ na jānāti, āpattisamuṭṭhānaṃ na jānāti, āpattiyā payogaṃ na jānāti, āpattiyā vūpasamaṃ na jānāti, āpattiyā na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:191.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:191.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:191.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:191.7"
msgid "Āpattiṃ jānāti, āpattisamuṭṭhānaṃ jānāti, āpattiyā payogaṃ jānāti, āpattiyā vūpasamaṃ jānāti, āpattiyā vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:191.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

#. HTML: </p><p>
#. REF: sc181
msgctxt "pli-tv-pvr17:192.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:192.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:192.3"
msgid "Adhikaraṇaṃ na jānāti, adhikaraṇasamuṭṭhānaṃ na jānāti, adhikaraṇassa payogaṃ na jānāti, adhikaraṇassa vūpasamaṃ na jānāti, adhikaraṇassa na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:192.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu bālotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:192.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchati."
msgstr ""

msgctxt "pli-tv-pvr17:192.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:192.7"
msgid "Adhikaraṇaṃ jānāti, adhikaraṇasamuṭṭhānaṃ jānāti, adhikaraṇassa payogaṃ jānāti, adhikaraṇassa vūpasamaṃ jānāti, adhikaraṇassa vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:192.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu paṇḍitotveva saṅkhaṃ gacchatī”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc182
msgctxt "pli-tv-pvr17:193.1"
msgid "Ubbāhikavaggo niṭṭhito navamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc183
msgctxt "pli-tv-pvr17:194.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc184
msgctxt "pli-tv-pvr17:195.1"
msgid "Na atthakusalo ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:195.2"
msgid "kodhano kuppatī ca yo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:195.3"
msgid "Pasāretā chandāgatiṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:195.4"
msgid "akusalo tatheva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc185
msgctxt "pli-tv-pvr17:196.1"
msgid "Suttaṃ dhammañca vatthuñca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:196.2"
msgid "āpatti adhikaraṇaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:196.3"
msgid "Dve dve pakāsitā sabbe,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:196.4"
msgid "kaṇhasukkaṃ vijānathāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:197.1"
msgid "10. Adhikaraṇavūpasamavagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc186
msgctxt "pli-tv-pvr17:198.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametun”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc187, pts-vp-pli5.200
msgctxt "pli-tv-pvr17:199.1"
msgid "“Pañcahupāli, aṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:199.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:199.3"
msgid "Āpattiṃ na jānāti, āpattisamuṭṭhānaṃ na jānāti, āpattiyā payogaṃ na jānāti, āpattiyā vūpasamaṃ na jānāti, āpattiyā na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:199.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:199.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:199.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:199.7"
msgid "Āpattiṃ jānāti, āpattisamuṭṭhānaṃ jānāti, āpattiyā payogaṃ jānāti, āpattiyā vūpasamaṃ jānāti, āpattiyā vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:199.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc188
msgctxt "pli-tv-pvr17:200.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:200.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:200.3"
msgid "Adhikaraṇaṃ na jānāti, adhikaraṇasamuṭṭhānaṃ na jānāti, adhikaraṇassa payogaṃ na jānāti, adhikaraṇassa vūpasamaṃ na jānāti, adhikaraṇassa na vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:200.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc189
msgctxt "pli-tv-pvr17:201.1"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:201.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:201.3"
msgid "Adhikaraṇaṃ jānāti, adhikaraṇasamuṭṭhānaṃ jānāti, adhikaraṇassa payogaṃ jānāti, adhikaraṇassa vūpasamaṃ jānāti, adhikaraṇassa vinicchayakusalo hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:201.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc190
msgctxt "pli-tv-pvr17:202.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:202.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:202.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, alajjī ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:202.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:202.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:202.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:202.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, lajjī ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:202.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc191
msgctxt "pli-tv-pvr17:203.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:203.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:203.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, appassuto ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:203.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:203.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:203.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:203.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, bahussuto ca hoti—"
msgstr ""

msgctxt "pli-tv-pvr17:203.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc192
msgctxt "pli-tv-pvr17:204.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:204.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:204.3"
msgid "Vatthuṃ na jānāti, nidānaṃ na jānāti, paññattiṃ na jānāti, padapaccābhaṭṭhaṃ na jānāti, anusandhivacanapathaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:204.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:204.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:204.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:204.7"
msgid "Vatthuṃ jānāti, nidānaṃ jānāti, paññattiṃ jānāti, padapaccābhaṭṭhaṃ jānāti, anusandhivacanapathaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:204.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc193
msgctxt "pli-tv-pvr17:205.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:205.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:205.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, akusalo ca hoti vinaye—"
msgstr ""

msgctxt "pli-tv-pvr17:205.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:205.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:205.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:205.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, kusalo ca hoti vinaye—"
msgstr ""

msgctxt "pli-tv-pvr17:205.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc194, pts-vp-pli5.201
msgctxt "pli-tv-pvr17:206.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:206.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:206.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, puggalagaru hoti no saṃghagaru—"
msgstr ""

msgctxt "pli-tv-pvr17:206.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:206.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:206.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:206.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, saṃghagaru hoti no puggalagaru—"
msgstr ""

msgctxt "pli-tv-pvr17:206.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc195
msgctxt "pli-tv-pvr17:207.1"
msgid "Aparehipi, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:207.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:207.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, āmisagaru hoti no saddhammagaru—"
msgstr ""

msgctxt "pli-tv-pvr17:207.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu nālaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:207.5"
msgid "Pañcahupāli, aṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametuṃ."
msgstr ""

msgctxt "pli-tv-pvr17:207.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:207.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, saddhammagaru hoti no āmisagaru—"
msgstr ""

msgctxt "pli-tv-pvr17:207.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhikkhu alaṃ adhikaraṇaṃ vūpasametun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc196
msgctxt "pli-tv-pvr17:208.1"
msgid "“Katihi nu kho, bhante, ākārehi saṃgho bhijjatī”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:208.2"
msgid "“Pañcahupāli, ākārehi saṃgho bhijjati."
msgstr ""

msgctxt "pli-tv-pvr17:208.3"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:208.4"
msgid "Kammena, uddesena, voharanto, anussāvanena, salākaggāhena—"
msgstr ""

msgctxt "pli-tv-pvr17:208.5"
msgid "imehi kho, upāli, pañcahākārehi saṃgho bhijjatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc197, pts-vp-pli5.202
msgctxt "pli-tv-pvr17:209.1"
msgid "“Saṃgharāji saṃgharājīti, bhante, vuccati."
msgstr ""

msgctxt "pli-tv-pvr17:209.2"
msgid "Kittāvatā nu kho, bhante, saṃgharāji hoti no ca saṃghabhedo?"
msgstr ""

msgctxt "pli-tv-pvr17:209.3"
msgid "Kittāvatā ca pana saṃgharāji ceva hoti saṃghabhedo cā”ti?"
msgstr ""

msgctxt "pli-tv-pvr17:209.4"
msgid "“Paññattetaṃ, upāli, mayā āgantukānaṃ bhikkhūnaṃ āgantukavattaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:209.5"
msgid "Evaṃ supaññatte kho, upāli, mayā sikkhāpade āgantukā bhikkhū āgantukavatte na vattanti."
msgstr ""

msgctxt "pli-tv-pvr17:209.6"
msgid "Evampi kho, upāli, saṃgharāji hoti no ca saṃghabhedo."
msgstr ""

msgctxt "pli-tv-pvr17:209.7"
msgid "Paññattetaṃ, upāli, mayā āvāsikānaṃ bhikkhūnaṃ āvāsikavattaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:209.8"
msgid "Evaṃ supaññatte kho, upāli, mayā sikkhāpade āvāsikā bhikkhū āvāsikavatte na vattanti."
msgstr ""

msgctxt "pli-tv-pvr17:209.9"
msgid "Evampi kho, upāli, saṃgharāji hoti no ca saṃghabhedo."
msgstr ""

msgctxt "pli-tv-pvr17:209.10"
msgid "Paññattetaṃ, upāli, mayā bhikkhūnaṃ bhattagge bhattaggavattaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:209.11"
msgid "yathāvuḍḍhaṃ yathārattaṃ yathāpatirūpaṃ aggāsanaṃ aggodakaṃ aggapiṇḍaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:209.12"
msgid "Evaṃ supaññatte kho, upāli, mayā sikkhāpade navā bhikkhū bhattagge therānaṃ bhikkhūnaṃ āsanaṃ paṭibāhanti."
msgstr ""

msgctxt "pli-tv-pvr17:209.13"
msgid "Evampi kho, upāli, saṃgharāji hoti no ca saṃghabhedo."
msgstr ""

msgctxt "pli-tv-pvr17:209.14"
msgid "Paññattetaṃ, upāli, mayā bhikkhūnaṃ senāsane senāsanavattaṃ—"
msgstr ""

msgctxt "pli-tv-pvr17:209.15"
msgid "yathāvuḍḍhaṃ yathārattaṃ yathāpatirūpaṃ."
msgstr ""

msgctxt "pli-tv-pvr17:209.16"
msgid "Evaṃ supaññatte kho, upāli, mayā sikkhāpade navā bhikkhū therānaṃ bhikkhūnaṃ senāsanaṃ paṭibāhanti."
msgstr ""

msgctxt "pli-tv-pvr17:209.17"
msgid "Evampi kho, upāli, saṃgharāji hoti no ca saṃghabhedo."
msgstr ""

msgctxt "pli-tv-pvr17:209.18"
msgid "Paññattetaṃ, upāli, mayā bhikkhūnaṃ antosīmāya ekaṃ uposathaṃ ekaṃ pavāraṇaṃ ekaṃ saṃghakammaṃ ekaṃ kammākammaṃ."
msgstr ""

#. VAR: āvenibhāvaṃ → āveṇibhāvaṃ (bj, s1-3, pts1)
#. VAR: āveniṃ → āveṇi (bj, s1-3, pts1)
msgctxt "pli-tv-pvr17:209.19"
msgid "Evaṃ supaññatte kho, upāli, mayā sikkhāpade tattheva antosīmāya āvenibhāvaṃ karitvā gaṇaṃ bandhitvā āveniṃ uposathaṃ karonti āveniṃ pavāraṇaṃ karonti āveniṃ saṃghakammaṃ karonti āveniṃ kammākammāni karonti."
msgstr ""

msgctxt "pli-tv-pvr17:209.20"
msgid "Evaṃ kho, upāli, saṃgharāji ceva hoti saṃghabhedo cā”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc198
msgctxt "pli-tv-pvr17:210.1"
msgid "Adhikaraṇavūpasamavaggo niṭṭhito dasamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc199
msgctxt "pli-tv-pvr17:211.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc200
msgctxt "pli-tv-pvr17:212.1"
msgid "Āpattiṃ adhikaraṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:212.2"
msgid "chandā appassutena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:212.3"
