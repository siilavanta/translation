msgctxt "pli-tv-pvr17:242.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, saṃghikaṃ puggalikaparibhogena paribhuñjati—"
msgstr ""

msgctxt "pli-tv-pvr17:242.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato āvāsiko bhikkhu yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

#. HTML: </p><p>
#. REF: sc228
msgctxt "pli-tv-pvr17:243.1"
msgid "Pañcahupāli, aṅgehi samannāgato āvāsiko bhikkhu yathābhataṃ nikkhitto evaṃ sagge."
msgstr ""

msgctxt "pli-tv-pvr17:243.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:243.3"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, saṃghikaṃ na puggalikaparibhogena paribhuñjati—"
msgstr ""

msgctxt "pli-tv-pvr17:243.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato āvāsiko bhikkhu yathābhataṃ nikkhitto evaṃ sagge”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc229
msgctxt "pli-tv-pvr17:244.1"
msgid "“Kati nu kho, bhante, adhammikā vinayabyākaraṇā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc230
msgctxt "pli-tv-pvr17:245.1"
msgid "“Pañcime, upāli, adhammikā vinayabyākaraṇā."
msgstr ""

msgctxt "pli-tv-pvr17:245.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:245.3"
msgid "Idhupāli, bhikkhu adhammaṃ dhammoti pariṇāmeti, dhammaṃ adhammoti pariṇāmeti, avinayaṃ vinayoti pariṇāmeti, vinayaṃ avinayoti pariṇāmeti, apaññattaṃ paññāpeti, paññattaṃ samucchindati—"
msgstr ""

msgctxt "pli-tv-pvr17:245.4"
msgid "ime kho, upāli, pañca adhammikā vinayabyākaraṇā."
msgstr ""

msgctxt "pli-tv-pvr17:245.5"
msgid "Pañcime, upāli, dhammikā vinayabyākaraṇā."
msgstr ""

msgctxt "pli-tv-pvr17:245.6"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:245.7"
msgid "Idhupāli, bhikkhu adhammaṃ adhammoti pariṇāmeti, dhammaṃ dhammoti pariṇāmeti, avinayaṃ avinayoti pariṇāmeti, vinayaṃ vinayoti pariṇāmeti, apaññattaṃ na paññapeti, paññattaṃ na samucchindati—"
msgstr ""

msgctxt "pli-tv-pvr17:245.8"
msgid "ime kho, upāli, pañca dhammikā vinayabyākaraṇā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc231
msgctxt "pli-tv-pvr17:246.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgato bhattuddesako yathābhataṃ nikkhitto evaṃ niraye”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc232
msgctxt "pli-tv-pvr17:247.1"
msgid "“Pañcahupāli, aṅgehi samannāgato bhattuddesako yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

msgctxt "pli-tv-pvr17:247.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:247.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, uddiṭṭhānuddiṭṭhaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:247.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhattuddesako yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

#. HTML: </p><p>
#. REF: sc233
msgctxt "pli-tv-pvr17:248.1"
msgid "Pañcahupāli, aṅgehi samannāgato bhattuddesako yathābhataṃ nikkhitto evaṃ sagge."
msgstr ""

msgctxt "pli-tv-pvr17:248.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:248.3"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, uddiṭṭhānuddiṭṭhaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:248.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato bhattuddesako yathābhataṃ nikkhitto evaṃ sagge”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc234
msgctxt "pli-tv-pvr17:249.1"
msgid "“Katihi nu kho, bhante, aṅgehi samannāgato senāsanapaññāpako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.2"
msgid "bhaṇḍāgāriko … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.3"
msgid "cīvarapaṭiggāhako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.4"
msgid "cīvarabhājako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.5"
msgid "yāgubhājako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.6"
msgid "phalabhājako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.7"
msgid "khajjabhājako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.8"
msgid "appamattakavissajjako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.9"
msgid "sāṭiyaggāhāpako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.10"
msgid "pattaggāhāpako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.11"
msgid "ārāmikapesako … pe …"
msgstr ""

msgctxt "pli-tv-pvr17:249.12"
msgid "sāmaṇerapesako yathābhataṃ nikkhitto evaṃ niraye”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc235
msgctxt "pli-tv-pvr17:250.1"
msgid "“Pañcahupāli, aṅgehi samannāgato sāmaṇerapesako yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

msgctxt "pli-tv-pvr17:250.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:250.3"
msgid "Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ gacchati, pesitāpesitaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:250.4"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato sāmaṇerapesako yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

msgctxt "pli-tv-pvr17:250.5"
msgid "Pañcahupāli, aṅgehi samannāgato sāmaṇerapesako yathābhataṃ nikkhitto evaṃ sagge."
msgstr ""

msgctxt "pli-tv-pvr17:250.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "pli-tv-pvr17:250.7"
msgid "Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na bhayāgatiṃ gacchati, pesitāpesitaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr17:250.8"
msgid "imehi kho, upāli, pañcahaṅgehi samannāgato sāmaṇerapesako yathābhataṃ nikkhitto evaṃ sagge”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc236
msgctxt "pli-tv-pvr17:251.1"
msgid "Āvāsikavaggo niṭṭhito terasamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc237
msgctxt "pli-tv-pvr17:252.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc238, pts-vp-pli5.205
msgctxt "pli-tv-pvr17:253.1"
msgid "Āvāsikabyākaraṇā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:253.2"
msgid "bhattusenāsanāni ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:253.3"
msgid "Bhaṇḍacīvaraggāho ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:253.4"
msgid "cīvarassa ca bhājako."
msgstr ""

#. HTML: </p><p>
#. REF: sc239
msgctxt "pli-tv-pvr17:254.1"
msgid "Yāgu phalaṃ khajjakañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:254.2"
msgid "appasāṭiyagāhako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:254.3"
msgid "Patto ārāmiko ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:254.4"
msgid "sāmaṇerena pesakoti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr17:255.1"
msgid "14. Kathinatthāravagga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc240
msgctxt "pli-tv-pvr17:256.1"
msgid "“Kati nu kho, bhante, ānisaṃsā kathinatthāre”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc241
msgctxt "pli-tv-pvr17:257.1"
msgid "“Pañcime, upāli, ānisaṃsā kathinatthāre."
msgstr ""

msgctxt "pli-tv-pvr17:257.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:257.3"
msgid "Anāmantacāro, asamādānacāro, gaṇabhojanaṃ, yāvadatthacīvaraṃ, yo ca tattha cīvaruppādo so nesaṃ bhavissati—"
msgstr ""

msgctxt "pli-tv-pvr17:257.4"
msgid "ime kho, upāli, pañca ānisaṃsā kathinatthāre”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc242
msgctxt "pli-tv-pvr17:258.1"
msgid "“Kati nu kho, bhante, ādīnavā muṭṭhassatissa asampajānassa niddaṃ okkamato”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc243
msgctxt "pli-tv-pvr17:259.1"
msgid "“Pañcime, upāli, ādīnavā muṭṭhassatissa asampajānassa niddaṃ okkamato."
msgstr ""

msgctxt "pli-tv-pvr17:259.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:259.3"
msgid "Dukkhaṃ supati, dukkhaṃ paṭibujjhati, pāpakaṃ supinaṃ passati, devatā na rakkhanti, asuci muccati—"
msgstr ""

msgctxt "pli-tv-pvr17:259.4"
msgid "ime kho, upāli, pañca ādīnavā muṭṭhassatissa asampajānassa niddaṃ okkamato."
msgstr ""

#. HTML: </p><p>
#. REF: sc244
msgctxt "pli-tv-pvr17:260.1"
msgid "Pañcime, upāli, ānisaṃsā upaṭṭhitassatissa sampajānassa niddaṃ okkamato."
msgstr ""

msgctxt "pli-tv-pvr17:260.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:260.3"
msgid "Sukhaṃ supati, sukhaṃ paṭibujjhati, na pāpakaṃ supinaṃ passati, devatā rakkhanti, asuci na muccati—"
msgstr ""

msgctxt "pli-tv-pvr17:260.4"
msgid "ime kho, upāli, pañca ānisaṃsā upaṭṭhitassatissa sampajānassa niddaṃ okkamato”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc245
msgctxt "pli-tv-pvr17:261.1"
msgid "“Kati nu kho, bhante, avandiyā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc246
msgctxt "pli-tv-pvr17:262.1"
msgid "“Pañcime, upāli, avandiyā."
msgstr ""

msgctxt "pli-tv-pvr17:262.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:262.3"
msgid "Antaragharaṃ paviṭṭho avandiyo, racchagato avandiyo, otamasiko avandiyo, asamannāharanto avandiyo, sutto avandiyo—"
msgstr ""

msgctxt "pli-tv-pvr17:262.4"
msgid "ime kho, upāli, pañca avandiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc247
msgctxt "pli-tv-pvr17:263.1"
msgid "Aparepi, upāli, pañca avandiyā."
msgstr ""

msgctxt "pli-tv-pvr17:263.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:263.3"
msgid "Yāgupāne avandiyo, bhattagge avandiyo, ekāvatto avandiyo, aññavihito avandiyo, naggo avandiyo—"
msgstr ""

msgctxt "pli-tv-pvr17:263.4"
msgid "ime kho, upāli, pañca avandiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc248
msgctxt "pli-tv-pvr17:264.1"
msgid "Aparepi, upāli, pañca avandiyā."
msgstr ""

msgctxt "pli-tv-pvr17:264.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:264.3"
msgid "Khādanto avandiyo, bhuñjanto avandiyo, uccāraṃ karonto avandiyo, passāvaṃ karonto avandiyo, ukkhittako avandiyo—"
msgstr ""

msgctxt "pli-tv-pvr17:264.4"
msgid "ime kho, upāli, pañca avandiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc249
msgctxt "pli-tv-pvr17:265.1"
msgid "Aparepi, upāli, pañca avandiyā."
msgstr ""

msgctxt "pli-tv-pvr17:265.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:265.3"
msgid "Pure upasampannena pacchā upasampanno avandiyo, anupasampanno avandiyo, nānāsaṃvāsako vuḍḍhataro adhammavādī avandiyo, mātugāmo avandiyo, paṇḍako avandiyo—"
msgstr ""

msgctxt "pli-tv-pvr17:265.4"
msgid "ime kho, upāli, pañca avandiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc250
msgctxt "pli-tv-pvr17:266.1"
msgid "Aparepi, upāli, pañca avandiyā."
msgstr ""

msgctxt "pli-tv-pvr17:266.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:266.3"
msgid "Pārivāsiko avandiyo, mūlāyapaṭikassanāraho avandiyo, mānattāraho avandiyo, mānattacāriko avandiyo, abbhānāraho avandiyo—"
msgstr ""

msgctxt "pli-tv-pvr17:266.4"
msgid "ime kho, upāli, pañca avandiyā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc251, pts-vp-pli5.206
msgctxt "pli-tv-pvr17:267.1"
msgid "“Kati nu kho, bhante, vandiyā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc252
msgctxt "pli-tv-pvr17:268.1"
msgid "“Pañcime, upāli, vandiyā."
msgstr ""

msgctxt "pli-tv-pvr17:268.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:268.3"
msgid "Pacchā upasampannena pure upasampanno vandiyo, nānāsaṃvāsako vuḍḍhataro dhammavādī vandiyo, ācariyo vandiyo, upajjhāyo vandiyo, sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya tathāgato arahaṃ sammāsambuddho vandiyo—"
msgstr ""

msgctxt "pli-tv-pvr17:268.4"
msgid "ime kho, upāli, pañca vandiyā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc253
msgctxt "pli-tv-pvr17:269.1"
msgid "“Navakatarena, bhante, bhikkhunā vuḍḍhatarassa bhikkhuno pāde vandantena kati dhamme ajjhattaṃ upaṭṭhāpetvā pādā vanditabbā”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc254
msgctxt "pli-tv-pvr17:270.1"
msgid "“Navakatarenupāli, bhikkhunā vuḍḍhatarassa bhikkhuno pāde vandantena pañca dhamme ajjhattaṃ upaṭṭhāpetvā pādā vanditabbā."
msgstr ""

msgctxt "pli-tv-pvr17:270.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pli-tv-pvr17:270.3"
msgid "Navakatarenupāli, bhikkhunā vuḍḍhatarassa bhikkhuno pāde vandantena ekaṃsaṃ uttarāsaṅgaṃ karitvā, añjaliṃ paggahetvā, ubhohi pāṇitalehi pādāni parisambāhantena, pemañca gāravañca upaṭṭhāpetvā pādā vanditabbā—"
msgstr ""

msgctxt "pli-tv-pvr17:270.4"
msgid "navakatarenupāli bhikkhunā vuḍḍhatarassa bhikkhuno pāde vandantena ime pañca dhamme ajjhattaṃ upaṭṭhāpetvā pādā vanditabbā”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc255
msgctxt "pli-tv-pvr17:271.1"
msgid "Kathinatthāravaggo niṭṭhito cuddasamo."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc256
msgctxt "pli-tv-pvr17:272.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc257
msgctxt "pli-tv-pvr17:273.1"
msgid "Kathinatthāraniddā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:273.2"
msgid "antarā yāgukhādane;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:273.3"
msgid "Pure ca pārivāsi ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:273.4"
msgid "vandiyo vanditabbakanti."
msgstr ""

#. HTML: </p></blockquote><p class="endsection">
#. REF: sc258
msgctxt "pli-tv-pvr17:274.1"
msgid "Upālipañcakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc259
msgctxt "pli-tv-pvr17:275.1"
msgid "Tesaṃ vaggānaṃ uddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc260
msgctxt "pli-tv-pvr17:276.1"
msgid "Anissitena kammañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:276.2"
msgid "vohārāvikammena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:276.3"
msgid "Codanā ca dhutaṅgā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:276.4"
msgid "musā bhikkhunimeva ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc261
msgctxt "pli-tv-pvr17:277.1"
msgid "Ubbāhikādhikaraṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:277.2"
msgid "bhedakā pañcamā pure;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:277.3"
msgid "Āvāsikā kathinañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr17:277.4"
msgid "cuddasā suppakāsitāti."
msgstr ""
