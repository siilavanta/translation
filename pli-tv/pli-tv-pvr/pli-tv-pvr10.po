#. HTML: <section class="sutta" id="pli-tv-pvr10"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr10:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr10:2.1"
msgid "Gāthāsaṅgaṇika"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr10:3.1"
msgid "1. Sattanagaresupaññattasikkhāpada"
msgstr ""

#. HTML: </h2><blockquote class="gatha"><p>
#. REF: sc1, pts-vp-pli5.144
msgctxt "pli-tv-pvr10:4.1"
msgid "“Ekaṃsaṃ cīvaraṃ katvā,"
msgstr ""

#. HTML: <br>
#. VAR: paggaṇhitvāna → paggahetvāna (bj)
msgctxt "pli-tv-pvr10:4.2"
msgid "paggaṇhitvāna añjaliṃ;"
msgstr ""

#. HTML: <br>
#. VAR: Āsīsamānarūpova → āsiṃsamānarūpova (bj, s1-3, pts1)
msgctxt "pli-tv-pvr10:4.3"
msgid "Āsīsamānarūpova,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:4.4"
msgid "kissa tvaṃ idha māgato”."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr10:5.1"
msgid "“Dvīsu vinayesu ye paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:5.2"
msgid "Uddesaṃ āgacchanti uposathesu;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:5.3"
msgid "Kati te sikkhāpadā honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:5.4"
msgid "Katisu nagaresu paññattā”."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
#. VAR: ummaṅgo → ummaggo (bj)
msgctxt "pli-tv-pvr10:6.1"
msgid "“Bhaddako te ummaṅgo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:6.2"
msgid "yoniso paripucchasi;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:6.3"
msgid "Taggha te ahamakkhissaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:6.4"
msgid "yathāsi kusalo tathā”."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr10:7.1"
msgid "“Dvīsu vinayesu ye paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:7.2"
msgid "Uddesaṃ āgacchanti uposathesu;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:7.3"
msgid "Aḍḍhuḍḍhasatāni te honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:7.4"
msgid "Sattasu nagaresu paññattā”."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr10:8.1"
msgid "“Katamesu sattasu nagaresu paññattā,"
msgstr ""

#. HTML: <br>
#. VAR: Iṅgha me tvaṃ byākara naṃ → iṅgha me tvaṃ byākara (bj, s1-3) | ingha me taṃ byākaraṇaṃ (pts1) | iṅgha me taṃ byākara (mr)
msgctxt "pli-tv-pvr10:8.2"
msgid "Iṅgha me tvaṃ byākara naṃ;"
msgstr ""

#. HTML: <br>
#. VAR: Taṃ vacanapathaṃ → tava vacanapathaṃ (s1-3)
msgctxt "pli-tv-pvr10:8.3"
msgid "Taṃ vacanapathaṃ nisāmayitvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:8.4"
msgid "Paṭipajjema hitāya no siyā”."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr10:9.1"
msgid "“Vesāliyaṃ rājagahe,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:9.2"
msgid "sāvatthiyañca āḷaviyaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:9.3"
msgid "Kosambiyañca sakkesu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:9.4"
msgid "bhaggesu ceva paññattā”."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr10:10.1"
msgid "“Kati vesāliyaṃ paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:10.2"
msgid "kati rājagahe katā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:10.3"
msgid "Sāvatthiyaṃ kati honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:10.4"
msgid "kati āḷaviyaṃ katā”."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr10:11.1"
msgid "“Kati kosambiyaṃ paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:11.2"
msgid "kati sakkesu vuccanti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:11.3"
msgid "Kati bhaggesu paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:11.4"
msgid "taṃ me akkhāhi pucchito”."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr10:12.1"
msgid "“Dasa vesāliyaṃ paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:12.2"
msgid "Ekavīsa rājagahe katā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:12.3"
msgid "Chaūna tīṇisatāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:12.4"
msgid "Sabbe sāvatthiyaṃ katā."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr10:13.1"
msgid "Cha āḷaviyaṃ paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:13.2"
msgid "aṭṭha kosambiyaṃ katā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:13.3"
msgid "Aṭṭha sakkesu vuccanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:13.4"
msgid "tayo bhaggesu paññattā."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr10:14.1"
msgid "Ye vesāliyaṃ paññattā,"
msgstr ""

#. HTML: <br>
#. VAR: yathātathaṃ → yathākathaṃ (bj, s1-3)
msgctxt "pli-tv-pvr10:14.2"
msgid "te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:14.3"
msgid "Methunaviggahuttari,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:14.4"
msgid "atirekañca kāḷakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr10:15.1"
msgid "Bhūtaṃ paramparabhattaṃ,"
msgstr ""

#. HTML: <br>
#. VAR: dantaponena → dantapoṇena (s1-3, pts1, mr)
msgctxt "pli-tv-pvr10:15.2"
msgid "dantaponena acelako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:15.3"
msgid "Bhikkhunīsu ca akkoso,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:15.4"
msgid "dasete vesāliyaṃ katā."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr10:16.1"
msgid "Ye rājagahe paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:16.2"
msgid "Te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:16.3"
msgid "Adinnādānaṃ rājagahe,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:16.4"
msgid "Dve anuddhaṃsanā dvepi ca bhedā."
msgstr ""

#. HTML: </p><p>
#. REF: sc14, pts-vp-pli5.145
msgctxt "pli-tv-pvr10:17.1"
msgid "Antaravāsakaṃ rūpiyaṃ suttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:17.2"
msgid "Ujjhāpanena ca pācitapiṇḍaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:17.3"
msgid "Gaṇabhojanaṃ vikāle ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:17.4"
msgid "Cārittaṃ nahānaṃ ūnavīsati."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr10:18.1"
msgid "Cīvaraṃ datvā vosāsanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:18.2"
msgid "Ete rājagahe katā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:18.3"
msgid "Giraggacariyā tattheva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:18.4"
msgid "Chandadānena ekavīsati."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr10:19.1"
msgid "Ye sāvatthiyaṃ paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:19.2"
msgid "Te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:19.3"
msgid "Pārājikāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:19.4"
msgid "Saṃghādisesā bhavanti soḷasa."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr10:20.1"
msgid "Aniyatā ca dve honti,"
msgstr ""

#. HTML: <br>
#. VAR: catuvīsati → catutiṃsati (bj)
msgctxt "pli-tv-pvr10:20.2"
msgid "nissaggiyā catuvīsati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:20.3"
msgid "Chapaññāsasatañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:20.4"
msgid "khuddakāni pavuccanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr10:21.1"
msgid "Dasayeva ca gārayhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:21.2"
msgid "dvesattati ca sekhiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:21.3"
msgid "Chaūna tīṇisatāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:21.4"
msgid "sabbe sāvatthiyaṃ katā."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-pvr10:22.1"
msgid "Ye āḷaviyaṃ paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:22.2"
msgid "te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:22.3"
msgid "Kuṭikosiyaseyyā ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:22.4"
msgid "khaṇane gaccha devate;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:22.5"
msgid "Sappāṇakañca siñcanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:22.6"
msgid "cha ete āḷaviyaṃ katā."
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-pvr10:23.1"
msgid "Ye kosambiyaṃ paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:23.2"
msgid "te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:23.3"
msgid "Mahāvihāro dovacassaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:23.4"
msgid "aññaṃ dvāraṃ surāya ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:23.5"
msgid "Anādariyaṃ sahadhammo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:23.6"
msgid "payopānena aṭṭhamaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr10:24.1"
msgid "Ye sakkesu paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:24.2"
msgid "te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:24.3"
msgid "Eḷakalomāni patto ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:24.4"
msgid "ovādo ceva bhesajjaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc22
#. VAR: āraññiko → āraññako (bj, s1-3, pts1)
msgctxt "pli-tv-pvr10:25.1"
msgid "Sūci āraññiko ceva,"
msgstr ""

#. HTML: <br>
#. VAR: aṭṭhete → cha ete (sabbattha)
msgctxt "pli-tv-pvr10:25.2"
msgid "aṭṭhete kāpilavatthave;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:25.3"
msgid "Udakasuddhiyā ovādo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:25.4"
msgid "bhikkhunīsu pavuccanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-pvr10:26.1"
msgid "Ye bhaggesu paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:26.2"
msgid "te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:26.3"
msgid "Samādahitvā visibbenti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:26.4"
msgid "sāmisena sasitthakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr10:27.1"
msgid "Pārājikāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:27.2"
msgid "saṃghādisesāni bhavanti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:27.3"
msgid "Satta ca nissaggiyāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:27.4"
msgid "aṭṭha dvattiṃsa khuddakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-pvr10:28.1"
msgid "Dve gārayhā tayo sekkhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:28.2"
msgid "chappaññāsa sikkhāpadā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:28.3"
msgid "Chasu nagaresu paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:28.4"
msgid "buddhenādiccabandhunā."
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr10:29.1"
msgid "Chaūna tīṇisatāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:29.2"
msgid "sabbe sāvatthiyaṃ katā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:29.3"
msgid "Kāruṇikena buddhena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:29.4"
msgid "gotamena yasassinā”."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr10:30.1"
msgid "2. Catuvipatti"
msgstr ""

#. HTML: </h2><blockquote class="gatha"><p>
#. REF: sc27
#. VAR: Yaṃ taṃ → yantvaṃ (s1-3) | yaṃ tvaṃ (pts1) | yaṃ yaṃ (mr)
msgctxt "pli-tv-pvr10:31.1"
msgid "“Yaṃ taṃ pucchimha akittayi no,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:31.2"
msgid "Taṃ taṃ byākataṃ anaññathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:31.3"
msgid "Aññaṃ taṃ pucchāmi tadiṅgha brūhi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:31.4"
msgid "Garuka lahukañcāpi sāvasesaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:31.5"
msgid "Anavasesaṃ duṭṭhullañca aduṭṭhullaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:31.6"
msgid "Ye ca yāvatatiyakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc28
#. VAR: Sādhāraṇaṃ asādhāraṇaṃ → sādhāraṇāsādhāraṇaṃ (bj)
msgctxt "pli-tv-pvr10:32.1"
msgid "Sādhāraṇaṃ asādhāraṇaṃ,"
msgstr ""

#. HTML: <br>
#. VAR: Vibhattiyo ca → vipattiyo (bj) | vipattiyo ca (s1-3, pts1)
msgctxt "pli-tv-pvr10:32.2"
msgid "Vibhattiyo ca yehi samathehi sammanti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:32.3"
msgid "Sabbānipetāni viyākarohi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:32.4"
msgid "Handa vākyaṃ suṇoma te”."
msgstr ""

#. HTML: </p><p>
#. REF: sc29, pts-vp-pli5.146
msgctxt "pli-tv-pvr10:33.1"
msgid "“Ekatiṃsā ye garukā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:33.2"
msgid "Aṭṭhettha anavasesā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:33.3"
msgid "Ye garukā te duṭṭhullā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:33.4"
msgid "Ye duṭṭhullā sā sīlavipatti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:33.5"
msgid "Pārājikaṃ saṃghādiseso,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:33.6"
msgid "‘Sīlavipattī’ti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-pvr10:34.1"
msgid "Thullaccayaṃ pācittiyā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:34.2"
msgid "Pāṭidesanīyaṃ dukkaṭaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:34.3"
msgid "Dubbhāsitaṃ yo cāyaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:34.4"
msgid "Akkosati hasādhippāyo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:34.5"
msgid "Ayaṃ sā ācāravipattisammatā."
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-pvr10:35.1"
msgid "Viparītadiṭṭhiṃ gaṇhanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:35.2"
msgid "Asaddhammehi purakkhatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:35.3"
msgid "Abbhācikkhanti sambuddhaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:35.4"
msgid "Duppaññā mohapārutā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:35.5"
msgid "Ayaṃ sā diṭṭhivipattisammatā."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc32
msgctxt "pli-tv-pvr10:36.1"
msgid "Ājīvahetu ājīvakāraṇā pāpiccho icchāpakato asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapati, ājīvahetu ājīvakāraṇā sañcarittaṃ samāpajjati, ājīvahetu ājīvakāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr10:36.2"
msgid "‘yo te vihāre vasati, so bhikkhu arahā’ti bhaṇati, ājīvahetu ājīvakāraṇā bhikkhu paṇītabhojanāni attano atthāya viññāpetvā bhuñjati, ājīvahetu ājīvakāraṇā bhikkhunī paṇītabhojanāni attano atthāya viññāpetvā bhuñjati, ājīvahetu ājīvakāraṇā sūpaṃ vā odanaṃ vā agilāno attano atthāya viññāpetvā bhuñjati."
msgstr ""

msgctxt "pli-tv-pvr10:36.3"
msgid "Ayaṃ sā ājīvavipatti sammatā."
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc33
msgctxt "pli-tv-pvr10:37.1"
msgid "Ekādasa yāvatatiyakā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:37.2"
msgid "Te suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:37.3"
msgid "Ukkhittānuvattikā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:37.4"
msgid "Aṭṭha yāvatatiyakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:37.5"
msgid "Ariṭṭho caṇḍakāḷī ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:37.6"
msgid "Ime te yāvatatiyakā”."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr10:38.1"
msgid "3. Chedanakādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc34
msgctxt "pli-tv-pvr10:39.1"
msgid "“Kati chedanakāni?"
msgstr ""

msgctxt "pli-tv-pvr10:39.2"
msgid "Kati bhedanakāni?"
msgstr ""

msgctxt "pli-tv-pvr10:39.3"
msgid "Kati uddālanakāni?"
msgstr ""

msgctxt "pli-tv-pvr10:39.4"
msgid "Kati anaññapācittiyāni?"
msgstr ""

msgctxt "pli-tv-pvr10:39.5"
msgid "Kati bhikkhusammutiyo?"
msgstr ""

msgctxt "pli-tv-pvr10:39.6"
msgid "Kati sāmīciyo?"
msgstr ""

msgctxt "pli-tv-pvr10:39.7"
msgid "Kati paramāni?"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc35
msgctxt "pli-tv-pvr10:40.1"
msgid "Kati jānanti paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:40.2"
msgid "buddhenādiccabandhunā”."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc36
msgctxt "pli-tv-pvr10:41.1"
msgid "“Cha chedanakāni."
msgstr ""

msgctxt "pli-tv-pvr10:41.2"
msgid "Ekaṃ bhedanakaṃ."
msgstr ""

msgctxt "pli-tv-pvr10:41.3"
msgid "Ekaṃ uddālanakaṃ."
msgstr ""

msgctxt "pli-tv-pvr10:41.4"
msgid "Cattāri anaññapācittiyāni."
msgstr ""

msgctxt "pli-tv-pvr10:41.5"
msgid "Catasso bhikkhusammutiyo."
msgstr ""

msgctxt "pli-tv-pvr10:41.6"
msgid "Satta sāmīciyo."
msgstr ""

msgctxt "pli-tv-pvr10:41.7"
msgid "Cuddasa paramāni."
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc37
#. VAR: Sodasa → soḷasa (bj, s1-3, pts1)
msgctxt "pli-tv-pvr10:42.1"
msgid "Sodasa jānanti paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:42.2"
msgid "buddhenādiccabandhunā”."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr10:43.1"
msgid "4. Asādhāraṇādi"
msgstr ""

#. HTML: </h2><blockquote class="gatha"><p>
#. REF: sc38
msgctxt "pli-tv-pvr10:44.1"
msgid "“Vīsaṃ dve satāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:44.2"
msgid "Bhikkhūnaṃ sikkhāpadāni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:44.3"
msgid "Uddesaṃ āgacchanti uposathesu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:44.4"
msgid "Tīṇi satāni cattāri;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:44.5"
msgid "Bhikkhunīnaṃ sikkhāpadāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:44.6"
msgid "Uddesaṃ āgacchanti uposathesu."
msgstr ""

#. HTML: </p><p>
#. REF: sc39
msgctxt "pli-tv-pvr10:45.1"
msgid "Chacattārīsā bhikkhūnaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:45.2"
msgid "bhikkhunīhi asādhāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:45.3"
msgid "Sataṃ tiṃsā ca bhikkhunīnaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:45.4"
msgid "bhikkhūhi asādhāraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-pvr10:46.1"
msgid "Sataṃ sattati chacceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:46.2"
msgid "ubhinnaṃ asādhāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:46.3"
msgid "Sataṃ sattati cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:46.4"
msgid "ubhinnaṃ samasikkhatā."
msgstr ""

#. HTML: </p><p>
#. REF: sc41, pts-vp-pli5.147
msgctxt "pli-tv-pvr10:47.1"
msgid "Vīsaṃ dve satāni,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:47.2"
msgid "Bhikkhūnaṃ sikkhāpadāni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:47.3"
msgid "Uddesaṃ āgacchanti uposathesu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:47.4"
msgid "Te suṇohi yathātathaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc42
msgctxt "pli-tv-pvr10:48.1"
msgid "Pārājikāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:48.2"
msgid "Saṃghādisesāni bhavanti terasa;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:48.3"
msgid "Aniyatā dve honti."
msgstr ""

#. HTML: </p><p>
#. REF: sc43
msgctxt "pli-tv-pvr10:49.1"
msgid "Nissaggiyāni tiṃseva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:49.2"
msgid "dvenavuti ca khuddakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:49.3"
msgid "Cattāro pāṭidesanīyā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:49.4"
msgid "pañcasattati sekhiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc44
msgctxt "pli-tv-pvr10:50.1"
msgid "Vīsaṃ dve satāni cime honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:50.2"
msgid "Bhikkhūnaṃ sikkhāpadāni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:50.3"
msgid "Uddesaṃ āgacchanti uposathesu."
msgstr ""

#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-pvr10:51.1"
msgid "Tīṇi satāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:51.2"
msgid "Bhikkhunīnaṃ sikkhāpadāni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:51.3"
msgid "Uddesaṃ āgacchanti uposathesu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:51.4"
msgid "Te suṇohi yathātathaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc46
msgctxt "pli-tv-pvr10:52.1"
msgid "Pārājikāni aṭṭha,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:52.2"
msgid "Saṃghādisesāni bhavanti sattarasa;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:52.3"
msgid "Nissaggiyāni tiṃseva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:52.4"
msgid "Sataṃ saṭṭhi cha ceva;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:52.5"
msgid "Khuddakāni pavuccanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc47
msgctxt "pli-tv-pvr10:53.1"
msgid "Aṭṭha pāṭidesanīyā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:53.2"
msgid "Pañcasattati sekhiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:53.3"
msgid "Tīṇi satāni cattāri cime honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:53.4"
msgid "Bhikkhunīnaṃ sikkhāpadāni;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:53.5"
msgid "Uddesaṃ āgacchanti uposathesu."
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-pvr10:54.1"
msgid "Chacattārīsā bhikkhūnaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:54.2"
msgid "bhikkhunīhi asādhāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:54.3"
msgid "Te suṇohi yathātathaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc49
msgctxt "pli-tv-pvr10:55.1"
msgid "Cha saṃghādisesā,"
msgstr ""

#. HTML: <br>
#. VAR: dve → dvīhi (bj, s1-3)
msgctxt "pli-tv-pvr10:55.2"
msgid "dve aniyatehi aṭṭha;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:55.3"
msgid "Nissaggiyāni dvādasa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:55.4"
msgid "tehi te honti vīsati."
msgstr ""

#. HTML: </p><p>
#. REF: sc50
#. VAR: Dvevīsati → dvāvīsati (bj)
msgctxt "pli-tv-pvr10:56.1"
msgid "Dvevīsati khuddakā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:56.2"
msgid "Caturo pāṭidesanīyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:56.3"
msgid "Chacattārīsā cime honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:56.4"
msgid "Bhikkhūnaṃ bhikkhunīhi asādhāraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc51
msgctxt "pli-tv-pvr10:57.1"
msgid "Sataṃ tiṃsā ca bhikkhunīnaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:57.2"
msgid "bhikkhūhi asādhāraṇā;"
msgstr ""

#. HTML: <br>
#. VAR: yathātathaṃ → yathākathaṃ (bj, s1-3)
msgctxt "pli-tv-pvr10:57.3"
msgid "Te suṇohi yathātathaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc52
msgctxt "pli-tv-pvr10:58.1"
msgid "Pārājikāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:58.2"
msgid "saṃghamhā dasa nissare;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:58.3"
msgid "Nissaggiyāni dvādasa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:58.4"
msgid "channavuti ca khuddakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:58.5"
msgid "Aṭṭha pāṭidesanīyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc53
msgctxt "pli-tv-pvr10:59.1"
msgid "Sataṃ tiṃsā cime honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:59.2"
msgid "Bhikkhunīnaṃ bhikkhūhi asādhāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:59.3"
msgid "Sataṃ sattati chacceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:59.4"
msgid "Ubhinnaṃ asādhāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:59.5"
msgid "Te suṇohi yathātathaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc54
msgctxt "pli-tv-pvr10:60.1"
msgid "Pārājikāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:60.2"
msgid "Saṃghādisesāni bhavanti soḷasa;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:60.3"
msgid "Aniyatā dve honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:60.4"
msgid "Nissaggiyāni catuvīsati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:60.5"
msgid "Sataṃ aṭṭhārasā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:60.6"
msgid "Khuddakāni pavuccanti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:60.7"
msgid "Dvādasa pāṭidesanīyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-pvr10:61.1"
msgid "Sataṃ sattati chaccevime honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:61.2"
msgid "Ubhinnaṃ asādhāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:61.3"
msgid "Sataṃ sattati cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:61.4"
msgid "Ubhinnaṃ samasikkhatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:61.5"
msgid "Te suṇohi yathātathaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-pvr10:62.1"
msgid "Pārājikāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:62.2"
msgid "Saṃghādisesāni bhavanti satta;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:62.3"
msgid "Nissaggiyāni aṭṭhārasa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:62.4"
msgid "Samasattati khuddakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:62.5"
msgid "Pañcasattati sekhiyāni."
msgstr ""

#. HTML: </p><p>
#. REF: sc57, pts-vp-pli5.148
msgctxt "pli-tv-pvr10:63.1"
msgid "Sataṃ sattati cattāri cime honti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:63.2"
msgid "Ubhinnaṃ samasikkhatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:63.3"
msgid "Aṭṭheva pārājikā ye durāsadā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:63.4"
msgid "Tālavatthusamūpamā."
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-pvr10:64.1"
msgid "Paṇḍupalāso puthusilā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:64.2"
msgid "sīsacchinnova so naro;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:64.3"
msgid "Tālova matthakacchinno,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:64.4"
msgid "aviruḷhī bhavanti te."
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr10:65.1"
msgid "Tevīsati saṃghādisesā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:65.2"
msgid "Dve aniyatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:65.3"
msgid "Dve cattārīsa nissaggiyā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:65.4"
msgid "Aṭṭhāsītisataṃ pācittiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:65.5"
msgid "Dvādasa pāṭidesanīyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc60
msgctxt "pli-tv-pvr10:66.1"
msgid "Pañcasattati sekhiyā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:66.2"
msgid "tīhi samathehi sammanti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:66.3"
msgid "Sammukhā ca paṭiññāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:66.4"
msgid "tiṇavatthārakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc61
msgctxt "pli-tv-pvr10:67.1"
msgid "Dve uposathā dve pavāraṇā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:67.2"
msgid "Cattāri kammāni jinena desitā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:67.3"
msgid "Pañceva uddesā caturo bhavanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:67.4"
msgid "Anaññathā āpattikkhandhā ca bhavanti satta."
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-pvr10:68.1"
msgid "Adhikaraṇāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:68.2"
msgid "Sattahi samathehi sammanti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:68.3"
msgid "Dvīhi catūhi tīhi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:68.4"
msgid "Kiccaṃ ekena sammati”."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr10:69.1"
msgid "5. Pārājikādiāpatti"
msgstr ""

#. HTML: </h2><blockquote class="gatha"><p>
#. REF: sc63
msgctxt "pli-tv-pvr10:70.1"
msgid "“Pārājikan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:70.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:70.3"
msgid "Cuto paraddho bhaṭṭho ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:70.4"
msgid "saddhammā hi niraṅkato;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:70.5"
msgid "Saṃvāsopi tahiṃ natthi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:70.6"
msgid "tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-pvr10:71.1"
msgid "“Saṃghādiseso”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:71.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:71.3"
msgid "Saṃghova deti parivāsaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:71.4"
msgid "mūlāya paṭikassati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:71.5"
msgid "Mānattaṃ deti abbheti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:71.6"
msgid "tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-pvr10:72.1"
msgid "“Aniyato”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:72.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:72.3"
msgid "Aniyato na niyato,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:72.4"
msgid "anekaṃsikataṃ padaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:72.5"
msgid "Tiṇṇamaññataraṃ ṭhānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:72.6"
msgid "“aniyato”ti pavuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr10:73.1"
msgid "“Thullaccayan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:73.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:73.3"
msgid "Ekassa mūle yo deseti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:73.4"
msgid "yo ca taṃ paṭigaṇhati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:73.5"
msgid "Accayo tena samo natthi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:73.6"
msgid "tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr10:74.1"
msgid "“Nissaggiyan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:74.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:74.3"
msgid "Saṃghamajjhe gaṇamajjhe,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:74.4"
msgid "ekasseva ca ekato;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:74.5"
msgid "Nissajjitvāna deseti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:74.6"
msgid "tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-pvr10:75.1"
msgid "“Pācittiyan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:75.2"
msgid "Taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:75.3"
msgid "Pāteti kusalaṃ dhammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:75.4"
msgid "Ariyamaggaṃ aparajjhati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:75.5"
msgid "Cittasammohanaṭṭhānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:75.6"
msgid "Tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc69
msgctxt "pli-tv-pvr10:76.1"
msgid "“Pāṭidesanīyan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:76.2"
msgid "Taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:76.3"
msgid "Bhikkhu aññātako santo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:76.4"
msgid "Kicchā laddhāya bhojanaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:76.5"
msgid "Sāmaṃ gahetvā bhuñjeyya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:76.6"
msgid "“Gārayhan”ti pavuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc70
#. VAR: bhuñjantā → bhuñjantaṃ (bj, pts1) | buñjanto (s1-3)
msgctxt "pli-tv-pvr10:77.1"
msgid "Nimantanāsu bhuñjantā chandāya,"
msgstr ""

#. HTML: <br>
#. VAR: bhikkhuniṃ → bhikkhunī (bj, s1-3, pts1)
msgctxt "pli-tv-pvr10:77.2"
msgid "Vosāsati tattha bhikkhuniṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:77.3"
msgid "Anivāretvā tahiṃ bhuñje,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:77.4"
msgid "“Gārayhan”ti pavuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc71, pts-vp-pli5.149
msgctxt "pli-tv-pvr10:78.1"
msgid "Saddhācittaṃ kulaṃ gantvā,"
msgstr ""

#. HTML: <br>
#. VAR: anāḷiyaṃ → anāḷhiyaṃ (bj, s1-3, pts1)
msgctxt "pli-tv-pvr10:78.2"
msgid "appabhogaṃ anāḷiyaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:78.3"
msgid "Agilāno tahiṃ bhuñje,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:78.4"
msgid "“gārayhan”ti pavuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-pvr10:79.1"
msgid "Yo ce araññe viharanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:79.2"
msgid "sāsaṅke sabhayānake;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:79.3"
msgid "Aviditaṃ tahiṃ bhuñje,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:79.4"
msgid "“gārayhan”ti pavuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-pvr10:80.1"
msgid "Bhikkhunī aññātikā santā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:80.2"
msgid "Yaṃ paresaṃ mamāyitaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:80.3"
msgid "Sappi telaṃ madhuṃ phāṇitaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:80.4"
msgid "Macchamaṃsaṃ atho khīraṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:80.5"
msgid "Dadhiṃ sayaṃ viññāpeyya bhikkhunī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:80.6"
msgid "Gārayhapattā sugatassa sāsane."
msgstr ""

#. HTML: </p><p>
#. REF: sc74
msgctxt "pli-tv-pvr10:81.1"
msgid "“Dukkaṭan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:81.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:81.3"
msgid "Aparaddhaṃ viraddhañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:81.4"
msgid "khalitaṃ yañca dukkaṭaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc75
msgctxt "pli-tv-pvr10:82.1"
msgid "Yaṃ manusso kare pāpaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:82.2"
msgid "āvi vā yadi vā raho;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:82.3"
msgid "“Dukkaṭan”ti pavedenti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:82.4"
msgid "tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-pvr10:83.1"
msgid "“Dubbhāsitan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:83.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:83.3"
msgid "Dubbhāsitaṃ durābhaṭṭhaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:83.4"
msgid "saṃkiliṭṭhañca yaṃ padaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:83.5"
msgid "Yañca viññū garahanti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:83.6"
msgid "tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-pvr10:84.1"
msgid "“Sekhiyan”ti yaṃ vuttaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:84.2"
msgid "taṃ suṇohi yathātathaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:84.3"
msgid "Sekkhassa sikkhamānassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:84.4"
msgid "ujumaggānusārino."
msgstr ""

#. HTML: </p><p>
#. REF: sc78
msgctxt "pli-tv-pvr10:85.1"
msgid "Ādi cetaṃ caraṇañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:85.2"
msgid "mukhaṃ saññamasaṃvaro;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:85.3"
msgid "Sikkhā etādisī natthi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:85.4"
msgid "tenetaṃ iti vuccati."
msgstr ""

#. HTML: </p><p>
#. REF: sc79
msgctxt "pli-tv-pvr10:86.1"
msgid "Channamativassati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:86.2"
msgid "vivaṭaṃ nātivassati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:86.3"
msgid "Tasmā channaṃ vivaretha,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:86.4"
msgid "evaṃ taṃ nātivassati."
msgstr ""

#. HTML: </p><p>
#. REF: sc80
msgctxt "pli-tv-pvr10:87.1"
msgid "Gati migānaṃ pavanaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:87.2"
msgid "Ākāso pakkhinaṃ gati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:87.3"
msgid "Vibhavo gati dhammānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:87.4"
msgid "Nibbānaṃ arahato gatīti."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc81
msgctxt "pli-tv-pvr10:88.1"
msgid "Gāthāsaṅgaṇikaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc82
msgctxt "pli-tv-pvr10:89.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc83
msgctxt "pli-tv-pvr10:90.1"
msgid "Sattanagaresu paññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:90.2"
msgid "vipatti caturopi ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:90.3"
msgid "Bhikkhūnaṃ bhikkhunīnañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:90.4"
msgid "sādhāraṇā asādhāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:90.5"
msgid "Sāsanaṃ anuggahāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr10:90.6"
msgid "gāthāsaṅgaṇikaṃ idanti."
msgstr ""

#. HTML: </p></blockquote><p class="endsection">
#. REF: sc84
msgctxt "pli-tv-pvr10:91.1"
msgid "Gāthāsaṅgaṇikaṃ niṭṭhitaṃ."
msgstr ""
