#. HTML: <section class="sutta" id="pli-tv-pvr2.10"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr2.10:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.10:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.10:3.1"
msgid "Dutiyabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.10:4.1"
msgid "Katthapaññattivāra"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr2.10:5.1"
msgid "2. Saṃghādisesakaṇḍādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1
#. VAR: aḍḍaṃ → aṭṭa (bj, s1-3) | aṭṭaṃ (pts1)
msgctxt "pli-tv-pvr2.10:6.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena ussayavādikāya bhikkhuniyā aḍḍaṃ karaṇapaccayā saṃghādiseso kattha paññatto?"
msgstr ""

msgctxt "pli-tv-pvr2.10:6.2"
msgid "Kaṃ ārabbha?"
msgstr ""

msgctxt "pli-tv-pvr2.10:6.3"
msgid "Kismiṃ vatthusmiṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr2.10:6.5"
msgid "kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.10:7.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena ussayavādikāya bhikkhuniyā aḍḍaṃ karaṇapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.6"
msgid "Thullanandā bhikkhunī ussayavādikā vihari, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.9"
msgid "Anupaññatti anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.16"
msgid "Catunnaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.19"
msgid "Tatiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.23"
msgid "Saṃghādisesāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr2.10:7.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.25"
msgid "Dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.26"
msgid "siyā kāyato ca vācato ca samuṭṭhāti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.27"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.29"
msgid "kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:7.30"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc3
msgctxt "pli-tv-pvr2.10:8.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.10:8.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.10:8.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.10:8.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc4
msgctxt "pli-tv-pvr2.10:9.1"
msgid "…"
msgstr ""

msgctxt "pli-tv-pvr2.10:9.2"
msgid "pe …"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc5
msgctxt "pli-tv-pvr2.10:10.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.10:10.2"
msgid "Vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.10:10.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.10:10.4"
msgid "Piṭakaṃ tambapaṇṇiyāti."
msgstr ""

msgctxt "pli-tv-pvr2.10:10.5"
msgid "(1)"
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc6
msgctxt "pli-tv-pvr2.10:11.1"
msgid "Coriṃ vuṭṭhāpanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:11.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:11.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:11.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:11.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:11.6"
msgid "Thullanandā bhikkhunī coriṃ vuṭṭhāpesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:11.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:11.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:11.9"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr2.10:11.10"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr2.10:11.11"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:11.12"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr2.10:12.1"
msgid "Ekā gāmantaraṃ gamanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:12.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:12.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:12.4"
msgid "Aññataraṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:12.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:12.6"
msgid "Aññatarā bhikkhunī ekā gāmantaraṃ gacchi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:12.7"
msgid "Ekā paññatti, tisso anupaññattiyo."
msgstr ""

msgctxt "pli-tv-pvr2.10:12.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:12.9"
msgid "paṭhamapārājike …"
msgstr ""

msgctxt "pli-tv-pvr2.10:12.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:12.11"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr2.10:13.1"
msgid "Samaggena saṃghena ukkhittaṃ bhikkhuniṃ dhammena vinayena satthusāsanena anapaloketvā kārakasaṃghaṃ anaññāya gaṇassa chandaṃ osāraṇapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:13.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:13.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:13.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:13.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:13.6"
msgid "Thullanandā bhikkhunī samaggena saṃghena ukkhittaṃ bhikkhuniṃ dhammena vinayena satthusāsanena anapaloketvā kārakasaṃghaṃ anaññāya gaṇassa chandaṃ osāresi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:13.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:13.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:13.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.10:13.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:13.11"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr2.10:14.1"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā bhuñjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:14.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:14.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:14.4"
msgid "Sundarīnandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:14.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:14.6"
msgid "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa hatthato āmisaṃ paṭiggahesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:14.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:14.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:14.9"
msgid "paṭhamapārājike …"
msgstr ""

msgctxt "pli-tv-pvr2.10:14.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:14.11"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr2.10:15.1"
msgid "“Kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-pvr2.10:15.2"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā”ti uyyojanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:15.3"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:15.4"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:15.5"
msgid "Aññataraṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:15.6"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:15.7"
msgid "Aññatarā bhikkhunī—"
msgstr ""

msgctxt "pli-tv-pvr2.10:15.8"
msgid "“kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-pvr2.10:15.9"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā”ti uyyojesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:15.10"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:15.11"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr2.10:15.12"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:15.13"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr2.10:16.1"
msgid "Kupitāya anattamanāya bhikkhuniyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:16.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:16.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:16.4"
msgid "Caṇḍakāḷiṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:16.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:16.6"
msgid "Caṇḍakāḷī bhikkhunī kupitā anattamanā evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-pvr2.10:16.7"
msgid "“buddhaṃ paccācikkhāmi, dhammaṃ paccācikkhāmi, saṃghaṃ paccācikkhāmi, sikkhaṃ paccācikkhāmī”ti, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:16.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:16.9"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:16.10"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.10:16.11"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:16.12"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr2.10:17.1"
msgid "Kismiñcideva adhikaraṇe paccākatāya bhikkhuniyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:17.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:17.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:17.4"
msgid "Caṇḍakāḷiṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:17.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:17.6"
msgid "Caṇḍakāḷī bhikkhunī kismiñcideva adhikaraṇe paccākatā kupitā anattamanā evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-pvr2.10:17.7"
msgid "“chandagāminiyo ca bhikkhuniyo, dosagāminiyo ca bhikkhuniyo, mohagāminiyo ca bhikkhuniyo, bhayagāminiyo ca bhikkhuniyo”ti, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:17.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:17.9"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:17.10"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.10:17.11"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:17.12"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr2.10:18.1"
msgid "Saṃsaṭṭhānaṃ bhikkhunīnaṃ yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:18.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:18.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:18.4"
msgid "Sambahulā bhikkhuniyo ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:18.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:18.6"
msgid "Sambahulā bhikkhuniyo saṃsaṭṭhā vihariṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:18.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:18.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:18.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.10:18.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:18.11"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc14, pts-vp-pli5.83
msgctxt "pli-tv-pvr2.10:19.1"
msgid "“Saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-pvr2.10:19.2"
msgid "Mā tumhe nānā viharitthā”ti uyyojentiyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:19.3"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.10:19.4"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:19.5"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:19.6"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:19.7"
msgid "Thullanandā bhikkhunī—"
msgstr ""

msgctxt "pli-tv-pvr2.10:19.8"
msgid "“saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-pvr2.10:19.9"
msgid "Mā tumhe nānā viharitthā”ti uyyojesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:19.10"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:19.11"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.10:19.12"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.10:19.13"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:19.14"
msgid "(10)"
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr2.10:20.1"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā pāṭidesanīyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:20.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:20.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:20.4"
msgid "Chabbaggiyā bhikkhuniyo ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.10:20.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.10:20.6"
msgid "Chabbaggiyā bhikkhuniyo dadhiṃ viññāpetvā bhuñjiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.10:20.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.10:20.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ catūhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr2.10:20.9"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.10:20.10"
msgid "(11)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc16
msgctxt "pli-tv-pvr2.10:21.1"
msgid "Katthapaññattivāro niṭṭhito paṭhamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr2.10:22.1"
msgid "Katāpattivāra"
msgstr ""

#. HTML: </h2><h2>
msgctxt "pli-tv-pvr2.10:23.1"
msgid "1. Pārājikakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc17
msgctxt "pli-tv-pvr2.10:24.1"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.10:24.2"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:24.3"
msgid "Avassutā bhikkhunī avassutassa purisapuggalassa adhakkhakaṃ ubbhajāṇumaṇḍalaṃ gahaṇaṃ sādiyati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:24.4"
msgid "bhikkhu kāyena kāyaṃ āmasati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:24.5"
msgid "kāyena kāyapaṭibaddhaṃ āmasati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:24.6"
msgid "kāyapaṭibaddhena kāyapaṭibaddhaṃ āmasati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:24.7"
msgid "aṅgulipatodake pācittiyaṃ—"
msgstr ""

msgctxt "pli-tv-pvr2.10:24.8"
msgid "kāyasaṃsaggaṃ sādiyanapaccayā imā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:24.9"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr2.10:25.1"
msgid "Vajjappaṭicchādanapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.10:25.2"
msgid "Vajjappaṭicchādanapaccayā catasso āpattiyo āpajjati."
msgstr ""

#. VAR: pārājikaṃ dhammaṃ → dhammaṃ ajjhāpannaṃ (s1-3)
msgctxt "pli-tv-pvr2.10:25.3"
msgid "Bhikkhunī jānaṃ pārājikaṃ dhammaṃ paṭicchādeti, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:25.4"
msgid "vematikā paṭicchādeti, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:25.5"
msgid "bhikkhu saṃghādisesaṃ paṭicchādeti, āpatti pācittiyassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:25.6"
msgid "ācāravipattiṃ paṭicchādeti, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr2.10:25.7"
msgid "vajjappaṭicchādanapaccayā imā catasso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:25.8"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-pvr2.10:26.1"
msgid "Yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.10:26.2"
msgid "Yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:26.3"
msgid "Ukkhittānuvattikā bhikkhunī yāvatatiyaṃ samanubhāsanāya na paṭinissajjati, ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr2.10:26.4"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr2.10:26.5"
msgid "kammavācāpariyosāne āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:26.6"
msgid "bhedakānuvattikā bhikkhunī yāvatatiyaṃ samanubhāsanāya na paṭinissajjati, āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:26.7"
msgid "pāpikāya diṭṭhiyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjati, āpatti pācittiyassa—"
msgstr ""

msgctxt "pli-tv-pvr2.10:26.8"
msgid "yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā imā pañca āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:26.9"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-pvr2.10:27.1"
msgid "Aṭṭhamaṃ vatthuṃ paripūraṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.10:27.2"
msgid "Aṭṭhamaṃ vatthuṃ paripūraṇapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:27.3"
msgid "Purisena—"
msgstr ""

msgctxt "pli-tv-pvr2.10:27.4"
msgid "“itthannāmaṃ okāsaṃ āgacchā”ti vuttā gacchati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:27.5"
msgid "purisassa hatthapāsaṃ okkantamatte āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:27.6"
msgid "aṭṭhamaṃ vatthuṃ paripūreti, āpatti pārājikassa—"
msgstr ""

msgctxt "pli-tv-pvr2.10:27.7"
msgid "aṭṭhamaṃ vatthuṃ paripūraṇapaccayā imā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:27.8"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc21
msgctxt "pli-tv-pvr2.10:28.1"
msgid "Pārājikā niṭṭhitā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr2.10:29.1"
msgid "2. Saṃghādisesakaṇḍādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc22
msgctxt "pli-tv-pvr2.10:30.1"
msgid "Ussayavādikā bhikkhunī aḍḍaṃ karaṇapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:30.2"
msgid "Ekassa āroceti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:30.3"
msgid "dutiyassa āroceti, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:30.4"
msgid "aḍḍapariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:30.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc23, pts-vp-pli5.84
msgctxt "pli-tv-pvr2.10:31.1"
msgid "Coriṃ vuṭṭhāpanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:31.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr2.10:31.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr2.10:31.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:31.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr2.10:32.1"
msgid "Ekā gāmantaraṃ gamanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:32.2"
msgid "Gacchati, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:32.3"
msgid "paṭhamaṃ pādaṃ parikkhepaṃ atikkāmeti, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:32.4"
msgid "dutiyaṃ pādaṃ atikkāmeti, āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:32.5"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-pvr2.10:33.1"
msgid "Samaggena saṃghena ukkhittaṃ bhikkhuniṃ dhammena vinayena satthusāsanena anapaloketvā kārakasaṃghaṃ anaññāya gaṇassa chandaṃ osāraṇapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:33.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr2.10:33.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr2.10:33.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:33.5"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr2.10:34.1"
msgid "Avassutā bhikkhunī avassutassa purisapuggalassa hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā bhuñjanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:34.2"
msgid "“Khādissāmi bhuñjissāmī”ti paṭiggaṇhāti, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:34.3"
msgid "ajjhohāre ajjhohāre āpatti saṃghādisesassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:34.4"
msgid "udakadantaponaṃ paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:34.5"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-pvr2.10:35.1"
msgid "“Kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-pvr2.10:35.2"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā”ti, uyyojanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:35.3"
msgid "Tassā vacanena khādissāmi bhuñjissāmīti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:35.4"
msgid "ajjhohāre ajjhohāre āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:35.5"
msgid "bhojanapariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:35.6"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc28
msgctxt "pli-tv-pvr2.10:36.1"
msgid "Kupitā bhikkhunī yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:36.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr2.10:36.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr2.10:36.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:36.5"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-pvr2.10:37.1"
msgid "Kismiñcideva adhikaraṇe paccākatā bhikkhunī yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:37.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr2.10:37.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr2.10:37.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:37.5"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-pvr2.10:38.1"
msgid "Saṃsaṭṭhā bhikkhunī yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:38.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr2.10:38.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr2.10:38.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:38.5"
msgid "(9)"
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-pvr2.10:39.1"
msgid "“Saṃsaṭṭhāva, ayye, tumhe viharatha, mā tumhe nānā viharitthā”ti uyyojentī yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:39.2"
msgid "Ñattiyā dukkaṭaṃ;"
msgstr ""

msgctxt "pli-tv-pvr2.10:39.3"
msgid "dvīhi kammavācāhi thullaccayā;"
msgstr ""

msgctxt "pli-tv-pvr2.10:39.4"
msgid "kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-pvr2.10:39.5"
msgid "(10)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc32
msgctxt "pli-tv-pvr2.10:40.1"
msgid "Dasa saṃghādisesā niṭṭhitā …"
msgstr ""

msgctxt "pli-tv-pvr2.10:40.2"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc33
msgctxt "pli-tv-pvr2.10:41.1"
msgid "(Yathā heṭṭhā tathā vitthāretabbā paccayameva nānākaraṇaṃ)"
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-pvr2.10:42.1"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.10:42.2"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:42.3"
msgid "Bhuñjissāmīti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.10:42.4"
msgid "ajjhohāre ajjhohāre āpatti pāṭidesanīyassa—"
msgstr ""

msgctxt "pli-tv-pvr2.10:42.5"
msgid "dadhiṃ viññāpetvā bhuñjanapaccayā imā dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.10:42.6"
msgid "(11)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc35
msgctxt "pli-tv-pvr2.10:43.1"
msgid "Katāpattivāro niṭṭhito dutiyo."
msgstr ""

