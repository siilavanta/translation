#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:168.1"
msgid "15. Samuṭṭhāpanavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc152
msgctxt "pli-tv-pvr5:169.1"
msgid "Vivādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ samuṭṭhāpeti?"
msgstr ""

msgctxt "pli-tv-pvr5:169.2"
msgid "Vivādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ na katamaṃ adhikaraṇaṃ samuṭṭhāpeti;"
msgstr ""

msgctxt "pli-tv-pvr5:169.3"
msgid "api ca vivādādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

msgctxt "pli-tv-pvr5:169.4"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr5:169.5"
msgid "Idha bhikkhū vivadanti—"
msgstr ""

msgctxt "pli-tv-pvr5:169.6"
msgid "“dhammo”ti vā, “adhammo”ti vā, “vinayo”ti vā, “avinayo”ti vā, “abhāsitaṃ alapitaṃ tathāgatenā”ti vā, “bhāsitaṃ lapitaṃ tathāgatenā”ti vā, “anāciṇṇaṃ tathāgatenā”ti vā, “āciṇṇaṃ tathāgatenā”ti vā, “apaññattaṃ tathāgatenā”ti vā, “paññattaṃ tathāgatenā”ti vā, “āpattī”ti vā, “anāpattī”ti vā, “lahukā āpattī”ti vā, “garukā āpattī”ti vā, “sāvasesā āpattī”ti vā, “anavasesā āpattī”ti vā, “duṭṭhullā āpattī”ti vā “aduṭṭhullā āpattī”ti vā."
msgstr ""

#. VAR: medhakaṃ → medhagaṃ (mr)
msgctxt "pli-tv-pvr5:169.7"
msgid "Yaṃ tattha bhaṇḍanaṃ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhakaṃ, idaṃ vuccati vivādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:169.8"
msgid "Vivādādhikaraṇe saṃgho vivadati."
msgstr ""

msgctxt "pli-tv-pvr5:169.9"
msgid "Vivādādhikaraṇaṃ vivadamāno anuvadati."
msgstr ""

msgctxt "pli-tv-pvr5:169.10"
msgid "Anuvādādhikaraṇaṃ anuvadamāno āpattiṃ āpajjati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:169.11"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:169.12"
msgid "Evaṃ vivādādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc153
msgctxt "pli-tv-pvr5:170.1"
msgid "Anuvādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ samuṭṭhāpeti?"
msgstr ""

msgctxt "pli-tv-pvr5:170.2"
msgid "Anuvādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ na katamaṃ adhikaraṇaṃ samuṭṭhāpeti;"
msgstr ""

msgctxt "pli-tv-pvr5:170.3"
msgid "api ca anuvādādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

msgctxt "pli-tv-pvr5:170.4"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr5:170.5"
msgid "Idha bhikkhū bhikkhuṃ anuvadanti sīlavipattiyā vā ācāravipattiyā vā diṭṭhivipattiyā vā ājīvavipattiyā vā."
msgstr ""

msgctxt "pli-tv-pvr5:170.6"
msgid "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṃ, idaṃ vuccati anuvādādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:170.7"
msgid "Anuvādādhikaraṇe saṃgho vivadati."
msgstr ""

msgctxt "pli-tv-pvr5:170.8"
msgid "Vivādādhikaraṇaṃ vivadamāno anuvadati."
msgstr ""

msgctxt "pli-tv-pvr5:170.9"
msgid "Anuvādādhikaraṇaṃ anuvadamāno āpattiṃ āpajjati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:170.10"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:170.11"
msgid "Evaṃ anuvādādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc154, pts-vp-pli5.112
msgctxt "pli-tv-pvr5:171.1"
msgid "Āpattādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ samuṭṭhāpeti?"
msgstr ""

msgctxt "pli-tv-pvr5:171.2"
msgid "Āpattādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ na katamaṃ adhikaraṇaṃ samuṭṭhāpeti;"
msgstr ""

msgctxt "pli-tv-pvr5:171.3"
msgid "api ca āpattādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

msgctxt "pli-tv-pvr5:171.4"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr5:171.5"
msgid "Pañcapi āpattikkhandhā āpattādhikaraṇaṃ, sattapi āpattikkhandhā āpattādhikaraṇaṃ, idaṃ vuccati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:171.6"
msgid "Āpattādhikaraṇe saṃgho vivadati."
msgstr ""

msgctxt "pli-tv-pvr5:171.7"
msgid "Vivādādhikaraṇaṃ vivadamāno anuvadati."
msgstr ""

msgctxt "pli-tv-pvr5:171.8"
msgid "Anuvādādhikaraṇaṃ anuvadamāno āpattiṃ āpajjati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:171.9"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:171.10"
msgid "Evaṃ āpattādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc155
msgctxt "pli-tv-pvr5:172.1"
msgid "Kiccādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ samuṭṭhāpeti?"
msgstr ""

msgctxt "pli-tv-pvr5:172.2"
msgid "Kiccādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ na katamaṃ adhikaraṇaṃ samuṭṭhāpeti, api ca kiccādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

msgctxt "pli-tv-pvr5:172.3"
msgid "Yathā kathaṃ viya?"
msgstr ""

msgctxt "pli-tv-pvr5:172.4"
msgid "Yā saṃghassa kiccayatā karaṇīyatā apalokanakammaṃ ñattikammaṃ ñattidutiyakammaṃ ñatticatutthakammaṃ, idaṃ vuccati kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:172.5"
msgid "Kiccādhikaraṇe saṃgho vivadati."
msgstr ""

msgctxt "pli-tv-pvr5:172.6"
msgid "Vivādādhikaraṇaṃ vivadamāno anuvadati."
msgstr ""

msgctxt "pli-tv-pvr5:172.7"
msgid "Anuvādādhikaraṇaṃ anuvadamāno āpattiṃ āpajjati āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:172.8"
msgid "Tāya āpattiyā saṃgho kammaṃ karoti kiccādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:172.9"
msgid "Evaṃ kiccādhikaraṇapaccayā cattāro adhikaraṇā jāyanti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc156
msgctxt "pli-tv-pvr5:173.1"
msgid "Samuṭṭhāpanavāro niṭṭhito vīsatimo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:174.1"
msgid "16. Bhajativāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc157
msgctxt "pli-tv-pvr5:175.1"
msgid "Vivādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ bhajati?"
msgstr ""

msgctxt "pli-tv-pvr5:175.2"
msgid "Katamaṃ adhikaraṇaṃ upanissitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:175.3"
msgid "Katamaṃ adhikaraṇaṃ pariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:175.4"
msgid "Katamena adhikaraṇena saṅgahitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:175.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc158
msgctxt "pli-tv-pvr5:176.1"
msgid "Anuvādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ bhajati?"
msgstr ""

msgctxt "pli-tv-pvr5:176.2"
msgid "Katamaṃ adhikaraṇaṃ upanissitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:176.3"
msgid "Katamaṃ adhikaraṇaṃ pariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:176.4"
msgid "Katamena adhikaraṇena saṅgahitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:176.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc159
msgctxt "pli-tv-pvr5:177.1"
msgid "Āpattādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ bhajati?"
msgstr ""

msgctxt "pli-tv-pvr5:177.2"
msgid "Katamaṃ adhikaraṇaṃ upanissitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:177.3"
msgid "Katamaṃ adhikaraṇaṃ pariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:177.4"
msgid "Katamena adhikaraṇena saṅgahitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:177.5"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc160
msgctxt "pli-tv-pvr5:178.1"
msgid "Kiccādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ bhajati?"
msgstr ""

msgctxt "pli-tv-pvr5:178.2"
msgid "Katamaṃ adhikaraṇaṃ upanissitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:178.3"
msgid "Katamaṃ adhikaraṇaṃ pariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:178.4"
msgid "Katamena adhikaraṇena saṅgahitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:178.5"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc161
msgctxt "pli-tv-pvr5:179.1"
msgid "Vivādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ vivādādhikaraṇaṃ bhajati, vivādādhikaraṇaṃ upanissitaṃ, vivādādhikaraṇaṃ pariyāpannaṃ, vivādādhikaraṇena saṅgahitaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:179.2"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc162
msgctxt "pli-tv-pvr5:180.1"
msgid "Anuvādādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ anuvādādhikaraṇaṃ bhajati, anuvādādhikaraṇaṃ upanissitaṃ, anuvādādhikaraṇaṃ pariyāpannaṃ, anuvādādhikaraṇena saṅgahitaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:180.2"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc163
msgctxt "pli-tv-pvr5:181.1"
msgid "Āpattādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ āpattādhikaraṇaṃ bhajati, āpattādhikaraṇaṃ upanissitaṃ, āpattādhikaraṇaṃ pariyāpannaṃ, āpattādhikaraṇena saṅgahitaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:181.2"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc164
msgctxt "pli-tv-pvr5:182.1"
msgid "Kiccādhikaraṇaṃ catunnaṃ adhikaraṇānaṃ kiccādhikaraṇaṃ bhajati, kiccādhikaraṇaṃ upanissitaṃ, kiccādhikaraṇaṃ pariyāpannaṃ, kiccādhikaraṇena saṅgahitaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:182.2"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc165
msgctxt "pli-tv-pvr5:183.1"
msgid "Vivādādhikaraṇaṃ sattannaṃ samathānaṃ kati samathe bhajati, kati samathe upanissitaṃ, kati samathe pariyāpannaṃ, katihi samathehi saṅgahitaṃ, katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:183.2"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc166
msgctxt "pli-tv-pvr5:184.1"
msgid "Anuvādādhikaraṇaṃ sattannaṃ samathānaṃ kati samathe bhajati, kati samathe upanissitaṃ, kati samathe pariyāpannaṃ, katihi samathehi saṅgahitaṃ, katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:184.2"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc167
msgctxt "pli-tv-pvr5:185.1"
msgid "Āpattādhikaraṇaṃ sattannaṃ samathānaṃ kati samathe bhajati, kati samathe upanissitaṃ, kati samathe pariyāpannaṃ, katihi samathehi saṅgahitaṃ, katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:185.2"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc168
msgctxt "pli-tv-pvr5:186.1"
msgid "Kiccādhikaraṇaṃ sattannaṃ samathānaṃ kati samathe bhajati, kati samathe upanissitaṃ, kati samathe pariyāpannaṃ, katihi samathehi saṅgahitaṃ, katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:186.2"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc169
msgctxt "pli-tv-pvr5:187.1"
msgid "Vivādādhikaraṇaṃ sattannaṃ samathānaṃ dve samathe bhajati, dve samathe upanissitaṃ, dve samathe pariyāpannaṃ, dvīhi samathehi saṅgahitaṃ, dvīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:187.2"
msgid "sammukhāvinayena ca yebhuyyasikāya ca."
msgstr ""

msgctxt "pli-tv-pvr5:187.3"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc170
msgctxt "pli-tv-pvr5:188.1"
msgid "Anuvādādhikaraṇaṃ sattannaṃ samathānaṃ cattāro samathe bhajati, cattāro samathe upanissitaṃ, cattāro samathe pariyāpannaṃ, catūhi samathehi saṅgahitaṃ, catūhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:188.2"
msgid "sammukhāvinayena ca sativinayena ca amūḷhavinayena ca tassapāpiyasikāya ca."
msgstr ""

msgctxt "pli-tv-pvr5:188.3"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc171, pts-vp-pli5.113
msgctxt "pli-tv-pvr5:189.1"
msgid "Āpattādhikaraṇaṃ sattannaṃ samathānaṃ tayo samathe bhajati, tayo samathe upanissitaṃ, tayo samathe pariyāpannaṃ, tīhi samathehi saṅgahitaṃ tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:189.2"
msgid "sammukhāvinayena ca paṭiññātakaraṇena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr5:189.3"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc172
msgctxt "pli-tv-pvr5:190.1"
msgid "Kiccādhikaraṇaṃ sattannaṃ samathānaṃ ekaṃ samathaṃ bhajati, ekaṃ samathaṃ upanissitaṃ, ekaṃ samathaṃ pariyāpannaṃ, ekena samathena saṅgahitaṃ, ekena samathena sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:190.2"
msgid "sammukhāvinayenāti."
msgstr ""

msgctxt "pli-tv-pvr5:190.3"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc173
msgctxt "pli-tv-pvr5:191.1"
msgid "Bhajativāro niṭṭhito ekavīsatimo."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc174
msgctxt "pli-tv-pvr5:192.1"
msgid "Samathabhedo niṭṭhito."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc175
msgctxt "pli-tv-pvr5:193.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc176
msgctxt "pli-tv-pvr5:194.1"
msgid "Adhikaraṇaṃ pariyāyaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:194.2"
msgid "sādhāraṇā ca bhāgiyā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:194.3"
msgid "Samathā sādhāraṇikā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:194.4"
msgid "samathassa tabbhāgiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc177
msgctxt "pli-tv-pvr5:195.1"
msgid "Samathā sammukhā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:195.2"
msgid "vinayena kusalena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:195.3"
msgid "Yattha samathasaṃsaṭṭhā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:195.4"
msgid "sammanti na sammanti ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:195.5"
msgid "Samathādhikaraṇañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:195.6"
msgid "samuṭṭhānaṃ bhajanti cāti."
msgstr ""

