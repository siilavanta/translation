#. HTML: <section class="sutta" id="pli-tv-pvr5"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr5:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr5:2.1"
msgid "Samathabheda"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr5:3.1"
msgid "1. Adhikaraṇapariyāyavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1
msgctxt "pli-tv-pvr5:4.1"
msgid "Vivādādhikaraṇassa—"
msgstr ""

msgctxt "pli-tv-pvr5:4.2"
msgid "kiṃ pubbaṅgamaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:4.3"
msgid "Kati ṭhānāni?"
msgstr ""

msgctxt "pli-tv-pvr5:4.4"
msgid "Kati vatthūni?"
msgstr ""

msgctxt "pli-tv-pvr5:4.5"
msgid "Kati bhūmiyo?"
msgstr ""

msgctxt "pli-tv-pvr5:4.6"
msgid "Kati hetū?"
msgstr ""

msgctxt "pli-tv-pvr5:4.7"
msgid "Kati mūlāni?"
msgstr ""

msgctxt "pli-tv-pvr5:4.8"
msgid "Katihākārehi vivadati?"
msgstr ""

msgctxt "pli-tv-pvr5:4.9"
msgid "Vivādādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:4.10"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr5:5.1"
msgid "Anuvādādhikaraṇassa—"
msgstr ""

msgctxt "pli-tv-pvr5:5.2"
msgid "kiṃ pubbaṅgamaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:5.3"
msgid "Kati ṭhānāni?"
msgstr ""

msgctxt "pli-tv-pvr5:5.4"
msgid "Kati vatthūni?"
msgstr ""

msgctxt "pli-tv-pvr5:5.5"
msgid "Kati bhūmiyo?"
msgstr ""

msgctxt "pli-tv-pvr5:5.6"
msgid "Kati hetū?"
msgstr ""

msgctxt "pli-tv-pvr5:5.7"
msgid "Kati mūlāni?"
msgstr ""

msgctxt "pli-tv-pvr5:5.8"
msgid "Katihākārehi anuvadati?"
msgstr ""

msgctxt "pli-tv-pvr5:5.9"
msgid "Anuvādādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:5.10"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr5:6.1"
msgid "Āpattādhikaraṇassa—"
msgstr ""

msgctxt "pli-tv-pvr5:6.2"
msgid "kiṃ pubbaṅgamaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:6.3"
msgid "Kati ṭhānāni?"
msgstr ""

msgctxt "pli-tv-pvr5:6.4"
msgid "Kati vatthūni?"
msgstr ""

msgctxt "pli-tv-pvr5:6.5"
msgid "Kati bhūmiyo?"
msgstr ""

msgctxt "pli-tv-pvr5:6.6"
msgid "Kati hetū?"
msgstr ""

msgctxt "pli-tv-pvr5:6.7"
msgid "Kati mūlāni?"
msgstr ""

msgctxt "pli-tv-pvr5:6.8"
msgid "Katihākārehi āpattiṃ āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr5:6.9"
msgid "Āpattādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:6.10"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr5:7.1"
msgid "Kiccādhikaraṇassa—"
msgstr ""

msgctxt "pli-tv-pvr5:7.2"
msgid "kiṃ pubbaṅgamaṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:7.3"
msgid "Kati ṭhānāni?"
msgstr ""

msgctxt "pli-tv-pvr5:7.4"
msgid "Kati vatthūni?"
msgstr ""

msgctxt "pli-tv-pvr5:7.5"
msgid "Kati bhūmiyo?"
msgstr ""

msgctxt "pli-tv-pvr5:7.6"
msgid "Kati hetū?"
msgstr ""

msgctxt "pli-tv-pvr5:7.7"
msgid "Kati mūlāni?"
msgstr ""

msgctxt "pli-tv-pvr5:7.8"
msgid "Katihākārehi kiccaṃ jāyati?"
msgstr ""

msgctxt "pli-tv-pvr5:7.9"
msgid "Kiccādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:7.10"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc5, pts-vp-pli5.102
msgctxt "pli-tv-pvr5:8.1"
msgid "Vivādādhikaraṇassa kiṃ pubbaṅgamanti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.2"
msgid "Lobho pubbaṅgamo, doso pubbaṅgamo, moho pubbaṅgamo, alobho pubbaṅgamo, adoso pubbaṅgamo, amoho pubbaṅgamo."
msgstr ""

msgctxt "pli-tv-pvr5:8.3"
msgid "Kati ṭhānānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.4"
msgid "Aṭṭhārasa bhedakaravatthūni ṭhānāni."
msgstr ""

msgctxt "pli-tv-pvr5:8.5"
msgid "Kati vatthūnīti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.6"
msgid "Aṭṭhārasa bhedakaravatthūni."
msgstr ""

msgctxt "pli-tv-pvr5:8.7"
msgid "Kati bhūmiyoti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.8"
msgid "Aṭṭhārasa bhedakaravatthūni bhūmiyo."
msgstr ""

msgctxt "pli-tv-pvr5:8.9"
msgid "Kati hetūti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.10"
msgid "Nava hetū—"
msgstr ""

msgctxt "pli-tv-pvr5:8.11"
msgid "tayo kusalahetū, tayo akusalahetū, tayo abyākatahetū."
msgstr ""

msgctxt "pli-tv-pvr5:8.12"
msgid "Kati mūlānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.13"
msgid "Dvādasa mūlāni."
msgstr ""

msgctxt "pli-tv-pvr5:8.14"
msgid "Katihākārehi vivadatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.15"
msgid "Dvīhākārehi vivadati—"
msgstr ""

msgctxt "pli-tv-pvr5:8.16"
msgid "dhammadiṭṭhi vā adhammadiṭṭhi vā."
msgstr ""

msgctxt "pli-tv-pvr5:8.17"
msgid "Vivādādhikaraṇaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:8.18"
msgid "Vivādādhikaraṇaṃ dvīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:8.19"
msgid "sammukhāvinayena ca yebhuyyasikāya ca."
msgstr ""

msgctxt "pli-tv-pvr5:8.20"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr5:9.1"
msgid "Anuvādādhikaraṇassa kiṃ pubbaṅgamanti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.2"
msgid "Lobho pubbaṅgamo, doso pubbaṅgamo, moho pubbaṅgamo, alobho pubbaṅgamo, adoso pubbaṅgamo, amoho pubbaṅgamo."
msgstr ""

msgctxt "pli-tv-pvr5:9.3"
msgid "Kati ṭhānānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.4"
msgid "Catasso vipattiyo ṭhānāni."
msgstr ""

msgctxt "pli-tv-pvr5:9.5"
msgid "Kati vatthūnīti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.6"
msgid "Catasso vipattiyo vatthūni."
msgstr ""

msgctxt "pli-tv-pvr5:9.7"
msgid "Kati bhūmiyoti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.8"
msgid "Catasso vipattiyo bhūmiyo."
msgstr ""

msgctxt "pli-tv-pvr5:9.9"
msgid "Kati hetūti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.10"
msgid "Nava hetū—"
msgstr ""

msgctxt "pli-tv-pvr5:9.11"
msgid "tayo kusalahetū, tayo akusalahetū, tayo abyākatahetū."
msgstr ""

msgctxt "pli-tv-pvr5:9.12"
msgid "Kati mūlānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.13"
msgid "Cuddasa mūlāni."
msgstr ""

msgctxt "pli-tv-pvr5:9.14"
msgid "Katihākārehi anuvadatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.15"
msgid "Dvīhākārehi anuvadati—"
msgstr ""

msgctxt "pli-tv-pvr5:9.16"
msgid "vatthuto vā āpattito vā."
msgstr ""

msgctxt "pli-tv-pvr5:9.17"
msgid "Anuvādādhikaraṇaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:9.18"
msgid "Anuvādādhikaraṇaṃ catūhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:9.19"
msgid "sammukhāvinayena ca sativinayena ca amūḷhavinayena ca tassapāpiyasikāya ca."
msgstr ""

msgctxt "pli-tv-pvr5:9.20"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr5:10.1"
msgid "Āpattādhikaraṇassa kiṃ pubbaṅgamanti?"
msgstr ""

msgctxt "pli-tv-pvr5:10.2"
msgid "Lobho pubbaṅgamo, doso pubbaṅgamo, moho pubbaṅgamo, alobho pubbaṅgamo, adoso pubbaṅgamo, amoho pubbaṅgamo."
msgstr ""

msgctxt "pli-tv-pvr5:10.3"
msgid "Kati ṭhānānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:10.4"
msgid "Satta āpattikkhandhā ṭhānāni."
msgstr ""

msgctxt "pli-tv-pvr5:10.5"
msgid "Kati vatthūnīti?"
msgstr ""

msgctxt "pli-tv-pvr5:10.6"
msgid "Satta āpattikkhandhā vatthūni."
msgstr ""

msgctxt "pli-tv-pvr5:10.7"
msgid "Kati bhūmiyoti?"
msgstr ""

msgctxt "pli-tv-pvr5:10.8"
msgid "Satta āpattikkhandhā bhūmiyo."
msgstr ""

msgctxt "pli-tv-pvr5:10.9"
msgid "Kati hetūti?"
msgstr ""

#. VAR: Cha hetū → tayo kusalahetū (bj, s1-3) | kati hetūti nava hetū (pts1)
msgctxt "pli-tv-pvr5:10.10"
msgid "Cha hetū—"
msgstr ""

msgctxt "pli-tv-pvr5:10.11"
msgid "tayo akusalahetū, tayo abyākatahetū."
msgstr ""

msgctxt "pli-tv-pvr5:10.12"
msgid "Kati mūlānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:10.13"
msgid "Cha āpattisamuṭṭhānāni mūlāni."
msgstr ""

msgctxt "pli-tv-pvr5:10.14"
msgid "Katihākārehi āpattiṃ āpajjatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:10.15"
msgid "Chahākārehi āpattiṃ āpajjati—"
msgstr ""

msgctxt "pli-tv-pvr5:10.16"
msgid "alajjitā, aññāṇatā, kukkuccapakatatā, akappiye kappiyasaññitā, kappiye akappiyasaññitā, satisammosā."
msgstr ""

msgctxt "pli-tv-pvr5:10.17"
msgid "Āpattādhikaraṇaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:10.18"
msgid "Āpattādhikaraṇaṃ tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:10.19"
msgid "sammukhāvinayena ca paṭiññātakaraṇena ca, sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr5:10.20"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr5:11.1"
msgid "Kiccādhikaraṇassa kiṃ pubbaṅgamanti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.2"
msgid "Lobho pubbaṅgamo, doso pubbaṅgamo, moho pubbaṅgamo, alobho pubbaṅgamo, adoso pubbaṅgamo, amoho pubbaṅgamo."
msgstr ""

msgctxt "pli-tv-pvr5:11.3"
msgid "Kati ṭhānānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.4"
msgid "Cattāri kammāni ṭhānāni."
msgstr ""

msgctxt "pli-tv-pvr5:11.5"
msgid "Kati vatthūnīti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.6"
msgid "Cattāri kammāni vatthūni."
msgstr ""

msgctxt "pli-tv-pvr5:11.7"
msgid "Kati bhūmiyoti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.8"
msgid "Cattāri kammāni bhūmiyo."
msgstr ""

msgctxt "pli-tv-pvr5:11.9"
msgid "Kati hetūti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.10"
msgid "Nava hetū—"
msgstr ""

msgctxt "pli-tv-pvr5:11.11"
msgid "tayo kusalahetū, tayo akusalahetū, tayo abyākatahetū."
msgstr ""

msgctxt "pli-tv-pvr5:11.12"
msgid "Kati mūlānīti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.13"
msgid "Ekaṃ mūlaṃ—"
msgstr ""

msgctxt "pli-tv-pvr5:11.14"
msgid "saṃgho."
msgstr ""

msgctxt "pli-tv-pvr5:11.15"
msgid "Katihākārehi kiccaṃ jāyatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.16"
msgid "Dvīhākārehi kiccaṃ jāyati—"
msgstr ""

msgctxt "pli-tv-pvr5:11.17"
msgid "ñattito vā apalokanato vā."
msgstr ""

msgctxt "pli-tv-pvr5:11.18"
msgid "Kiccādhikaraṇaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr5:11.19"
msgid "Kiccādhikaraṇaṃ ekena samathena sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:11.20"
msgid "sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-pvr5:11.21"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc9, pts-vp-pli5.103
msgctxt "pli-tv-pvr5:12.1"
msgid "Kati samathā?"
msgstr ""

msgctxt "pli-tv-pvr5:12.2"
msgid "Satta samathā."
msgstr ""

msgctxt "pli-tv-pvr5:12.3"
msgid "Sammukhāvinayo, sativinayo, amūḷhavinayo, paṭiññātakaraṇaṃ, yebhuyyasikā, tassapāpiyasikā, tiṇavatthārako—"
msgstr ""

msgctxt "pli-tv-pvr5:12.4"
msgid "ime satta samathā."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr5:13.1"
msgid "Siyā ime satta samathā dasa samathā honti, dasa samathā satta samathā honti vatthuvasena pariyāyena?"
msgstr ""

msgctxt "pli-tv-pvr5:13.2"
msgid "Siyāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr5:14.1"
msgid "Kathañca siyā?"
msgstr ""

msgctxt "pli-tv-pvr5:14.2"
msgid "Vivādādhikaraṇassa dve samathā, anuvādādhikaraṇassa cattāro samathā, āpattādhikaraṇassa tayo samathā, kiccādhikaraṇassa eko samatho."
msgstr ""

msgctxt "pli-tv-pvr5:14.3"
msgid "Evaṃ ime satta samathā dasa samathā honti, dasa samathā satta samathā honti vatthuvasena pariyāyena."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-pvr5:15.1"
msgid "Pariyāyavāro niṭṭhito chaṭṭho."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:16.1"
msgid "2. Sādhāraṇavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc13
msgctxt "pli-tv-pvr5:17.1"
msgid "Kati samathā vivādādhikaraṇassa sādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:17.2"
msgid "Kati samathā vivādādhikaraṇassa asādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:17.3"
msgid "Kati samathā anuvādādhikaraṇassa sādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:17.4"
msgid "Kati samathā anuvādādhikaraṇassa asādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:17.5"
msgid "Kati samathā āpattādhikaraṇassa sādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:17.6"
msgid "Kati samathā āpattādhikaraṇassa asādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:17.7"
msgid "Kati samathā kiccādhikaraṇassa sādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:17.8"
msgid "Kati samathā kiccādhikaraṇassa asādhāraṇā?"
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr5:18.1"
msgid "Dve samathā vivādādhikaraṇassa sādhāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr5:18.2"
msgid "sammukhāvinayo, yebhuyyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:18.3"
msgid "Pañca samathā vivādādhikaraṇassa asādhāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr5:18.4"
msgid "sativinayo, amūḷhavinayo, paṭiññātakaraṇaṃ, tassapāpiyasikā, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:18.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr5:19.1"
msgid "Cattāro samathā anuvādādhikaraṇassa sādhāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr5:19.2"
msgid "sammukhāvinayo;"
msgstr ""

msgctxt "pli-tv-pvr5:19.3"
msgid "sativinayo, amūḷhavinayo, tassapāpiyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:19.4"
msgid "Tayo samathā anuvādādhikaraṇassa asādhāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr5:19.5"
msgid "yebhuyyasikā, paṭiññātakaraṇaṃ, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:19.6"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr5:20.1"
msgid "Tayo samathā āpattādhikaraṇassa sādhāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr5:20.2"
msgid "sammukhāvinayo, paṭiññātakaraṇaṃ, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:20.3"
msgid "Cattāro samathā āpattādhikaraṇassa asādhāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr5:20.4"
msgid "yebhuyyasikā, sativinayo, amūḷhavinayo, tassapāpiyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:20.5"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr5:21.1"
msgid "Eko samatho kiccādhikaraṇassa sādhāraṇo—"
msgstr ""

msgctxt "pli-tv-pvr5:21.2"
msgid "sammukhāvinayo."
msgstr ""

msgctxt "pli-tv-pvr5:21.3"
msgid "Cha samathā kiccādhikaraṇassa asādhāraṇā—"
msgstr ""

msgctxt "pli-tv-pvr5:21.4"
msgid "yebhuyyasikā, sativinayo, amūḷhavinayo, paṭiññātakaraṇaṃ, tassapāpiyasikā, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:21.5"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc18
msgctxt "pli-tv-pvr5:22.1"
msgid "Sādhāraṇavāro niṭṭhito sattamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:23.1"
msgid "3. Tabbhāgiyavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc19
msgctxt "pli-tv-pvr5:24.1"
msgid "Kati samathā vivādādhikaraṇassa tabbhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:24.2"
msgid "Kati samathā vivādādhikaraṇassa aññabhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:24.3"
msgid "Kati samathā anuvādādhikaraṇassa tabbhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:24.4"
msgid "Kati samathā anuvādādhikaraṇassa aññabhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:24.5"
msgid "Kati samathā āpattādhikaraṇassa tabbhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:24.6"
msgid "Kati samathā āpattādhikaraṇassa aññabhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:24.7"
msgid "Kati samathā kiccādhikaraṇassa tabbhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:24.8"
msgid "Kati samathā kiccādhikaraṇassa aññabhāgiyā?"
msgstr ""

#. HTML: </p><p>
#. REF: sc20, pts-vp-pli5.104
msgctxt "pli-tv-pvr5:25.1"
msgid "Dve samathā vivādādhikaraṇassa tabbhāgiyā—"
msgstr ""

msgctxt "pli-tv-pvr5:25.2"
msgid "sammukhāvinayo, yebhuyyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:25.3"
msgid "Pañca samathā vivādādhikaraṇassa aññabhāgiyā—"
msgstr ""

msgctxt "pli-tv-pvr5:25.4"
msgid "sativinayo, amūḷhavinayo, paṭiññātakaraṇaṃ, tassapāpiyasikā, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:25.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr5:26.1"
msgid "Cattāro samathā anuvādādhikaraṇassa tabbhāgiyā—"
msgstr ""

msgctxt "pli-tv-pvr5:26.2"
msgid "sammukhāvinayo, sativinayo, amūḷhavinayo, tassapāpiyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:26.3"
msgid "Tayo samathā anuvādādhikaraṇassa aññabhāgiyā—"
msgstr ""

msgctxt "pli-tv-pvr5:26.4"
msgid "yebhuyyasikā, paṭiññātakaraṇaṃ, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:26.5"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc22
msgctxt "pli-tv-pvr5:27.1"
msgid "Tayo samathā āpattādhikaraṇassa tabbhāgiyā—"
msgstr ""

msgctxt "pli-tv-pvr5:27.2"
msgid "sammukhāvinayo, paṭiññātakaraṇaṃ, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:27.3"
msgid "Cattāro samathā āpattādhikaraṇassa aññabhāgiyā—"
msgstr ""

msgctxt "pli-tv-pvr5:27.4"
msgid "yebhuyyasikā, sativinayo, amūḷhavinayo, tassapāpiyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:27.5"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-pvr5:28.1"
msgid "Eko samatho kiccādhikaraṇassa tabbhāgiyo—"
msgstr ""

msgctxt "pli-tv-pvr5:28.2"
msgid "sammukhāvinayo."
msgstr ""

msgctxt "pli-tv-pvr5:28.3"
msgid "Cha samathā kiccādhikaraṇassa aññabhāgiyā—"
msgstr ""

msgctxt "pli-tv-pvr5:28.4"
msgid "yebhuyyasikā, sativinayo, amūḷhavinayo, paṭiññātakaraṇaṃ, tassapāpiyasikā, tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:28.5"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc24
msgctxt "pli-tv-pvr5:29.1"
msgid "Tabbhāgiyavāro niṭṭhito aṭṭhamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:30.1"
msgid "4. Samathāsamathassasādhāraṇavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc25
msgctxt "pli-tv-pvr5:31.1"
msgid "Samathā samathassa sādhāraṇā, samathā samathassa asādhāraṇā."
msgstr ""

msgctxt "pli-tv-pvr5:31.2"
msgid "Siyā samathā samathassa sādhāraṇā, siyā samathā samathassa asādhāraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr5:32.1"
msgid "Kathaṃ siyā samathā samathassa sādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:32.2"
msgid "Kathaṃ siyā samathā samathassa asādhāraṇā?"
msgstr ""

msgctxt "pli-tv-pvr5:32.3"
msgid "Yebhuyyasikā sammukhāvinayassa sādhāraṇā, sativinayassa amūḷhavinayassa paṭiññātakaraṇassa tassapāpiyasikāya tiṇavatthārakassa asādhāraṇā."
msgstr ""

msgctxt "pli-tv-pvr5:32.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-pvr5:33.1"
msgid "Sativinayo sammukhāvinayassa sādhāraṇo, amūḷhavinayassa paṭiññātakaraṇassa tassapāpiyasikāya tiṇavatthārakassa yebhuyyasikāya asādhāraṇo."
msgstr ""

msgctxt "pli-tv-pvr5:33.2"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc28
msgctxt "pli-tv-pvr5:34.1"
msgid "Amūḷhavinayo sammukhāvinayassa sādhāraṇo, paṭiññātakaraṇassa tassapāpiyasikāya tiṇavatthārakassa yebhuyyasikāya sativinayassa asādhāraṇo."
msgstr ""

msgctxt "pli-tv-pvr5:34.2"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-pvr5:35.1"
msgid "Paṭiññātakaraṇaṃ sammukhāvinayassa sādhāraṇaṃ, tassapāpiyasikāya tiṇavatthārakassa yebhuyyasikāya sativinayassa amūḷhavinayassa asādhāraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:35.2"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-pvr5:36.1"
msgid "Tassapāpiyasikā sammukhāvinayassa sādhāraṇā, tiṇavatthārakassa yebhuyyasikāya sativinayassa amūḷhavinayassa paṭiññātakaraṇassa asādhāraṇā."
msgstr ""

msgctxt "pli-tv-pvr5:36.2"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-pvr5:37.1"
msgid "Tiṇavatthārako sammukhāvinayassa sādhāraṇo, yebhuyyasikāya sativinayassa amūḷhavinayassa paṭiññātakaraṇassa tassapāpiyasikāya asādhāraṇo."
msgstr ""

msgctxt "pli-tv-pvr5:37.2"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-pvr5:38.1"
msgid "Evaṃ siyā samathā samathassa sādhāraṇā;"
msgstr ""

msgctxt "pli-tv-pvr5:38.2"
msgid "evaṃ siyā samathā samathassa asādhāraṇā."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc33
msgctxt "pli-tv-pvr5:39.1"
msgid "Samathā samathassa sādhāraṇavāro niṭṭhito navamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:40.1"
msgid "5. Samathāsamathassatabbhāgiyavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc34
msgctxt "pli-tv-pvr5:41.1"
msgid "Samathā samathassa tabbhāgiyā, samathā samathassa aññabhāgiyā."
msgstr ""

msgctxt "pli-tv-pvr5:41.2"
msgid "Siyā samathā samathassa tabbhāgiyā, siyā samathā samathassa aññabhāgiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-pvr5:42.1"
msgid "Kathaṃ siyā samathā samathassa tabbhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:42.2"
msgid "Kathaṃ siyā samathā samathassa aññabhāgiyā?"
msgstr ""

msgctxt "pli-tv-pvr5:42.3"
msgid "Yebhuyyasikā sammukhāvinayassa tabbhāgiyā, sativinayassa amūḷhavinayassa paṭiññātakaraṇassa tassapāpiyasikāya tiṇavatthārakassa aññabhāgiyā."
msgstr ""

msgctxt "pli-tv-pvr5:42.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc36
msgctxt "pli-tv-pvr5:43.1"
msgid "Sativinayo sammukhāvinayassa tabbhāgiyo, amūḷhavinayassa paṭiññātakaraṇassa tassapāpiyasikāya tiṇavatthārakassa yebhuyyasikāya aññabhāgiyo."
msgstr ""

msgctxt "pli-tv-pvr5:43.2"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc37
msgctxt "pli-tv-pvr5:44.1"
msgid "Amūḷhavinayo sammukhāvinayassa tabbhāgiyo, paṭiññātakaraṇassa tassapāpiyasikāya tiṇavatthārakassa yebhuyyasikāya sativinayassa aññabhāgiyo."
msgstr ""

msgctxt "pli-tv-pvr5:44.2"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc38
msgctxt "pli-tv-pvr5:45.1"
msgid "Paṭiññātakaraṇaṃ sammukhāvinayassa tabbhāgiyaṃ, tassapāpiyasikāya tiṇavatthārakassa yebhuyyasikāya sativinayassa amūḷhavinayassa aññabhāgiyaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:45.2"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc39
msgctxt "pli-tv-pvr5:46.1"
msgid "Tassapāpiyasikā sammukhāvinayassa tabbhāgiyā, tiṇavatthārakassa yebhuyyasikāya sativinayassa amūḷhavinayassa paṭiññātakaraṇassa aññabhāgiyā."
msgstr ""

msgctxt "pli-tv-pvr5:46.2"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-pvr5:47.1"
msgid "Tiṇavatthārako sammukhāvinayassa tabbhāgiyo, yebhuyyasikāya sativinayassa amūḷhavinayassa paṭiññātakaraṇassa tassapāpiyasikāya aññabhāgiyo."
msgstr ""

msgctxt "pli-tv-pvr5:47.2"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc41
msgctxt "pli-tv-pvr5:48.1"
msgid "Evaṃ siyā samathā samathassa tabbhāgiyā, evaṃ siyā samathā samathassa aññabhāgiyā."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc42
msgctxt "pli-tv-pvr5:49.1"
msgid "Samathā samathassa tabbhāgiyavāro niṭṭhito dasamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:50.1"
msgid "6. Samathasammukhāvinayavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc43, pts-vp-pli5.105
msgctxt "pli-tv-pvr5:51.1"
msgid "Samatho sammukhāvinayo, sammukhāvinayo samatho?"
msgstr ""

msgctxt "pli-tv-pvr5:51.2"
msgid "Samatho yebhuyyasikā, yebhuyyasikā samatho?"
msgstr ""

msgctxt "pli-tv-pvr5:51.3"
msgid "Samatho sativinayo, sativinayo samatho?"
msgstr ""

msgctxt "pli-tv-pvr5:51.4"
msgid "Samatho amūḷhavinayo, amūḷhavinayo samatho?"
msgstr ""

msgctxt "pli-tv-pvr5:51.5"
msgid "Samatho paṭiññātakaraṇaṃ, paṭiññātakaraṇaṃ samatho?"
msgstr ""

msgctxt "pli-tv-pvr5:51.6"
msgid "Samatho tassapāpiyasikā, tassapāpiyasikā samatho?"
msgstr ""

msgctxt "pli-tv-pvr5:51.7"
msgid "Samatho tiṇavatthārako, tiṇavatthārako samatho?"
msgstr ""

#. HTML: </p><p>
#. REF: sc44
msgctxt "pli-tv-pvr5:52.1"
msgid "Yebhuyyasikā sativinayo amūḷhavinayo paṭiññātakaraṇaṃ tassapāpiyasikā tiṇavatthārako—"
msgstr ""

msgctxt "pli-tv-pvr5:52.2"
msgid "ime samathā samathā, no sammukhāvinayo."
msgstr ""

msgctxt "pli-tv-pvr5:52.3"
msgid "Sammukhāvinayo samatho ceva sammukhāvinayo ca."
msgstr ""

msgctxt "pli-tv-pvr5:52.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc45
msgctxt "pli-tv-pvr5:53.1"
msgid "Sativinayo amūḷhavinayo paṭiññātakaraṇaṃ tassapāpiyasikā tiṇavatthārako sammukhāvinayo—"
msgstr ""

msgctxt "pli-tv-pvr5:53.2"
msgid "ime samathā samathā, no yebhuyyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:53.3"
msgid "Yebhuyyasikā samatho ceva yebhuyyasikā ca."
msgstr ""

msgctxt "pli-tv-pvr5:53.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc46
msgctxt "pli-tv-pvr5:54.1"
msgid "Amūḷhavinayo paṭiññātakaraṇaṃ tassapāpiyasikā tiṇavatthārako sammukhāvinayo yebhuyyasikā—"
msgstr ""

msgctxt "pli-tv-pvr5:54.2"
msgid "ime samathā samathā, no sativinayo."
msgstr ""

msgctxt "pli-tv-pvr5:54.3"
msgid "Sativinayo samatho ceva sativinayo ca."
msgstr ""

msgctxt "pli-tv-pvr5:54.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc47
msgctxt "pli-tv-pvr5:55.1"
msgid "Paṭiññātakaraṇaṃ tassapāpiyasikā tiṇavatthārako sammukhāvinayo yebhuyyasikā sativinayo—"
msgstr ""

msgctxt "pli-tv-pvr5:55.2"
msgid "ime samathā samathā, no amūḷhavinayo."
msgstr ""

msgctxt "pli-tv-pvr5:55.3"
msgid "Amūḷhavinayo samatho ceva amūḷhavinayo ca."
msgstr ""

msgctxt "pli-tv-pvr5:55.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc48
msgctxt "pli-tv-pvr5:56.1"
msgid "Tassapāpiyasikā tiṇavatthārako sammukhāvinayo yebhuyyasikā sativinayo amūḷhavinayo—"
msgstr ""

msgctxt "pli-tv-pvr5:56.2"
msgid "ime samathā samathā, no paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:56.3"
msgid "Paṭiññātakaraṇaṃ samatho ceva paṭiññātakaraṇañca."
msgstr ""

msgctxt "pli-tv-pvr5:56.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc49
msgctxt "pli-tv-pvr5:57.1"
msgid "Tiṇavatthārako sammukhāvinayo yebhuyyasikā sativinayo amūḷhavinayo paṭiññātakaraṇaṃ—"
msgstr ""

msgctxt "pli-tv-pvr5:57.2"
msgid "ime samathā samathā, no tassapāpiyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:57.3"
msgid "Tassapāpiyasikā samatho ceva tassapāpiyasikā ca."
msgstr ""

msgctxt "pli-tv-pvr5:57.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc50
msgctxt "pli-tv-pvr5:58.1"
msgid "Sammukhāvinayo yebhuyyasikā sativinayo amūḷhavinayo paṭiññātakaraṇaṃ tassapāpiyasikā—"
msgstr ""

msgctxt "pli-tv-pvr5:58.2"
msgid "ime samathā samathā, no tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:58.3"
msgid "Tiṇavatthārako samatho ceva tiṇavatthārako ca."
msgstr ""

msgctxt "pli-tv-pvr5:58.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc51
msgctxt "pli-tv-pvr5:59.1"
msgid "Samathasammukhāvinayavāro niṭṭhito ekādasamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:60.1"
msgid "7. Vinayavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc52
msgctxt "pli-tv-pvr5:61.1"
msgid "Vinayo sammukhāvinayo, sammukhāvinayo vinayo?"
msgstr ""

msgctxt "pli-tv-pvr5:61.2"
msgid "Vinayo yebhuyyasikā, yebhuyyasikā vinayo?"
msgstr ""

msgctxt "pli-tv-pvr5:61.3"
msgid "Vinayo sativinayo, sativinayo vinayo?"
msgstr ""

msgctxt "pli-tv-pvr5:61.4"
msgid "Vinayo amūḷhavinayo, amūḷhavinayo vinayo?"
msgstr ""

msgctxt "pli-tv-pvr5:61.5"
msgid "Vinayo paṭiññātakaraṇaṃ, paṭiññātakaraṇaṃ vinayo?"
msgstr ""

msgctxt "pli-tv-pvr5:61.6"
msgid "Vinayo tassapāpiyasikā, tassapāpiyasikā vinayo?"
msgstr ""

msgctxt "pli-tv-pvr5:61.7"
msgid "Vinayo tiṇavatthārako, tiṇavatthārako vinayo?"
msgstr ""

#. HTML: </p><p>
#. REF: sc53
msgctxt "pli-tv-pvr5:62.1"
msgid "Vinayo siyā sammukhāvinayo siyā na sammukhāvinayo."
msgstr ""

msgctxt "pli-tv-pvr5:62.2"
msgid "Sammukhāvinayo vinayo ceva sammukhāvinayo ca."
msgstr ""

msgctxt "pli-tv-pvr5:62.3"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc54
msgctxt "pli-tv-pvr5:63.1"
msgid "Vinayo siyā yebhuyyasikā, siyā na yebhuyyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:63.2"
msgid "Yebhuyyasikā vinayo ceva yebhuyyasikā ca."
msgstr ""

msgctxt "pli-tv-pvr5:63.3"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc55
msgctxt "pli-tv-pvr5:64.1"
msgid "Vinayo siyā sativinayo, siyā na sativinayo."
msgstr ""

msgctxt "pli-tv-pvr5:64.2"
msgid "Sativinayo vinayo ceva sativinayo ca."
msgstr ""

msgctxt "pli-tv-pvr5:64.3"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc56
msgctxt "pli-tv-pvr5:65.1"
msgid "Vinayo siyā amūḷhavinayo, siyā na amūḷhavinayo."
msgstr ""

msgctxt "pli-tv-pvr5:65.2"
msgid "Amūḷhavinayo vinayo ceva amūḷhavinayo ca."
msgstr ""

msgctxt "pli-tv-pvr5:65.3"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc57
msgctxt "pli-tv-pvr5:66.1"
msgid "Vinayo siyā paṭiññātakaraṇaṃ, siyā na paṭiññātakaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:66.2"
msgid "Paṭiññātakaraṇaṃ vinayo ceva paṭiññātakaraṇañca."
msgstr ""

msgctxt "pli-tv-pvr5:66.3"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc58
msgctxt "pli-tv-pvr5:67.1"
msgid "Vinayo siyā tassapāpiyasikā, siyā na tassapāpiyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:67.2"
msgid "Tassapāpiyasikā vinayo ceva tassapāpiyasikā ca."
msgstr ""

msgctxt "pli-tv-pvr5:67.3"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr5:68.1"
msgid "Vinayo siyā tiṇavatthārako, siyā na tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:68.2"
msgid "Tiṇavatthārako vinayo ceva tiṇavatthārako ca."
msgstr ""

msgctxt "pli-tv-pvr5:68.3"
msgid "(7)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc60
msgctxt "pli-tv-pvr5:69.1"
msgid "Vinayavāro niṭṭhito dvādasamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:70.1"
msgid "8. Kusalavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc61
msgctxt "pli-tv-pvr5:71.1"
msgid "Sammukhāvinayo kusalo akusalo abyākato?"
msgstr ""

msgctxt "pli-tv-pvr5:71.2"
msgid "Yebhuyyasikā kusalā akusalā abyākatā?"
msgstr ""

msgctxt "pli-tv-pvr5:71.3"
msgid "Sativinayo kusalo akusalo abyākato?"
msgstr ""

msgctxt "pli-tv-pvr5:71.4"
msgid "Amūḷhavinayo kusalo akusalo abyākato?"
msgstr ""

msgctxt "pli-tv-pvr5:71.5"
msgid "Paṭiññātakaraṇaṃ kusalaṃ akusalaṃ abyākataṃ?"
msgstr ""

msgctxt "pli-tv-pvr5:71.6"
msgid "Tassapāpiyasikā kusalā akusalā abyākatā?"
msgstr ""

msgctxt "pli-tv-pvr5:71.7"
msgid "Tiṇavatthārako kusalo akusalo abyākato?"
msgstr ""

#. HTML: </p><p>
#. REF: sc62
msgctxt "pli-tv-pvr5:72.1"
msgid "Sammukhāvinayo siyā kusalo, siyā abyākato."
msgstr ""

msgctxt "pli-tv-pvr5:72.2"
msgid "Natthi sammukhāvinayo akusalo."
msgstr ""

msgctxt "pli-tv-pvr5:72.3"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc63
msgctxt "pli-tv-pvr5:73.1"
msgid "Yebhuyyasikā siyā kusalā, siyā akusalā, siyā abyākatā."
msgstr ""

msgctxt "pli-tv-pvr5:73.2"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-pvr5:74.1"
msgid "Sativinayo siyā kusalo, siyā akusalo, siyā abyākato."
msgstr ""

msgctxt "pli-tv-pvr5:74.2"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-pvr5:75.1"
msgid "Amūḷhavinayo siyā kusalo, siyā akusalo, siyā abyākato."
msgstr ""

msgctxt "pli-tv-pvr5:75.2"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr5:76.1"
msgid "Paṭiññātakaraṇaṃ siyā kusalaṃ, siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:76.2"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr5:77.1"
msgid "Tassapāpiyasikā siyā kusalā, siyā akusalā siyā abyākatā."
msgstr ""

msgctxt "pli-tv-pvr5:77.2"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-pvr5:78.1"
msgid "Tiṇavatthārako siyā kusalo, siyā akusalo, siyā abyākato."
msgstr ""

msgctxt "pli-tv-pvr5:78.2"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc69
msgctxt "pli-tv-pvr5:79.1"
msgid "Vivādādhikaraṇaṃ kusalaṃ akusalaṃ abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:79.2"
msgid "Anuvādādhikaraṇaṃ kusalaṃ akusalaṃ abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:79.3"
msgid "Āpattādhikaraṇaṃ kusalaṃ akusalaṃ abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:79.4"
msgid "Kiccādhikaraṇaṃ kusalaṃ akusalaṃ abyākataṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc70, pts-vp-pli5.106
msgctxt "pli-tv-pvr5:80.1"
msgid "Vivādādhikaraṇaṃ siyā kusalaṃ, siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:80.2"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-pvr5:81.1"
msgid "Anuvādādhikaraṇaṃ siyā kusalaṃ, siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:81.2"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-pvr5:82.1"
msgid "Āpattādhikaraṇaṃ siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:82.2"
msgid "Natthi āpattādhikaraṇaṃ kusalaṃ."
msgstr ""

msgctxt "pli-tv-pvr5:82.3"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-pvr5:83.1"
msgid "Kiccādhikaraṇaṃ siyā kusalaṃ, siyā akusalaṃ, siyā abyākataṃ."
msgstr ""

msgctxt "pli-tv-pvr5:83.2"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc74
msgctxt "pli-tv-pvr5:84.1"
msgid "Kusalavāro niṭṭhito terasamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:85.1"
msgid "9. Yatthavāra, pucchāvāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc75
msgctxt "pli-tv-pvr5:86.1"
msgid "Yattha yebhuyyasikā labbhati, tattha sammukhāvinayo labbhati;"
msgstr ""

msgctxt "pli-tv-pvr5:86.2"
msgid "yattha sammukhāvinayo labbhati, tattha yebhuyyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:86.3"
msgid "Na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:86.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-pvr5:87.1"
msgid "Yattha sativinayo labbhati, tattha sammukhāvinayo labbhati;"
msgstr ""

msgctxt "pli-tv-pvr5:87.2"
msgid "yattha sammukhāvinayo labbhati, tattha sativinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:87.3"
msgid "Na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:87.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-pvr5:88.1"
msgid "Yattha amūḷhavinayo labbhati, tattha sammukhāvinayo labbhati;"
msgstr ""

msgctxt "pli-tv-pvr5:88.2"
msgid "yattha sammukhāvinayo labbhati, tattha amūḷhavinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:88.3"
msgid "Na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:88.4"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc78
msgctxt "pli-tv-pvr5:89.1"
msgid "Yattha paṭiññātakaraṇaṃ labbhati, tattha sammukhāvinayo labbhati;"
msgstr ""

msgctxt "pli-tv-pvr5:89.2"
msgid "yattha sammukhāvinayo labbhati, tattha paṭiññātakaraṇaṃ labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:89.3"
msgid "Na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:89.4"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc79
msgctxt "pli-tv-pvr5:90.1"
msgid "Yattha tassapāpiyasikā labbhati, tattha sammukhāvinayo labbhati;"
msgstr ""

msgctxt "pli-tv-pvr5:90.2"
msgid "yattha sammukhāvinayo labbhati, tattha tassapāpiyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:90.3"
msgid "Na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:90.4"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc80
msgctxt "pli-tv-pvr5:91.1"
msgid "Yattha tiṇavatthārako labbhati, tattha sammukhāvinayo labbhati;"
msgstr ""

msgctxt "pli-tv-pvr5:91.2"
msgid "yattha sammukhāvinayo labbhati, tattha tiṇavatthārako labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:91.3"
msgid "Na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:91.4"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc81
msgctxt "pli-tv-pvr5:92.1"
msgid "Yattha yebhuyyasikā tattha sammukhāvinayo;"
msgstr ""

msgctxt "pli-tv-pvr5:92.2"
msgid "yattha sammukhāvinayo tattha yebhuyyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:92.3"
msgid "Na tattha sativinayo, na tattha amūḷhavinayo, na tattha paṭiññātakaraṇaṃ, na tattha tassapāpiyasikā, na tattha tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:92.4"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc82
msgctxt "pli-tv-pvr5:93.1"
msgid "Yattha sativinayo tattha sammukhāvinayo;"
msgstr ""

msgctxt "pli-tv-pvr5:93.2"
msgid "yattha sammukhāvinayo tattha sativinayo."
msgstr ""

msgctxt "pli-tv-pvr5:93.3"
msgid "Na tattha amūḷhavinayo, na tattha paṭiññātakaraṇaṃ, na tattha tassapāpiyasikā, na tattha tiṇavatthārako, na tattha yebhuyyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:93.4"
msgid "Sammukhāvinayaṃ kātuna mūlaṃ …"
msgstr ""

msgctxt "pli-tv-pvr5:93.5"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr5:93.6"
msgid "(8)"
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-pvr5:94.1"
msgid "Yattha tiṇavatthārako tattha sammukhāvinayo;"
msgstr ""

msgctxt "pli-tv-pvr5:94.2"
msgid "yattha sammukhāvinayo tattha tiṇavatthārako."
msgstr ""

msgctxt "pli-tv-pvr5:94.3"
msgid "Na tattha yebhuyyasikā, na tattha sativinayo, na tattha amūḷhavinayo, na tattha paṭiññātakaraṇaṃ, na tattha tassapāpiyasikā."
msgstr ""

msgctxt "pli-tv-pvr5:94.4"
msgid "(9)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc84
msgctxt "pli-tv-pvr5:95.1"
msgid "Cakkapeyyālaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr5:95.2"
msgid "Yatthavāro niṭṭhito cuddasamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:96.1"
msgid "10. Samathavāra, vissajjanāvāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc85
msgctxt "pli-tv-pvr5:97.1"
msgid "Yasmiṃ samaye sammukhāvinayena ca yebhuyyasikāya ca adhikaraṇaṃ vūpasammati, yattha yebhuyyasikā labbhati tattha sammukhāvinayo labbhati, yattha sammukhāvinayo labbhati tattha yebhuyyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:97.2"
msgid "Na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:97.3"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc86, pts-vp-pli5.107
msgctxt "pli-tv-pvr5:98.1"
msgid "Yasmiṃ samaye sammukhāvinayena ca sativinayena ca adhikaraṇaṃ vūpasammati, yattha sativinayo labbhati tattha sammukhāvinayo labbhati, yattha sammukhāvinayo labbhati tattha sativinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:98.2"
msgid "Na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:98.3"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc87
msgctxt "pli-tv-pvr5:99.1"
msgid "Yasmiṃ samaye sammukhāvinayena ca amūḷhavinayena ca adhikaraṇaṃ vūpasammati, yattha amūḷhavinayo labbhati tattha sammukhāvinayo labbhati, yattha sammukhāvinayo labbhati tattha amūḷhavinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:99.2"
msgid "Na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:99.3"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc88
msgctxt "pli-tv-pvr5:100.1"
msgid "Yasmiṃ samaye sammukhāvinayena ca paṭiññātakaraṇena ca adhikaraṇaṃ vūpasammati, yattha paṭiññātakaraṇaṃ labbhati tattha sammukhāvinayo labbhati, yattha sammukhāvinayo labbhati tattha paṭiññātakaraṇaṃ labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:100.2"
msgid "Na tattha tassapāpiyasikā labbhati, na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:100.3"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc89
msgctxt "pli-tv-pvr5:101.1"
msgid "Yasmiṃ samaye sammukhāvinayena ca tassapāpiyasikāya ca adhikaraṇaṃ vūpasammati, yattha tassapāpiyasikā labbhati tattha sammukhāvinayo labbhati, yattha sammukhāvinayo labbhati tattha tassapāpiyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:101.2"
msgid "Na tattha tiṇavatthārako labbhati, na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:101.3"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc90
msgctxt "pli-tv-pvr5:102.1"
msgid "Yasmiṃ samaye sammukhāvinayena ca tiṇavatthārakena ca adhikaraṇaṃ vūpasammati, yattha tiṇavatthārako labbhati tattha sammukhāvinayo labbhati, yattha sammukhāvinayo labbhati tattha tiṇavatthārako labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:102.2"
msgid "Na tattha yebhuyyasikā labbhati, na tattha sativinayo labbhati, na tattha amūḷhavinayo labbhati, na tattha paṭiññātakaraṇaṃ labbhati, na tattha tassapāpiyasikā labbhati."
msgstr ""

msgctxt "pli-tv-pvr5:102.3"
msgid "(6)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc91
msgctxt "pli-tv-pvr5:103.1"
msgid "Samathavāro niṭṭhito pannarasamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:104.1"
msgid "11. Saṃsaṭṭhavāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc92
msgctxt "pli-tv-pvr5:105.1"
msgid "Adhikaraṇanti vā samathāti vā ime dhammā saṃsaṭṭhā udāhu visaṃsaṭṭhā?"
msgstr ""

#. VAR: vinibbhujitvā vinibbhujitvā → vinibbhujjitvā vinibbhujjitvā (mr)
msgctxt "pli-tv-pvr5:105.2"
msgid "Labbhā ca panimesaṃ dhammānaṃ vinibbhujitvā vinibbhujitvā nānākaraṇaṃ paññāpetunti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc93
msgctxt "pli-tv-pvr5:106.1"
msgid "Adhikaraṇanti vā samathāti vā ime dhammā visaṃsaṭṭhā, no saṃsaṭṭhā."
msgstr ""

msgctxt "pli-tv-pvr5:106.2"
msgid "Labbhā ca panimesaṃ dhammānaṃ vinibbhujitvā vinibbhujitvā nānākaraṇaṃ paññāpetunti."
msgstr ""

msgctxt "pli-tv-pvr5:106.3"
msgid "So—"
msgstr ""

msgctxt "pli-tv-pvr5:106.4"
msgid "“mā hevan”tissa vacanīyo."
msgstr ""

msgctxt "pli-tv-pvr5:106.5"
msgid "Adhikaraṇanti vā samathāti vā ime dhammā saṃsaṭṭhā, no visaṃsaṭṭhā."
msgstr ""

#. VAR: ca labbhā → na ca labbhā (bj, s1-3, mr)
msgctxt "pli-tv-pvr5:106.6"
msgid "No ca labbhā imesaṃ dhammānaṃ vinibbhujitvā vinibbhujitvā nānākaraṇaṃ paññāpetuṃ."
msgstr ""

msgctxt "pli-tv-pvr5:106.7"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "pli-tv-pvr5:106.8"
msgid "Nanu vuttaṃ bhagavatā—"
msgstr ""

msgctxt "pli-tv-pvr5:106.9"
msgid "“cattārimāni, bhikkhave, adhikaraṇāni, satta samathā."
msgstr ""

msgctxt "pli-tv-pvr5:106.10"
msgid "Adhikaraṇā samathehi sammanti, samathā adhikaraṇehi sammanti."
msgstr ""

msgctxt "pli-tv-pvr5:106.11"
msgid "Evaṃ, ime dhammā saṃsaṭṭhā no visaṃsaṭṭhā;"
msgstr ""

msgctxt "pli-tv-pvr5:106.12"
msgid "no ca labbhā imesaṃ dhammānaṃ vinibbhujitvā vinibbhujitvā nānākaraṇaṃ paññāpetun”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc94
msgctxt "pli-tv-pvr5:107.1"
msgid "Saṃsaṭṭhavāro niṭṭhito soḷasamo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr5:108.1"
msgid "12. Sammativāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc95
msgctxt "pli-tv-pvr5:109.1"
msgid "Vivādādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:109.2"
msgid "Anuvādādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:109.3"
msgid "Āpattādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr5:109.4"
msgid "Kiccādhikaraṇaṃ katihi samathehi sammati?"
msgstr ""

#. HTML: </p><p>
#. REF: sc96
msgctxt "pli-tv-pvr5:110.1"
msgid "Vivādādhikaraṇaṃ dvīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:110.2"
msgid "sammukhāvinayena ca yebhuyyasikāya ca."
msgstr ""

msgctxt "pli-tv-pvr5:110.3"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc97
msgctxt "pli-tv-pvr5:111.1"
msgid "Anuvādādhikaraṇaṃ catūhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:111.2"
msgid "sammukhāvinayena ca sativinayena ca amūḷhavinayena ca tassapāpiyasikāya ca."
msgstr ""

msgctxt "pli-tv-pvr5:111.3"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc98
msgctxt "pli-tv-pvr5:112.1"
msgid "Āpattādhikaraṇaṃ tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:112.2"
msgid "sammukhāvinayena ca paṭiññātakaraṇena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr5:112.3"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc99
msgctxt "pli-tv-pvr5:113.1"
msgid "Kiccādhikaraṇaṃ ekena samathena sammati—"
msgstr ""

msgctxt "pli-tv-pvr5:113.2"
msgid "sammukhāvinayena."
msgstr ""

msgctxt "pli-tv-pvr5:113.3"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc100
msgctxt "pli-tv-pvr5:114.1"
msgid "Vivādādhikaraṇañca anuvādādhikaraṇañca katihi samathehi sammanti?"
