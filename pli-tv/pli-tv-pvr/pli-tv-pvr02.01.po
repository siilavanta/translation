#. HTML: <section class="sutta" id="pli-tv-pvr2.1"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr2.1:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.1:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.1:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.1:4.1"
msgid "Katthapaññattivāra"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr2.1:5.1"
msgid "1. Pārājikakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli5.54
msgctxt "pli-tv-pvr2.1:6.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena bhikkhunīnaṃ pañcamaṃ pārājikaṃ kattha paññattaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.2"
msgid "Kaṃ ārabbha?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.3"
msgid "Kismiṃ vatthusmiṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.4"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.5"
msgid "Sabbatthapaññatti, padesapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.6"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.7"
msgid "Ekatopaññatti, ubhatopaññatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.8"
msgid "Catunnaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.9"
msgid "Katamena uddesena uddesaṃ āgacchati?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.10"
msgid "Catunnaṃ vipattīnaṃ katamā vipatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.11"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandho?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.12"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.13"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.14"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.15"
msgid "Ko tattha vinayo?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.16"
msgid "Ko tattha abhivinayo?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.17"
msgid "Kiṃ tattha pātimokkhaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.18"
msgid "Kiṃ tattha adhipātimokkhaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.19"
msgid "Kā vipatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.20"
msgid "Kā sampatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.21"
msgid "Kā paṭipatti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.22"
msgid "Kati atthavase paṭicca bhagavatā bhikkhunīnaṃ pañcamaṃ pārājikaṃ paññattaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.23"
msgid "Kā sikkhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.24"
msgid "Kā sikkhitasikkhā?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.25"
msgid "Kattha ṭhitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.26"
msgid "Kā dhārenti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.27"
msgid "Kassa vacanaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:6.28"
msgid "Kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.1:7.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena bhikkhunīnaṃ pañcamaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.4"
msgid "Sundarīnandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.6"
msgid "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.7"
msgid "Atthi tattha paññatti anupaññatti anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.9"
msgid "Anupaññatti anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.16"
msgid "Catunnaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.19"
msgid "Dutiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.23"
msgid "Pārājikāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.25"
msgid "Ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.26"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.27"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.28"
msgid "Āpattādhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.29"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.30"
msgid "Dvīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.31"
msgid "sammukhāvinayena ca paṭiññātakaraṇena ca."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.32"
msgid "Ko tattha vinayo, ko tattha abhivinayoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.33"
msgid "Paññatti vinayo, vibhatti abhivinayo."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.34"
msgid "Kiṃ tattha pātimokkhaṃ, kiṃ tattha adhipātimokkhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.35"
msgid "Paññatti pātimokkhaṃ, vibhatti adhipātimokkhaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.36"
msgid "Kā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.37"
msgid "Asaṃvaro vipatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.38"
msgid "Kā sampattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.39"
msgid "Saṃvaro sampatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.40"
msgid "Kā paṭipattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.41"
msgid "“Na evarūpaṃ karissāmī”ti yāvajīvaṃ āpāṇakoṭikaṃ samādāya sikkhati sikkhāpadesu."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.42"
msgid "Kati atthavase paṭicca bhagavatā bhikkhunīnaṃ pañcamaṃ pārājikaṃ paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.43"
msgid "Dasa atthavase paṭicca bhagavatā bhikkhunīnaṃ pañcamaṃ pārājikaṃ paññattaṃ—"
msgstr ""

#. VAR: dummaṅkūnaṃ → dummaṅkunīnaṃ (bj)
msgctxt "pli-tv-pvr2.1:7.44"
msgid "saṃghasuṭṭhutāya, saṃghaphāsutāya, dummaṅkūnaṃ bhikkhunīnaṃ niggahāya, pesalānaṃ bhikkhunīnaṃ phāsuvihārāya, diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya, samparāyikānaṃ āsavānaṃ paṭighātāya, appasannānaṃ pasādāya, pasannānaṃ bhiyyobhāvāya, saddhammaṭṭhitiyā, vinayānuggahāya."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.45"
msgid "Kā sikkhantīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.46"
msgid "Sekkhā ca puthujjanakalyāṇikā ca sikkhanti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.47"
msgid "Kā sikkhitasikkhāti?"
msgstr ""

#. VAR: Arahantiyo → arahantā (pts1, mr)
msgctxt "pli-tv-pvr2.1:7.48"
msgid "Arahantiyo sikkhitasikkhā."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.49"
msgid "Kattha ṭhitanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.50"
msgid "Sikkhākāmāsu ṭhitaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.51"
msgid "Kā dhārentīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.52"
msgid "Yāsaṃ vattati tā dhārenti."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.53"
msgid "Kassa vacananti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.54"
msgid "Bhagavato vacanaṃ arahato sammāsambuddhassa."
msgstr ""

msgctxt "pli-tv-pvr2.1:7.55"
msgid "Kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:7.56"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc3
msgctxt "pli-tv-pvr2.1:8.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:8.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:8.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:8.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr2.1:9.1"
msgid "Tato mahindo iṭṭiyo,"
msgstr ""

#. HTML: <br>
#. VAR: sambalo tathā → uttiyo ceva sambalo (bj, s1-3)
msgctxt "pli-tv-pvr2.1:9.2"
msgid "uttiyo sambalo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:9.3"
msgid "Bhaddanāmo ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr2.1:10.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:10.2"
msgid "jambudīpā idhāgatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:10.3"
msgid "Vinayaṃ te vācayiṃsu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:10.4"
msgid "piṭakaṃ tambapaṇṇiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr2.1:11.1"
msgid "Nikāye pañca vācesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:11.2"
msgid "satta ceva pakaraṇe;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:11.3"
msgid "Tato ariṭṭho medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:11.4"
msgid "tissadatto ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr2.1:12.1"
msgid "Visārado kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:12.2"
msgid "thero ca dīghanāmako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:12.3"
msgid "Dīghasumano ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr2.1:13.1"
msgid "Punadeva kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:13.2"
msgid "Nāgatthero ca buddharakkhito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:13.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:13.4"
msgid "Devatthero ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr2.1:14.1"
msgid "Punadeva sumano medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:14.2"
msgid "Vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:14.3"
msgid "Bahussuto cūḷanāgo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:14.4"
msgid "Gajova duppadhaṃsiyo."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr2.1:15.1"
msgid "Dhammapālitanāmo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:15.2"
msgid "rohaṇe sādhupūjito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:15.3"
msgid "Tassa sisso mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:15.4"
msgid "khemanāmo tipeṭako."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr2.1:16.1"
msgid "Dīpe tārakarājāva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:16.2"
msgid "paññāya atirocatha;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:16.3"
msgid "Upatisso ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:16.4"
msgid "phussadevo mahākathī."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr2.1:17.1"
msgid "Punadeva sumano medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:17.2"
msgid "Pupphanāmo bahussuto;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:17.3"
msgid "Mahākathī mahāsivo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:17.4"
msgid "Piṭake sabbattha kovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr2.1:18.1"
msgid "Punadeva upāli medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:18.2"
msgid "Vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:18.3"
msgid "Mahānāgo mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:18.4"
msgid "Saddhammavaṃsakovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr2.1:19.1"
msgid "Punadeva abhayo medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:19.2"
msgid "Piṭake sabbattha kovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:19.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:19.4"
msgid "Vinaye ca visārado."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr2.1:20.1"
msgid "Tassa sisso mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:20.2"
msgid "pupphanāmo bahussuto;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:20.3"
msgid "Sāsanaṃ anurakkhanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:20.4"
msgid "jambudīpe patiṭṭhito."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr2.1:21.1"
msgid "Cūḷābhayo ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:21.2"
msgid "vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:21.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:21.4"
msgid "saddhammavaṃsakovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr2.1:22.1"
msgid "Cūḷadevo ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:22.2"
msgid "vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:22.3"
msgid "Sivatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:22.4"
msgid "vinaye sabbattha kovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc18, pts-vp-pli5.55
msgctxt "pli-tv-pvr2.1:23.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:23.2"
msgid "Vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:23.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:23.4"
msgid "Piṭakaṃ tambapaṇṇiyāti."
msgstr ""

msgctxt "pli-tv-pvr2.1:23.5"
msgid "(1)"
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc19
msgctxt "pli-tv-pvr2.1:24.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena bhikkhunīnaṃ chaṭṭhaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:24.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:24.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:24.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:24.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:24.6"
msgid "Thullanandā bhikkhunī jānaṃ pārājikaṃ dhammaṃ ajjhāpannaṃ bhikkhuniṃ nevattanā paṭicodesi na gaṇassa ārocesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:24.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:24.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:24.9"
msgid "kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr2.1:24.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:24.11"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc20
msgctxt "pli-tv-pvr2.1:25.1"
msgid "Bhikkhunīnaṃ sattamaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:25.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:25.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:25.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:25.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:25.6"
msgid "Thullanandā bhikkhunī samaggena saṃghena ukkhittaṃ ariṭṭhaṃ bhikkhuṃ gaddhabādhipubbaṃ anuvatti, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:25.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:25.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:25.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.1:25.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:25.11"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr2.1:26.1"
msgid "Bhikkhunīnaṃ aṭṭhamaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:26.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:26.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:26.4"
msgid "Chabbaggiyā bhikkhuniyo ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:26.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:26.6"
msgid "Chabbaggiyā bhikkhuniyo aṭṭhamaṃ vatthuṃ paripūresuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:26.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:26.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:26.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.1:26.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:26.11"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc22
msgctxt "pli-tv-pvr2.1:27.1"
msgid "Aṭṭha pārājikā niṭṭhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc23
msgctxt "pli-tv-pvr2.1:28.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc24
msgctxt "pli-tv-pvr2.1:29.1"
msgid "Methunādinnādānañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:29.2"
msgid "manussaviggahuttari;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:29.3"
msgid "Kāyasaṃsaggaṃ chādeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:29.4"
msgid "ukkhittā aṭṭha vatthukā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:29.5"
msgid "Paññāpesi mahāvīro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:29.6"
msgid "chejjavatthū asaṃsayāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr2.1:30.1"
msgid "2. Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc25
msgctxt "pli-tv-pvr2.1:31.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena ussayavādikāya bhikkhuniyā aḍḍaṃ karontiyā saṃghādiseso kattha paññatto?"
msgstr ""

msgctxt "pli-tv-pvr2.1:31.2"
msgid "Kaṃ ārabbha?"
msgstr ""

msgctxt "pli-tv-pvr2.1:31.3"
msgid "Kismiṃ vatthusmiṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr2.1:31.5"
msgid "kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc26, pts-vp-pli5.56
msgctxt "pli-tv-pvr2.1:32.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena ussayavādikāya bhikkhuniyā aḍḍaṃ karontiyā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.6"
msgid "Thullanandā bhikkhunī ussayavādikā vihari, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.9"
msgid "Anupaññatti anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.16"
msgid "Catunnaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.19"
msgid "Tatiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.23"
msgid "Saṃghādisesāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr2.1:32.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.25"
msgid "Dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.26"
msgid "siyā kāyato ca vācato ca samuṭṭhāti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.27"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti … pe …"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.29"
msgid "kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:32.30"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc27
msgctxt "pli-tv-pvr2.1:33.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:33.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:33.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:33.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc28
msgctxt "pli-tv-pvr2.1:34.1"
msgid "…"
msgstr ""

msgctxt "pli-tv-pvr2.1:34.2"
msgid "pe …"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc29
msgctxt "pli-tv-pvr2.1:35.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:35.2"
msgid "Vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:35.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:35.4"
msgid "Piṭakaṃ tambapaṇṇiyāti."
msgstr ""

msgctxt "pli-tv-pvr2.1:35.5"
msgid "(1)"
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc30
msgctxt "pli-tv-pvr2.1:36.1"
msgid "Coriṃ vuṭṭhāpentiyā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:36.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.1:36.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:36.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:36.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:36.6"
msgid "Thullanandā bhikkhunī coriṃ vuṭṭhāpesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:36.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:36.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:36.9"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr2.1:36.10"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr2.1:36.11"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:36.12"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-pvr2.1:37.1"
msgid "Ekāya gāmantaraṃ gacchantiyā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:37.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.1:37.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:37.4"
msgid "Aññataraṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:37.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:37.6"
msgid "Aññatarā bhikkhunī ekā gāmantaraṃ gacchi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:37.7"
msgid "Ekā paññatti, tisso anupaññattiyo."
msgstr ""

msgctxt "pli-tv-pvr2.1:37.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:37.9"
msgid "paṭhamapārājike …"
msgstr ""

msgctxt "pli-tv-pvr2.1:37.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:37.11"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-pvr2.1:38.1"
msgid "Samaggena saṃghena ukkhittaṃ bhikkhuniṃ dhammena vinayena satthusāsanena anapaloketvā kārakasaṃghaṃ anaññāya gaṇassa chandaṃ osārentiyā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:38.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.1:38.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:38.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:38.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:38.6"
msgid "Thullanandā bhikkhunī samaggena saṃghena ukkhittaṃ bhikkhuniṃ dhammena vinayena satthusāsanena anapaloketvā kārakasaṃghaṃ anaññāya gaṇassa chandaṃ osāresi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:38.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:38.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:38.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.1:38.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:38.11"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
#. REF: sc33
msgctxt "pli-tv-pvr2.1:39.1"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa hatthato khādanīyaṃ vā bhojanīyaṃ vā sahatthā paṭiggahetvā bhuñjantiyā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:39.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.1:39.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:39.4"
msgid "Sundarīnandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:39.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:39.6"
msgid "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa hatthato āmisaṃ paṭiggahesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:39.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:39.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:39.9"
msgid "paṭhamapārājike …"
msgstr ""

msgctxt "pli-tv-pvr2.1:39.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:39.11"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-pvr2.1:40.1"
msgid "“Kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-pvr2.1:40.2"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā, taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā”ti uyyojentiyā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:40.3"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.1:40.4"
msgid "Kaṃ arabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:40.5"
msgid "Aññataraṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:40.6"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:40.7"
msgid "Aññatarā bhikkhunī—"
msgstr ""

msgctxt "pli-tv-pvr2.1:40.8"
msgid "“kiṃ te, ayye, eso purisapuggalo karissati avassuto vā anavassuto vā, yato tvaṃ anavassutā."
msgstr ""

msgctxt "pli-tv-pvr2.1:40.9"
msgid "Iṅgha, ayye, yaṃ te eso purisapuggalo deti khādanīyaṃ vā bhojanīyaṃ vā, taṃ tvaṃ sahatthā paṭiggahetvā khāda vā bhuñja vā”ti uyyojesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:40.10"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:40.11"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr2.1:40.12"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:40.13"
msgid "(6)"
msgstr ""

#. HTML: </p><p>
#. REF: sc35, pts-vp-pli5.57
#. VAR: na paṭinissajjantiyā → nappaṭinissajjantiyā (bj, s1-3)
msgctxt "pli-tv-pvr2.1:41.1"
msgid "Kupitāya anattamanāya bhikkhuniyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjantiyā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:41.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr2.1:41.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:41.4"
msgid "Caṇḍakāḷiṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:41.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:41.6"
msgid "Caṇḍakāḷī bhikkhunī kupitā anattamanā evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-pvr2.1:41.7"
msgid "“buddhaṃ paccācikkhāmi, dhammaṃ paccācikkhāmi, saṃghaṃ paccācikkhāmi, sikkhaṃ paccācikkhāmī”ti, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.1:41.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.1:41.9"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.1:41.10"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.1:41.11"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:41.12"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc36
msgctxt "pli-tv-pvr2.1:42.1"
msgid "Kismiñcideva adhikaraṇe paccākatāya bhikkhuniyā yāvatatiyaṃ samanubhāsanāya na paṭinissajjantiyā saṃghādiseso kattha paññattoti?"
