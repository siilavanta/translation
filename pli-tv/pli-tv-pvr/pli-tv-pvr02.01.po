msgctxt "pli-tv-pvr2.1:42.10"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.1:42.11"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.1:42.12"
msgid "(8)"
msgstr ""

