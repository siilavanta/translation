msgctxt "pli-tv-pvr2.1:42.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:42.4"
msgid "Caṇḍakāḷiṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.1:42.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.1:42.6"
msgid "Caṇḍakāḷī bhikkhunī kismiñcideva adhikaraṇe paccākatā kupitā anattamanā evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-pvr2.1:42.7"
msgid "“chandagāminiyo ca bhikkhuniyo, dosagāminiyo ca bhikkhuniyo, mohagāminiyo ca bhikkhuniyo, bhayagāminiyo ca bhikkhuniyo”ti, tasmiṃ vatthusmiṃ."
