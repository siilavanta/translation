msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc162
msgctxt "pli-tv-pvr2.1:179.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc163
msgctxt "pli-tv-pvr2.1:180.1"
msgid "Lasuṇaṃ saṃhare lomaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:180.2"
msgid "talamaṭṭhañca suddhikaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:180.3"
msgid "Bhuñjantāmakadhaññānaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:180.4"
msgid "dve vighāsena dassanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc164
msgctxt "pli-tv-pvr2.1:181.1"
msgid "Andhakāre paṭicchanne,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:181.2"
msgid "ajjhokāse rathikāya ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:181.3"
msgid "Pure pacchā vikāle ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:181.4"
msgid "duggahi niraye vadhi."
msgstr ""

#. HTML: </p><p>
#. REF: sc165
msgctxt "pli-tv-pvr2.1:182.1"
msgid "Naggodakā visibbetvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:182.2"
msgid "pañcāhikaṃ saṅkamanīyaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:182.3"
msgid "Gaṇaṃ vibhaṅgasamaṇaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:182.4"
msgid "dubbalaṃ kathinena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc166
msgctxt "pli-tv-pvr2.1:183.1"
msgid "Ekamañcattharaṇena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:183.2"
msgid "sañcicca sahajīvinī;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:183.3"
msgid "Datvā saṃsaṭṭhaanto ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:183.4"
msgid "tirovassaṃ na pakkame."
msgstr ""

#. HTML: </p><p>
#. REF: sc167
msgctxt "pli-tv-pvr2.1:184.1"
msgid "Rājā āsandi suttañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:184.2"
msgid "gihi vūpasamena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:184.3"
msgid "Dade cīvarāvasathaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:184.4"
msgid "pariyāpuṇañca vācaye."
msgstr ""

#. HTML: </p><p>
#. REF: sc168
msgctxt "pli-tv-pvr2.1:185.1"
msgid "Ārāmakkosacaṇḍī ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:185.2"
msgid "bhuñjeyya kulamaccharī;"
msgstr ""

#. HTML: <br>
#. VAR: pavāraṇovādaṃ → vāso pavāraṇovādā (bj) | vase pavāraṇovādā (s1-3)
msgctxt "pli-tv-pvr2.1:185.3"
msgid "Vāse pavāraṇovādaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:185.4"
msgid "dve dhammā pasākhena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc169, pts-vp-pli5.71
#. VAR: Gabbhī → gabbhinī (bj, s1-3, pts1)
msgctxt "pli-tv-pvr2.1:186.1"
msgid "Gabbhī pāyantī cha dhamme,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:186.2"
msgid "asammatūnadvādasa;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:186.3"
msgid "Paripuṇṇañca saṃghena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:186.4"
msgid "saha vuṭṭhā cha pañca ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc170
msgctxt "pli-tv-pvr2.1:187.1"
msgid "Kumārī dve ca saṃghena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:187.2"
msgid "dvādasa sammatena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:187.3"
msgid "Alaṃ sace ca dvevassaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.1:187.4"
msgid "saṃsaṭṭhā sāmikena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc171
msgctxt "pli-tv-pvr2.1:188.1"
msgid "Pārivāsikānuvassaṃ,"
