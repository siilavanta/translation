#. HTML: <section class="sutta" id="pli-tv-pvr2.9"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr2.9:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.9:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.9:3.1"
msgid "Dutiyabhāga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.9:4.1"
msgid "Katthapaññattivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.9:5.1"
msgid "1. Pārājikakaṇḍa"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.9:6.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṃsaggaṃ sādiyanapaccayā pārājikaṃ kattha paññattaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.9:6.2"
msgid "Kaṃ ārabbha?"
msgstr ""

msgctxt "pli-tv-pvr2.9:6.3"
msgid "Kismiṃ vatthusmiṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr2.9:6.5"
msgid "kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.9:7.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṃsaggaṃ sādiyanapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.4"
msgid "Sundarīnandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.6"
msgid "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.9"
msgid "Anupaññatti anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.16"
msgid "Catunnaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.19"
msgid "Dutiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.23"
msgid "Pārājikāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr2.9:7.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.25"
msgid "Ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.26"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato … pe …"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.28"
msgid "kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:7.29"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc3
msgctxt "pli-tv-pvr2.9:8.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.9:8.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.9:8.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.9:8.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc4
msgctxt "pli-tv-pvr2.9:9.1"
msgid "…"
msgstr ""

msgctxt "pli-tv-pvr2.9:9.2"
msgid "pe …"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc5
msgctxt "pli-tv-pvr2.9:10.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.9:10.2"
msgid "Vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.9:10.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr2.9:10.4"
msgid "Piṭakaṃ tambapaṇṇiyāti."
msgstr ""

msgctxt "pli-tv-pvr2.9:10.5"
msgid "(1)"
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc6, pts-vp-pli5.82
msgctxt "pli-tv-pvr2.9:11.1"
msgid "Vajjappaṭicchādanapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:11.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:11.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:11.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.9:11.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:11.6"
msgid "Thullanandā bhikkhunī jānaṃ pārājikaṃ dhammaṃ ajjhāpannaṃ bhikkhuniṃ nevattanā paṭicodesi na gaṇassa ārocesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:11.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:11.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.9:11.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.9:11.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.9:11.11"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr2.9:12.1"
msgid "Yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:12.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:12.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:12.4"
msgid "Thullanandaṃ bhikkhuniṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.9:12.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:12.6"
msgid "Thullanandā bhikkhunī samaggena saṃghena ukkhittaṃ ariṭṭhaṃ bhikkhuṃ gaddhabādhipubbaṃ anuvatti, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:12.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:12.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.9:12.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.9:12.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.9:12.11"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr2.9:13.1"
msgid "Aṭṭhamaṃ vatthuṃ paripūraṇapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:13.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:13.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:13.4"
msgid "Chabbaggiyā bhikkhuniyo ārabbha."
msgstr ""

msgctxt "pli-tv-pvr2.9:13.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr2.9:13.6"
msgid "Chabbaggiyā bhikkhuniyo aṭṭhamaṃ vatthuṃ paripūresuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.9:13.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr2.9:13.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr2.9:13.9"
msgid "dhuranikkhepe …"
msgstr ""

msgctxt "pli-tv-pvr2.9:13.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.9:13.11"
msgid "(4)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-pvr2.9:14.1"
msgid "Pārājikā niṭṭhitā."
msgstr ""

