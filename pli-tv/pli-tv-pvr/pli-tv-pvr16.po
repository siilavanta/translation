#. HTML: <section class="sutta" id="pli-tv-pvr16"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr16:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr16:2.1"
msgid "Kathinabheda"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr16:3.1"
msgid "1. Kathinaatthatādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli5.172
#. VAR: kathinaṃ → kaṭhinaṃ (bj, s1-3, pts1)
msgctxt "pli-tv-pvr16:4.1"
msgid "Kassa kathinaṃ anatthataṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:4.2"
msgid "Kassa kathinaṃ atthataṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:4.3"
msgid "Kinti kathinaṃ anatthataṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:4.4"
msgid "Kinti kathinaṃ atthataṃ?"
msgstr ""

#. HTML: </p><p><b>
#. REF: sc2
msgctxt "pli-tv-pvr16:5.1"
msgid "Kassa kathinaṃ anatthatanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:5.2"
msgid "Dvinnaṃ puggalānaṃ anatthataṃ hoti kathinaṃ—"
msgstr ""

msgctxt "pli-tv-pvr16:5.3"
msgid "anatthārakassa ca ananumodakassa ca."
msgstr ""

msgctxt "pli-tv-pvr16:5.4"
msgid "Imesaṃ dvinnaṃ puggalānaṃ anatthataṃ hoti kathinaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc3
msgctxt "pli-tv-pvr16:6.1"
msgid "Kassa kathinaṃ atthatanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:6.2"
msgid "Dvinnaṃ puggalānaṃ atthataṃ hoti kathinaṃ—"
msgstr ""

msgctxt "pli-tv-pvr16:6.3"
msgid "atthārakassa ca anumodakassa ca."
msgstr ""

msgctxt "pli-tv-pvr16:6.4"
msgid "Imesaṃ dvinnaṃ puggalānaṃ atthataṃ hoti kathinaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc4
msgctxt "pli-tv-pvr16:7.1"
msgid "Kinti kathinaṃ anatthatanti?"
msgstr ""

#. HTML: </b>
#. VAR: na kaṇḍusakaraṇamattena → na kaṇḍūsakaraṇamattena (s1-3) | na gaṇḍusakaraṇamattena (mr)
#. VAR: na ovaddheyyakaraṇamattena → na ovaṭṭeyyakaraṇamattena (bj, s1-3) | na ovadeyyakaraṇamattena (mr)
#. VAR: ovaṭṭiyakaraṇamattena → ovattikaraṇa ... (bj) | ovaṭṭikaraṇa ... (s1-3)
msgctxt "pli-tv-pvr16:7.2"
msgid "Catuvīsatiyā ākārehi anatthataṃ hoti kathinaṃ, na ullikhitamattena atthataṃ hoti kathinaṃ, na dhovanamattena atthataṃ hoti kathinaṃ, na cīvaravicāraṇamattena atthataṃ hoti kathinaṃ, na chedanamattena atthataṃ hoti kathinaṃ, na bandhanamattena atthataṃ hoti kathinaṃ, na ovaṭṭiyakaraṇamattena atthataṃ hoti kathinaṃ, na kaṇḍusakaraṇamattena atthataṃ hoti kathinaṃ, na daḷhīkammakaraṇamattena atthataṃ hoti kathinaṃ, na anuvātakaraṇamattena atthataṃ hoti kathinaṃ, na paribhaṇḍakaraṇamattena atthataṃ hoti kathinaṃ, na ovaddheyyakaraṇamattena atthataṃ hoti kathinaṃ, na kambalamaddanamattena atthataṃ hoti kathinaṃ, na nimittakatena atthataṃ hoti kathinaṃ, na parikathākatena atthataṃ hoti kathinaṃ, na kukkukatena atthataṃ hoti kathinaṃ, na sannidhikatena atthataṃ hoti kathinaṃ, na nissaggiyena atthataṃ hoti kathinaṃ, na akappakatena atthataṃ hoti kathinaṃ, na aññatra saṅghāṭiyā atthataṃ hoti kathinaṃ, na aññatra uttarāsaṅgena atthataṃ hoti kathinaṃ, na aññatra antaravāsakena atthataṃ hoti kathinaṃ, na aññatra pañcakena vā atirekapañcakena vā tadaheva sañchinnena samaṇḍalīkatena atthataṃ hoti kathinaṃ, na aññatra puggalassa atthārā atthataṃ hoti kathinaṃ, sammā ce atthataṃ hoti kathinaṃ, tañce nissīmaṭṭho anumodati."
msgstr ""

msgctxt "pli-tv-pvr16:7.3"
msgid "Evampi anatthataṃ hoti kathinaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-pvr16:8.1"
msgid "Nimittakammaṃ nāma nimittaṃ karoti—"
msgstr ""

msgctxt "pli-tv-pvr16:8.2"
msgid "“iminā dussena kathinaṃ attharissāmī”ti."
msgstr ""

msgctxt "pli-tv-pvr16:8.3"
msgid "Parikathā nāma parikathaṃ karoti—"
msgstr ""

msgctxt "pli-tv-pvr16:8.4"
msgid "“imāya parikathāya kathinadussaṃ nibbattessāmī”ti."
msgstr ""

msgctxt "pli-tv-pvr16:8.5"
msgid "Kukkukataṃ nāma anādiyadānaṃ vuccati."
msgstr ""

msgctxt "pli-tv-pvr16:8.6"
msgid "Sannidhi nāma dve sannidhiyo—"
msgstr ""

msgctxt "pli-tv-pvr16:8.7"
msgid "karaṇasannidhi vā nicayasannidhi vā."
msgstr ""

#. VAR: uṭṭhahati → udriyati (bj, s1-3, pts1)
msgctxt "pli-tv-pvr16:8.8"
msgid "Nissaggiyaṃ nāma kayiramāne aruṇaṃ uṭṭhahati."
msgstr ""

msgctxt "pli-tv-pvr16:8.9"
msgid "Imehi catuvīsatiyā ākārehi anatthataṃ hoti kathinaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-pvr16:9.1"
msgid "Kinti kathinaṃ atthatanti?"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:9.2"
msgid "Sattarasahi ākārehi atthataṃ hoti kathinaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:9.3"
msgid "Ahatena atthataṃ hoti kathinaṃ, ahatakappena atthataṃ hoti kathinaṃ, pilotikāya atthataṃ hoti kathinaṃ, paṃsukūlena atthataṃ hoti kathinaṃ, pāpaṇikena atthataṃ hoti kathinaṃ, animittakatena atthataṃ hoti kathinaṃ, aparikathākatena atthataṃ hoti kathinaṃ, akukkukatena atthataṃ hoti kathinaṃ, asannidhikatena atthataṃ hoti kathinaṃ, anissaggiyena atthataṃ hoti kathinaṃ, kappakatena atthataṃ hoti kathinaṃ, saṅghāṭiyā atthataṃ hoti kathinaṃ, uttarāsaṅgena atthataṃ hoti kathinaṃ, antaravāsakena atthataṃ hoti kathinaṃ, pañcakena vā atirekapañcakena vā tadaheva sañchinnena samaṇḍalīkatena atthataṃ hoti kathinaṃ, puggalassa atthārā atthataṃ hoti kathinaṃ, sammā ce atthataṃ hoti kathinaṃ, tañce sīmaṭṭho anumodati, evampi atthataṃ hoti kathinaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:9.4"
msgid "Imehi sattarasahi ākārehi atthataṃ hoti kathinaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr16:10.1"
msgid "Saha kathinassa atthārā kati dhammā jāyanti?"
msgstr ""

msgctxt "pli-tv-pvr16:10.2"
msgid "Saha kathinassa atthārā pannarasa dhammā jāyanti—"
msgstr ""

msgctxt "pli-tv-pvr16:10.3"
msgid "aṭṭha mātikā, dve palibodhā, pañcānisaṃsā."
msgstr ""

msgctxt "pli-tv-pvr16:10.4"
msgid "Saha kathinassa atthārā ime pannarasa dhammā jāyanti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr16:11.1"
msgid "2. Kathinaanantarapaccayādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc8, pts-vp-pli5.173
msgctxt "pli-tv-pvr16:12.1"
msgid "Payogassa katame dhammā anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo, purejātapaccayena paccayo, pacchājātapaccayena paccayo, sahajātapaccayena paccayo?"
msgstr ""

msgctxt "pli-tv-pvr16:12.2"
msgid "Pubbakaraṇassa katame dhammā anantarapaccayena paccayo … pe …"
msgstr ""

msgctxt "pli-tv-pvr16:12.4"
msgid "paccuddhārassa katame dhammā …"
msgstr ""

msgctxt "pli-tv-pvr16:12.5"
msgid "adhiṭṭhānassa katame dhammā …"
msgstr ""

msgctxt "pli-tv-pvr16:12.6"
msgid "atthārassa katame dhammā …"
msgstr ""

msgctxt "pli-tv-pvr16:12.7"
msgid "mātikānañca palibodhānañca katame dhammā …"
msgstr ""

msgctxt "pli-tv-pvr16:12.8"
msgid "vatthussa katame dhammā anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo, purejātapaccayena paccayo, pacchājātapaccayena paccayo, sahajātapaccayena paccayo?"
msgstr ""

#. HTML: </p><p>
#. REF: sc9, pts-vp-pli5.174
msgctxt "pli-tv-pvr16:13.1"
msgid "Pubbakaraṇaṃ payogassa anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.2"
msgid "Payogo pubbakaraṇassa purejātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.3"
msgid "Pubbakaraṇaṃ payogassa pacchājātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.4"
msgid "Pannarasa dhammā sahajātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.5"
msgid "Paccuddhāro pubbakaraṇassa anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.6"
msgid "Pubbakaraṇaṃ paccuddhārassa purejātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.7"
msgid "Paccuddhāro pubbakaraṇassa pacchājātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.8"
msgid "Pannarasa dhammā sahajātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.9"
msgid "Adhiṭṭhānaṃ paccuddhārassa anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.10"
msgid "Paccuddhāro adhiṭṭhānassa purejātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.11"
msgid "Adhiṭṭhānaṃ paccuddhārassa pacchājātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.12"
msgid "Pannarasa dhammā sahajātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.13"
msgid "Atthāro adhiṭṭhānassa anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.14"
msgid "Adhiṭṭhānaṃ atthārassa purejātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.15"
msgid "Atthāro adhiṭṭhānassa pacchājātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.16"
msgid "Pannarasa dhammā sahajātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.17"
msgid "Mātikā ca palibodhā ca atthārassa anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.18"
msgid "Atthāro mātikānañca palibodhānañca purejātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.19"
msgid "Mātikā ca palibodhā ca atthārassa pacchājātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.20"
msgid "Pannarasa dhammā sahajātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.21"
msgid "Āsā ca anāsā ca vatthussa anantarapaccayena paccayo, samanantarapaccayena paccayo, nissayapaccayena paccayo, upanissayapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.22"
msgid "Vatthu āsānañca anāsānañca purejātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.23"
msgid "Āsā ca anāsā ca vatthussa pacchājātapaccayena paccayo."
msgstr ""

msgctxt "pli-tv-pvr16:13.24"
msgid "Pannarasa dhammā sahajātapaccayena paccayo."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr16:14.1"
msgid "3. Pubbakaraṇanidānādivibhāga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc10
msgctxt "pli-tv-pvr16:15.1"
msgid "Pubbakaraṇaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:15.2"
msgid "Paccuddhāro kiṃnidāno, kiṃsamudayo, kiṃjātiko, kiṃpabhavo, kiṃsambhāro, kiṃsamuṭṭhāno?"
msgstr ""

msgctxt "pli-tv-pvr16:15.3"
msgid "Adhiṭṭhānaṃ kiṃnidānaṃ, kiṃsamudayaṃ, kiṃjātikaṃ, kiṃpabhavaṃ, kiṃsambhāraṃ, kiṃsamuṭṭhānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:15.4"
msgid "Atthāro kiṃnidāno, kiṃsamudayo, kiṃjātiko, kiṃpabhavo, kiṃsambhāro, kiṃsamuṭṭhāno?"
msgstr ""

msgctxt "pli-tv-pvr16:15.5"
msgid "Mātikā ca palibodhā ca kiṃnidānā, kiṃsamudayā, kiṃjātikā, kiṃpabhavā, kiṃsambhārā kiṃsamuṭṭhānā?"
msgstr ""

msgctxt "pli-tv-pvr16:15.6"
msgid "Āsā ca anāsā ca kiṃnidānā, kiṃsamudayā, kiṃjātikā, kiṃpabhavā, kiṃsambhārā, kiṃsamuṭṭhānā?"
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr16:16.1"
msgid "Pubbakaraṇaṃ payoganidānaṃ, payogasamudayaṃ, payogajātikaṃ, payogapabhavaṃ, payogasambhāraṃ, payogasamuṭṭhānaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:16.2"
msgid "Paccuddhāro pubbakaraṇanidāno, pubbakaraṇasamudayo, pubbakaraṇajātiko, pubbakaraṇapabhavo, pubbakaraṇasambhāro, pubbakaraṇasamuṭṭhāno."
msgstr ""

msgctxt "pli-tv-pvr16:16.3"
msgid "Adhiṭṭhānaṃ paccuddhāranidānaṃ, paccuddhārasamudayaṃ, paccuddhārajātikaṃ, paccuddhārapabhavaṃ, paccuddhārasambhāraṃ, paccuddhārasamuṭṭhānaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:16.4"
msgid "Atthāro adhiṭṭhānanidāno, adhiṭṭhānasamudayo, adhiṭṭhānajātiko, adhiṭṭhānapabhavo, adhiṭṭhānasambhāro, adhiṭṭhānasamuṭṭhāno."
msgstr ""

msgctxt "pli-tv-pvr16:16.5"
msgid "Mātikā ca palibodhā ca atthāranidānā, atthārasamudayā, atthārajātikā, atthārapabhavā, atthārasambhārā, atthārasamuṭṭhānā."
msgstr ""

msgctxt "pli-tv-pvr16:16.6"
msgid "Āsā ca anāsā ca vatthunidānā, vatthusamudayā, vatthujātikā, vatthupabhavā, vatthusambhārā, vatthusamuṭṭhānā."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr16:17.1"
msgid "Payogo kiṃnidāno, kiṃsamudayo, kiṃjātiko, kiṃpabhavo, kiṃsambhāro, kiṃsamuṭṭhāno, pubbakaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr16:17.3"
msgid "paccuddhāro …"
msgstr ""

msgctxt "pli-tv-pvr16:17.4"
msgid "adhiṭṭhānaṃ …"
msgstr ""

msgctxt "pli-tv-pvr16:17.5"
msgid "atthāro …"
msgstr ""

msgctxt "pli-tv-pvr16:17.6"
msgid "mātikā ca palibodhā ca …"
msgstr ""

msgctxt "pli-tv-pvr16:17.7"
msgid "vatthu …"
msgstr ""

msgctxt "pli-tv-pvr16:17.8"
msgid "āsā ca anāsā ca kiṃnidānā, kiṃsamudayā, kiṃjātikā, kiṃpabhavā, kiṃsambhārā, kiṃsamuṭṭhānā?"
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr16:18.1"
msgid "Payogo hetunidāno, hetusamudayo, hetujātiko, hetupabhavo, hetusambhāro, hetusamuṭṭhāno."
msgstr ""

msgctxt "pli-tv-pvr16:18.2"
msgid "Pubbakaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr16:18.4"
msgid "paccuddhāro …"
msgstr ""

msgctxt "pli-tv-pvr16:18.5"
msgid "adhiṭṭhānaṃ …"
msgstr ""

msgctxt "pli-tv-pvr16:18.6"
msgid "atthāro …"
msgstr ""

msgctxt "pli-tv-pvr16:18.7"
msgid "mātikā ca palibodhā ca …"
msgstr ""

msgctxt "pli-tv-pvr16:18.8"
msgid "vatthu …"
msgstr ""

msgctxt "pli-tv-pvr16:18.9"
msgid "āsā ca anāsā ca hetunidānā, hetusamudayā, hetujātikā, hetupabhavā, hetusambhārā, hetusamuṭṭhānā."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr16:19.1"
msgid "Payogo kiṃnidāno, kiṃsamudayo, kiṃjātiko, kiṃpabhavo, kiṃsambhāro, kiṃsamuṭṭhāno?"
msgstr ""

msgctxt "pli-tv-pvr16:19.2"
msgid "Pubbakaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr16:19.4"
msgid "paccuddhāro …"
msgstr ""

msgctxt "pli-tv-pvr16:19.5"
msgid "adhiṭṭhānaṃ …"
msgstr ""

msgctxt "pli-tv-pvr16:19.6"
msgid "atthāro …"
msgstr ""

msgctxt "pli-tv-pvr16:19.7"
msgid "mātikā ca palibodhā ca …"
msgstr ""

msgctxt "pli-tv-pvr16:19.8"
msgid "vatthu …"
msgstr ""

msgctxt "pli-tv-pvr16:19.9"
msgid "āsā ca anāsā ca kiṃnidānā, kiṃsamudayā, kiṃjātikā, kiṃpabhavā, kiṃsambhārā, kiṃsamuṭṭhānā?"
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr16:20.1"
msgid "Payogo paccayanidāno, paccayasamudayo, paccayajātiko, paccayapabhavo, paccayasambhāro, paccayasamuṭṭhāno."
msgstr ""

msgctxt "pli-tv-pvr16:20.2"
msgid "Pubbakaraṇaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr16:20.4"
msgid "paccuddhāro …"
msgstr ""

msgctxt "pli-tv-pvr16:20.5"
msgid "adhiṭṭhānaṃ …"
msgstr ""

msgctxt "pli-tv-pvr16:20.6"
msgid "atthāro …"
msgstr ""

msgctxt "pli-tv-pvr16:20.7"
msgid "mātikā ca palibodhā ca …"
msgstr ""

msgctxt "pli-tv-pvr16:20.8"
msgid "vatthu …"
msgstr ""

msgctxt "pli-tv-pvr16:20.9"
msgid "āsā ca anāsā ca paccayanidānā, paccayasamudayā, paccayajātikā, paccayapabhavā, paccayasambhārā, paccayasamuṭṭhānā."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr16:21.1"
msgid "Pubbakaraṇaṃ katihi dhammehi saṅgahitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:21.2"
msgid "Pubbakaraṇaṃ sattahi dhammehi saṅgahitaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:21.3"
msgid "Dhovanena, vicāraṇena, chedanena, bandhanena, sibbanena, rajanena, kappakaraṇena—"
msgstr ""

msgctxt "pli-tv-pvr16:21.4"
msgid "pubbakaraṇaṃ imehi sattahi dhammehi saṅgahitaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr16:22.1"
msgid "Paccuddhāro katihi dhammehi saṅgahito?"
msgstr ""

msgctxt "pli-tv-pvr16:22.2"
msgid "Paccuddhāro tīhi dhammehi saṅgahito—"
msgstr ""

msgctxt "pli-tv-pvr16:22.3"
msgid "saṅghāṭiyā, uttarāsaṅgena, antaravāsakena."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-pvr16:23.1"
msgid "Adhiṭṭhānaṃ katihi dhammehi saṅgahitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:23.2"
msgid "Adhiṭṭhānaṃ tīhi dhammehi saṅgahitaṃ—"
msgstr ""

msgctxt "pli-tv-pvr16:23.3"
msgid "saṅghāṭiyā, uttarāsaṅgena, antaravāsakena."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-pvr16:24.1"
msgid "Atthāro katihi dhammehi saṅgahito?"
msgstr ""

msgctxt "pli-tv-pvr16:24.2"
msgid "Atthāro ekena dhammena saṅgahito—"
msgstr ""

msgctxt "pli-tv-pvr16:24.3"
msgid "vacībhedena."
msgstr ""

#. HTML: </p><p>
#. REF: sc20, pts-vp-pli5.175
msgctxt "pli-tv-pvr16:25.1"
msgid "Kathinassa kati mūlāni, kati vatthūni, kati bhūmiyo?"
msgstr ""

msgctxt "pli-tv-pvr16:25.2"
msgid "Kathinassa ekaṃ mūlaṃ—"
msgstr ""

msgctxt "pli-tv-pvr16:25.3"
msgid "saṃgho;"
msgstr ""

msgctxt "pli-tv-pvr16:25.4"
msgid "tīṇi vatthūni—"
msgstr ""

msgctxt "pli-tv-pvr16:25.5"
msgid "saṅghāṭi, uttarāsaṅgo, antaravāsako, cha bhūmiyo—"
msgstr ""

msgctxt "pli-tv-pvr16:25.6"
msgid "khomaṃ, kappāsikaṃ, koseyyaṃ, kambalaṃ, sāṇaṃ, bhaṅgaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr16:26.1"
msgid "Kathinassa ko ādi, kiṃ majjhe, kiṃ pariyosānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:26.2"
msgid "Kathinassa pubbakaraṇaṃ ādi, kriyā majjhe, atthāro pariyosānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc22
msgctxt "pli-tv-pvr16:27.1"
msgid "Katihaṅgehi samannāgato puggalo abhabbo kathinaṃ attharituṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:27.2"
msgid "Katihaṅgehi samannāgato puggalo bhabbo kathinaṃ attharituṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:27.3"
msgid "Aṭṭhahaṅgehi samannāgato puggalo abhabbo kathinaṃ attharituṃ."
msgstr ""

msgctxt "pli-tv-pvr16:27.4"
msgid "Aṭṭhahaṅgehi samannāgato puggalo bhabbo kathinaṃ attharituṃ."
msgstr ""

msgctxt "pli-tv-pvr16:27.5"
msgid "Katamehi aṭṭhahaṅgehi samannāgato puggalo abhabbo kathinaṃ attharituṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:27.6"
msgid "Pubbakaraṇaṃ na jānāti, paccuddhāraṃ na jānāti, adhiṭṭhānaṃ na jānāti, atthāraṃ na jānāti, mātikaṃ na jānāti, palibodhaṃ na jānāti, uddhāraṃ na jānāti, ānisaṃsaṃ na jānāti—"
msgstr ""

msgctxt "pli-tv-pvr16:27.7"
msgid "imehi aṭṭhahaṅgehi samannāgato puggalo abhabbo kathinaṃ attharituṃ."
msgstr ""

msgctxt "pli-tv-pvr16:27.8"
msgid "Katamehi aṭṭhahaṅgehi samannāgato puggalo bhabbo kathinaṃ attharituṃ?"
msgstr ""

msgctxt "pli-tv-pvr16:27.9"
msgid "Pubbakaraṇaṃ jānāti, paccuddhāraṃ jānāti, adhiṭṭhānaṃ jānāti, atthāraṃ jānāti, mātikaṃ jānāti, palibodhaṃ jānāti, uddhāraṃ jānāti, ānisaṃsaṃ jānāti—"
msgstr ""

msgctxt "pli-tv-pvr16:27.10"
msgid "imehi aṭṭhahaṅgehi samannāgato puggalo bhabbo kathinaṃ attharituṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc23
#. VAR: Katinaṃ → katinnaṃ (bj) | katīnaṃ (s1-3, pts1)
msgctxt "pli-tv-pvr16:28.1"
msgid "Katinaṃ puggalānaṃ kathinatthārā na ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:28.2"
msgid "Katinaṃ puggalānaṃ kathinatthārā ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:28.3"
msgid "Tiṇṇaṃ puggalānaṃ kathinatthārā na ruhanti."
msgstr ""

msgctxt "pli-tv-pvr16:28.4"
msgid "Tiṇṇaṃ puggalānaṃ kathinatthārā ruhanti."
msgstr ""

msgctxt "pli-tv-pvr16:28.5"
msgid "Katamesaṃ tiṇṇaṃ puggalānaṃ kathinatthārā na ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:28.6"
msgid "Nissīmaṭṭho anumodati, anumodento na vācaṃ bhindati, vācaṃ bhindanto na paraṃ viññāpeti—"
msgstr ""

msgctxt "pli-tv-pvr16:28.7"
msgid "imesaṃ tiṇṇaṃ puggalānaṃ kathinatthārā na ruhanti."
msgstr ""

msgctxt "pli-tv-pvr16:28.8"
msgid "Katamesaṃ tiṇṇaṃ puggalānaṃ kathinatthārā ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:28.9"
msgid "Sīmaṭṭho anumodati, anumodento vācaṃ bhindati, vācaṃ bhindanto paraṃ viññāpeti—"
msgstr ""

msgctxt "pli-tv-pvr16:28.10"
msgid "imesaṃ tiṇṇaṃ puggalānaṃ kathinatthārā ruhanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr16:29.1"
msgid "Kati kathinatthārā na ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:29.2"
msgid "Kati kathinatthārā ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:29.3"
msgid "Tayo kathinatthārā na ruhanti."
msgstr ""

msgctxt "pli-tv-pvr16:29.4"
msgid "Tayo kathinatthārā ruhanti."
msgstr ""

msgctxt "pli-tv-pvr16:29.5"
msgid "Katame tayo kathinatthārā na ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:29.6"
msgid "Vatthuvipannañceva hoti, kālavipannañca, karaṇavipannañca—"
msgstr ""

msgctxt "pli-tv-pvr16:29.7"
msgid "ime tayo kathinatthārā na ruhanti."
msgstr ""

msgctxt "pli-tv-pvr16:29.8"
msgid "Katame tayo kathinatthārā ruhanti?"
msgstr ""

msgctxt "pli-tv-pvr16:29.9"
msgid "Vatthusampannañceva hoti, kālasampannañca, karaṇasampannañca—"
msgstr ""

msgctxt "pli-tv-pvr16:29.10"
msgid "ime tayo kathinatthārā ruhanti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr16:30.1"
msgid "4. Kathinādijānitabbavibhāga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc25
msgctxt "pli-tv-pvr16:31.1"
msgid "Kathinaṃ jānitabbaṃ, kathinatthāro jānitabbo, kathinassa atthāramāso jānitabbo, kathinassa atthāravipatti jānitabbā, kathinassa atthārasampatti jānitabbā, nimittakammaṃ jānitabbaṃ, parikathā jānitabbā, kukkukataṃ jānitabbaṃ, sannidhi jānitabbā, nissaggiyaṃ jānitabbaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc26, pts-vp-pli5.176
msgctxt "pli-tv-pvr16:32.1"
msgid "Kathinaṃ jānitabbanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:32.1"
msgid "tesaññeva dhammānaṃ saṅgaho samavāyo nāmaṃ nāmakammaṃ nāmadheyyaṃ nirutti byañjanaṃ abhilāpo yadidaṃ kathinanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc27
msgctxt "pli-tv-pvr16:33.1"
msgid "Kathinassa atthāramāso jānitabboti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:33.1"
msgid "vassānassa pacchimo māso jānitabbo."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc28
msgctxt "pli-tv-pvr16:34.1"
msgid "Kathinassa atthāravipatti jānitabbāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:34.1"
msgid "catuvīsatiyā ākārehi kathinassa atthāravipatti jānitabbā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc29
msgctxt "pli-tv-pvr16:35.1"
msgid "Kathinassa atthārasampatti jānitabbāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:35.1"
msgid "sattarasahi ākārehi kathinassa atthārasampatti jānitabbā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc30
msgctxt "pli-tv-pvr16:36.1"
msgid "Nimittakammaṃ jānitabbanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:36.1"
msgid "nimittaṃ karoti iminā dussena kathinaṃ attharissāmīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc31
msgctxt "pli-tv-pvr16:37.1"
msgid "Parikathā jānitabbāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:37.1"
msgid "parikathaṃ karoti imāya parikathāya kathinadussaṃ nibbattessāmīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc32
msgctxt "pli-tv-pvr16:38.1"
msgid "Kukkukataṃ jānitabbanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:38.1"
msgid "anādiyadānaṃ jānitabbaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc33
msgctxt "pli-tv-pvr16:39.1"
msgid "Sannidhi jānitabbāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:39.1"
msgid "dve sannidhiyo jānitabbā—"
msgstr ""

msgctxt "pli-tv-pvr16:39.2"
msgid "karaṇasannidhi vā nicayasannidhi vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc34
#. VAR: uṭṭhahati → udrīyati (bj) | udriyati (s1-3, pts1)+. HTML: <b>
msgctxt "pli-tv-pvr16:40.1"
msgid "Nissaggiyaṃ jānitabbanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:40.1"
msgid "kariyamāne aruṇaṃ uṭṭhahati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc35
msgctxt "pli-tv-pvr16:41.1"
msgid "Kathinatthāro jānitabboti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-pvr16:41.1"
msgid "sace saṃghassa kathinadussaṃ uppannaṃ hoti, saṃghena kathaṃ paṭipajjitabbaṃ, atthārakena kathaṃ paṭipajjitabbaṃ, anumodakena kathaṃ paṭipajjitabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc36, pts-vp-pli5.177
msgctxt "pli-tv-pvr16:42.1"
msgid "Saṃghena ñattidutiyena kammena kathinatthārakassa bhikkhuno dātabbaṃ, tena kathinatthārakena bhikkhunā tadaheva dhovitvā vimajjitvā vicāretvā chinditvā sibbetvā rajitvā kappaṃ katvā kathinaṃ attharitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:42.2"
msgid "Sace saṅghāṭiyā kathinaṃ attharitukāmo hoti, porāṇikā saṅghāṭi paccuddharitabbā, navā saṅghāṭi adhiṭṭhātabbā."
msgstr ""

msgctxt "pli-tv-pvr16:42.3"
msgid "Imāya saṅghāṭiyā kathinaṃ attharāmīti vācā bhinditabbā."
msgstr ""

msgctxt "pli-tv-pvr16:42.4"
msgid "Sace uttarāsaṅgena kathinaṃ attharitukāmo hoti, porāṇako uttarāsaṅgo paccuddharitabbo, navo uttarāsaṅgo adhiṭṭhātabbo."
msgstr ""

msgctxt "pli-tv-pvr16:42.5"
msgid "Iminā uttarāsaṅgena kathinaṃ attharāmīti vācā bhinditabbā."
msgstr ""

msgctxt "pli-tv-pvr16:42.6"
msgid "Sace antaravāsakena kathinaṃ attharitukāmo hoti, porāṇako antaravāsako paccuddharitabbo, navo antaravāsako adhiṭṭhātabbo."
msgstr ""

msgctxt "pli-tv-pvr16:42.7"
msgid "Iminā antaravāsakena kathinaṃ attharāmīti vācā bhinditabbā."
msgstr ""

msgctxt "pli-tv-pvr16:42.8"
msgid "Tena kathinatthārakena bhikkhunā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr16:42.9"
msgid "“atthataṃ, bhante, saṃghassa kathinaṃ, dhammiko kathinatthāro, anumodathā”ti."
msgstr ""

msgctxt "pli-tv-pvr16:42.10"
msgid "Tehi anumodakehi bhikkhūhi ekaṃsaṃ uttarāsaṅgaṃ karitvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr16:42.11"
msgid "“atthataṃ, āvuso, saṃghassa kathinaṃ, dhammiko kathinatthāro, anumodāmā”ti."
msgstr ""

msgctxt "pli-tv-pvr16:42.12"
msgid "Tena kathinatthārakena bhikkhunā sambahule bhikkhū upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā añjaliṃ paggahetvā evamassu vacanīyā—"
msgstr ""

msgctxt "pli-tv-pvr16:42.13"
msgid "“atthataṃ, bhante, saṃghassa kathinaṃ, dhammiko kathinatthāro, anumodathā”ti."
msgstr ""

msgctxt "pli-tv-pvr16:42.14"
msgid "Tehi anumodakehi bhikkhūhi ekaṃsaṃ uttarāsaṅgaṃ karitvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr16:42.15"
msgid "“atthataṃ, āvuso, saṃghassa kathinaṃ, dhammiko kathinatthāro anumodāmā”ti."
msgstr ""

msgctxt "pli-tv-pvr16:42.16"
msgid "Tena kathinatthārakena bhikkhunā ekaṃ bhikkhuṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr16:42.17"
msgid "“atthataṃ, āvuso, saṃghassa kathinaṃ, dhammiko kathinatthāro, anumodāhī”ti."
msgstr ""

msgctxt "pli-tv-pvr16:42.18"
msgid "Tena anumodakena bhikkhunā ekaṃsaṃ uttarāsaṅgaṃ karitvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr16:42.19"
msgid "“atthataṃ, āvuso, saṃghassa kathinaṃ, dhammiko kathinatthāro, anumodāmī”ti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr16:43.1"
msgid "5. Puggalassevakathinatthāra"
msgstr ""

#. HTML: </h2><p>
#. REF: sc37
msgctxt "pli-tv-pvr16:44.1"
msgid "Saṃgho kathinaṃ attharati, gaṇo kathinaṃ attharati, puggalo kathinaṃ attharatīti?"
msgstr ""

msgctxt "pli-tv-pvr16:44.2"
msgid "Na saṃgho kathinaṃ attharati, na gaṇo kathinaṃ attharati, puggalo kathinaṃ attharatīti."
msgstr ""

msgctxt "pli-tv-pvr16:44.3"
msgid "Hañci na saṃgho kathinaṃ attharati, na gaṇo kathinaṃ attharati, puggalo kathinaṃ attharati."
msgstr ""

msgctxt "pli-tv-pvr16:44.4"
msgid "Saṃghassa anatthataṃ hoti kathinaṃ, gaṇassa anatthataṃ hoti kathinaṃ, puggalassa atthataṃ hoti kathinaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:44.5"
msgid "Saṃgho pātimokkhaṃ uddisati gaṇo pātimokkhaṃ uddisati puggalo pātimokkhaṃ uddisatīti?"
msgstr ""

msgctxt "pli-tv-pvr16:44.6"
msgid "Na saṃgho pātimokkhaṃ uddisati, na gaṇo pātimokkhaṃ uddisati, puggalo pātimokkhaṃ uddisatīti."
msgstr ""

msgctxt "pli-tv-pvr16:44.7"
msgid "Hañci na saṃgho pātimokkhaṃ uddisati, na gaṇo pātimokkhaṃ uddisati, puggalo pātimokkhaṃ uddisati."
msgstr ""

msgctxt "pli-tv-pvr16:44.8"
msgid "Saṃghassa anuddiṭṭhaṃ hoti pātimokkhaṃ, gaṇassa anuddiṭṭhaṃ hoti pātimokkhaṃ, puggalassa uddiṭṭhaṃ hoti pātimokkhaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:44.9"
msgid "Saṃghassa sāmaggiyā gaṇassa sāmaggiyā puggalassa uddesā saṃghassa uddiṭṭhaṃ hoti pātimokkhaṃ, gaṇassa uddiṭṭhaṃ hoti pātimokkhaṃ, puggalassa uddiṭṭhaṃ hoti pātimokkhaṃ."
msgstr ""

msgctxt "pli-tv-pvr16:44.10"
msgid "Evameva na saṃgho kathinaṃ attharati, na gaṇo kathinaṃ attharati, puggalo kathinaṃ attharati."
msgstr ""

msgctxt "pli-tv-pvr16:44.11"
msgid "Saṃghassa anumodanāya gaṇassa anumodanāya puggalassa atthārā saṃghassa atthataṃ hoti kathinaṃ, gaṇassa atthataṃ hoti kathinaṃ, puggalassa atthataṃ hoti kathinanti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr16:45.1"
msgid "6. Palibodhapañhābyākaraṇa"
msgstr ""

#. HTML: </h2><blockquote class="gatha"><p>
#. REF: sc38
msgctxt "pli-tv-pvr16:46.1"
msgid "Pakkamanantiko kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:46.2"
msgid "Vutto ādiccabandhunā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:46.3"
msgid "Etañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:46.4"
msgid "Katamo palibodho paṭhamaṃ chijjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc39, pts-vp-pli5.178
msgctxt "pli-tv-pvr16:47.1"
msgid "Pakkamanantiko kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:47.2"
msgid "Vutto ādiccabandhunā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:47.3"
msgid "Etañca tāhaṃ vissajjissaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:47.4"
msgid "Cīvarapalibodho paṭhamaṃ chijjati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:47.5"
msgid "Tassa saha bahisīmagamanā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:47.6"
msgid "Āvāsapalibodho chijjati."
msgstr ""

msgctxt "pli-tv-pvr16:47.7"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc40
msgctxt "pli-tv-pvr16:48.1"
msgid "Niṭṭhānantiko kathinuddhāro,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:48.2"
msgid "Vutto ādiccabandhunā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:48.3"
msgid "Etañca tāhaṃ pucchāmi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr16:48.4"
msgid "Katamo palibodho paṭhamaṃ chijjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc41
msgctxt "pli-tv-pvr16:49.1"
msgid "Niṭṭhānantiko kathinuddhāro,"
