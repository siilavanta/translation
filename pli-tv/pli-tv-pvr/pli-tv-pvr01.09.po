msgctxt "pli-tv-pvr1.9:24.1"
msgid "Sañcarittaṃ samāpajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:24.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.4"
msgid "Āyasmantaṃ udāyiṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:24.5"
msgid "Kismiṃ vatthusminti?"
