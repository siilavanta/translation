msgctxt "pli-tv-pvr1.9:25.4"
msgid "Āḷavake bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:25.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:25.6"
msgid "Āḷavakā bhikkhū saññācikāya kuṭiyo kārāpesuṃ, tasmiṃ vatthusmiṃ."
