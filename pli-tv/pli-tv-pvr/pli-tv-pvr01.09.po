#. HTML: <section class="sutta" id="pli-tv-pvr1.9"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr1.9:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.9:2.1"
msgid "Bhikkhuvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.9:3.1"
msgid "Dutiyabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr1.9:4.1"
msgid "Katthapaññattivāra"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr1.9:5.1"
msgid "1. Pārājikakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1
msgctxt "pli-tv-pvr1.9:6.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena methunaṃ dhammaṃ paṭisevanapaccayā pārājikaṃ kattha paññattaṃ, kaṃ ārabbha, kismiṃ vatthusmiṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr1.9:6.3"
msgid "kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli5.49
msgctxt "pli-tv-pvr1.9:7.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena methunaṃ dhammaṃ paṭisevanapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.4"
msgid "Sudinnaṃ kalandaputtaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.6"
msgid "Sudinno kalandaputto purāṇadutiyikāya methunaṃ dhammaṃ paṭisevi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.8"
msgid "Ekā paññatti, dve anupaññattiyo."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.9"
msgid "Anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.10"
msgid "Sabbattha paññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.13"
msgid "Sādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.15"
msgid "Ubhatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.16"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.19"
msgid "Dutiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.23"
msgid "Pārājikāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr1.9:7.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.25"
msgid "Ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.26"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato … pe …"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.28"
msgid "kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:7.29"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc3
msgctxt "pli-tv-pvr1.9:8.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:8.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:8.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:8.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc4
msgctxt "pli-tv-pvr1.9:9.1"
msgid "…"
msgstr ""

msgctxt "pli-tv-pvr1.9:9.2"
msgid "pe …"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc5
msgctxt "pli-tv-pvr1.9:10.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:10.2"
msgid "vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:10.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:10.4"
msgid "piṭakaṃ tambapaṇṇiyāti."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc6
#. VAR: adinnaṃ → adinnāni (bj)
msgctxt "pli-tv-pvr1.9:11.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena adinnaṃ ādiyanapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:11.2"
msgid "Rājagahe paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:11.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:11.4"
msgid "Dhaniyaṃ kumbhakāraputtaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:11.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:11.6"
msgid "Dhaniyo kumbhakāraputto rañño dārūni adinnaṃ ādiyi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:11.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:11.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:11.9"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:11.10"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:11.11"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:11.12"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr1.9:12.1"
msgid "Sañcicca manussaviggahaṃ jīvitā voropanapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:12.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:12.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:12.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:12.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:12.6"
msgid "Sambahulā bhikkhū aññamaññaṃ jīvitā voropesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:12.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:12.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:12.9"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:12.10"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:12.11"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:12.12"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr1.9:13.1"
msgid "Asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapanapaccayā pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:13.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:13.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:13.4"
msgid "Vaggumudātīriye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:13.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:13.6"
msgid "Vaggumudātīriyā bhikkhū gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇaṃ bhāsiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:13.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:13.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:13.9"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:13.10"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:13.11"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:13.12"
msgid "pe …."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-pvr1.9:14.1"
msgid "Cattāro pārājikā niṭṭhitā."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr1.9:15.1"
msgid "2. Saṃghādisesakaṇḍādi"
msgstr ""

#. HTML: </h2><p>
#. REF: sc10
msgctxt "pli-tv-pvr1.9:16.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena upakkamitvā asuciṃ mocanapaccayā saṃghādiseso kattha paññatto, kaṃ ārabbha, kismiṃ vatthusmiṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr1.9:16.3"
msgid "kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr1.9:17.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena upakkamitvā asuciṃ mocanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.4"
msgid "Āyasmantaṃ seyyasakaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.6"
msgid "Āyasmā seyyasako upakkamitvā asuciṃ mocesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.8"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.9"
msgid "Anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.16"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.19"
msgid "Tatiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.23"
msgid "Saṃghādisesāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr1.9:17.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.25"
msgid "Ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.26"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato … pe …"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.28"
msgid "kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:17.29"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc12
msgctxt "pli-tv-pvr1.9:18.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:18.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:18.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:18.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc13
msgctxt "pli-tv-pvr1.9:19.1"
msgid "…"
msgstr ""

msgctxt "pli-tv-pvr1.9:19.2"
msgid "pe …"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc14
msgctxt "pli-tv-pvr1.9:20.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:20.2"
msgid "Vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:20.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.9:20.4"
msgid "Piṭakaṃ tambapaṇṇiyāti."
msgstr ""

msgctxt "pli-tv-pvr1.9:20.5"
msgid "(1)"
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc15
msgctxt "pli-tv-pvr1.9:21.1"
msgid "Mātugāmena saddhiṃ kāyasaṃsaggaṃ samāpajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:21.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:21.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:21.4"
msgid "Āyasmantaṃ udāyiṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:21.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:21.6"
msgid "Āyasmā udāyī mātugāmena saddhiṃ kāyasaṃsaggaṃ samāpajji, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:21.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:21.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:21.9"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato …"
msgstr ""

msgctxt "pli-tv-pvr1.9:21.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:21.11"
msgid "(2)"
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr1.9:22.1"
msgid "Mātugāmaṃ duṭṭhullāhi vācāhi obhāsanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:22.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:22.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:22.4"
msgid "Āyasmantaṃ udāyiṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:22.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:22.6"
msgid "Āyasmā udāyī mātugāmaṃ duṭṭhullāhi vācāhi obhāsi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:22.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:22.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:22.9"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:22.10"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:22.11"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:22.12"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:22.13"
msgid "(3)"
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr1.9:23.1"
msgid "Mātugāmassa santike attakāmapāricariyāya vaṇṇaṃ bhāsanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:23.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:23.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:23.4"
msgid "Āyasmantaṃ udāyiṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:23.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:23.6"
msgid "Āyasmā udāyī mātugāmassa santike attakāmapāricariyāya vaṇṇaṃ bhāsi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:23.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:23.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:23.9"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:23.10"
msgid "(4)"
msgstr ""

#. HTML: </p><p>
