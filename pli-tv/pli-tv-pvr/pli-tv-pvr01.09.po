msgctxt "pli-tv-pvr1.9:26.2"
msgid "Kosambiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:26.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:26.4"
msgid "Āyasmantaṃ channaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:26.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:26.6"
msgid "Āyasmā channo vihāravatthuṃ sodhento aññataraṃ cetiyarukkhaṃ chedāpesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:26.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:26.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ chahi samuṭṭhānehi samuṭṭhāti …"
