msgctxt "pli-tv-pvr1.9:26.9"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:26.10"
msgid "(7)"
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr1.9:27.1"
msgid "Bhikkhuṃ amūlakena pārājikena dhammena anuddhaṃsanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:27.2"
msgid "Rājagahe paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:27.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:27.4"
msgid "Mettiyabhūmajake bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:27.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:27.6"
msgid "Mettiyabhūmajakā bhikkhū āyasmantaṃ dabbaṃ mallaputtaṃ amūlakena pārājikena dhammena anuddhaṃsesuṃ, tasmiṃ vatthusmiṃ."
