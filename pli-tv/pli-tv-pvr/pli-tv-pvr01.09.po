msgctxt "pli-tv-pvr1.9:24.9"
msgid "siyā kāyato samuṭṭhāti, na vācato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.10"
msgid "siyā vācato samuṭṭhāti, na kāyato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.11"
msgid "siyā kāyato ca vācato ca samuṭṭhāti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.12"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.13"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.14"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:24.15"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:24.16"
msgid "(5)"
msgstr ""

#. HTML: </p><p>
