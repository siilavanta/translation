#. HTML: </p><p>
#. REF: sc23
msgctxt "pli-tv-pvr1.9:29.1"
msgid "Saṃghabhedakassa bhikkhuno yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:29.2"
msgid "Rājagahe paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:29.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:29.4"
msgid "Devadattaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:29.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:29.6"
msgid "Devadatto samaggassa saṃghassa bhedāya parakkami, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:29.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:29.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:29.9"
msgid "kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:29.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:29.11"
msgid "(10)"
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-pvr1.9:30.1"
msgid "Bhedakānuvattakānaṃ bhikkhūnaṃ yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:30.2"
msgid "Rājagahe paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:30.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:30.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:30.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:30.6"
msgid "Sambahulā bhikkhū devadattassa saṃghabhedāya parakkamantassa anuvattakā ahesuṃ vaggavādakā, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:30.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:30.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:30.9"
msgid "kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:30.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:30.11"
msgid "(11)"
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-pvr1.9:31.1"
msgid "Dubbacassa bhikkhuno yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:31.2"
msgid "Kosambiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:31.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:31.4"
msgid "Āyasmantaṃ channaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:31.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:31.6"
msgid "Āyasmā channo bhikkhūhi sahadhammikaṃ vuccamāno attānaṃ avacanīyaṃ akāsi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:31.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:31.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:31.9"
msgid "kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:31.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:31.11"
msgid "(12)"
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-pvr1.9:32.1"
msgid "Kuladūsakassa bhikkhuno yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:32.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.9:32.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:32.4"
msgid "Assajipunabbasuke bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:32.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:32.6"
msgid "Assajipunabbasukā bhikkhū saṃghena pabbājanīyakammakatā bhikkhū chandagāmitā dosagāmitā mohagāmitā bhayagāmitā pāpesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:32.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:32.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:32.9"
msgid "kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.9:32.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:32.11"
msgid "(13)"
msgstr ""

#. HTML: </p><p>
#. REF: sc27
msgctxt "pli-tv-pvr1.9:33.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karaṇapaccayā dukkaṭaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:33.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:33.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:33.4"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.9:33.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.9:33.6"
msgid "Chabbaggiyā bhikkhū udake uccārampi passāvampi kheḷampi akaṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.9:33.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.9:33.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.9:33.9"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato …"
msgstr ""

msgctxt "pli-tv-pvr1.9:33.10"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.9:33.11"
msgid "(14)"
msgstr ""

