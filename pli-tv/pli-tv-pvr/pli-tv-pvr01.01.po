msgid "Accekacīvaraṃ paṭiggahetvā cīvarakālasamayaṃ atikkāmentassa nissaggiyaṃ pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:122.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:122.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:122.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:122.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:122.6"
msgid "Sambahulā bhikkhū accekacīvaraṃ paṭiggahetvā cīvarakālasamayaṃ atikkāmesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:122.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:122.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti kathinake …"
msgstr ""

msgctxt "pli-tv-pvr1.1:122.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc112, pts-vp-pli5.13
msgctxt "pli-tv-pvr1.1:123.1"
msgid "Tiṇṇaṃ cīvarānaṃ aññataraṃ cīvaraṃ antaraghare nikkhipitvā atirekachārattaṃ vippavasantassa nissaggiyaṃ pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:123.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:123.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:123.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:123.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:123.6"
msgid "Sambahulā bhikkhū tiṇṇaṃ cīvarānaṃ aññataraṃ cīvaraṃ antaraghare nikkhipitvā atirekachārattaṃ vippavasiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:123.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:123.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti kathinake …"
msgstr ""

msgctxt "pli-tv-pvr1.1:123.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc113
msgctxt "pli-tv-pvr1.1:124.1"
msgid "Jānaṃ saṃghikaṃ lābhaṃ pariṇataṃ attano pariṇāmentassa nissaggiyaṃ pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:124.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:124.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:124.4"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:124.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:124.6"
msgid "Chabbaggiyā bhikkhū jānaṃ saṃghikaṃ lābhaṃ pariṇataṃ attano pariṇāmesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:124.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:124.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:124.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc114
msgctxt "pli-tv-pvr1.1:125.1"
msgid "Pattavaggo tatiyo."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:125.2"
msgid "Tiṃsa nissaggiyā pācittiyā niṭṭhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc115
msgctxt "pli-tv-pvr1.1:126.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><div class="var" title=" atirekañca pattañca,"><p>
#. REF: sc116
msgctxt "pli-tv-pvr1.1:127.1"
msgid "Dasekarattimāso ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:127.2"
msgid "dhovanañca paṭiggaho;"
msgstr ""

#. HTML: <br>
#. VAR: Aññātaṃ tañca → aññātakañca (bj, pts1, mr)
msgctxt "pli-tv-pvr1.1:127.3"
msgid "Aññātaṃ tañca uddissa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:127.4"
msgid "ubhinnaṃ dūtakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc117
msgctxt "pli-tv-pvr1.1:128.1"
msgid "Kosiyā suddhadvebhāgā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:128.2"
msgid "chabbassāni nisīdanaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:128.3"
msgid "Dve ca lomāni uggaṇhe,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:128.4"
msgid "ubho nānappakārakā."
msgstr ""

#. HTML: </p><p>
#. REF: sc118
msgctxt "pli-tv-pvr1.1:129.1"
msgid "Dve ca pattāni bhesajjaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:129.2"
msgid "Vassikā dānapañcamaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:129.3"
msgid "Sāmaṃ vāyāpanacceko,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:129.4"
msgid "Sāsaṅkaṃ saṃghikena cāti."
msgstr ""

#. HTML: </p></div></blockquote><h2>
msgctxt "pli-tv-pvr1.1:130.1"
msgid "5. 5 Pācittiyakaṇḍa"
msgstr ""

#. HTML: </h2><h3>
msgctxt "pli-tv-pvr1.1:131.1"
msgid "5.1. Musāvādavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc119
msgctxt "pli-tv-pvr1.1:132.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena sampajānamusāvāde pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:132.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:132.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:132.4"
msgid "Hatthakaṃ sakyaputtaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:132.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:132.6"
msgid "Āyasmā hatthako sakyaputto titthiyehi saddhiṃ sallapanto avajānitvā paṭijāni, paṭijānitvā avajāni, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:132.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:132.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:132.9"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:132.10"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:132.11"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:132.12"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc120
msgctxt "pli-tv-pvr1.1:133.1"
msgid "Omasavāde pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:133.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:133.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:133.4"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:133.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

#. VAR: bhaṇḍantā → bhaṇḍentā (bj, pts1, mr)
msgctxt "pli-tv-pvr1.1:133.6"
msgid "Chabbaggiyā bhikkhū pesalehi bhikkhūhi saddhiṃ bhaṇḍantā pesale bhikkhū omasiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:133.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:133.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:133.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc121
msgctxt "pli-tv-pvr1.1:134.1"
msgid "Bhikkhupesuññe pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:134.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:134.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:134.4"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:134.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:134.6"
msgid "Chabbaggiyā bhikkhū bhikkhūnaṃ bhaṇḍanajātānaṃ kalahajātānaṃ vivādāpannānaṃ pesuññaṃ upasaṃhariṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:134.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:134.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:134.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc122, pts-vp-pli5.14
msgctxt "pli-tv-pvr1.1:135.1"
msgid "Anupasampannaṃ padaso dhammaṃ vācentassa pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:135.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:135.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:135.4"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:135.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:135.6"
msgid "Chabbaggiyā bhikkhū upāsake padaso dhammaṃ vācesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:135.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:135.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:135.9"
msgid "siyā vācato samuṭṭhāti, na kāyato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:135.10"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato …"
msgstr ""

msgctxt "pli-tv-pvr1.1:135.11"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc123
msgctxt "pli-tv-pvr1.1:136.1"
msgid "Anupasampannena uttaridirattatirattaṃ sahaseyyaṃ kappentassa pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:136.2"
msgid "Āḷaviyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:136.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:136.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:136.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:136.6"
msgid "Sambahulā bhikkhū anupasampannena sahaseyyaṃ kappesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:136.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:136.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:136.9"
msgid "siyā kāyato samuṭṭhāti, na vācato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:136.10"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato …"
msgstr ""

msgctxt "pli-tv-pvr1.1:136.11"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc124
msgctxt "pli-tv-pvr1.1:137.1"
msgid "Mātugāmena sahaseyyaṃ kappentassa pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:137.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:137.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:137.4"
msgid "Āyasmantaṃ anuruddhaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:137.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:137.6"
msgid "Āyasmā anuruddho mātugāmena sahaseyyaṃ kappesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:137.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:137.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti eḷakalomake …"
msgstr ""

msgctxt "pli-tv-pvr1.1:137.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc125
msgctxt "pli-tv-pvr1.1:138.1"
msgid "Mātugāmassa uttarichappañcavācāhi dhammaṃ desentassa pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:138.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:138.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:138.4"
msgid "Āyasmantaṃ udāyiṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:138.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:138.6"
msgid "Āyasmā udāyī mātugāmassa dhammaṃ desesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:138.7"
msgid "Ekā paññatti, dve anupaññattiyo."
msgstr ""

msgctxt "pli-tv-pvr1.1:138.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti padasodhamme …"
msgstr ""

msgctxt "pli-tv-pvr1.1:138.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc126
msgctxt "pli-tv-pvr1.1:139.1"
msgid "Anupasampannassa uttarimanussadhammaṃ bhūtaṃ ārocentassa pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:139.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:139.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:139.4"
msgid "Vaggumudātīriye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:139.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:139.6"
msgid "Vaggumudātīriyā bhikkhū gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇaṃ bhāsiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:139.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:139.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:139.9"
msgid "siyā kāyato samuṭṭhāti, na vācato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:139.10"
msgid "siyā vācato samuṭṭhāti, na kāyato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:139.11"
msgid "siyā kāyato ca vācato ca samuṭṭhāti, na cittato …"
msgstr ""

msgctxt "pli-tv-pvr1.1:139.12"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc127
msgctxt "pli-tv-pvr1.1:140.1"
msgid "Bhikkhussa duṭṭhullaṃ āpattiṃ anupasampannassa ārocentassa pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:140.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:140.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:140.4"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:140.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:140.6"
msgid "Chabbaggiyā bhikkhū bhikkhussa duṭṭhullāpattiṃ anupasampannassa ārocesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:140.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:140.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:140.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc128
msgctxt "pli-tv-pvr1.1:141.1"
msgid "Pathaviṃ khaṇantassa pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:141.2"
msgid "Āḷaviyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:141.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:141.4"
msgid "Āḷavake bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:141.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:141.6"
msgid "Āḷavakā bhikkhū pathaviṃ khaṇiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:141.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:141.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:141.9"
msgid "pe …."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc129
msgctxt "pli-tv-pvr1.1:142.1"
msgid "Musāvādavaggo paṭhamo."
msgstr ""

#. HTML: </p><h3>
msgctxt "pli-tv-pvr1.1:143.1"
msgid "5.2. Bhūtagāmavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc130, pts-vp-pli5.15
msgctxt "pli-tv-pvr1.1:144.1"
msgid "Bhūtagāmapātabyatāya pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:144.2"
msgid "Āḷaviyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:144.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

