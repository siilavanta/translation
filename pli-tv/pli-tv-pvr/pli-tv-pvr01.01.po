msgstr ""

msgctxt "pli-tv-pvr1.1:61.6"
msgid "Assajipunabbasukā bhikkhū saṃghena pabbājanīyakammakatā bhikkhū chandagāmitā dosagāmitā mohagāmitā bhayagāmitā pāpesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:61.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:61.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:61.9"
msgid "kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:61.10"
msgid "pe …."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc56
msgctxt "pli-tv-pvr1.1:62.1"
msgid "Terasa saṃghādisesā niṭṭhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc57
msgctxt "pli-tv-pvr1.1:63.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc58
msgctxt "pli-tv-pvr1.1:64.1"
msgid "Vissaṭṭhi kāyasaṃsaggaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:64.2"
msgid "duṭṭhullaṃ attakāmañca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:64.3"
msgid "Sañcarittaṃ kuṭī ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:64.4"
msgid "vihāro ca amūlakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc59
msgctxt "pli-tv-pvr1.1:65.1"
msgid "Kiñcidesañca bhedo ca,"
msgstr ""

#. HTML: <br>
#. VAR: tasseva → tatheva (s1-3, pts1, mr)
msgctxt "pli-tv-pvr1.1:65.2"
msgid "tasseva anuvattakā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:65.3"
msgid "Dubbacaṃ kuladūsañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:65.4"
msgid "saṃghādisesā terasāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr1.1:66.1"
msgid "3. Aniyatakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc60
msgctxt "pli-tv-pvr1.1:67.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena paṭhamo aniyato kattha paññatto?"
msgstr ""

msgctxt "pli-tv-pvr1.1:67.2"
msgid "Kaṃ ārabbha?"
msgstr ""

msgctxt "pli-tv-pvr1.1:67.3"
msgid "Kismiṃ vatthusmiṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:67.4"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññatti, sabbatthapaññatti padesapaññatti, sādhāraṇapaññatti asādhāraṇapaññatti, ekatopaññatti ubhatopaññatti, pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannaṃ, katamena uddesena uddesaṃ āgacchati, catunnaṃ vipattīnaṃ katamā vipatti, sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandho, channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhāti, catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ, sattannaṃ samathānaṃ katihi samathehi sammati, ko tattha vinayo, ko tattha abhivinayo, kiṃ tattha pātimokkhaṃ, kiṃ tattha adhipātimokkhaṃ, kā vipatti, kā sampatti, kā paṭipatti, kati atthavase paṭicca bhagavatā paṭhamo aniyato paññatto, ke sikkhanti, ke sikkhitasikkhā, kattha ṭhitaṃ, ke dhārenti, kassa vacanaṃ kenābhatanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc61, pts-vp-pli5.8
msgctxt "pli-tv-pvr1.1:68.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena paṭhamo aniyato kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.4"
msgid "Āyasmantaṃ udāyiṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.6"
msgid "Āyasmā udāyī mātugāmena saddhiṃ eko ekāya raho paṭicchanne āsane alaṃkammaniye nisajjaṃ kappesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.9"
msgid "Anupaññatti anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.16"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.19"
msgid "Catutthena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.21"
msgid "Siyā sīlavipatti siyā ācāravipatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.23"
msgid "Siyā pārājikāpattikkhandho siyā saṃghādisesāpattikkhandho siyā pācittiyāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.25"
msgid "Ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.26"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.27"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.28"
msgid "Āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.29"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.30"
msgid "Tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.31"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.32"
msgid "Ko tattha vinayo, ko tattha abhivinayoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.33"
msgid "Paññatti vinayo, vibhatti abhivinayo."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.34"
msgid "Kiṃ tattha pātimokkhaṃ, kiṃ tattha adhipātimokkhanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.35"
msgid "Paññatti pātimokkhaṃ, vibhatti adhipātimokkhaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.36"
msgid "Kā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.37"
msgid "Asaṃvaro vipatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.38"
msgid "Kā sampattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.39"
msgid "Saṃvaro sampatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.40"
msgid "Kā paṭipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.41"
msgid "Na evarūpaṃ karissāmīti yāvajīvaṃ āpāṇakoṭikaṃ samādāya sikkhati sikkhāpadesu."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.42"
msgid "Kati atthavase paṭicca bhagavatā paṭhamo aniyato paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.43"
msgid "Dasa atthavase paṭicca bhagavatā paṭhamo aniyato paññatto—"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.44"
msgid "saṃghasuṭṭhutāya, saṃghaphāsutāya, dummaṅkūnaṃ puggalānaṃ niggahāya, pesalānaṃ bhikkhūnaṃ phāsuvihārāya, diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya, samparāyikānaṃ āsavānaṃ paṭighātāya, appasannānaṃ pasādāya, pasannānaṃ bhiyyobhāvāya, saddhammaṭṭhitiyā, vinayānuggahāya."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.45"
msgid "Ke sikkhantīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.46"
msgid "Sekkhā ca puthujjanakalyāṇakā ca sikkhanti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.47"
msgid "Ke sikkhitasikkhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.48"
msgid "Arahanto sikkhitasikkhā."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.49"
msgid "Kattha ṭhitanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.50"
msgid "Sikkhākāmesu ṭhitaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.51"
msgid "Ke dhārentīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.52"
msgid "Yesaṃ vattati te dhārenti."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.53"
msgid "Kassa vacananti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.54"
msgid "Bhagavato vacanaṃ arahato sammāsambuddhassa."
msgstr ""

msgctxt "pli-tv-pvr1.1:68.55"
msgid "Kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:68.56"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc62
msgctxt "pli-tv-pvr1.1:69.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:69.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:69.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:69.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p><p>
#. REF: sc63
msgctxt "pli-tv-pvr1.1:70.1"
msgid "Tato mahindo iṭṭiyo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:70.2"
msgid "uttiyo sambalo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:70.3"
msgid "Bhaddanāmo ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc64
msgctxt "pli-tv-pvr1.1:71.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:71.2"
msgid "jambudīpā idhāgatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:71.3"
msgid "Vinayaṃ te vācayiṃsu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:71.4"
msgid "piṭakaṃ tambapaṇṇiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc65
msgctxt "pli-tv-pvr1.1:72.1"
msgid "Nikāye pañca vācesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:72.2"
msgid "satta ceva pakaraṇe;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:72.3"
msgid "Tato ariṭṭho medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:72.4"
msgid "tissadatto ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc66
msgctxt "pli-tv-pvr1.1:73.1"
msgid "Visārado kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:73.2"
msgid "thero ca dīghanāmako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:73.3"
msgid "Dīghasumano ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc67
msgctxt "pli-tv-pvr1.1:74.1"
msgid "Punadeva kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:74.2"
msgid "Nāgatthero ca buddharakkhito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:74.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:74.4"
msgid "Devatthero ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc68
msgctxt "pli-tv-pvr1.1:75.1"
msgid "Punadeva sumano medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:75.2"
msgid "Vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:75.3"
msgid "Bahussuto cūḷanāgo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:75.4"
msgid "Gajova duppadhaṃsiyo."
msgstr ""

#. HTML: </p><p>
#. REF: sc69
msgctxt "pli-tv-pvr1.1:76.1"
msgid "Dhammapālitanāmo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:76.2"
msgid "rohaṇe sādhupūjito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:76.3"
msgid "Tassa sisso mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:76.4"
msgid "khemanāmo tipeṭako."
msgstr ""

#. HTML: </p><p>
#. REF: sc70
msgctxt "pli-tv-pvr1.1:77.1"
msgid "Dīpe tārakarājāva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:77.2"
msgid "paññāya atirocatha;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:77.3"
msgid "Upatisso ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:77.4"
msgid "phussadevo mahākathī."
msgstr ""

#. HTML: </p><p>
#. REF: sc71
msgctxt "pli-tv-pvr1.1:78.1"
msgid "Punadeva sumano medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:78.2"
msgid "Pupphanāmo bahussuto;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:78.3"
msgid "Mahākathī mahāsivo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:78.4"
msgid "Piṭake sabbattha kovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc72
msgctxt "pli-tv-pvr1.1:79.1"
msgid "Punadeva upāli medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:79.2"
msgid "Vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:79.3"
msgid "Mahānāgo mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:79.4"
msgid "Saddhammavaṃsakovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc73
msgctxt "pli-tv-pvr1.1:80.1"
msgid "Punadeva abhayo medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:80.2"
msgid "Piṭake sabbattha kovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:80.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:80.4"
msgid "Vinaye ca visārado."
msgstr ""

#. HTML: </p><p>
#. REF: sc74
msgctxt "pli-tv-pvr1.1:81.1"
msgid "Tassa sisso mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:81.2"
msgid "pupphanāmo bahussuto;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:81.3"
msgid "Sāsanaṃ anurakkhanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:81.4"
msgid "jambudīpe patiṭṭhito."
msgstr ""

#. HTML: </p><p>
#. REF: sc75
msgctxt "pli-tv-pvr1.1:82.1"
msgid "Cūḷābhayo ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:82.2"
msgid "vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:82.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:82.4"
msgid "saddhammavaṃsakovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc76
msgctxt "pli-tv-pvr1.1:83.1"
msgid "Cūḷadevo ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:83.2"
msgid "vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:83.3"
msgid "Sivatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:83.4"
msgid "vinaye sabbattha kovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc77
msgctxt "pli-tv-pvr1.1:84.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:84.2"
msgid "vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:84.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:84.4"
msgid "piṭakaṃ tambapaṇṇiyāti."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc78
msgctxt "pli-tv-pvr1.1:85.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena dutiyo aniyato kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.4"
msgid "Āyasmantaṃ udāyiṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.6"
msgid "Āyasmā udāyī mātugāmena saddhiṃ eko ekāya raho nisajjaṃ kappesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.7"
msgid "Atthi tattha paññatti, anupaññatti anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.8"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.9"
msgid "Anupaññatti anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.12"
msgid "Sādhāraṇapaññatti asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.14"
msgid "Ekatopaññatti ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.16"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.19"
msgid "Catutthena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.21"
msgid "Siyā sīlavipatti, siyā ācāravipatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.23"
msgid "Siyā saṃghādisesāpattikkhandho, siyā pācittiyāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.25"
msgid "Tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.26"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.27"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.28"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.29"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.30"
msgid "Āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:85.31"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.32"
msgid "Tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.33"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca …"
msgstr ""

msgctxt "pli-tv-pvr1.1:85.34"
msgid "pe …."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc79
msgctxt "pli-tv-pvr1.1:86.1"
msgid "Dve aniyatā niṭṭhitā."
msgstr ""

