#. REF: sc81
msgctxt "pli-tv-pvr1.1:88.1"
msgid "Alaṅkammaniyañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:88.2"
msgid "tatheva ca na heva kho;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:88.3"
msgid "Aniyatā supaññattā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:88.4"
msgid "buddhaseṭṭhena tādināti."
msgstr ""

