msgctxt "pli-tv-pvr1.1:181.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:181.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:181.4"
msgid "Āyasmantaṃ upanandaṃ sakyaputtaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:181.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:181.6"
msgid "Āyasmā upanando sakyaputto bhikkhuṃ “ehāvuso, gāmaṃ piṇḍāya pavisissāmā”ti, tassa adāpetvā uyyojesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:181.7"
msgid "Ekā paññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:181.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ tīhi samuṭṭhānehi samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:181.9"
