msgctxt "pli-tv-pvr1.1:89.1"
msgid "4. 4 Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </h2><h3>
#. VAR: 4.1 Kathinavagga → cīvaravagga (^pi-tv-bu-vb-np1#2 [Nissaggiyā Pācittiyā 1: Kathinasikkhāpada])
msgctxt "pli-tv-pvr1.1:90.1"
msgid "4.1 Kathinavagga"
msgstr ""

#. HTML: </h3><p>
#. REF: sc82
#. VAR: atirekacīvaraṃ → atirekacīvaraṃ dasāhātikkantaṃ (bj)
msgctxt "pli-tv-pvr1.1:91.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena atirekacīvaraṃ dasāhaṃ atikkāmentassa nissaggiyaṃ pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:91.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:91.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:91.4"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:91.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:91.6"
msgid "Chabbaggiyā bhikkhū atirekacīvaraṃ dhāresuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:91.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:91.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:91.9"
msgid "siyā kāyato ca vācato ca samuṭṭhāti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:91.10"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:91.11"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc83
msgctxt "pli-tv-pvr1.1:92.1"
msgid "Ekarattaṃ ticīvarena vippavasantassa nissaggiyaṃ pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:92.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:92.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:92.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:92.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:92.6"
msgid "Sambahulā bhikkhū bhikkhūnaṃ hatthe cīvaraṃ nikkhipitvā santaruttarena janapadacārikaṃ pakkamiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:92.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:92.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:92.9"
msgid "siyā kāyato ca vācato ca samuṭṭhāti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:92.10"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:92.11"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc84
msgctxt "pli-tv-pvr1.1:93.1"
msgid "Akālacīvaraṃ paṭiggahetvā māsaṃ atikkāmentassa nissaggiyaṃ pācittiyaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:93.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:93.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:93.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:93.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:93.6"
msgid "Sambahulā bhikkhū akālacīvaraṃ paṭiggahetvā māsaṃ atikkāmesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:93.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:93.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ dvīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:93.9"
msgid "siyā kāyato ca vācato ca samuṭṭhāti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:93.10"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:93.11"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
