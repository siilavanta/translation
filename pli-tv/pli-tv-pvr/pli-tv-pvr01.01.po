msgstr ""

msgctxt "pli-tv-pvr1.1:348.7"
msgid "(14)"
msgstr ""

#. HTML: </p><p>
#. REF: sc319
msgctxt "pli-tv-pvr1.1:349.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karontassa dukkaṭaṃ … pe …"
msgstr ""

msgctxt "pli-tv-pvr1.1:349.3"
msgid "kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:349.4"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:349.5"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:349.6"
msgid "Chabbaggiye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:349.7"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:349.8"
msgid "Chabbaggiyā bhikkhū udake uccārampi passāvampi kheḷampi akaṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:349.9"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:349.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:349.11"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato …"
msgstr ""

msgctxt "pli-tv-pvr1.1:349.12"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.1:349.13"
msgid "(15)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc320
#. VAR: Pādukavaggo → pādukāvaggo (bj)
msgctxt "pli-tv-pvr1.1:350.1"
msgid "Pādukavaggo sattamo."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:350.2"
msgid "Pañcasattati sekhiyā niṭṭhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc321
msgctxt "pli-tv-pvr1.1:351.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc322
msgctxt "pli-tv-pvr1.1:352.1"
msgid "Parimaṇḍalaṃ paṭicchannaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:352.2"
msgid "susaṃvutokkhittacakkhu;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:352.3"
msgid "Ukkhittojjagghikā saddo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:352.4"
msgid "tayo ceva pacālanā."
msgstr ""

#. HTML: </p><p>
#. REF: sc323
msgctxt "pli-tv-pvr1.1:353.1"
msgid "Khambhaṃ oguṇṭhito cevuk-"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:353.2"
msgid "kuṭipallatthikāya ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:353.3"
msgid "Sakkaccaṃ pattasaññī ca,"
msgstr ""

#. HTML: <br>
#. VAR: samatittikaṃ → samatitthikaṃ (pts1, mr)
msgctxt "pli-tv-pvr1.1:353.4"
msgid "samasūpaṃ samatittikaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc324
msgctxt "pli-tv-pvr1.1:354.1"
msgid "Sakkaccaṃ pattasaññī ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:354.2"
msgid "sapadānaṃ samasūpakaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:354.3"
msgid "Thūpakato paṭicchannaṃ,"
msgstr ""

#. HTML: <br>
#. VAR: viññattujjhānasaññinā → ... saññitā (bj)
msgctxt "pli-tv-pvr1.1:354.4"
msgid "viññattujjhānasaññinā."
msgstr ""

#. HTML: </p><p>
#. REF: sc325
msgctxt "pli-tv-pvr1.1:355.1"
msgid "Na mahantaṃ maṇḍalaṃ dvāraṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:355.2"
msgid "Sabbaṃ hatthaṃ na byāhare;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:355.3"
msgid "Ukkhepo chedanā gaṇḍo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:355.4"
msgid "Dhunaṃ sitthāvakārakaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc326, pts-vp-pli5.33
msgctxt "pli-tv-pvr1.1:356.1"
msgid "Jivhānicchārakañceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:356.2"
msgid "capucapu surusuru;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:356.3"
msgid "Hattho patto ca oṭṭho ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:356.4"
msgid "sāmisaṃ sitthakena ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc327
msgctxt "pli-tv-pvr1.1:357.1"
msgid "Chattapāṇissa saddhammaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:357.2"
msgid "na desenti tathāgatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:357.3"
msgid "Evameva daṇḍapāṇissa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:357.4"
msgid "satthaāvudhapāṇinaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc328
msgctxt "pli-tv-pvr1.1:358.1"
msgid "Pādukā upāhanā ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:358.2"
msgid "yānaseyyāgatassa ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:358.3"
msgid "Pallatthikā nisinnassa,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:358.4"
msgid "veṭhitoguṇṭhitassa ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc329
msgctxt "pli-tv-pvr1.1:359.1"
msgid "Chamā nīcāsane ṭhāne,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:359.2"
msgid "pacchato uppathena ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:359.3"
msgid "Ṭhitakena na kātabbaṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:359.4"
msgid "harite udakamhi cāti."
msgstr ""

#. HTML: </p></blockquote><p class="uddana-intro">
#. REF: sc330
msgctxt "pli-tv-pvr1.1:360.1"
msgid "Tesaṃ vaggānamuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc331
msgctxt "pli-tv-pvr1.1:361.1"
msgid "Parimaṇḍalaujjagghi,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:361.2"
msgid "khambhaṃ piṇḍaṃ tatheva ca;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:361.3"
msgid "Kabaḷā surusuru ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:361.4"
msgid "pādukena ca sattamāti."
msgstr ""

#. HTML: </p></blockquote><p class="endsutta">
#. REF: sc332
msgctxt "pli-tv-pvr1.1:362.1"
msgid "Mahāvibhaṅge katthapaññattivāro niṭṭhito."
msgstr ""

