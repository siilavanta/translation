#. HTML: <section class="sutta" id="pli-tv-pvr1.1"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr1.1:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.1:2.1"
msgid "Bhikkhuvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.1:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr1.1:4.1"
msgid "Katthapaññattivāra"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr1.1:5.1"
msgid "1. Pārājikakaṇḍa"
msgstr ""

#. HTML: </h2><p class="namo">
#. REF: sc1
msgctxt "pli-tv-pvr1.1:6.1"
msgid "Namo tassa Bhagavato Arahato Sammāsambuddhassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli5.1
msgctxt "pli-tv-pvr1.1:7.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena paṭhamaṃ pārājikaṃ kattha paññattaṃ, kaṃ ārabbha, kismiṃ vatthusmiṃ?"
msgstr ""

#. VAR: anupaññatti → anuppaññatti (s1-3)
msgctxt "pli-tv-pvr1.1:7.2"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.3"
msgid "Sabbatthapaññatti, padesapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.4"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.5"
msgid "Ekatopaññatti, ubhatopaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.6"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.7"
msgid "Katamena uddesena uddesaṃ āgacchati?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.8"
msgid "Catunnaṃ vipattīnaṃ katamā vipatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.9"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandho?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.11"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.12"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.13"
msgid "Ko tattha vinayo, ko tattha abhivinayo?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.14"
msgid "Kiṃ tattha pātimokkhaṃ, kiṃ tattha adhipātimokkhaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.15"
msgid "Kā vipatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.16"
msgid "Kā sampatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.17"
msgid "Kā paṭipatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.18"
msgid "Kati atthavase paṭicca bhagavatā paṭhamaṃ pārājikaṃ paññattaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.19"
msgid "Ke sikkhanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.20"
msgid "Ke sikkhitasikkhā?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.21"
msgid "Kattha ṭhitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.22"
msgid "Ke dhārenti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.23"
msgid "Kassa vacanaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:7.24"
msgid "Kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc3, pts-vp-pli5.2
msgctxt "pli-tv-pvr1.1:8.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena paṭhamaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.4"
msgid "Sudinnaṃ kalandaputtaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.6"
msgid "Sudinno kalandaputto purāṇadutiyikāya methunaṃ dhammaṃ paṭisevi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.8"
msgid "Ekā paññatti, dve anupaññattiyo."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.9"
msgid "Anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.10"
msgid "Sabbatthapaññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.13"
msgid "Sādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.15"
msgid "Ubhatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.16"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.17"
msgid "Nidānogadhaṃ, nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.19"
msgid "Dutiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.23"
msgid "Pārājikāpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.25"
msgid "Ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.26"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.27"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.28"
msgid "Āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.29"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.30"
msgid "Dvīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.31"
msgid "sammukhāvinayena ca paṭiññātakaraṇena ca."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.32"
msgid "Ko tattha vinayo, ko tattha abhivinayoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.33"
msgid "Paññatti vinayo, vibhatti abhivinayo."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.34"
msgid "Kiṃ tattha pātimokkhaṃ, kiṃ tattha adhipātimokkhanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.35"
msgid "Paññatti pātimokkhaṃ, vibhatti adhipātimokkhaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.36"
msgid "Kā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.37"
msgid "Asaṃvaro vipatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.38"
msgid "Kā sampattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.39"
msgid "Saṃvaro sampatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.40"
msgid "Kā paṭipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.41"
msgid "“Na evarūpaṃ karissāmī”ti yāvajīvaṃ āpāṇakoṭikaṃ samādāya sikkhati sikkhāpadesu."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.42"
msgid "Kati atthavase paṭicca bhagavatā paṭhamaṃ pārājikaṃ paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.43"
msgid "Dasa atthavase paṭicca bhagavatā paṭhamaṃ pārājikaṃ paññattaṃ—"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.44"
msgid "saṃghasuṭṭhutāya, saṃghaphāsutāya, dummaṅkūnaṃ puggalānaṃ niggahāya, pesalānaṃ bhikkhūnaṃ phāsuvihārāya, diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya, samparāyikānaṃ āsavānaṃ paṭighātāya, appasannānaṃ pasādāya, pasannānaṃ bhiyyobhāvāya, saddhammaṭṭhitiyā, vinayānuggahāya."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.45"
msgid "Ke sikkhantīti?"
msgstr ""

#. VAR: Sekkhā → sekhā (bj, pts1)
msgctxt "pli-tv-pvr1.1:8.46"
msgid "Sekkhā ca puthujjanakalyāṇakā ca sikkhanti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.47"
msgid "Ke sikkhitasikkhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.48"
msgid "Arahanto sikkhitasikkhā."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.49"
msgid "Kattha ṭhitanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.50"
msgid "Sikkhākāmesu ṭhitaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.51"
msgid "Ke dhārentīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.52"
msgid "Yesaṃ vattati te dhārenti."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.53"
msgid "Kassa vacananti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.54"
msgid "Bhagavato vacanaṃ arahato sammāsambuddhassa."
msgstr ""

msgctxt "pli-tv-pvr1.1:8.55"
msgid "Kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:8.56"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc4
msgctxt "pli-tv-pvr1.1:9.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:9.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
#. VAR: Moggaliputtena → moggalīputtena (s1-3)
msgctxt "pli-tv-pvr1.1:9.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
#. VAR: jambusirivhaye → jambusarivhaye (sārattha)
msgctxt "pli-tv-pvr1.1:9.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p><p>
#. REF: sc5, pts-vp-pli5.3
#. VAR: iṭṭiyo → iṭṭhiyo (bj)
msgctxt "pli-tv-pvr1.1:10.1"
msgid "Tato mahindo iṭṭiyo,"
msgstr ""

#. HTML: <br>
#. VAR: sambalo tathā → uttiyo ceva sambalo (bj, s1-3)
msgctxt "pli-tv-pvr1.1:10.2"
msgid "uttiyo sambalo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:10.3"
msgid "Bhaddanāmo ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr1.1:11.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:11.2"
msgid "jambudīpā idhāgatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:11.3"
msgid "Vinayaṃ te vācayiṃsu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:11.4"
msgid "piṭakaṃ tambapaṇṇiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-pvr1.1:12.1"
msgid "Nikāye pañca vācesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:12.2"
msgid "satta ceva pakaraṇe;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:12.3"
msgid "Tato ariṭṭho medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:12.4"
msgid "tissadatto ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-pvr1.1:13.1"
msgid "Visārado kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:13.2"
msgid "thero ca dīghanāmako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:13.3"
msgid "Dīghasumano ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
#. VAR: Punadeva → punareva (s1-3, pts1)
msgctxt "pli-tv-pvr1.1:14.1"
msgid "Punadeva kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:14.2"
msgid "Nāgatthero ca buddharakkhito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:14.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:14.4"
msgid "Devatthero ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr1.1:15.1"
msgid "Punadeva sumano medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:15.2"
msgid "Vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:15.3"
msgid "Bahussuto cūḷanāgo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:15.4"
msgid "Gajova duppadhaṃsiyo."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr1.1:16.1"
msgid "Dhammapālitanāmo ca,"
msgstr ""

#. HTML: <br>
#. VAR: rohaṇe → rohaṇo (bj)
msgctxt "pli-tv-pvr1.1:16.2"
msgid "rohaṇe sādhupūjito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:16.3"
msgid "Tassa sisso mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:16.4"
msgid "khemanāmo tipeṭako."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-pvr1.1:17.1"
msgid "Dīpe tārakarājāva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:17.2"
msgid "paññāya atirocatha;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:17.3"
msgid "Upatisso ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:17.4"
msgid "phussadevo mahākathī."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-pvr1.1:18.1"
msgid "Punadeva sumano medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:18.2"
msgid "Pupphanāmo bahussuto;"
msgstr ""

#. HTML: <br>
#. VAR: mahāsivo → mahāsīvo (bj, s1-3, pts1)
msgctxt "pli-tv-pvr1.1:18.3"
msgid "Mahākathī mahāsivo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:18.4"
msgid "Piṭake sabbattha kovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr1.1:19.1"
msgid "Punadeva upāli medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:19.2"
msgid "Vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:19.3"
msgid "Mahānāgo mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:19.4"
msgid "Saddhammavaṃsakovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr1.1:20.1"
msgid "Punadeva abhayo medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:20.2"
msgid "Piṭake sabbattha kovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:20.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:20.4"
msgid "Vinaye ca visārado."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr1.1:21.1"
msgid "Tassa sisso mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:21.2"
msgid "pupphanāmo bahussuto;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:21.3"
msgid "Sāsanaṃ anurakkhanto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:21.4"
msgid "jambudīpe patiṭṭhito."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr1.1:22.1"
msgid "Cūḷābhayo ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:22.2"
msgid "vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:22.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:22.4"
msgid "saddhammavaṃsakovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
#. VAR: Cūḷadevo → phussadevo (bj) | cūḷādevo (s1-3) | cūladevo (pts1)
msgctxt "pli-tv-pvr1.1:23.1"
msgid "Cūḷadevo ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:23.2"
msgid "vinaye ca visārado;"
msgstr ""

#. HTML: <br>
#. VAR: Sivatthero → sīvatthero (bj, s1-3, pts1)
msgctxt "pli-tv-pvr1.1:23.3"
msgid "Sivatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:23.4"
msgid "vinaye sabbattha kovido."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-pvr1.1:24.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:24.2"
msgid "vinayaññū maggakovidā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:24.3"
msgid "Vinayaṃ dīpe pakāsesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:24.4"
msgid "piṭakaṃ tambapaṇṇiyāti."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc20, pts-vp-pli5.4
msgctxt "pli-tv-pvr1.1:25.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena dutiyaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:25.2"
msgid "Rājagahe paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:25.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:25.4"
msgid "Dhaniyaṃ kumbhakāraputtaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:25.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

#. VAR: adinnaṃ → adinnāni (bj, s1-3)
msgctxt "pli-tv-pvr1.1:25.6"
msgid "Dhaniyo kumbhakāraputto rañño dārūni adinnaṃ ādiyi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:25.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:25.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:25.9"
msgid "Tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:25.10"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:25.11"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:25.12"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:25.13"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc21
msgctxt "pli-tv-pvr1.1:26.1"
msgid "Tatiyaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:26.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.4"
msgid "Sambahule bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:26.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.6"
msgid "Sambahulā bhikkhū aññamaññaṃ jīvitā voropesuṃ, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:26.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:26.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.9"
msgid "Tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.10"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.11"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.12"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:26.13"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc22
msgctxt "pli-tv-pvr1.1:27.1"
msgid "Catutthaṃ pārājikaṃ kattha paññattanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.2"
msgid "Vesāliyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:27.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.4"
msgid "Vaggumudātīriye bhikkhū ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:27.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.6"
msgid "Vaggumudātīriyā bhikkhū gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇaṃ bhāsiṃsu, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:27.7"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:27.8"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.9"
msgid "Tīhi samuṭṭhānehi samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.10"
msgid "siyā kāyato ca cittato ca samuṭṭhāti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.11"
msgid "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.12"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …"
msgstr ""

msgctxt "pli-tv-pvr1.1:27.13"
msgid "pe …."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc23
msgctxt "pli-tv-pvr1.1:28.1"
msgid "Cattāro pārājikā niṭṭhitā."
msgstr ""

#. HTML: </p><p class="uddana-intro">
#. REF: sc24
msgctxt "pli-tv-pvr1.1:29.1"
msgid "Tassuddānaṃ"
msgstr ""

#. HTML: </p><blockquote class="uddanagatha"><p>
#. REF: sc25
msgctxt "pli-tv-pvr1.1:30.1"
msgid "Methunādinnādānañca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:30.2"
msgid "manussaviggahuttari;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:30.3"
msgid "Pārājikāni cattāri,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:30.4"
msgid "chejjavatthū asaṃsayāti."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr1.1:31.1"
msgid "2. Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </h2><p>
#. REF: sc26
msgctxt "pli-tv-pvr1.1:32.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena upakkamitvā asuciṃ mocentassa saṃghādiseso kattha paññatto?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.2"
msgid "Kaṃ ārabbha?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.3"
msgid "Kismiṃ vatthusmiṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.4"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.5"
msgid "Sabbatthapaññatti, padesapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.6"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.7"
msgid "Ekatopaññatti, ubhatopaññatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.8"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.9"
msgid "Katamena uddesena uddesaṃ āgacchati?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.10"
msgid "Catunnaṃ vipattīnaṃ katamā vipatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.11"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandho?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.12"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.13"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.14"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.15"
msgid "Ko tattha vinayo, ko tattha abhivinayo?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.16"
msgid "Kiṃ tattha pātimokkhaṃ, kiṃ tattha adhipātimokkhaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.17"
msgid "Kā vipatti, kā sampatti, kā paṭipatti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.18"
msgid "Kati atthavase paṭicca bhagavatā upakkamitvā asuciṃ mocentassa saṃghādiseso paññatto?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.19"
msgid "Ke sikkhanti, ke sikkhitasikkhā?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.20"
msgid "Kattha ṭhitaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.21"
msgid "Ke dhārenti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.22"
msgid "Kassa vacanaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.1:32.23"
msgid "Kenābhatanti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc27, pts-vp-pli5.5
msgctxt "pli-tv-pvr1.1:33.1"
msgid "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena upakkamitvā asuciṃ mocentassa saṃghādiseso kattha paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.2"
msgid "Sāvatthiyaṃ paññatto."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.4"
msgid "Āyasmantaṃ seyyasakaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.5"
msgid "Kismiṃ vatthusminti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.6"
msgid "Āyasmā seyyasako hatthena upakkamitvā asuciṃ mocesi, tasmiṃ vatthusmiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.7"
msgid "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.8"
msgid "Ekā paññatti, ekā anupaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.9"
msgid "Anuppannapaññatti tasmiṃ natthi."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.10"
msgid "Sabbattha paññatti, padesapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.11"
msgid "Sabbatthapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.12"
msgid "Sādhāraṇapaññatti, asādhāraṇapaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.13"
msgid "Asādhāraṇapaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.14"
msgid "Ekatopaññatti, ubhatopaññattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.15"
msgid "Ekatopaññatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.16"
msgid "Pañcannaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.17"
msgid "Nidānogadhaṃ nidānapariyāpannaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.18"
msgid "Katamena uddesena uddesaṃ āgacchatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.19"
msgid "Tatiyena uddesena uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.20"
msgid "Catunnaṃ vipattīnaṃ katamā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.21"
msgid "Sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.22"
msgid "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.23"
msgid "Saṃghādiseso āpattikkhandho."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.24"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.25"
msgid "Ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.26"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.27"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.28"
msgid "Āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.29"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammatīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.30"
msgid "Dvīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.31"
msgid "sammukhāvinayena ca paṭiññātakaraṇena ca."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.32"
msgid "Ko tattha vinayo, ko tattha abhivinayoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.33"
msgid "Paññatti vinayo, vibhatti abhivinayo."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.34"
msgid "Kiṃ tattha pātimokkhaṃ, kiṃ tattha adhipātimokkhanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.35"
msgid "Paññatti pātimokkhaṃ, vibhatti adhipātimokkhaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.36"
msgid "Kā vipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.37"
msgid "Asaṃvaro vipatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.38"
msgid "Kā sampattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.39"
msgid "Saṃvaro sampatti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.40"
msgid "Kā paṭipattīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.41"
msgid "Na evarūpaṃ karissāmīti yāvajīvaṃ āpāṇakoṭikaṃ samādāya sikkhati sikkhāpadesu."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.42"
msgid "Kati atthavase paṭicca bhagavatā upakkamitvā asuciṃ mocentassa saṃghādiseso paññattoti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.43"
msgid "Dasa atthavase paṭicca bhagavatā upakkamitvā asuciṃ mocentassa saṃghādiseso paññatto—"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.44"
msgid "saṃghasuṭṭhutāya, saṃghaphāsutāya, dummaṅkūnaṃ puggalānaṃ niggahāya, pesalānaṃ bhikkhūnaṃ phāsuvihārāya, diṭṭhadhammikānaṃ āsavānaṃ saṃvarāya, samparāyikānaṃ āsavānaṃ paṭighātāya, appasannānaṃ pasādāya, pasannānaṃ bhiyyobhāvāya, saddhammaṭṭhitiyā, vinayānuggahāya."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.45"
msgid "Ke sikkhantīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.46"
msgid "Sekkhā ca puthujjanakalyāṇakā ca sikkhanti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.47"
msgid "Ke sikkhitasikkhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.48"
msgid "Arahanto sikkhitasikkhā."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.49"
msgid "Kattha ṭhitanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.50"
msgid "Sikkhākāmesu ṭhitaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.51"
msgid "Ke dhārentīti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.52"
msgid "Yesaṃ vattati, te dhārenti."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.53"
msgid "Kassa vacananti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.54"
msgid "Bhagavato vacanaṃ arahato sammāsambuddhassa."
msgstr ""

msgctxt "pli-tv-pvr1.1:33.55"
msgid "Kenābhatanti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:33.56"
msgid "Paramparābhataṃ—"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc28
msgctxt "pli-tv-pvr1.1:34.1"
msgid "Upāli dāsako ceva,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:34.2"
msgid "soṇako siggavo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:34.3"
msgid "Moggaliputtena pañcamā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:34.4"
msgid "ete jambusirivhaye."
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-pvr1.1:35.1"
msgid "Tato mahindo iṭṭiyo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:35.2"
msgid "uttiyo sambalo tathā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:35.3"
msgid "Bhaddanāmo ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc30
msgctxt "pli-tv-pvr1.1:36.1"
msgid "Ete nāgā mahāpaññā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:36.2"
msgid "jambudīpā idhāgatā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:36.3"
msgid "Vinayaṃ te vācayiṃsu,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:36.4"
msgid "piṭakaṃ tambapaṇṇiyā."
msgstr ""

#. HTML: </p><p>
#. REF: sc31
msgctxt "pli-tv-pvr1.1:37.1"
msgid "Nikāye pañca vācesuṃ,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:37.2"
msgid "satta ceva pakaraṇe;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:37.3"
msgid "Tato ariṭṭho medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:37.4"
msgid "tissadatto ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc32
msgctxt "pli-tv-pvr1.1:38.1"
msgid "Visārado kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:38.2"
msgid "thero ca dīghanāmako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:38.3"
msgid "Dīghasumano ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc33
msgctxt "pli-tv-pvr1.1:39.1"
msgid "Punadeva kāḷasumano,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:39.2"
msgid "Nāgatthero ca buddharakkhito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:39.3"
msgid "Tissatthero ca medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:39.4"
msgid "Devatthero ca paṇḍito."
msgstr ""

#. HTML: </p><p>
#. REF: sc34
msgctxt "pli-tv-pvr1.1:40.1"
msgid "Punadeva sumano medhāvī,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:40.2"
msgid "Vinaye ca visārado;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:40.3"
msgid "Bahussuto cūḷanāgo,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:40.4"
msgid "Gajova duppadhaṃsiyo."
msgstr ""

#. HTML: </p><p>
#. REF: sc35
msgctxt "pli-tv-pvr1.1:41.1"
msgid "Dhammapālitanāmo ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:41.2"
msgid "rohaṇe sādhupūjito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:41.3"
msgid "Tassa sisso mahāpañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.1:41.4"
msgid "khemanāmo tipeṭako."
msgstr ""

#. HTML: </p><p>
#. REF: sc36
msgctxt "pli-tv-pvr1.1:42.1"
msgid "Dīpe tārakarājāva,"
msgstr ""

#. HTML: <br>
