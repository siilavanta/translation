msgctxt "pli-tv-pvr1.1:96.2"
msgid "Sāvatthiyaṃ paññattaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.1:96.3"
msgid "Kaṃ ārabbhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.1:96.4"
msgid "Āyasmantaṃ upanandaṃ sakyaputtaṃ ārabbha."
msgstr ""

msgctxt "pli-tv-pvr1.1:96.5"
